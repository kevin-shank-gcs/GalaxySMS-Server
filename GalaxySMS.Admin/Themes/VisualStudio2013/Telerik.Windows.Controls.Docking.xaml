            </VisualStateManager.VisualStateGroups>
            <Border x:Name="MouseOverVisual" Background="{telerik:VisualStudio2013Resource ResourceKey=SemiBasicBrush}" Opacity="0"/>
            <Border x:Name="PressedVisual" Background="{telerik:VisualStudio2013Resource ResourceKey=AccentMainBrush}" Opacity="0"/>
            <Path x:Name="PathVisual"
                    Data="M0,2 L6,2 L3,6 z M0,0 L6,0 L6,1 L0,1 z"
                    Fill="{TemplateBinding Foreground}"
                    Stretch="None"
                    HorizontalAlignment="Center"
                    VerticalAlignment="Center"
                    SnapsToDevicePixels="True"/>
            <Popup x:Name="DropDownPopup" AllowsTransparency="True" StaysOpen="False" Focusable="False" Placement="Bottom">
                <Grid>
                    <Border x:Name="DropDownPopupBorder"
