                <VisualStateGroup x:Name="HighlightStates">
                    <VisualState x:Name="NotHighlighted"/>
                    <VisualState x:Name="Highlighted">
                        <Storyboard>
                            <ObjectAnimationUsingKeyFrames Storyboard.TargetName="Content" Storyboard.TargetProperty="Background">
                                <DiscreteObjectKeyFrame KeyTime="0:0:0" Value="{telerik:VisualStudio2013Resource ResourceKey=HeaderBrush}"/>
                            </ObjectAnimationUsingKeyFrames>
                            <ObjectAnimationUsingKeyFrames Storyboard.TargetName="TitleElement" Storyboard.TargetProperty="Foreground">
                                <DiscreteObjectKeyFrame KeyTime="0:0:0" Value="{telerik:VisualStudio2013Resource ResourceKey=SelectedBrush}"/>
                            </ObjectAnimationUsingKeyFrames>
                            <ObjectAnimationUsingKeyFrames Storyboard.TargetName="PanePatternTop" Storyboard.TargetProperty="Stroke">
                                <DiscreteObjectKeyFrame KeyTime="0:0:0" Value="{telerik:VisualStudio2013Resource ResourceKey=SelectedBrush}"/>
                            </ObjectAnimationUsingKeyFrames>
                            <ObjectAnimationUsingKeyFrames Storyboard.TargetName="PanePatternCenter" Storyboard.TargetProperty="Stroke">
                                <DiscreteObjectKeyFrame KeyTime="0:0:0" Value="{telerik:VisualStudio2013Resource ResourceKey=SelectedBrush}"/>
                            </ObjectAnimationUsingKeyFrames>
                            <ObjectAnimationUsingKeyFrames Storyboard.TargetName="PanePatternBottom" Storyboard.TargetProperty="Stroke">
                                <DiscreteObjectKeyFrame KeyTime="0:0:0" Value="{telerik:VisualStudio2013Resource ResourceKey=SelectedBrush}"/>
                            </ObjectAnimationUsingKeyFrames>
                            <ObjectAnimationUsingKeyFrames Storyboard.TargetName="HeaderDropDownMenu" Storyboard.TargetProperty="Foreground">
                                <DiscreteObjectKeyFrame KeyTime="0:0:0" Value="{telerik:VisualStudio2013Resource ResourceKey=SelectedBrush}"/>
                            </ObjectAnimationUsingKeyFrames>
                            <ObjectAnimationUsingKeyFrames Storyboard.TargetName="HeaderUnPinButton" Storyboard.TargetProperty="Foreground">
                                <DiscreteObjectKeyFrame KeyTime="0:0:0" Value="{telerik:VisualStudio2013Resource ResourceKey=SelectedBrush}"/>
                            </ObjectAnimationUsingKeyFrames>
                            <ObjectAnimationUsingKeyFrames Storyboard.TargetName="HeaderPinButton" Storyboard.TargetProperty="Foreground">
                                <DiscreteObjectKeyFrame KeyTime="0:0:0" Value="{telerik:VisualStudio2013Resource ResourceKey=SelectedBrush}"/>
                            </ObjectAnimationUsingKeyFrames>
                            <ObjectAnimationUsingKeyFrames Storyboard.TargetName="HeaderCloseButton" Storyboard.TargetProperty="Foreground">
                                <DiscreteObjectKeyFrame KeyTime="0:0:0" Value="{telerik:VisualStudio2013Resource ResourceKey=SelectedBrush}"/>
                            </ObjectAnimationUsingKeyFrames>
                        </Storyboard>
                    </VisualState>
                </VisualStateGroup>
            </VisualStateManager.VisualStateGroups>
            <Grid x:Name="Content">
                <Grid.ColumnDefinitions>
                    <ColumnDefinition Width="*"/>
                    <ColumnDefinition Width="Auto"/>
                    <ColumnDefinition Width="Auto"/>
                    <ColumnDefinition Width="Auto"/>
                    <ColumnDefinition Width="2"/>
                </Grid.ColumnDefinitions>
                <Grid Margin="0 0 3 0">
                    <Grid.ColumnDefinitions>
                        <ColumnDefinition Width="Auto"/>
                        <ColumnDefinition Width="*"/>
                    </Grid.ColumnDefinitions>
                    <ContentControl x:Name="TitleElement"
                            VerticalContentAlignment="{TemplateBinding VerticalAlignment}"
                            FontFamily="{TemplateBinding FontFamily}"
                            FontSize="{TemplateBinding FontSize}"
                            Foreground="{TemplateBinding Foreground}"
                            Margin="{TemplateBinding Padding}"
                            Content="{Binding SelectedPane.Title, RelativeSource={RelativeSource TemplatedParent}}"
                            ContentTemplate="{Binding SelectedPane.TitleTemplate, RelativeSource={RelativeSource TemplatedParent}}"
                            HorizontalContentAlignment="Stretch"/>
                    <Grid Height="7" Grid.Column="1" Margin="8 0" UseLayoutRounding="True" SnapsToDevicePixels="True">
                        <Path x:Name="PanePatternTop"
                                Data="M0,1 L1500,1"
                                Height="1"
                                Stretch="None"
                                Stroke="{telerik:VisualStudio2013Resource ResourceKey=StrongBrush}"
                                VerticalAlignment="Top"
                                StrokeDashArray="1 2"
                                StrokeThickness="1"
                                StrokeDashCap="Flat"/>
                        <Path x:Name="PanePatternCenter"
                                Data="M0,1 L1500,1"
                                Height="1"
                                Stretch="None"
                                Stroke="{telerik:VisualStudio2013Resource ResourceKey=StrongBrush}"
                                VerticalAlignment="Center"
                                StrokeDashArray="1 2"
                                StrokeThickness="1"
                                StrokeDashCap="Flat"
                                Margin="2 0 0 0"/>
                        <Path x:Name="PanePatternBottom"
                                Data="M0,1 L1500,1"
                                Height="1"
                                Stretch="None"
                                Stroke="{telerik:VisualStudio2013Resource ResourceKey=StrongBrush}"
                                VerticalAlignment="Bottom"
                                StrokeDashArray="1 2"
                                StrokeThickness="1"
                                StrokeDashCap="Flat"/>
                    </Grid>
                </Grid>
                <telerik:RadDropDownButton x:Name="HeaderDropDownMenu"
                        Grid.Column="1"
                        Command="local:RadDockingCommands.ContextMenuOpen"
                        Style="{StaticResource DropDownButtonDropDownStyle}"
                        CommandParameter="{TemplateBinding SelectedPane}"/>
                <Grid Grid.Column="2" Visibility="{Binding SelectedPane.PinButtonVisibility, RelativeSource={RelativeSource TemplatedParent}}" MinWidth="16">
                    <telerik:RadButton x:Name="HeaderPinButton" Command="local:RadDockingCommands.Pin" CommandParameter="{TemplateBinding SelectedPane}" Style="{StaticResource RadButtonPinStyle}"/>
                    <telerik:RadButton x:Name="HeaderUnPinButton"
                            Visibility="Collapsed"
                            Command="local:RadDockingCommands.Pin"
                            CommandParameter="{TemplateBinding SelectedPane}"
                            Style="{StaticResource RadButtonUnpinStyle}"/>
                </Grid>
                <telerik:RadButton x:Name="HeaderCloseButton"
                        Grid.Column="3"
                        Command="local:RadDockingCommands.Close"
                        CommandParameter="{TemplateBinding SelectedPane}"
                        Style="{StaticResource RadButtonCloseStyle}"/>
            </Grid>
        </Border>
    </ControlTemplate>
