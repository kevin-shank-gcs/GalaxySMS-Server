                        <RowDefinition Height="Auto" MinHeight="22"/>
                        <RowDefinition Height="*"/>
                    </Grid.RowDefinitions>
                    <Grid.ColumnDefinitions>
                        <ColumnDefinition Width="*"/>
                        <ColumnDefinition Width="Auto"/>
                    </Grid.ColumnDefinitions>
                    <Rectangle x:Name="HeaderVisual" Grid.ColumnSpan="3" Fill="{telerik:VisualStudio2013Resource ResourceKey=PrimaryBrush}"/>
                    <Grid x:Name="HeaderElement" Background="Transparent">
                        <Grid.ColumnDefinitions>
                            <ColumnDefinition Width="Auto"/>
                            <ColumnDefinition Width="*"/>
                        </Grid.ColumnDefinitions>
                        <Grid.InputBindings>
                            <MouseBinding Command="local:RadDockingCommands.PaneHeaderMenuOpen">
                                <MouseBinding.Gesture>
                                    <MouseGesture MouseAction="RightClick"/>
                                </MouseBinding.Gesture>
                            </MouseBinding>
                        </Grid.InputBindings>
                        <ContentControl x:Name="Header"
                                Margin="6 0"
                                FontFamily="{TemplateBinding FontFamily}"
                                FontSize="{TemplateBinding FontSize}"
                                VerticalAlignment="{TemplateBinding VerticalContentAlignment}"
                                Content="{TemplateBinding Header}"
                                ContentTemplate="{TemplateBinding HeaderTemplate}"
                                HorizontalAlignment="{TemplateBinding HorizontalContentAlignment}"
                                IsTabStop="False"/>
                        <Grid Height="7" Grid.Column="1" Margin="8 0" UseLayoutRounding="True" SnapsToDevicePixels="True">
                            <Path x:Name="PanePatternTop"
                                    Data="M0,1 L1500,1"
                                    Height="1"
                                    Stretch="None"
                                    Stroke="{telerik:VisualStudio2013Resource ResourceKey=StrongBrush}"
                                    VerticalAlignment="Top"
                                    StrokeDashArray="1 2"
                                    StrokeThickness="1"
                                    StrokeDashCap="Flat"/>
                            <Path x:Name="PanePatternCenter"
                                    Data="M0,1 L1500,1"
                                    Height="1"
                                    Stretch="None"
                                    Stroke="{telerik:VisualStudio2013Resource ResourceKey=StrongBrush}"
                                    VerticalAlignment="Center"
                                    StrokeDashArray="1 2"
                                    StrokeThickness="1"
                                    StrokeDashCap="Flat"
                                    Margin="2 0 0 0"/>
                            <Path x:Name="PanePatternBottom"
                                    Data="M0,1 L1500,1"
                                    Height="1"
                                    Stretch="None"
                                    Stroke="{telerik:VisualStudio2013Resource ResourceKey=StrongBrush}"
                                    VerticalAlignment="Bottom"
                                    StrokeDashArray="1 2"
                                    StrokeThickness="1"
                                    StrokeDashCap="Flat"/>
                        </Grid>
                    </Grid>
                    <telerik:RadButton x:Name="CloseButton" Grid.Column="2" Command="{x:Static telerik:WindowCommands.Close}" Style="{StaticResource RadButtonCloseStyle}" Margin="0 0 4 0"/>
                    <ContentPresenter x:Name="Content" Margin="{TemplateBinding Padding}" Grid.Row="1" Grid.ColumnSpan="3" Content="{TemplateBinding Content}"/>
                </Grid>
            </Border>
        </Grid>
