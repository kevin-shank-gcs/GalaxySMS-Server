                        <dock:RadGridResizer x:Name="Resizer" Grid.Row="0" Height="4" Placement="Top" HorizontalAlignment="Stretch" VerticalAlignment="Top" Background="#01010101" Cursor="SizeNS"/>
                        <ContentControl x:Name="ContentBackground"
