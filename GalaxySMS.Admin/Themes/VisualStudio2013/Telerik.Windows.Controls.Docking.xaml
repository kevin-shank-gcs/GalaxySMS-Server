                                <ContentPresenter x:Name="ContentElement" Grid.Row="1" ContentTemplate="{TemplateBinding SelectedContentTemplate}"/>
                            </Grid>
                        </ContentControl>
                    </Grid>
                </Grid>
            </Popup>
        </Grid>
    </ControlTemplate>
