                    <dock:AutoHideArea x:Name="RightAutoHide" telerik:RadDockPanel.Dock="Right" TabStripPlacement="Right" BackgroundVisibility="Collapsed" TabOrientation="Vertical"/>
                    <dock:AutoHideArea x:Name="TopAutoHide" telerik:RadDockPanel.Dock="Top" TabStripPlacement="Top" BackgroundVisibility="Collapsed" TabOrientation="Horizontal"/>
