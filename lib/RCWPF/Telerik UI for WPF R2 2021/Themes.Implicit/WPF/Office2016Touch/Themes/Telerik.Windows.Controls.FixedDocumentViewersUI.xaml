    <Style x:Key="DropDownButtonStyle" TargetType="ToggleButton">
        <Setter Property="Foreground" Value="{telerik1:Office2016TouchResource ResourceKey=MarkerInvertedBrush}"/>
        <Setter Property="Background" Value="Transparent"/>
        <Setter Property="BorderBrush" Value="Transparent"/>
        <Setter Property="BorderThickness" Value="1"/>
        <Setter Property="Margin" Value="2 2 5 2"/>
        <Setter Property="Padding" Value="6"/>
        <Setter Property="Content" Value=""/>
        <Setter Property="telerik:RadDockPanel.Dock" Value="Right"/>
        <Setter Property="IsTabStop" Value="False"/>
        <Setter Property="Template">
            <Setter.Value>
                <ControlTemplate TargetType="ToggleButton">
                    <Grid Background="#01FFFFFF">
                        <Border x:Name="BorderVisual"
                                BorderThickness="{TemplateBinding BorderThickness}"
                                BorderBrush="{TemplateBinding BorderBrush}"
                                Background="{TemplateBinding Background}"
                                CornerRadius="{telerik1:Office2016TouchResource ResourceKey=CornerRadius}">
                            <TextBlock
                                    Text="{StaticResource GlyphArrowOverflowDown}"
                                    FontSize="16"
                                    Foreground="{TemplateBinding Foreground}"
                                    FontFamily="{StaticResource TelerikWebUI}"
                                    FontWeight="Normal"
                                    FontStyle="Normal"
                                    HorizontalAlignment="Center"
                                    VerticalAlignment="Center"
                                    Margin="{TemplateBinding Padding}"
                                    telerik:GlyphAdorner.ShowGlyphInDesignTime="True"/>
                        </Border>
                    </Grid>
                    <ControlTemplate.Triggers>
                        <Trigger Property="IsMouseOver" Value="True">
                            <Setter TargetName="BorderVisual" Property="BorderBrush" Value="{telerik1:Office2016TouchResource ResourceKey=AccentMouseOverBrush}"/>
                            <Setter TargetName="BorderVisual" Property="Background" Value="{telerik1:Office2016TouchResource ResourceKey=AccentMouseOverBrush}"/>
                        </Trigger>
                        <Trigger Property="IsPressed" Value="True">
                            <Setter TargetName="BorderVisual" Property="BorderBrush" Value="{telerik1:Office2016TouchResource ResourceKey=AccentPressedBrush}"/>
                            <Setter TargetName="BorderVisual" Property="Background" Value="{telerik1:Office2016TouchResource ResourceKey=AccentPressedBrush}"/>
                        </Trigger>
                        <Trigger Property="IsChecked" Value="True">
                            <Setter TargetName="BorderVisual" Property="BorderBrush" Value="{telerik1:Office2016TouchResource ResourceKey=AccentPressedBrush}"/>
                            <Setter TargetName="BorderVisual" Property="Background" Value="{telerik1:Office2016TouchResource ResourceKey=AccentPressedBrush}"/>
