                            <Setter TargetName="BorderVisual" Property="BorderBrush" Value="{telerik1:Office2016TouchResource ResourceKey=AccentMouseOverBrush}"/>
                            <Setter Property="Foreground" Value="{telerik1:Office2016TouchResource ResourceKey=MarkerInvertedBrush}"/>
                            <Setter TargetName="arrow" Property="Foreground" Value="{telerik1:Office2016TouchResource ResourceKey=MarkerInvertedBrush}"/>
                        </Trigger>
                        <Trigger Property="IsPressed" Value="True">
                            <Setter TargetName="FocusVisual" Property="Opacity" Value="0"/>
                            <Setter TargetName="BorderVisual" Property="Background" Value="{telerik1:Office2016TouchResource ResourceKey=AccentPressedBrush}"/>
                            <Setter TargetName="BorderVisual" Property="BorderBrush" Value="{telerik1:Office2016TouchResource ResourceKey=AccentPressedBrush}"/>
                            <Setter Property="Foreground" Value="{telerik1:Office2016TouchResource ResourceKey=MarkerInvertedBrush}"/>
                            <Setter TargetName="arrow" Property="Foreground" Value="{telerik1:Office2016TouchResource ResourceKey=MarkerInvertedBrush}"/>
                        </Trigger>
                        <Trigger Property="IsOpen" Value="True">
                            <Setter Property="Foreground" Value="{telerik1:Office2016TouchResource ResourceKey=MarkerInvertedBrush}"/>
                            <Setter TargetName="arrow" Property="Foreground" Value="{telerik1:Office2016TouchResource ResourceKey=MarkerInvertedBrush}"/>
                            <Setter TargetName="BorderVisual" Property="BorderBrush" Value="{telerik1:Office2016TouchResource ResourceKey=AccentPressedBrush}"/>
                            <Setter TargetName="BorderVisual" Property="Background" Value="{telerik1:Office2016TouchResource ResourceKey=AccentPressedBrush}"/>
                        </Trigger>

                        <!-- disabled -->
