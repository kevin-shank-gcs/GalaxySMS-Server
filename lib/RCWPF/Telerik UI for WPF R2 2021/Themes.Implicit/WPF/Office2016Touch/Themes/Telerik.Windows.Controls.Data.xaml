                            Margin="{TemplateBinding Padding}"
                            HorizontalAlignment="{TemplateBinding HorizontalContentAlignment}"
                            VerticalAlignment="{TemplateBinding VerticalContentAlignment}"/>
                </ControlTemplate>
            </Setter.Value>
        </Setter>
    </Style>
    <DataTemplate x:Key="FilterActionTemplate">
        <TextBlock Text="{Binding Converter={StaticResource CardViewFilterOperatorConverter}}"/>
    </DataTemplate>
    <DataTemplate x:Key="FilterLogicalOperatorTemplate">
        <TextBlock Text="{Binding Converter={StaticResource FilterCompositionLogicalOperatorConverter}}"/>
    </DataTemplate>
    <ControlTemplate x:Key="CardViewFilteringControlTemplate" TargetType="cardView:FilteringControl">
        <Border
                BorderThickness="{TemplateBinding BorderThickness}"
                Background="{TemplateBinding Background}"
                Margin="{TemplateBinding Margin}"
                CornerRadius="{Binding RelativeSource={RelativeSource TemplatedParent}, Path=(helpers:ThemeHelper.CornerRadius)}"
                BorderBrush="{TemplateBinding BorderBrush}">
            <Grid>
                <StackPanel Margin="{TemplateBinding Padding}" VerticalAlignment="{TemplateBinding VerticalContentAlignment}" HorizontalAlignment="{TemplateBinding HorizontalContentAlignment}">
                    <StackPanel x:Name="PART_DistinctFilter" Visibility="{TemplateBinding DistinctFiltersVisibility}">
                        <CheckBox x:Name="PART_SelectAllCheckBox"
                                AutomationProperties.Name="select all checkbox"
                                IsChecked="{Binding SelectAll, Mode=TwoWay}"
                                Margin="0 4 0 0"
                                telerik:LocalizationManager.ResourceKey="CardViewFilterSelectAll"/>
                        <ListBox x:Name="PART_DistinctValuesList"
                                AutomationProperties.Name="distinct values list"
                                Margin="0 4 0 0"
                                ItemsSource="{Binding DistinctValues}"
                                ScrollViewer.HorizontalScrollBarVisibility="Auto"
                                SelectionMode="Multiple"
                                ItemContainerStyle="{StaticResource FilterDistinctValueListBoxItemStyle}">
                            <ListBox.ItemsPanel>
                                <ItemsPanelTemplate>
                                    <VirtualizingStackPanel MaxHeight="242"/>
                                </ItemsPanelTemplate>
                            </ListBox.ItemsPanel>
                            <ListBox.ItemTemplate>
                                <DataTemplate>
                                    <CheckBox IsChecked="{Binding IsActive, Mode=TwoWay}" Content="{Binding ConvertedValue, Converter={StaticResource DistinctValueConverter}}" VerticalAlignment="Center"/>
                                </DataTemplate>
                            </ListBox.ItemTemplate>
                        </ListBox>
                    </StackPanel>
                    <StackPanel Margin="0 2" Visibility="{TemplateBinding FieldFiltersVisibility}">
                        <TextBlock telerik:LocalizationManager.ResourceKey="CardViewFilterShowRowsWithValueThat" Margin="0 2 0 0"/>
                        <input:RadComboBox x:Name="PART_Filter1ComboBox"
                                MinHeight="{StaticResource FilteringControlComboBoxMinHeight}"
                                AutomationProperties.Name="first filter combobox"
                                Margin="0 4 0 0"
                                ItemTemplate="{StaticResource FilterActionTemplate}"
                                ItemsSource="{Binding AvailableActions}"
                                SelectedItem="{Binding Filter1.Operator, Mode=TwoWay}"/>
                        <ContentControl x:Name="PART_Filter1ContentControl"
                                DataContext="{Binding Filter1}"
                                Margin="0 4 0 0"
                                IsEnabled="{Binding Operator, Converter={StaticResource FilterOperatorToFilterEditorIsEnabledConverter}}"
                                HorizontalContentAlignment="Stretch"
                                VerticalContentAlignment="Stretch"/>
                        <input:RadComboBox x:Name="PART_LogicalOperatorsComboBox"
                                AutomationProperties.Name="logical operators combobox"
                                Margin="0 4 0 0"
                                ItemTemplate="{StaticResource FilterLogicalOperatorTemplate}"
                                ItemsSource="{Binding LogicalOperators}"
                                SelectedItem="{Binding FieldFilterLogicalOperator, Mode=TwoWay}"/>
                        <input:RadComboBox x:Name="PART_Filter2ComboBox"
                                AutomationProperties.Name="second filter combobox"
                                Margin="0 4 0 0"
                                ItemTemplate="{StaticResource FilterActionTemplate}"
                                ItemsSource="{Binding AvailableActions}"
                                SelectedItem="{Binding Filter2.Operator, Mode=TwoWay}"/>
                        <ContentControl x:Name="PART_Filter2ContentControl"
                                DataContext="{Binding Filter2}"
                                Margin="0 4 0 0"
                                IsEnabled="{Binding Operator, Converter={StaticResource FilterOperatorToFilterEditorIsEnabledConverter}}"
                                HorizontalContentAlignment="Stretch"
                                VerticalContentAlignment="Stretch"/>
                    </StackPanel>
                    <Grid>
                        <Grid.ColumnDefinitions>
                            <ColumnDefinition/>
                            <ColumnDefinition/>
                        </Grid.ColumnDefinitions>
                        <Button x:Name="PART_ApplyFilterButton"
                                Grid.Column="0"
                                MinWidth="89"
                                Margin="0 4 4 0"
                                MinHeight="22"
                                Visibility="{TemplateBinding FilterButtonVisibility}"
                                telerik:LocalizationManager.ResourceKey="CardViewFilter"
                                VerticalAlignment="Center"/>
                        <Button x:Name="PART_ClearFilterButton"
                                Grid.Column="1"
                                Margin="0 4 0 0"
                                MinHeight="22"
                                telerik:LocalizationManager.ResourceKey="CardViewClearFilter"
                                VerticalAlignment="Center"/>
                    </Grid>
                </StackPanel>
                <telerik:RadButton x:Name="PART_FilterCloseButton"
                        HorizontalAlignment="Right"
                        VerticalAlignment="Top"
                        Margin="{TemplateBinding Padding}"
                        BorderThickness="0"
                        Padding="0"
                        MinHeight="0"
                        Height="16"
                        Width="16">
                    <telerik:RadGlyph x:Name="GlyphClose" Glyph="{StaticResource GlyphClose}" FontSize="16" Foreground="{telerik:Office2016TouchResource ResourceKey=IconBrush}"/>
                </telerik:RadButton>
            </Grid>
        </Border>
        <ControlTemplate.Triggers>
            <Trigger SourceName="PART_FilterCloseButton" Property="IsMouseOver" Value="True">
                <Setter TargetName="GlyphClose" Property="Foreground" Value="{telerik:Office2016TouchResource ResourceKey=SelectedBrush}"/>
            </Trigger>
            <Trigger SourceName="PART_FilterCloseButton" Property="IsPressed" Value="True">
                <Setter TargetName="GlyphClose" Property="Foreground" Value="{telerik:Office2016TouchResource ResourceKey=SelectedBrush}"/>
            </Trigger>
        </ControlTemplate.Triggers>
    </ControlTemplate>
    <Style x:Key="CardViewFilteringControlStyle" TargetType="cardView:FilteringControl">
        <Setter Property="Template" Value="{StaticResource CardViewFilteringControlTemplate}"/>
        <Setter Property="Background" Value="{telerik:Office2016TouchResource ResourceKey=MainBrush}"/>
        <Setter Property="BorderBrush" Value="{telerik:Office2016TouchResource ResourceKey=BasicBrush}"/>
        <Setter Property="Foreground" Value="{telerik:Office2016TouchResource ResourceKey=MarkerBrush}"/>
        <Setter Property="Padding" Value="4"/>
        <Setter Property="MinWidth" Value="230"/>
        <Setter Property="Margin" Value="0 0 0 0"/>
        <Setter Property="BorderThickness" Value="1"/>
        <Setter Property="VerticalContentAlignment" Value="Stretch"/>
        <Setter Property="HorizontalContentAlignment" Value="Stretch"/>
        <Setter Property="SnapsToDevicePixels" Value="True"/>
        <Setter Property="helpers:ThemeHelper.CornerRadius" Value="1"/>
    </Style>
    <Style TargetType="cardView:FilteringControl" BasedOn="{StaticResource CardViewFilteringControlStyle}"/>
    <ControlTemplate x:Key="CardViewPanelItemDropDownButtonTemplate" TargetType="telerik:RadDropDownButton">
        <Grid SnapsToDevicePixels="True">
            <Border x:Name="BorderVisual"
                    SnapsToDevicePixels="{TemplateBinding SnapsToDevicePixels}"
                    BorderThickness="{TemplateBinding BorderThickness}"
                    BorderBrush="{TemplateBinding BorderBrush}"
                    Background="{TemplateBinding Background}"
                    CornerRadius="{TemplateBinding CornerRadius}"/>
            <Border x:Name="MouseOverVisual"
                    Opacity="0"
                    Background="{Binding RelativeSource={RelativeSource TemplatedParent}, Path=(helpers:ThemeHelper.MouseOverBrush)}"
                    CornerRadius="{TemplateBinding CornerRadius}"
                    BorderThickness="{TemplateBinding BorderThickness}"
                    BorderBrush="{telerik:Office2016TouchResource ResourceKey=AccentMouseOverBrush}"/>
            <Border x:Name="PressedVisual"
                    Opacity="0"
                    Background="{Binding RelativeSource={RelativeSource TemplatedParent}, Path=(helpers:ThemeHelper.PressedBrush)}"
                    CornerRadius="{TemplateBinding CornerRadius}"
                    BorderBrush="{telerik:Office2016TouchResource ResourceKey=AccentMouseOverBrush}"
                    BorderThickness="{TemplateBinding BorderThickness}"/>
            <Border x:Name="CheckedVisual"
                    Opacity="0"
                    BorderThickness="{TemplateBinding BorderThickness}"
                    BorderBrush="{telerik:Office2016TouchResource ResourceKey=AccentFocusedBrush}"
                    Background="{Binding RelativeSource={RelativeSource TemplatedParent}, Path=(helpers:ThemeHelper.CheckedBrush)}"
                    CornerRadius="{TemplateBinding CornerRadius}"/>
            <Grid>
                <Grid.RowDefinitions>
                    <RowDefinition Height="Auto"/>
                    <RowDefinition Height="*"/>
                    <RowDefinition Height="Auto"/>
                </Grid.RowDefinitions>
                <Grid.ColumnDefinitions>
                    <ColumnDefinition Width="Auto"/>
                    <ColumnDefinition Width="*"/>
                    <ColumnDefinition Width="Auto"/>
                </Grid.ColumnDefinitions>
                <ContentPresenter x:Name="Content"
                        Grid.Row="1"
                        Grid.Column="1"
                        TextBlock.Foreground="{TemplateBinding Foreground}"
                        Margin="{TemplateBinding Padding}"
                        Content="{TemplateBinding Content}"
                        ContentTemplate="{TemplateBinding ContentTemplate}"
                        VerticalAlignment="{TemplateBinding VerticalContentAlignment}"
                        HorizontalAlignment="{TemplateBinding HorizontalContentAlignment}"
                        ContentTemplateSelector="{TemplateBinding ContentTemplateSelector}"
                        ContentStringFormat="{TemplateBinding ContentStringFormat}"
                        RecognizesAccessKey="True"/>
                <Grid x:Name="DropDownIndicator"
                        Grid.Column="1"
                        MinWidth="22"
                        Grid.Row="1"
                        Canvas.ZIndex="2"
                        HorizontalAlignment="Right"
                        IsHitTestVisible="False"
                        Visibility="{TemplateBinding DropDownIndicatorVisibility}">
                    <telerik:RadGlyph x:Name="ArrowGlyph" FontSize="12" Foreground="{TemplateBinding Foreground}" Glyph="{StaticResource GlyphArrowChevronDown}"/>
                </Grid>
            </Grid>
            <Popup x:Name="DropDownPopup" SnapsToDevicePixels="True" AllowsTransparency="True" Focusable="False" StaysOpen="False" Placement="Bottom">
                <Grid x:Name="PopupGrid">
                    <Border x:Name="DropDownPopupBorder"
                            BorderBrush="{telerik:Office2016TouchResource ResourceKey=BasicBrush}"
                            Background="{telerik:Office2016TouchResource ResourceKey=MainBrush}"
                            BorderThickness="1"
                            Visibility="{Binding DropDownContent,RelativeSource={RelativeSource TemplatedParent},Converter={StaticResource NullToVisibilityConverter}}"
                            CornerRadius="{TemplateBinding CornerRadius}"
                            Width="{TemplateBinding DropDownWidth}"
                            Height="{TemplateBinding DropDownHeight}"
                            MinWidth="{TemplateBinding DropDownMinWidth}"
                            MinHeight="{TemplateBinding DropDownMinHeight}"
                            MaxWidth="{TemplateBinding DropDownMaxWidth}"
                            MaxHeight="{TemplateBinding DropDownMaxHeight}"
                            SnapsToDevicePixels="True">
                        <ContentControl x:Name="DropDownPopupContent"
                                Content="{TemplateBinding DropDownContent}"
                                HorizontalContentAlignment="Stretch"
                                VerticalContentAlignment="Stretch"
                                ContentTemplate="{TemplateBinding DropDownContentTemplate}"
                                HorizontalAlignment="Stretch"
                                VerticalAlignment="Stretch"
                                DataContext="{Binding RelativeSource={RelativeSource TemplatedParent}, Path=DataContext}"/>
                    </Border>
                </Grid>
            </Popup>
            <Border x:Name="FocusVisual"
                    Opacity="0"
                    BorderThickness="1"
                    CornerRadius="{TemplateBinding CornerRadius}"
                    BorderBrush="{telerik:Office2016TouchResource ResourceKey=AccentFocusedBrush}"/>
        </Grid>
        <ControlTemplate.Triggers>

            <!-- General IsKeyboardFocused setters -->
            <MultiTrigger>
                <MultiTrigger.Conditions>
                    <Condition Property="IsEnabled" Value="True"/>
                    <Condition Property="IsKeyboardFocused" Value="True"/>
                    <Condition Property="IsOpen" Value="False"/>
                </MultiTrigger.Conditions>
                <Setter TargetName="FocusVisual" Property="Opacity" Value="1"/>
            </MultiTrigger>

            <!-- General setters for IsMouseOver -->
            <Trigger Property="IsMouseOver" Value="True">
                <Setter TargetName="MouseOverVisual" Property="Opacity" Value="1"/>
            </Trigger>

            <!-- General setters for IsPressed -->
            <MultiTrigger>
                <MultiTrigger.Conditions>
                    <Condition Property="IsOpen" Value="False"/>
                    <Condition Property="IsPressed" Value="True"/>
                </MultiTrigger.Conditions>
                <Setter TargetName="PressedVisual" Property="Opacity" Value="1"/>
            </MultiTrigger>

            <!-- General setters for IsOpen -->
            <Trigger Property="IsOpen" Value="True">
                <Setter Property="Foreground" Value="{telerik:Office2016TouchResource ResourceKey=MarkerInvertedBrush}"/>
                <Setter TargetName="CheckedVisual" Property="Opacity" Value="1"/>
            </Trigger>

            <!-- Disabled -->
            <Trigger Property="IsEnabled" Value="False">
                <Setter TargetName="Content" Property="Opacity" Value="{telerik:Office2016TouchResource ResourceKey=DisabledOpacity}"/>
                <Setter TargetName="DropDownIndicator" Property="Opacity" Value="{telerik:Office2016TouchResource ResourceKey=DisabledOpacity}"/>
                <Setter TargetName="BorderVisual" Property="Background" Value="{Binding RelativeSource={RelativeSource TemplatedParent}, Path=(helpers:ThemeHelper.DisabledBrush)}"/>
            </Trigger>
        </ControlTemplate.Triggers>
    </ControlTemplate>
    <Style x:Key="CardViewPanelItemDropDownButtonStyle" TargetType="telerik:RadDropDownButton">
        <Setter Property="helpers:ThemeHelper.MouseOverBrush" Value="{telerik:Office2016TouchResource ResourceKey=AccentMouseOverBrush}"/>
        <Setter Property="helpers:ThemeHelper.FocusBrush" Value="Transparent"/>
        <Setter Property="helpers:ThemeHelper.PressedBrush" Value="{telerik:Office2016TouchResource ResourceKey=AccentMouseOverBrush}"/>
        <Setter Property="helpers:ThemeHelper.CheckedBrush" Value="{telerik:Office2016TouchResource ResourceKey=AccentFocusedBrush}"/>
        <Setter Property="helpers:ThemeHelper.DisabledBrush" Value="{telerik:Office2016TouchResource ResourceKey=AccentPressedBrush}"/>
        <Setter Property="IsOpen" Value="False"/>
        <Setter Property="BorderBrush" Value="{telerik:Office2016TouchResource ResourceKey=AccentBrush}"/>
        <Setter Property="Background" Value="{telerik:Office2016TouchResource ResourceKey=AccentPressedBrush}"/>
        <Setter Property="Foreground" Value="{telerik:Office2016TouchResource ResourceKey=MarkerInvertedBrush}"/>
        <Setter Property="CornerRadius" Value="0"/>
        <Setter Property="HorizontalContentAlignment" Value="Left"/>
        <Setter Property="VerticalContentAlignment" Value="Center"/>
        <Setter Property="VerticalAlignment" Value="Center"/>
        <Setter Property="DropDownIndicatorVisibility" Value="Visible"/>
        <Setter Property="FocusVisualStyle" Value="{x:Null}"/>
        <Setter Property="BorderThickness" Value="1"/>
        <Setter Property="Padding" Value="6 2"/>
        <Setter Property="MinWidth" Value="68"/>
        <Setter Property="MinHeight" Value="30"/>
        <Setter Property="SnapsToDevicePixels" Value="True"/>
        <Setter Property="FlowDirection" Value="LeftToRight"/>
        <Setter Property="DropDownButtonPosition" Value="Left"/>
        <Setter Property="Template" Value="{StaticResource CardViewPanelItemDropDownButtonTemplate}"/>
    </Style>
    <ControlTemplate x:Key="RadCardViewItemTemplate" TargetType="controls:RadCardViewItem">
        <telerik:RadExpander
                Header="{TemplateBinding Header}"
                FontSize="{TemplateBinding FontSize}"
                FontFamily="{TemplateBinding FontFamily}"
                BorderBrush="{TemplateBinding BorderBrush}"
                Background="{TemplateBinding Background}"
                BorderThickness="{TemplateBinding BorderThickness}"
                HeaderTemplate="{TemplateBinding HeaderTemplate}"
                Style="{StaticResource CardViewItemExpanderStyle}"
                IsExpanded="{Binding IsExpanded, RelativeSource={RelativeSource Mode=TemplatedParent}, Mode=TwoWay}">
            <cardView:CardViewDataForm x:Name="PART_CardViewDataForm" BorderThickness="0" Background="Transparent" BorderBrush="Transparent" CurrentItem="{Binding Data}"/>
        </telerik:RadExpander>
    </ControlTemplate>

    <!-- RadCardViewItem Style -->
    <Style x:Key="RadCardViewItemStyle" TargetType="controls:RadCardViewItem">
        <Setter Property="Background" Value="{telerik:Office2016TouchResource ResourceKey=MainBrush}"/>
        <Setter Property="BorderBrush" Value="{telerik:Office2016TouchResource ResourceKey=BasicBrush}"/>
        <Setter Property="Foreground" Value="{telerik:Office2016TouchResource ResourceKey=MarkerBrush}"/>
        <Setter Property="BorderThickness" Value="1"/>
        <Setter Property="SnapsToDevicePixels" Value="True"/>
        <Setter Property="FontFamily" Value="{telerik:Office2016TouchResource ResourceKey=FontFamily}"/>
        <Setter Property="FontSize" Value="{telerik:Office2016TouchResource ResourceKey=FontSizeL}"/>
        <Setter Property="Template" Value="{StaticResource RadCardViewItemTemplate}"/>
        <Setter Property="FocusVisualStyle" Value="{x:Null}"/>
    </Style>
    <Style x:Key="CardDataFieldDescriptors_ListBoxItemStyle" TargetType="telerik:RadListBoxItem">
        <Setter Property="telerik:DragDropManager.AllowCapturedDrag" Value="True"/>
        <Setter Property="Padding" Value="0"/>
        <Setter Property="Template">
            <Setter.Value>
                <ControlTemplate TargetType="telerik:RadListBoxItem">
                    <cardView:CardDataFieldDescriptorHeaderCell Content="{Binding Converter={StaticResource CardDataFieldDescriptorHeaderConverter}}"/>
                </ControlTemplate>
            </Setter.Value>
        </Setter>
    </Style>
    <ItemsPanelTemplate x:Key="CardDataFieldDescriptors_ItemsPanel">
        <StackPanel/>
    </ItemsPanelTemplate>
    <ControlTemplate x:Key="RadCardViewTemplate" TargetType="controls:RadCardView">
        <Grid>
            <Grid.ColumnDefinitions>
                <ColumnDefinition Width="*"/>
                <ColumnDefinition Width="Auto"/>
            </Grid.ColumnDefinitions>
            <Grid.RowDefinitions>
                <RowDefinition Height="Auto"/>
                <RowDefinition Height="*"/>
                <RowDefinition Height="Auto"/>
            </Grid.RowDefinitions>
            <Grid x:Name="GroupPanel" Grid.ColumnSpan="3">
                <Grid.ColumnDefinitions>
                    <ColumnDefinition Width="*"/>
                    <ColumnDefinition Width="Auto"/>
                    <ColumnDefinition Width="Auto"/>
                </Grid.ColumnDefinitions>
                <Border x:Name="GroupPanelBorderVisual"
                        Grid.ColumnSpan="3"
                        MinHeight="40"
                        BorderBrush="{TemplateBinding BorderBrush}"
                        BorderThickness="{TemplateBinding BorderThickness}"
                        Background="{TemplateBinding GroupPanelBackground}"/>
                <cardView:CardViewGroupPanel x:Name="PART_GroupPanel" Visibility="{Binding ShowGroupPanel, RelativeSource={RelativeSource TemplatedParent},Converter={StaticResource BooleanToVisibilityConverter}}" Foreground="{TemplateBinding GroupPanelForeground}"/>
                <ItemsControl x:Name="PART_ControlPanelItemsControl" Grid.Column="1" IsTabStop="False" HorizontalAlignment="Right">
                    <ItemsControl.ItemsPanel>
                        <ItemsPanelTemplate>
                            <StackPanel Orientation="Horizontal"/>
                        </ItemsPanelTemplate>
                    </ItemsControl.ItemsPanel>
                    <ItemsControl.ItemTemplate>
                        <DataTemplate>
                            <cardView:ControlPanelItemControl/>
                        </DataTemplate>
                    </ItemsControl.ItemTemplate>
                </ItemsControl>
                <telerik:RadDropDownButton x:Name="PART_DataFieldDescriptorsButton"
                        Grid.Column="2"
                        Margin="{Binding Margin, ElementName=PART_GroupPanel, Converter={StaticResource ThicknessToOrientedThicknessConverter}, ConverterParameter=Right}"
                        Style="{StaticResource CardViewPanelItemDropDownButtonStyle}"
                        IsTabStop="False"
                        Visibility="{Binding DataFieldDescriptorsButtonVisibility, RelativeSource={RelativeSource TemplatedParent}}">
                    <ToolTipService.ToolTip>
                        <ToolTip Placement="Right" telerik:LocalizationManager.ResourceKey="CardViewDataFieldDescriptorsButtonTooltip"/>
                    </ToolTipService.ToolTip>
                    <telerik:RadDropDownButton.Content>
                        <telerik:RadGlyph
                                Glyph="{StaticResource GlyphGridLayout}"
                                FontSize="16"
                                Foreground="{Binding Foreground, ElementName=PART_DataFieldDescriptorsButton}"
                                VerticalAlignment="Center"
                                HorizontalAlignment="Left"/>
                    </telerik:RadDropDownButton.Content>
                    <telerik:RadDropDownButton.DropDownContent>
                        <telerik:RadListBox
                                ItemsSource="{Binding DataFieldDescriptors}"
                                ItemContainerStyle="{StaticResource CardDataFieldDescriptors_ListBoxItemStyle}"
                                ItemsPanel="{StaticResource CardDataFieldDescriptors_ItemsPanel}"
                                BorderThickness="0"
                                Padding="0"
                                MaxWidth="200"
                                MaxHeight="250"/>
                    </telerik:RadDropDownButton.DropDownContent>
                </telerik:RadDropDownButton>
            </Grid>
            <Rectangle x:Name="BackgroundVisual" Grid.ColumnSpan="2" Grid.Row="1" Grid.RowSpan="2" Fill="{TemplateBinding Background}"/>
            <layouts:LayoutPresenter x:Name="PART_LayoutPresenter" Grid.Row="1" Margin="{Binding RelativeSource={RelativeSource TemplatedParent}, Path=Padding, Converter={StaticResource ThicknessToOrientedThicknessConverter}, ConverterParameter=TopBottom}"/>
            <Border x:Name="BorderVisual"
                    Grid.ColumnSpan="3"
                    Grid.Row="1"
                    Grid.RowSpan="2"
                    Background="{x:Null}"
                    BorderBrush="{TemplateBinding BorderBrush}"
                    BorderThickness="{Binding RelativeSource={RelativeSource TemplatedParent}, Path=BorderThickness, Converter={StaticResource ThicknessToOrientedThicknessConverter}, ConverterParameter=LeftRightBottom}"/>
            <ScrollBar x:Name="PART_VerticalScrollBar" Margin="0 -1 0 0" Orientation="Vertical" Grid.Row="1" Grid.Column="1"/>
            <ScrollBar x:Name="PART_HorizontalScrollBar" Margin="0" Orientation="Horizontal" Grid.Row="2"/>
        </Grid>
        <ControlTemplate.Triggers>
            <Trigger SourceName="PART_ControlPanelItemsControl" Property="HasItems" Value="True">
                <Setter TargetName="PART_ControlPanelItemsControl" Property="MinHeight" Value="40"/>
            </Trigger>
            <Trigger Property="CardLayout" Value="Columns">
                <Setter TargetName="PART_LayoutPresenter" Property="Margin" Value="{Binding RelativeSource={RelativeSource TemplatedParent}, Path=Padding, Converter={StaticResource ThicknessToOrientedThicknessConverter}, ConverterParameter=LeftRight}"/>
            </Trigger>
            <Trigger SourceName="PART_GroupPanel" Property="HasItems" Value="False">
                <Setter TargetName="PART_LayoutPresenter" Property="Margin" Value="{Binding Padding, RelativeSource={RelativeSource TemplatedParent}}"/>
            </Trigger>

            <!-- Setters for hide GroupPanel -->
            <MultiTrigger>
                <MultiTrigger.Conditions>
                    <Condition Property="DataFieldDescriptorsButtonVisibility" Value="Collapsed"/>
                    <Condition Property="ShowGroupPanel" Value="False"/>
                </MultiTrigger.Conditions>
                <Setter TargetName="GroupPanel" Property="Visibility" Value="Collapsed"/>
                <Setter TargetName="BorderVisual" Property="BorderThickness" Value="{Binding BorderThickness, RelativeSource={RelativeSource TemplatedParent}}"/>
            </MultiTrigger>
        </ControlTemplate.Triggers>
    </ControlTemplate>

    <!-- RadCardView Style -->
    <Style x:Key="RadCardViewStyle" TargetType="controls:RadCardView">
        <Setter Property="Template" Value="{StaticResource RadCardViewTemplate}"/>
        <Setter Property="Padding" Value="1"/>
        <Setter Property="Background" Value="{telerik:Office2016TouchResource ResourceKey=MainBrush}"/>
        <Setter Property="BorderThickness" Value="1"/>
        <Setter Property="BorderBrush" Value="{telerik:Office2016TouchResource ResourceKey=BasicBrush}"/>
        <Setter Property="GroupPanelBackground" Value="{telerik:Office2016TouchResource ResourceKey=AccentBrush}"/>
        <Setter Property="GroupPanelForeground" Value="{telerik:Office2016TouchResource ResourceKey=MarkerInvertedBrush}"/>
        <Setter Property="CollapsedLength" Value="32"/>
        <Setter Property="SnapsToDevicePixels" Value="True"/>
        <Setter Property="MajorLength" Value="300"/>
        <Setter Property="MinorLength" Value="280"/>
        <Setter Property="CardMargin" Value="5"/>
        <Style.Triggers>

            <!-- Swap MajorLength/MinorLength values when CardLayout is set to Columns -->
            <Trigger Property="CardLayout" Value="Columns">
                <Setter Property="MajorLength" Value="280"/>
                <Setter Property="MinorLength" Value="300"/>
            </Trigger>
        </Style.Triggers>
    </Style>
    <Style x:Key="CardViewDataFormStyle" TargetType="cardView:CardViewDataForm" BasedOn="{StaticResource RadDataFormStyle}">
        <Setter Property="FocusVisualStyle" Value="{x:Null}"/>
        <Setter Property="Padding" Value="0"/>
    </Style>
    <Style TargetType="controls:RadCardView" BasedOn="{StaticResource RadCardViewStyle}"/>
    <Style TargetType="controls:RadCardViewItem" BasedOn="{StaticResource RadCardViewItemStyle}"/>
    <Style TargetType="cardView:CardViewDataFormDataField" BasedOn="{StaticResource DataFormDataFieldStyle}"/>
    <Style TargetType="cardView:CardViewDataForm" BasedOn="{StaticResource CardViewDataFormStyle}"/>
</ResourceDictionary>
