                    <TextBlock x:Name="PART_DescriptionContentTextBlock"
                            Text="{Binding SelectedPropertyDefinition.Description,ElementName=PropertyGridPresenter}"
                            Margin="9 0 20 5"
                            TextWrapping="Wrap"/>
                </StackPanel>
            </Border>
            <GridSplitter x:Name="PART_DescriptionPanelSplitter"
                    Grid.Row="2"
                    HorizontalAlignment="Stretch"
                    VerticalAlignment="Top"
                    Background="Transparent"
                    Visibility="{Binding CanUserResizeDescriptionPanel, Converter={StaticResource BooleanToVisibilityConverter}, RelativeSource={RelativeSource TemplatedParent}}"
                    Height="3"/>
        </Grid>
