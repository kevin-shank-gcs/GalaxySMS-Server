    <Style x:Key="PropertyGridColumnResizerStyle" TargetType="propertyGrid:PropertyGridColumnResizer">
        <Setter Property="Template" Value="{StaticResource PropertyGridColumnResizerTemplate}"/>
        <Setter Property="IsTabStop" Value="False"/>
    </Style>
    <ControlTemplate x:Key="PropertyGridFieldTemplate" TargetType="propertyGrid:PropertyGridField">
        <Border x:Name="PART_RootElement" BorderThickness="{TemplateBinding BorderThickness}" Background="{TemplateBinding Background}" BorderBrush="{TemplateBinding BorderBrush}">
            <Grid>
                <Grid.RowDefinitions>
                    <RowDefinition/>
                    <RowDefinition Height="Auto"/>
                </Grid.RowDefinitions>
                <Grid x:Name="PART_PropertyGridFieldGridN" Height="{TemplateBinding Height}">
                    <Grid.ColumnDefinitions>
                        <ColumnDefinition Width="Auto"/>
                        <ColumnDefinition Width="{Binding LabelColumnWidth, ElementName=PropertyGridPresenter}"/>
                        <ColumnDefinition Width="*"/>
                    </Grid.ColumnDefinitions>
                    <Rectangle x:Name="Background_Visual"
                            Fill="{telerik1:Office2016TouchResource ResourceKey=MainBrush}"
                            Grid.Column="1"
                            Grid.ColumnSpan="2"
                            HorizontalAlignment="Stretch"
                            VerticalAlignment="Stretch"/>
                    <Rectangle x:Name="Background_Over"
                            Grid.Column="1"
                            Grid.ColumnSpan="2"
                            Fill="{telerik1:Office2016TouchResource ResourceKey=AccentMouseOverBrush}"
                            HorizontalAlignment="Stretch"
                            Visibility="{Binding IsMouseOver, Converter={StaticResource BooleanToVisibilityConverter}, RelativeSource={RelativeSource TemplatedParent}}"
                            VerticalAlignment="Stretch"/>
                    <Rectangle x:Name="PART_SelectionBackgroundN"
                            Grid.Column="1"
                            Visibility="Collapsed"
                            Fill="{TemplateBinding BorderBrush}"
                            HorizontalAlignment="Stretch"
                            VerticalAlignment="Stretch"/>
                    <Rectangle x:Name="Background_Selected"
                            Grid.Column="1"
                            Grid.ColumnSpan="2"
                            Fill="{telerik1:Office2016TouchResource ResourceKey=AccentBrush}"
                            HorizontalAlignment="Stretch"
                            Visibility="{Binding IsSelected, Converter={StaticResource BooleanToVisibilityConverter}, RelativeSource={RelativeSource TemplatedParent}}"
                            VerticalAlignment="Stretch"/>
                    <Grid x:Name="FieldIndicator" Visibility="{Binding RelativeSource={RelativeSource AncestorType={x:Type controls:RadPropertyGrid}}, Path=FieldIndicatorVisibility}">
                        <Border BorderThickness="0 0 1 0" BorderBrush="{TemplateBinding BorderBrush}" Width="30" Background="{telerik1:Office2016TouchResource ResourceKey=AlternativeBrush}">
                            <TextBlock x:Name="NavigatorIndicator"
                                    Text="{StaticResource GlyphArrowChevronRight}"
                                    telerik:GlyphAdorner.ShowGlyphInDesignTime="True"
                                    FontFamily="{StaticResource TelerikWebUI}"
                                    Foreground="{telerik1:Office2016TouchResource ResourceKey=IconBrush}"
                                    FontSize="16"
                                    FontStyle="Normal"
                                    FontWeight="Normal"
                                    Visibility="Collapsed"
                                    HorizontalAlignment="Center"
                                    VerticalAlignment="Center"/>
                        </Border>
                    </Grid>
                    <Border Grid.Column="1" BorderThickness="0 0 1 1" BorderBrush="{TemplateBinding BorderBrush}">
                        <StackPanel Orientation="Horizontal">
                            <propertyGrid:PropertyGridIndentPresenter IndentLevel="{Binding RelativeSource={RelativeSource TemplatedParent}, Path=IndentLevel}">
                                <propertyGrid:PropertyGridIndentPresenter.ItemsPanel>
                                    <ItemsPanelTemplate>
                                        <StackPanel Orientation="Horizontal"/>
                                    </ItemsPanelTemplate>
                                </propertyGrid:PropertyGridIndentPresenter.ItemsPanel>
                                <propertyGrid:PropertyGridIndentPresenter.ItemTemplate>
                                    <DataTemplate>
                                        <propertyGrid:PropertyGridIndentCell/>
                                    </DataTemplate>
                                </propertyGrid:PropertyGridIndentPresenter.ItemTemplate>
                                <propertyGrid:PropertyGridIndentPresenter.Template>
                                    <ControlTemplate>
                                        <ItemsPresenter/>
                                    </ControlTemplate>
                                </propertyGrid:PropertyGridIndentPresenter.Template>
                            </propertyGrid:PropertyGridIndentPresenter>
                            <telerik:RadToggleButton x:Name="PART_NestedPropertiesButton"
                                    Style="{StaticResource PropertyGridNestedToggleButtonStyle}"
                                    Visibility="{Binding ShouldDisplayNestedProperties, RelativeSource={RelativeSource TemplatedParent}, Converter={StaticResource BooleanToVisibilityConverter}}"
                                    IsChecked="{Binding RelativeSource={RelativeSource TemplatedParent}, Path=IsExpanded, Mode=TwoWay}"/>
                            <TextBlock x:Name="PART_FieldLabelN" Text="{Binding DisplayName}" IsHitTestVisible="True" VerticalAlignment="Center" Margin="4 2"/>
                        </StackPanel>
                    </Border>
                    <Border x:Name="BorderVisual" Grid.Column="2" BorderThickness="0 0 0 1" BorderBrush="{TemplateBinding BorderBrush}"/>
                    <ContentControl x:Name="Content"
                            Content="{TemplateBinding Content}"
                            MinHeight="22"
                            Margin="1 1 2 2"
                            ContentTemplate="{TemplateBinding ContentTemplate}"
                            Grid.Column="2"
                            HorizontalContentAlignment="{TemplateBinding HorizontalContentAlignment}"
                            VerticalContentAlignment="{TemplateBinding VerticalContentAlignment}"
                            IsTabStop="False"/>
                    <propertyGrid:PropertyGridColumnResizer x:Name="ColumnResizer" Grid.Column="1" Margin="0 0 -4 0" HorizontalAlignment="Right"/>
                </Grid>
                <ItemsControl Visibility="{Binding RelativeSource={RelativeSource TemplatedParent}, Converter={StaticResource BooleanToVisibilityConverter}, Path=ShouldDisplayNestedItemsControl }" Grid.Row="1" ItemsSource="{Binding NestedProperties, Converter={StaticResource FlatItemSourceConverter}}">
                    <ItemsControl.ItemTemplate>
                        <DataTemplate>
                            <propertyGrid:PropertyGridField Content="{Binding}" Visibility="{Binding Visibility}"/>
                        </DataTemplate>
                    </ItemsControl.ItemTemplate>
                    <ItemsControl.ItemsPanel>
                        <ItemsPanelTemplate>
                            <VirtualizingStackPanel/>
                        </ItemsPanelTemplate>
                    </ItemsControl.ItemsPanel>
                </ItemsControl>
            </Grid>
        </Border>
        <ControlTemplate.Triggers>
            <Trigger Property="IsSelected" Value="False">
                <Setter TargetName="PART_FieldLabelN" Property="Foreground" Value="{telerik1:Office2016TouchResource ResourceKey=MarkerBrush}"/>
                <Setter TargetName="PART_SelectionBackgroundN" Property="Visibility" Value="Collapsed"/>
                <Setter TargetName="PART_NestedPropertiesButton" Property="Foreground" Value="{telerik1:Office2016TouchResource ResourceKey=IconBrush}"/>
            </Trigger>
            <Trigger Property="IsSelected" Value="True">
                <Setter TargetName="PART_FieldLabelN" Property="Foreground" Value="{telerik1:Office2016TouchResource ResourceKey=MarkerInvertedBrush}"/>
                <Setter TargetName="PART_NestedPropertiesButton" Property="Foreground" Value="{telerik1:Office2016TouchResource ResourceKey=MarkerInvertedBrush}"/>
                <Setter Property="Foreground" Value="{telerik1:Office2016TouchResource ResourceKey=MarkerInvertedBrush}"/>
                <Setter TargetName="PART_SelectionBackgroundN" Property="Visibility" Value="Visible"/>
            </Trigger>
            <Trigger Property="IsCurrent" Value="True">
                <Setter TargetName="NavigatorIndicator" Property="Visibility" Value="Visible"/>
            </Trigger>
            <Trigger Property="IsMouseOver" Value="True">
                <Setter Property="Foreground" Value="{telerik1:Office2016TouchResource ResourceKey=MarkerInvertedBrush}"/>
                <Setter TargetName="PART_FieldLabelN" Property="Foreground" Value="{telerik1:Office2016TouchResource ResourceKey=MarkerInvertedBrush}"/>
                <Setter TargetName="PART_NestedPropertiesButton" Property="Foreground" Value="{telerik1:Office2016TouchResource ResourceKey=MarkerInvertedBrush}"/>
            </Trigger>
            <DataTrigger Binding="{Binding ComputedVerticalScrollBarVisibility, RelativeSource={RelativeSource AncestorType={x:Type ScrollViewer}}}" Value="Visible">
                <Setter TargetName="BorderVisual" Property="BorderThickness" Value="0 0 1 1"/>
            </DataTrigger>
            <Trigger Property="FlowDirection" Value="RightToLeft">
                <Setter TargetName="NavigatorIndicator" Property="Text" Value="{StaticResource GlyphArrowChevronLeft}"/>
            </Trigger>
        </ControlTemplate.Triggers>
    </ControlTemplate>
    <ControlTemplate x:Key="PropertyGridValidationTemplate">
        <StackPanel Orientation="Horizontal">
            <Grid>
                <Border Background="{telerik1:Office2016TouchResource ResourceKey=ValidationBrush}" Margin="0 0 2 0" MinHeight="32">
                    <TextBlock x:Name="PART_ToolTip"
                            FontSize="{telerik1:Office2016TouchResource ResourceKey=FontSize}"
                            FontFamily="{telerik:Office2016TouchResource ResourceKey=FontFamily}"
                            Foreground="{telerik1:Office2016TouchResource ResourceKey=MarkerInvertedBrush}"
                            VerticalAlignment="Center"
                            Padding="4 6 4 5"
                            Text="{Binding ElementName=Holder, Path=AdornedElement.(Validation.Errors).CurrentItem.ErrorContent}"/>
                </Border>
            </Grid>
            <Grid SnapsToDevicePixels="True" VerticalAlignment="Top">
                <Border Background="Transparent" HorizontalAlignment="Right" VerticalAlignment="Top" Width="3" Height="3"/>
                <AdornedElementPlaceholder x:Name="Holder"/>
                <Border BorderBrush="{telerik1:Office2016TouchResource ResourceKey=ValidationBrush}" BorderThickness="1" CornerRadius="{telerik1:Office2016TouchResource ResourceKey=CornerRadius}"/>
            </Grid>
        </StackPanel>
