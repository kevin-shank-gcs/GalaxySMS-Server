                                        ScrollViewer.HorizontalScrollBarVisibility="Auto"
                                        BorderBrush="{TemplateBinding BorderBrush}"
                                        BorderThickness="{TemplateBinding BorderThickness}">
                                    <ItemsPresenter x:Name="Content" Margin="{TemplateBinding Padding}"/>
                                </ScrollViewer>
                            </ControlTemplate>
                        </Setter.Value>
                    </Setter>
                    <Setter Property="ItemTemplate">
                        <Setter.Value>
                            <DataTemplate>
                                <StackPanel Orientation="Horizontal" VerticalAlignment="Center">
                                    <TextBlock Foreground="{telerik1:Office2016TouchResource ResourceKey=ValidationBrush}" Text="{Binding SourceFieldDisplayName}"/>
                                    <TextBlock Text=":" Margin="4 0" Foreground="{telerik1:Office2016TouchResource ResourceKey=ValidationBrush}"/>
                                    <TextBlock Text="{Binding ErrorContent}"/>
                                </StackPanel>
                            </DataTemplate>
                        </Setter.Value>
                    </Setter>
                    <Style.Triggers>
                        <DataTrigger Binding="{Binding HasItems, RelativeSource={RelativeSource Self}}" Value="False">
                            <Setter Property="Padding" Value="0"/>
                            <Setter Property="BorderThickness" Value="0"/>
