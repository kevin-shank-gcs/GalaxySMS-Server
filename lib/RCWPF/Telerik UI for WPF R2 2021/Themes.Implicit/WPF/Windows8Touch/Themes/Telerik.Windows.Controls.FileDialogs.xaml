                                    Grid.ColumnSpan="4"/>
                            <Border x:Name="hover"
                                    BorderBrush="{TemplateBinding BorderBrush}"
                                    BorderThickness="{TemplateBinding BorderThickness}"
                                    Margin="0 1"
                                    Background="{telerik:Windows8TouchResource ResourceKey=InvertedBrush}"
                                    Grid.Column="2"
                                    Grid.ColumnSpan="4"
                                    Opacity="0"/>
                            <Border x:Name="selected"
                                    BorderBrush="{TemplateBinding BorderBrush}"
                                    BorderThickness="{TemplateBinding BorderThickness}"
                                    Margin="0 1"
                                    Background="{telerik:Windows8TouchResource ResourceKey=LowBrush}"
                                    Grid.Column="2"
                                    Grid.ColumnSpan="4"
                                    Opacity="0"/>
                            <StackPanel x:Name="IndentContainer" Orientation="Horizontal">
                                <Rectangle x:Name="IndentFirstVerticalLine" Width="1" Visibility="Collapsed" Stroke="{telerik:Windows8TouchResource ResourceKey=MediumBrush}" VerticalAlignment="Top"/>
                            </StackPanel>
                            <Grid Grid.Column="1" x:Name="ListRootContainer" HorizontalAlignment="Center" MinWidth="{TemplateBinding ExpanderButtonMinWidth}">
                                <Rectangle x:Name="HorizontalLine" Height="1" Stroke="{telerik:Windows8TouchResource ResourceKey=MediumBrush}" VerticalAlignment="Center" HorizontalAlignment="Right"/>
                                <Rectangle x:Name="VerticalLine" Width="1" Stroke="{telerik:Windows8TouchResource ResourceKey=MediumBrush}" VerticalAlignment="Top" HorizontalAlignment="Center"/>
                                <ToggleButton x:Name="Expander" IsTabStop="False" AutomationProperties.Name="ExpanderButton" Background="{TemplateBinding Background}"/>
                                <Grid x:Name="LoadingVisual" RenderTransformOrigin="0.5 0.5" Visibility="Collapsed" VerticalAlignment="Center" HorizontalAlignment="Center">
                                    <Grid.RenderTransform>
                                        <TransformGroup>
                                            <RotateTransform x:Name="LoadingVisualAngleTransform" Angle="0" CenterX="0.5" CenterY="0.5"/>
                                        </TransformGroup>
                                    </Grid.RenderTransform>
                                    <Path
                                            Width="10"
                                            Height="10"
                                            Stretch="Fill"
                                            Stroke="{telerik:Windows8TouchResource ResourceKey=InvertedBrush}"
                                            StrokeThickness="1"
                                            StrokeStartLineCap="Round"
                                            Data="M1,0 A1,1,90,1,1,0,-1"/>
                                    <Path
                                            Margin="5 -1.5 0 0"
                                            HorizontalAlignment="Left"
                                            VerticalAlignment="Top"
                                            Width="4"
                                            Height="4"
                                            Stretch="Fill"
                                            Fill="{telerik:Windows8TouchResource ResourceKey=InvertedBrush}"
                                            Data="M0,-1.1 L0.1,-1 L0,-0.9"
                                            StrokeThickness="1"/>
                                </Grid>
                            </Grid>
                            <Image
                                    Grid.Column="3"
                                    x:Name="Image"
                                    Tag="Wow"
                                    MaxWidth="16"
                                    MaxHeight="16"
                                    Margin="5 0 0 0"
                                    Visibility="{Binding RelativeSource={RelativeSource Self}, Path=Source, Converter={StaticResource NullToVisibilityConverter}}"
                                    HorizontalAlignment="Center"
                                    VerticalAlignment="Center"/>
                            <Border x:Name="focused"
                                    BorderBrush="{TemplateBinding BorderBrush}"
                                    BorderThickness="{TemplateBinding BorderThickness}"
                                    Margin="0 1"
                                    IsHitTestVisible="False"
                                    Background="{telerik:Windows8TouchResource ResourceKey=InvertedBrush}"
                                    Grid.Column="2"
                                    Grid.ColumnSpan="4"
                                    Opacity="0.05"
                                    Visibility="Collapsed"/>
                            <Grid x:Name="HeaderContentPanel" Grid.Column="4" Grid.ColumnSpan="2" Background="Transparent">
                                <ContentControl x:Name="Header"
                                        IsTabStop="False"
                                        Foreground="{Binding RelativeSource={RelativeSource TemplatedParent}, Path=Foreground, Mode=OneWay}"
                                        Content="{TemplateBinding Header}"
                                        ContentTemplate="{TemplateBinding HeaderTemplate}"
                                        Margin="{TemplateBinding Padding}"
                                        VerticalAlignment="{TemplateBinding VerticalContentAlignment}"
                                        HorizontalAlignment="{TemplateBinding HorizontalContentAlignment}"
                                        ContentTemplateSelector="{TemplateBinding HeaderTemplateSelector}"
                                        ContentStringFormat="{TemplateBinding HeaderStringFormat}"/>
                                <ContentPresenter x:Name="EditHeaderElement"
                                        Visibility="Collapsed"
                                        HorizontalAlignment="{TemplateBinding HorizontalContentAlignment}"
                                        VerticalAlignment="{TemplateBinding VerticalContentAlignment}"
                                        Margin="1"
                                        ContentTemplateSelector="{TemplateBinding HeaderEditTemplateSelector}"
                                        ContentTemplate="{TemplateBinding HeaderEditTemplate}"/>
                            </Grid>
                            <CheckBox x:Name="CheckBoxElement" Padding="0" Margin="2 0 0 0" IsTabStop="False" Grid.Column="2" VerticalAlignment="Center" Visibility="Collapsed"/>
                            <RadioButton x:Name="RadioButtonElement" Padding="0" Margin="2 0 0 0" IsTabStop="False" Grid.Column="2" VerticalAlignment="Center" Visibility="Collapsed"/>
                        </Grid>
                        <ItemsPresenter Grid.Row="1" x:Name="ItemsHost" Visibility="Collapsed"/>
                    </Grid>
                </ControlTemplate>
            </Setter.Value>
        </Setter>
    </Style>
