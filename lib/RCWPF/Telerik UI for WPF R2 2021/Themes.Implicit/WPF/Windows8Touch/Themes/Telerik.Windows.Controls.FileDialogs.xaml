                            <DoubleAnimation To="24" Duration="0:0:0" Storyboard.TargetName="Icon" Storyboard.TargetProperty="MinWidth"/>
                            <ObjectAnimationUsingKeyFrames Duration="0:0:0" Storyboard.TargetName="IconHost" Storyboard.TargetProperty="Margin">
                                <DiscreteObjectKeyFrame KeyTime="0:0:0">
                                    <DiscreteObjectKeyFrame.Value>
                                        <Thickness>-2 1 -5 1</Thickness>
