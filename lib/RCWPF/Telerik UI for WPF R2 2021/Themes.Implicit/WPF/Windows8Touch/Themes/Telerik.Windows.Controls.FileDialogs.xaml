                                        Stretch="Fill"
                                        VerticalAlignment="Center"
                                        Margin="0 0 5 0"
                                        Visibility="{Binding DropPosition, Converter={StaticResource DragDropBooleanToVisibilityConverter}, ConverterParameter=After}"/>
                                <TextBlock Text="{Binding DropPosition}" Foreground="{telerik1:Windows8TouchResource ResourceKey=InvertedForegroundBrush}"/>
                                <TextBlock Text=":" Foreground="{telerik1:Windows8TouchResource ResourceKey=InvertedForegroundBrush}" Margin="1 0 5 0"/>
