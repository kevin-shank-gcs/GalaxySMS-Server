                            </VisualStateGroup>
                            <VisualStateGroup x:Name="DropDownButtonPosition">
                                <VisualState x:Name="DropDownButtonAtLeft"/>
                                <VisualState x:Name="DropDownButtonAtTop"/>
                                <VisualState x:Name="DropDownButtonAtRight"/>
                                <VisualState x:Name="DropDownButtonAtBottom"/>
                            </VisualStateGroup>
                            <VisualStateGroup x:Name="ButtonPartVisibility">
                                <VisualState x:Name="ButtonInvisible"/>
                                <VisualState x:Name="ButtonVisible"/>
                            </VisualStateGroup>
                            <VisualStateGroup x:Name="FocusStatesGroup">
                                <VisualState x:Name="Unfocused"/>
                                <VisualState x:Name="Focused"/>
                            </VisualStateGroup>
                        </VisualStateManager.VisualStateGroups>
                        <Grid.ColumnDefinitions>
                            <ColumnDefinition Width="*"/>
                            <ColumnDefinition Width="Auto"/>
                        </Grid.ColumnDefinitions>
                        <telerik:RadToggleButton x:Name="DropDownPart"
                                IsTabStop="False"
                                Grid.Column="1"
                                Style="{TemplateBinding TogglePartStyle}"
                                IsChecked="{Binding IsOpen, Mode=TwoWay, RelativeSource={RelativeSource TemplatedParent}}"/>
                        <ContentControl x:Name="Content"
                                Content="{TemplateBinding Content}"
                                IsTabStop="False"
                                Foreground="{TemplateBinding Foreground}"
                                Margin="{TemplateBinding Padding}"
                                VerticalContentAlignment="{TemplateBinding VerticalContentAlignment}"
                                HorizontalContentAlignment="{TemplateBinding HorizontalContentAlignment}"
                                ContentTemplate="{TemplateBinding ContentTemplate}"/>
                        <Popup x:Name="DropDownPopup"
                                AllowsTransparency="True"
                                StaysOpen="False"
                                Focusable="False"
                                Placement="Bottom"
                                IsOpen="{Binding Path=IsOpen, Mode=TwoWay, RelativeSource={RelativeSource TemplatedParent}}">
                            <Border x:Name="DropDownPopupBorder"
                                    Margin="0 4 0 0"
                                    Background="{telerik:Windows8TouchResource ResourceKey=MainBrush}"
                                    BorderBrush="{telerik:Windows8TouchResource ResourceKey=MediumBrush}"
                                    BorderThickness="1"
                                    CornerRadius="{TemplateBinding CornerRadius}"
                                    Width="{TemplateBinding DropDownWidth}"
                                    Height="{TemplateBinding DropDownHeight}"
                                    MinWidth="5"
                                    MinHeight="40"
                                    MaxWidth="{TemplateBinding DropDownMaxWidth}"
                                    MaxHeight="{TemplateBinding DropDownMaxHeight}">
                                <ContentPresenter x:Name="DropDownPopupContent"
                                        Content="{TemplateBinding DropDownContent}"
                                        ContentTemplate="{TemplateBinding DropDownContentTemplate}"
                                        HorizontalAlignment="Stretch"
                                        VerticalAlignment="Stretch"
                                        DataContext="{Binding RelativeSource={RelativeSource TemplatedParent}, Path=DataContext}"/>
                            </Border>
                        </Popup>
                    </Grid>
                </ControlTemplate>
            </Setter.Value>
        </Setter>
    </Style>
    <ControlTemplate x:Key="BarItemControlTemplate" TargetType="telerikBreadcrumb:RadBreadcrumbBarItem">
        <Grid>
            <VisualStateManager.VisualStateGroups>
                <VisualStateGroup x:Name="ItemsCount">
                    <VisualState x:Name="NoItems">
                        <Storyboard>
                            <ObjectAnimationUsingKeyFrames Duration="0" Storyboard.TargetName="SplitButton" Storyboard.TargetProperty="DropDownIndicatorVisibility">
                                <DiscreteObjectKeyFrame KeyTime="0">
                                    <DiscreteObjectKeyFrame.Value>
                                        <Visibility>Collapsed</Visibility>
                                    </DiscreteObjectKeyFrame.Value>
                                </DiscreteObjectKeyFrame>
                            </ObjectAnimationUsingKeyFrames>
                            <DoubleAnimation Duration="0" Storyboard.TargetName="ArrowFocusVisual" Storyboard.TargetProperty="Opacity" To="0"/>
                            <ObjectAnimationUsingKeyFrames Storyboard.TargetName="BorderFocusVisual" Storyboard.TargetProperty="(Grid.ColumnSpan)">
                                <DiscreteObjectKeyFrame KeyTime="0">
                                    <DiscreteObjectKeyFrame.Value>
                                        <sys:Int32>2</sys:Int32>
                                    </DiscreteObjectKeyFrame.Value>
                                </DiscreteObjectKeyFrame>
                            </ObjectAnimationUsingKeyFrames>
                            <ObjectAnimationUsingKeyFrames Storyboard.TargetName="BorderFocusVisual" Storyboard.TargetProperty="BorderThickness">
                                <DiscreteObjectKeyFrame KeyTime="0">
                                    <DiscreteObjectKeyFrame.Value>
                                        <Thickness>0 1 1 1</Thickness>
                                    </DiscreteObjectKeyFrame.Value>
                                </DiscreteObjectKeyFrame>
                            </ObjectAnimationUsingKeyFrames>
                        </Storyboard>
                    </VisualState>
                    <VisualState x:Name="HasItems">
                        <Storyboard>
                            <ObjectAnimationUsingKeyFrames Duration="0" Storyboard.TargetName="SplitButton" Storyboard.TargetProperty="DropDownIndicatorVisibility">
                                <DiscreteObjectKeyFrame KeyTime="0">
                                    <DiscreteObjectKeyFrame.Value>
                                        <Visibility>Visible</Visibility>
                                    </DiscreteObjectKeyFrame.Value>
                                </DiscreteObjectKeyFrame>
                            </ObjectAnimationUsingKeyFrames>
                            <DoubleAnimation Duration="0" Storyboard.TargetName="ArrowFocusVisual" Storyboard.TargetProperty="Opacity" To="1"/>
                            <ObjectAnimationUsingKeyFrames Storyboard.TargetName="BorderFocusVisual" Storyboard.TargetProperty="(Grid.ColumnSpan)">
                                <DiscreteObjectKeyFrame KeyTime="0">
                                    <DiscreteObjectKeyFrame.Value>
                                        <sys:Int32>1</sys:Int32>
                                    </DiscreteObjectKeyFrame.Value>
                                </DiscreteObjectKeyFrame>
                            </ObjectAnimationUsingKeyFrames>
                            <ObjectAnimationUsingKeyFrames Storyboard.TargetName="BorderFocusVisual" Storyboard.TargetProperty="BorderThickness">
                                <DiscreteObjectKeyFrame KeyTime="0">
                                    <DiscreteObjectKeyFrame.Value>
                                        <Thickness>0 1</Thickness>
                                    </DiscreteObjectKeyFrame.Value>
                                </DiscreteObjectKeyFrame>
                            </ObjectAnimationUsingKeyFrames>
                        </Storyboard>
                    </VisualState>
                </VisualStateGroup>
                <VisualStateGroup x:Name="IsOverflow">
                    <VisualState x:Name="NotOverflowButton"/>
                    <VisualState x:Name="OverflowButton"/>
                </VisualStateGroup>
                <VisualStateGroup x:Name="ButtonPartVisibility">
                    <VisualState x:Name="ButtonPartVisible"/>
                    <VisualState x:Name="ButtonPartHidden">
                        <Storyboard>
                            <ObjectAnimationUsingKeyFrames Duration="0" Storyboard.TargetName="SplitButton" Storyboard.TargetProperty="IsButtonPartVisible">
                                <DiscreteObjectKeyFrame KeyTime="0">
                                    <DiscreteObjectKeyFrame.Value>
                                        <sys:Boolean>False</sys:Boolean>
                                    </DiscreteObjectKeyFrame.Value>
                                </DiscreteObjectKeyFrame>
                            </ObjectAnimationUsingKeyFrames>
                        </Storyboard>
                    </VisualState>
                </VisualStateGroup>
                <VisualStateGroup x:Name="SplitButtonVisibility">
                    <VisualState x:Name="SplitButtonVisible"/>
                    <VisualState x:Name="SplitButtonHidden">
                        <Storyboard>
                            <ObjectAnimationUsingKeyFrames Duration="0" Storyboard.TargetName="SplitButton" Storyboard.TargetProperty="Visibility">
                                <DiscreteObjectKeyFrame KeyTime="0">
                                    <DiscreteObjectKeyFrame.Value>
                                        <Visibility>Collapsed</Visibility>
                                    </DiscreteObjectKeyFrame.Value>
                                </DiscreteObjectKeyFrame>
                            </ObjectAnimationUsingKeyFrames>
                        </Storyboard>
                    </VisualState>
                </VisualStateGroup>
                <VisualStateGroup x:Name="FocusStatesGroup">
                    <VisualState x:Name="Unfocused">
                        <Storyboard>
                            <ObjectAnimationUsingKeyFrames Storyboard.TargetName="FocusVisual" Storyboard.TargetProperty="Visibility">
                                <DiscreteObjectKeyFrame KeyTime="00:00:00.150">
                                    <DiscreteObjectKeyFrame.Value>
                                        <Visibility>Collapsed</Visibility>
                                    </DiscreteObjectKeyFrame.Value>
                                </DiscreteObjectKeyFrame>
                            </ObjectAnimationUsingKeyFrames>
                        </Storyboard>
                    </VisualState>
                    <VisualState x:Name="Focused">
                        <Storyboard>
                            <ObjectAnimationUsingKeyFrames Storyboard.TargetName="FocusVisual" Storyboard.TargetProperty="Visibility">
                                <DiscreteObjectKeyFrame KeyTime="00:00:00.115">
                                    <DiscreteObjectKeyFrame.Value>
                                        <Visibility>Visible</Visibility>
                                    </DiscreteObjectKeyFrame.Value>
                                </DiscreteObjectKeyFrame>
                            </ObjectAnimationUsingKeyFrames>
                        </Storyboard>
                    </VisualState>
                </VisualStateGroup>
            </VisualStateManager.VisualStateGroups>
            <telerik:RadSplitButton x:Name="SplitButton"
                    Foreground="{TemplateBinding Foreground}"
                    Command="telerikBreadcrumb:RadBreadcrumbCommands.ClearSelectedItem"
                    Content="{TemplateBinding Header}"
                    ContentTemplate="{TemplateBinding HeaderTemplate}"
                    IsOpen="{Binding Path=IsPopupOpen, Mode=TwoWay, RelativeSource={RelativeSource TemplatedParent}}"
                    Style="{TemplateBinding SplitButtonStyle}"
                    ContentTemplateSelector="{TemplateBinding HeaderTemplateSelector}">
                <telerik:RadSplitButton.DropDownContent>
                    <ScrollViewer HorizontalScrollBarVisibility="Auto" VerticalScrollBarVisibility="Auto" BorderThickness="0">
                        <ItemsPresenter/>
                    </ScrollViewer>
                </telerik:RadSplitButton.DropDownContent>
            </telerik:RadSplitButton>
            <Grid x:Name="FocusVisual" Margin="-12 0 0 0" Visibility="Collapsed" Opacity="0.3">
                <Grid.ColumnDefinitions>
                    <ColumnDefinition Width="Auto"/>
                    <ColumnDefinition Width="*"/>
                    <ColumnDefinition Width="Auto"/>
                </Grid.ColumnDefinitions>
                <Path
                        Grid.Column="0"
                        Data="M9 36 0 36 9 18 0 0 9 0"
                        Fill="{x:Null}"
                        Stretch="Fill"
                        Stroke="{telerik:Windows8TouchResource ResourceKey=InvertedBrush}"
                        SnapsToDevicePixels="True"/>
                <Border x:Name="BorderFocusVisual"
                        Grid.Column="1"
                        Background="{x:Null}"
                        BorderBrush="{telerik:Windows8TouchResource ResourceKey=InvertedBrush}"
                        BorderThickness="0 1"
                        VerticalAlignment="Stretch"
                        HorizontalAlignment="Stretch"
                        Margin="-1 0"
                        SnapsToDevicePixels="True"/>
                <Path x:Name="ArrowFocusVisual"
                        Grid.Column="2"
                        Data="M0 0 26 0 36 18 26 36 0 36"
                        Fill="{x:Null}"
                        Stretch="Fill"
                        Stroke="{telerik:Windows8TouchResource ResourceKey=InvertedBrush}"
                        SnapsToDevicePixels="True"/>
            </Grid>
        </Grid>
    </ControlTemplate>
    <ControlTemplate x:Key="DropDownItemControlTemplate" TargetType="telerikBreadcrumb:RadBreadcrumbBarItem">
        <Grid>
            <VisualStateManager.VisualStateGroups>
                <VisualStateGroup x:Name="Mouse">
                    <VisualState x:Name="MouseEnter">
                        <Storyboard>
                            <DoubleAnimation Storyboard.TargetName="headerContainerOver" Storyboard.TargetProperty="Opacity" To="0.05" Duration="0"/>
                        </Storyboard>
                    </VisualState>
                    <VisualState x:Name="MouseLeave"/>
                </VisualStateGroup>
                <VisualStateGroup x:Name="Selection">
                    <VisualState x:Name="Select">
                        <Storyboard>
                            <ObjectAnimationUsingKeyFrames Duration="0" Storyboard.TargetName="contentControl" Storyboard.TargetProperty="FontWeight">
                                <DiscreteObjectKeyFrame KeyTime="0">
                                    <DiscreteObjectKeyFrame.Value>
                                        <FontWeight>Bold</FontWeight>
                                    </DiscreteObjectKeyFrame.Value>
                                </DiscreteObjectKeyFrame>
                            </ObjectAnimationUsingKeyFrames>
                        </Storyboard>
                    </VisualState>
                    <VisualState x:Name="UnSelect"/>
                </VisualStateGroup>
            </VisualStateManager.VisualStateGroups>
            <Grid.ColumnDefinitions>
                <ColumnDefinition Width="Auto"/>
                <ColumnDefinition Width="*"/>
            </Grid.ColumnDefinitions>
            <Border x:Name="headerContainerOver" Grid.ColumnSpan="2" Background="{telerik:Windows8TouchResource ResourceKey=InvertedBrush}" Opacity="0"/>
            <Image Style="{StaticResource BreadcrumbImageStyle}" Source="{Binding Image, RelativeSource={RelativeSource TemplatedParent}}" Visibility="{Binding IsIconVisible, Converter={StaticResource BooleanToVisibilityConverter}, RelativeSource={RelativeSource TemplatedParent}}"/>
            <ContentControl x:Name="contentControl"
                    Grid.Column="1"
                    Margin="5 8"
                    Content="{TemplateBinding DropDownHeader}"
                    VerticalAlignment="Center"
                    HorizontalAlignment="Stretch"
                    ContentTemplate="{TemplateBinding DropDownHeaderTemplate}"
                    HorizontalContentAlignment="Stretch"
                    ContentTemplateSelector="{TemplateBinding HeaderTemplateSelector}"/>
        </Grid>
    </ControlTemplate>
    <Style x:Key="RadBreadcrumbStyle" TargetType="telerikNavigation:RadBreadcrumb">
        <Setter Property="Background" Value="{telerik:Windows8TouchResource ResourceKey=MainBrush}"/>
        <Setter Property="BorderBrush" Value="{telerik:Windows8TouchResource ResourceKey=MediumBrush}"/>
        <Setter Property="BorderThickness" Value="1"/>
        <Setter Property="Foreground" Value="{telerik:Windows8TouchResource ResourceKey=MainForegroundBrush}"/>
        <Setter Property="FontSize" Value="{telerik:Windows8TouchResource ResourceKey=FontSizeL}"/>
        <Setter Property="FontFamily" Value="{telerik:Windows8TouchResource ResourceKey=FontFamily}"/>
        <Setter Property="BarItemControlTemplate" Value="{StaticResource BarItemControlTemplate}"/>
        <Setter Property="DropDownItemControlTemplate" Value="{StaticResource DropDownItemControlTemplate}"/>
        <Setter Property="SplitButtonStyle" Value="{StaticResource BreadcrumbSplitButtonStyle}"/>
        <Setter Property="OverflowButtonStyle" Value="{StaticResource BreadcrumbOverflowSplitButtonStyle}"/>
        <Setter Property="IsTabStop" Value="False"/>
        <Setter Property="VerticalAlignment" Value="Center"/>
        <Setter Property="MinHeight" Value="37"/>
        <Setter Property="FocusVisualStyle" Value="{x:Null}"/>
        <Setter Property="Focusable" Value="False"/>
        <Setter Property="Template">
            <Setter.Value>
                <ControlTemplate TargetType="telerikNavigation:RadBreadcrumb">
                    <Grid x:Name="outerGrid">
                        <VisualStateManager.VisualStateGroups>
                            <VisualStateGroup x:Name="Visibility">
                                <VisualState x:Name="TextBoxVisibile">
                                    <Storyboard>
                                        <ObjectAnimationUsingKeyFrames Duration="0" Storyboard.TargetName="BreadcrumbBar" Storyboard.TargetProperty="Visibility">
                                            <DiscreteObjectKeyFrame KeyTime="0">
                                                <DiscreteObjectKeyFrame.Value>
                                                    <Visibility>Collapsed</Visibility>
                                                </DiscreteObjectKeyFrame.Value>
                                            </DiscreteObjectKeyFrame>
                                        </ObjectAnimationUsingKeyFrames>
                                        <DoubleAnimation Duration="0" To="1" Storyboard.TargetName="pathTextBox" Storyboard.TargetProperty="Opacity"/>
                                        <ObjectAnimationUsingKeyFrames Duration="0" Storyboard.TargetName="focused" Storyboard.TargetProperty="Visibility">
                                            <DiscreteObjectKeyFrame KeyTime="0">
                                                <DiscreteObjectKeyFrame.Value>
                                                    <Visibility>Visible</Visibility>
                                                </DiscreteObjectKeyFrame.Value>
                                            </DiscreteObjectKeyFrame>
                                        </ObjectAnimationUsingKeyFrames>
                                    </Storyboard>
                                </VisualState>
                                <VisualState x:Name="ItemsPresenterVisible"/>
                            </VisualStateGroup>
                            <VisualStateGroup x:Name="IsTextModeEnabled">
                                <VisualState x:Name="TextModeDisabled">
                                    <Storyboard>
                                        <ObjectAnimationUsingKeyFrames Duration="0" Storyboard.TargetName="historySplitButton" Storyboard.TargetProperty="Visibility">
                                            <DiscreteObjectKeyFrame KeyTime="0">
                                                <DiscreteObjectKeyFrame.Value>
                                                    <Visibility>Collapsed</Visibility>
                                                </DiscreteObjectKeyFrame.Value>
                                            </DiscreteObjectKeyFrame>
                                        </ObjectAnimationUsingKeyFrames>
                                    </Storyboard>
                                </VisualState>
                                <VisualState x:Name="TextModeEnabled"/>
                            </VisualStateGroup>
                        </VisualStateManager.VisualStateGroups>
                        <Grid.ColumnDefinitions>
                            <ColumnDefinition Width="Auto"/>
                            <ColumnDefinition Width="*"/>
                            <ColumnDefinition Width="35"/>
                        </Grid.ColumnDefinitions>
                        <Border x:Name="normal"
                                Background="{TemplateBinding Background}"
                                BorderBrush="{TemplateBinding BorderBrush}"
                                BorderThickness="{TemplateBinding BorderThickness}"
                                Grid.ColumnSpan="3"
                                SnapsToDevicePixels="True"/>
                        <Border x:Name="focused"
                                Background="{TemplateBinding Background}"
                                BorderBrush="{telerik:Windows8TouchResource ResourceKey=AccentBrush}"
                                Visibility="Collapsed"
                                BorderThickness="{TemplateBinding BorderThickness}"
                                Grid.ColumnSpan="3"
                                SnapsToDevicePixels="True"/>
                        <Image x:Name="image"
                                Style="{StaticResource BreadcrumbImageStyle}"
                                Margin="10 0 17 0"
                                Source="{Binding RelativeSource={RelativeSource TemplatedParent}, Path=CurrentContainer.Image}"
                                Visibility="{Binding RelativeSource={RelativeSource TemplatedParent}, Path=IsCurrentIconVisible, Converter={StaticResource BooleanToVisibilityConverter}}"/>
                        <telerik:RadSplitButton x:Name="historySplitButton"
                                Style="{StaticResource BreadcrumbTextSplitButton}"
                                IsOpen="{Binding RelativeSource={RelativeSource TemplatedParent}, Path=IsHistoryOpen, Mode=TwoWay}"
                                Grid.Column="1"
                                Grid.ColumnSpan="2">
                            <telerik:RadSplitButton.Content>
                                <TextBox x:Name="pathTextBox" AutomationProperties.Name="history" BorderThickness="0" Margin="0 1" Opacity="0" Padding="5 4"/>
                            </telerik:RadSplitButton.Content>
                            <telerik:RadSplitButton.DropDownContent>
                                <ListBox x:Name="HistoryListBox"
                                        BorderThickness="0"
                                        Padding="2"
                                        HorizontalAlignment="Stretch"
                                        VerticalAlignment="Stretch"
                                        HorizontalContentAlignment="Stretch"
                                        VerticalContentAlignment="Stretch"
                                        ItemContainerStyle="{StaticResource BreadcrumbListBoxItemStyle}"
                                        ItemsSource="{TemplateBinding HistoryItems}"
                                        SelectedItem="{Binding RelativeSource={RelativeSource TemplatedParent}, Path=SelectedHistoryItem, Mode=TwoWay}"/>
                            </telerik:RadSplitButton.DropDownContent>
                        </telerik:RadSplitButton>
                        <telerikBreadcrumb:RadBreadcrumbBar x:Name="BreadcrumbBar"
                                Grid.Column="1"
                                Foreground="{TemplateBinding Foreground}"
                                Background="Transparent"
                                ItemTemplate="{TemplateBinding ItemTemplate}"
                                ItemTemplateSelector="{TemplateBinding ItemTemplateSelector}"/>
                        <Popup x:Name="AutocompletePopup" AllowsTransparency="True" StaysOpen="False" Focusable="False" Placement="Bottom">
                            <Border x:Name="DropDownPopupBorder"
                                    Background="{telerik:Windows8TouchResource ResourceKey=MainBrush}"
                                    BorderBrush="{telerik:Windows8TouchResource ResourceKey=MediumBrush}"
                                    BorderThickness="1"
                                    Margin="0 4 0 0">
                                <ListBox x:Name="AutoCompleteListBox"
                                        ItemContainerStyle="{StaticResource BreadcrumbListBoxItemStyle}"
                                        BorderThickness="0"
                                        Padding="2"
                                        ItemsSource="{TemplateBinding AutocompleteItems}"
                                        MinWidth="200"
                                        MinHeight="40"
                                        MaxWidth="400"
                                        MaxHeight="700"
                                        HorizontalContentAlignment="Stretch"
                                        VerticalContentAlignment="Stretch"
                                        HorizontalAlignment="Stretch"
                                        VerticalAlignment="Stretch"
                                        SelectedItem="{Binding SelectedAutocompleteItem, Mode=TwoWay, RelativeSource={RelativeSource TemplatedParent}}"/>
                            </Border>
                        </Popup>
                    </Grid>
                </ControlTemplate>
            </Setter.Value>
        </Setter>
    </Style>
