                        AutomationProperties.Name="listlayout"
                        DataContext="{Binding DataContext, RelativeSource={RelativeSource AncestorType={x:Type historyNavigationPane:FileBrowserTabControl}}}">
                    <historyNavigationPane:FileBrowserListBox.ItemsPanel>
                        <ItemsPanelTemplate>
                            <telerik:VirtualizingWrapPanel Orientation="Vertical" ItemHeight="36" ScrollStep="108" ItemWidth="230"/>
