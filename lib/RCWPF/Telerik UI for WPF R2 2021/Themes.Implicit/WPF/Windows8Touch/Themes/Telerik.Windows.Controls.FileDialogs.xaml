                    </historyNavigationPane:FileBrowserListBox.ItemsPanel>
                    <historyNavigationPane:FileBrowserListBox.ItemTemplate>
                        <DataTemplate>
                            <StackPanel Orientation="Horizontal">
                                <Image x:Name="image" Source="{Binding SmallImage}" Width="16" Height="16" Stretch="Uniform"/>
                                <TextBlock
                                        Margin="4 0 0 0"
                                        Text="{Binding Label}"
                                        MaxWidth="200"
                                        TextTrimming="CharacterEllipsis"
                                        VerticalAlignment="Center"
                                        Visibility="{Binding IsInEditMode, Converter={StaticResource ResourceKey=InvertedBooleanToVisibilityConverter}}"/>
                                <historyNavigationPane:FileBrowserEditTextBox
                                        Margin="4 0 0 0"
                                        Text="{Binding Name, Mode=TwoWay}"
                                        MaxWidth="200"
                                        VerticalAlignment="Center"
                                        Padding="2 0 0 0"
                                        Visibility="{Binding IsInEditMode, Converter={StaticResource ResourceKey=BooleanToVisibilityConverter}}"/>
                            </StackPanel>
