        <Grid x:Name="LayoutRoot">
            <Border x:Name="Shadow" Margin="8 0 8 8" Background="{TemplateBinding Background}">
                <Border.Effect>
                    <DropShadowEffect Opacity="0.3" BlurRadius="10" Direction="280" ShadowDepth="3"/>
                </Border.Effect>
            </Border>
            <Border x:Name="WindowOuterBorder"
                    Grid.RowSpan="2"
                    BorderBrush="{TemplateBinding BorderBrush}"
                    Margin="8 0 8 8"
                    BorderThickness="1"
                    Background="{TemplateBinding Background}">
                <AdornerDecorator>
                    <Grid x:Name="AdornerLayoutRoot">
                        <Grid.RowDefinitions>
                            <RowDefinition x:Name="WindowHeaderRow" Height="Auto" MinHeight="40"/>
                            <RowDefinition Height="*"/>
                        </Grid.RowDefinitions>
                        <Grid x:Name="Header" Background="{telerik:Windows8TouchResource ResourceKey=AccentBrush}">
                            <Grid.ColumnDefinitions>
                                <ColumnDefinition Width="*"/>
                                <ColumnDefinition Width="Auto"/>
                            </Grid.ColumnDefinitions>
                            <Grid Grid.Column="0" Margin="6 0">
                                <Grid.ColumnDefinitions>
                                    <ColumnDefinition Width="Auto"/>
                                    <ColumnDefinition Width="*"/>
                                </Grid.ColumnDefinitions>
                                <ContentPresenter x:Name="IconContent"
                                        Grid.Column="0"
                                        HorizontalAlignment="Left"
                                        VerticalAlignment="Center"
                                        Content="{TemplateBinding Icon}"
                                        ContentTemplate="{TemplateBinding IconTemplate}"/>
                                <ContentControl x:Name="HeaderContent"
                                        Grid.Column="1"
                                        IsTabStop="False"
                                        HorizontalAlignment="Stretch"
                                        VerticalAlignment="Center"
                                        HorizontalContentAlignment="Stretch"
                                        Content="{TemplateBinding Header}"
                                        ContentTemplate="{TemplateBinding HeaderTemplate}"
                                        ContentTemplateSelector="{TemplateBinding HeaderTemplateSelector}"
                                        SnapsToDevicePixels="True"
                                        FontSize="{telerik:Windows8TouchResource ResourceKey=FontSizeXL}"
                                        FontFamily="{telerik:Windows8TouchResource ResourceKey=FontFamilyLight}"
                                        Foreground="{telerik:Windows8TouchResource ResourceKey=InvertedForegroundBrush}"
                                        Padding="0 0 5 0"/>
                            </Grid>
                            <Border x:Name="PART_HeaderButtonsBorder" Grid.Column="1" Margin="4 0">
                                <telerik:RadButton x:Name="PART_CloseButton"
                                        Command="{x:Static telerik:WindowCommands.Close}"
                                        Style="{StaticResource WindowButtonStyle}"
                                        Visibility="{Binding IsEnabled, RelativeSource={RelativeSource Self}, Converter={StaticResource BooleanToVisibilityConverter}}">
                                    <ToolTipService.ToolTip>
                                        <ToolTip telerik:LocalizationManager.ResourceKey="Close"/>
                                    </ToolTipService.ToolTip>
                                    <Grid>
                                        <Ellipse Stretch="Fill" Stroke="{telerik:Windows8TouchResource ResourceKey=MainBrush}" StrokeThickness="2" Width="30" Height="30"/>
                                        <Path
                                                Data="F1M12.488,5.1L10.967,3.579L8,6.545L5.034,3.579L3.512,5.1L6.479,8.066L3.512,11.033L5.034,12.555L8,9.588L10.967,12.554L12.488,11.033L9.521,8.066z"
                                                Stretch="Fill"
                                                Fill="{telerik:Windows8TouchResource ResourceKey=MainBrush}"
                                                Height="16"
                                                Width="16"/>
                                    </Grid>
                                </telerik:RadButton>
                            </Border>
                        </Grid>
                        <Border x:Name="ContentOuterBorder" Grid.Row="1" Margin="{TemplateBinding BorderThickness}">
                            <ContentPresenter x:Name="ContentElement" Margin="{TemplateBinding Padding}" Content="{TemplateBinding Content}" ContentTemplate="{TemplateBinding ContentTemplate}"/>
                        </Border>
                        <Path x:Name="ResizeIcon"
                                Grid.Row="1"
                                Fill="{TemplateBinding BorderBrush}"
                                HorizontalAlignment="Right"
                                VerticalAlignment="Bottom"
                                Margin="0 0 3 3"
                                Stretch="None"
                                Stroke="{x:Null}"
                                Data="M8,8 L10,8 10,10 8,10 z M4,8 L6,8 6,10 4,10 z M0,8 L2,8 2,10 0,10 z M8,4 L10,4 10,6 8,6 z M4,4 L6,4 6,6 4,6 z M8,0 L10,0 10,2 8,2 z"/>
                    </Grid>
                </AdornerDecorator>
            </Border>
        </Grid>
        <ControlTemplate.Triggers>
            <Trigger Property="IsInActiveState" Value="False">
                <Setter TargetName="HeaderContent" Property="Opacity" Value="0.7"/>
                <Setter TargetName="PART_HeaderButtonsBorder" Property="Opacity" Value="0.7"/>
                <Setter TargetName="ResizeIcon" Property="Opacity" Value="0.5"/>
                <Setter TargetName="Shadow" Property="Opacity" Value="0.5"/>
            </Trigger>
            <Trigger Property="WindowState" Value="Maximized">
                <Setter Property="BorderThickness" Value="0"/>
                <Setter TargetName="WindowOuterBorder" Property="BorderThickness" Value="0"/>
                <Setter TargetName="Shadow" Property="Visibility" Value="Collapsed"/>
                <Setter TargetName="WindowOuterBorder" Property="Margin" Value="0"/>
                <Setter TargetName="ResizeIcon" Property="Visibility" Value="Collapsed"/>
            </Trigger>
            <Trigger Property="WindowState" Value="Minimized">
                <Setter TargetName="WindowHeaderRow" Property="Height" Value="28"/>
                <Setter TargetName="WindowHeaderRow" Property="MinHeight" Value="28"/>
                <Setter TargetName="HeaderContent" Property="Margin" Value="0"/>
                <Setter TargetName="LayoutRoot" Property="HorizontalAlignment" Value="Left"/>
                <Setter TargetName="LayoutRoot" Property="VerticalAlignment" Value="Top"/>
                <Setter TargetName="PART_CloseButton" Property="Padding" Value="0"/>
                <Setter TargetName="AdornerLayoutRoot" Property="Width" Value="145"/>
            </Trigger>
        </ControlTemplate.Triggers>
    </ControlTemplate>
