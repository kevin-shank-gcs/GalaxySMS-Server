                                Stretch="Fill"
                                RenderTransformOrigin="0.5,0.5"
                                Opacity="0"
                                SnapsToDevicePixels="True">
                            <Path.RenderTransform>
                                <TranslateTransform X="-1"/>
                            </Path.RenderTransform>
                        </Path>
                        <Path x:Name="arrowChecked"
                                Data="M0,0 26,0 36,18 26,36 0,36"
                                Stroke="{TemplateBinding BorderBrush}"
                                StrokeThickness="1"
                                Fill="{telerik:Windows8TouchResource ResourceKey=MainBrush}"
                                Stretch="Fill"
                                RenderTransformOrigin="0.5,0.5"
                                Opacity="0"
                                SnapsToDevicePixels="True">
                            <Path.RenderTransform>
                                <TranslateTransform X="-1"/>
                            </Path.RenderTransform>
                        </Path>
                        <ContentControl
                                Margin="{TemplateBinding Padding}"
                                IsTabStop="False"
                                Content="{TemplateBinding Content}"
                                FontSize="{telerik:Windows8TouchResource ResourceKey=FontSizeL}"
                                FontFamily="{telerik:Windows8TouchResource ResourceKey=FontFamily}"
                                Foreground="{TemplateBinding Foreground}"
                                ContentTemplate="{TemplateBinding ContentTemplate}"
                                VerticalAlignment="{TemplateBinding VerticalContentAlignment}"
                                HorizontalAlignment="{TemplateBinding HorizontalContentAlignment}"
                                ContentTemplateSelector="{TemplateBinding ContentTemplateSelector}"/>
                    </Grid>
                </ControlTemplate>
            </Setter.Value>
        </Setter>
