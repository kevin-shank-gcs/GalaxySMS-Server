    <ControlTemplate x:Key="ValidationTooltipTemplate">
        <StackPanel Orientation="Horizontal">
            <Grid SnapsToDevicePixels="True">
                <AdornedElementPlaceholder x:Name="Holder"/>
                <Border BorderBrush="{telerik1:Windows8TouchResource ResourceKey=ValidationBrush}" BorderThickness="1"/>
            </Grid>
            <Grid MinHeight="36" Margin="2 0" SnapsToDevicePixels="True">
                <Grid>
                    <Grid.ColumnDefinitions>
                        <ColumnDefinition/>
                        <ColumnDefinition/>
                    </Grid.ColumnDefinitions>
                    <Path
                            Width="5"
                            Height="8"
                            Margin="0 0 -1 0"
                            Data="M4,0 L0,4 4,8Z"
                            Stretch="None"
                            StrokeThickness="0"
                            Fill="{telerik1:Windows8TouchResource ResourceKey=ValidationBrush}"
                            HorizontalAlignment="Left"
                            VerticalAlignment="Top"/>
                    <Border
                            Background="{telerik1:Windows8TouchResource ResourceKey=ValidationBrush}"
                            BorderBrush="{telerik1:Windows8TouchResource ResourceKey=ValidationBrush}"
                            BorderThickness="1"
                            Grid.Column="1"
                            Padding="1"
                            VerticalAlignment="Top"
                            MinHeight="36">
                        <Label
                                Foreground="{telerik1:Windows8TouchResource ResourceKey=InvertedForegroundBrush}"
                                FontSize="{telerik1:Windows8TouchResource ResourceKey=FontSizeL}"
                                FontFamily="{telerik1:Windows8TouchResource ResourceKey=FontFamily}"
                                Content="{Binding ElementName=Holder, Path=AdornedElement.(Validation.Errors).CurrentItem.ErrorContent}"/>
                    </Border>
                </Grid>
            </Grid>
        </StackPanel>
    </ControlTemplate>
    <telerik1:DragDropBooleanToVisibilityConverter x:Key="DragDropBooleanToVisibilityConverter"/>
    <telerik1:BooleanToVisibilityConverter x:Key="BooleanToVisibilityConverter"/>
    <telerik1:InvertedBooleanToVisibilityConverter x:Key="InvertedBooleanToVisibilityConverter"/>
    <telerik1:NullToVisibilityConverter x:Key="NullToVisibilityConverter"/>
    <sys:String x:Key="GlyphMoreHorizontal"></sys:String>
    <sys:String x:Key="GlyphClose"></sys:String>
    <sys:String x:Key="GlyphPin"></sys:String>
    <sys:String x:Key="GlyphUnpin"></sys:String>
    <sys:String x:Key="GlyphFile"></sys:String>
    <telerik1:ThicknessToOrientedThicknessConverter x:Key="ThicknessToOrientedThicknessConverter"/>
    <telerik1:CornerRadiusConverter x:Key="CornerRadiusConverter"/>
    <SolidColorBrush x:Key="TransparentBrush" Color="#01FFFFFF"/>
    <SolidColorBrush x:Key="RadHyperlinkButtonHoverBrush" Color="#74C2E7"/>
    <SolidColorBrush x:Key="RadHyperlinkButtonVisitedBrush" Color="#2B6E8E"/>
    <SolidColorBrush x:Key="WindowBorderBackground" Color="#FFCCE4FC"/>
    <SolidColorBrush x:Key="WindowModalBackground" Color="#333333FF"/>
    <Thickness x:Key="WindowButtonPadding">0</Thickness>
    <Thickness x:Key="WindowButtonMargin">0 0 7 0</Thickness>
    <SolidColorBrush x:Key="DisabledBorderBrush" Color="#E6E6E6"/>
