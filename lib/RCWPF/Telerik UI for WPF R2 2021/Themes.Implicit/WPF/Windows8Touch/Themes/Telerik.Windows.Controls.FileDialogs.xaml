                                Fill="White"
                                Height="12"
                                Stretch="Fill"
                                Width="12"/>
                    </Grid>
                </DataTemplate>
            </Setter.Value>
        </Setter>
    </Style>
    <Style TargetType="telerikNavigation:RadTreeView" x:Key="RadTreeViewStyle">
