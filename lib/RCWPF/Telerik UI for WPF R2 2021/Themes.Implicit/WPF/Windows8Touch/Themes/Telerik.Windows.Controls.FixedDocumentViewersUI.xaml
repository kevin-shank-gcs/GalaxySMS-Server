    <Style TargetType="treeView:TreeViewDragCue" x:Key="TreeViewDragCueStyle">
        <Setter Property="IsHitTestVisible" Value="False"/>
        <Setter Property="Padding" Value="5 8"/>
        <Setter Property="BorderBrush" Value="{telerik:Windows8TouchResource ResourceKey=MediumBrush}"/>
        <Setter Property="Background" Value="{telerik:Windows8TouchResource ResourceKey=MainBrush}"/>
        <Setter Property="BorderThickness" Value="1"/>
        <Setter Property="FontFamily" Value="{telerik:Windows8TouchResource ResourceKey=FontFamily}"/>
        <Setter Property="FontSize" Value="{telerik:Windows8TouchResource ResourceKey=FontSizeL}"/>
        <Setter Property="Foreground" Value="{telerik:Windows8TouchResource ResourceKey=MainForegroundBrush}"/>
        <Setter Property="Template">
            <Setter.Value>
                <ControlTemplate TargetType="treeView:TreeViewDragCue">
                    <Grid>
                        <Grid.RowDefinitions>
                            <RowDefinition Height="Auto"/>
                            <RowDefinition Height="Auto"/>
                        </Grid.RowDefinitions>
                        <VisualStateManager.VisualStateGroups>
                            <VisualStateGroup x:Name="DropStates">
                                <VisualState x:Name="DropPossible"/>
                                <VisualState x:Name="DropImpossible">
                                    <Storyboard>
                                        <ObjectAnimationUsingKeyFrames Storyboard.TargetName="DropImpossibleIconElement" Storyboard.TargetProperty="Visibility">
                                            <DiscreteObjectKeyFrame KeyTime="0">
                                                <DiscreteObjectKeyFrame.Value>
                                                    <Visibility>Visible</Visibility>
                                                </DiscreteObjectKeyFrame.Value>
                                            </DiscreteObjectKeyFrame>
                                        </ObjectAnimationUsingKeyFrames>
                                        <ObjectAnimationUsingKeyFrames Storyboard.TargetName="DropPossibleIconElement" Storyboard.TargetProperty="Visibility">
                                            <DiscreteObjectKeyFrame KeyTime="0">
                                                <DiscreteObjectKeyFrame.Value>
                                                    <Visibility>Collapsed</Visibility>
                                                </DiscreteObjectKeyFrame.Value>
                                            </DiscreteObjectKeyFrame>
                                        </ObjectAnimationUsingKeyFrames>
                                    </Storyboard>
                                </VisualState>
                            </VisualStateGroup>
                        </VisualStateManager.VisualStateGroups>
                        <Border BorderThickness="{TemplateBinding BorderThickness}" Grid.RowSpan="2" BorderBrush="{TemplateBinding BorderBrush}" Background="{TemplateBinding Background}"/>
                        <Grid HorizontalAlignment="Left" Visibility="{TemplateBinding DragPreviewVisibility}">
                            <ItemsPresenter Margin="{TemplateBinding Padding}"/>
                            <Grid x:Name="DraggedItemsCountContainer" Visibility="Collapsed" HorizontalAlignment="Center" VerticalAlignment="Center" Width="Auto" Height="Auto">
                                <Rectangle Stroke="{telerik:Windows8TouchResource ResourceKey=MediumBrush}" Fill="{TemplateBinding Background}" StrokeThickness="1"/>
                                <TextBlock x:Name="DraggedItemsCountText"
                                        Foreground="{telerik:Windows8TouchResource ResourceKey=InvertedForegroundBrush}"
                                        HorizontalAlignment="{TemplateBinding HorizontalAlignment}"
                                        VerticalAlignment="{TemplateBinding VerticalAlignment}"
                                        Margin="8 3"
                                        Text="5"/>
                            </Grid>
                        </Grid>
                        <Border Background="{telerik:Windows8TouchResource ResourceKey=AccentBrush}" Grid.Row="1"/>
                        <Grid Visibility="{TemplateBinding DragTooltipVisibility}" Grid.Row="1">
                            <Grid.ColumnDefinitions>
                                <ColumnDefinition Width="Auto"/>
                                <ColumnDefinition Width="Auto"/>
                            </Grid.ColumnDefinitions>
                            <ContentPresenter x:Name="DropImpossibleIconElement"
                                    Margin="5 8"
                                    Content="{TemplateBinding DropImpossibleIcon}"
                                    ContentTemplate="{TemplateBinding DropImpossibleIconTemplate}"
                                    Visibility="Collapsed"/>
                            <ContentPresenter x:Name="DropPossibleIconElement" Margin="5 8" Content="{TemplateBinding DropPossibleIcon}" ContentTemplate="{TemplateBinding DropPossibleIconTemplate}"/>
                            <StackPanel Orientation="Horizontal" VerticalAlignment="Center" Grid.Column="1" Margin="5 8">
                                <ContentControl
                                        VerticalAlignment="Center"
                                        Content="{TemplateBinding DragActionContent}"
                                        ContentTemplate="{TemplateBinding DragActionContentTemplate}"
                                        Foreground="{telerik:Windows8TouchResource ResourceKey=InvertedForegroundBrush}"
                                        FontSize="{telerik:Windows8TouchResource ResourceKey=FontSizeL}"/>
                                <ContentControl
                                        VerticalAlignment="Center"
                                        Margin="0 0 15 0"
                                        Content="{TemplateBinding DragTooltipContent}"
                                        ContentTemplate="{TemplateBinding DragTooltipContentTemplate}"
                                        Foreground="{telerik:Windows8TouchResource ResourceKey=InvertedForegroundBrush}"/>
                            </StackPanel>
                        </Grid>
                    </Grid>
                </ControlTemplate>
            </Setter.Value>
        </Setter>
        <Setter Property="DropPossibleIcon" Value="The icon is assigned as DataTemplate because assigning as Content triggers a bug in Silverlight"/>
        <Setter Property="DropImpossibleIcon" Value="The icon is assigned as DataTemplate because assigning as Content triggers a bug in Silverlight"/>
        <Setter Property="DropPossibleIconTemplate">
            <Setter.Value>
                <DataTemplate>
                    <Grid>
                        <Path
                                Height="11"
                                Width="12"
                                Stretch="Fill"
                                Data="M 0,10 C0,10 0,16 0,16 0,16 14,16 14,16 14,16 8,22 8,22 8,22 12,26 12,26 12,26 25,13 25,13 25,13 12,0 12,0 12,0 8,4 8,4 8,4 14,10 14,10 14,10 0,10 0,10 z"
                                Fill="White"
                                UseLayoutRounding="False"/>
