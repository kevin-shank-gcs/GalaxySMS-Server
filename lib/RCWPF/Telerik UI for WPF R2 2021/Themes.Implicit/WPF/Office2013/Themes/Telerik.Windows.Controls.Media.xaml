                                    IsBackgroundVisible="{TemplateBinding IsBackgroundVisible}"
                                    Visibility="{TemplateBinding DropDownIndicatorVisibility}"
                                    IsChecked="{Binding Path=IsOpen, Mode=TwoWay, RelativeSource={RelativeSource TemplatedParent}}"
                                    MinWidth="16"
                                    MinHeight="16"
                                    Focusable="False">
                                <Grid x:Name="PART_DropDownIndicatorInner"
                                        IsHitTestVisible="False"
                                        HorizontalAlignment="Center"
                                        Canvas.ZIndex="2"
                                        VerticalAlignment="Center"
                                        RenderTransformOrigin="0.5 0.5">
                                    <Path x:Name="Path"
                                            Fill="{Binding (TextElement.Foreground), RelativeSource={RelativeSource AncestorType=ContentPresenter}}"
                                            Margin="0 1"
                                            Data="M0,0 L2,0 1,1 Z"
                                            Width="{StaticResource ControlElement_Width}"
                                            Height="{StaticResource ControlElement_Height}"
                                            Stretch="Fill"/>
                                </Grid>
                            </telerik1:RadToggleButton>
                            <Grid Grid.Row="1" Grid.Column="1" Visibility="{Binding RelativeSource={RelativeSource TemplatedParent}, Mode=TwoWay, Path=IsButtonPartVisible, Converter={StaticResource BooleanToVisibilityConverter}}">
                                <Border x:Name="MouseOverButtonVisual"
                                        Background="{x:Null}"
                                        BorderThickness="1"
                                        IsHitTestVisible="False"
                                        Opacity="0"
                                        BorderBrush="{telerik1:Office2013Resource ResourceKey=AccentMainBrush}"/>
                            </Grid>
                        </Grid>
                        <Border x:Name="NormalVisual"
                                BorderThickness="{TemplateBinding BorderThickness}"
                                BorderBrush="{TemplateBinding BorderBrush}"
                                Visibility="Visible"
                                CornerRadius="{TemplateBinding CornerRadius}"
                                Background="{x:Null}"
                                IsHitTestVisible="False"/>
                        <Border x:Name="MouseOverVisual"
                                Visibility="Collapsed"
                                Background="{x:Null}"
                                BorderThickness="1"
                                CornerRadius="{TemplateBinding CornerRadius}"
                                IsHitTestVisible="False"
                                BorderBrush="{telerik1:Office2013Resource ResourceKey=AccentMainBrush}"/>
                        <Border x:Name="CommonStatesWrapper" Visibility="{Binding Path=IsPressed, ElementName = DropDownPart, Converter={StaticResource InvertedBooleanToVisibilityConverter}}">
                            <Border x:Name="FocusVisual"
                                    Visibility="{Binding Path=IsPressed, ElementName = ButtonPart, Converter={StaticResource InvertedBooleanToVisibilityConverter}}"
                                    Background="{x:Null}"
                                    BorderThickness="{TemplateBinding BorderThickness}"
                                    CornerRadius="{TemplateBinding CornerRadius}"
                                    Opacity="0"
                                    BorderBrush="{telerik1:Office2013Resource ResourceKey=AccentBrush}"/>
                        </Border>
                        <Popup x:Name="DropDownPopup" PopupAnimation="{TemplateBinding PopupAnimation}" AllowsTransparency="True" StaysOpen="False" Focusable="False" Placement="Bottom">
                            <Grid>
                                <Border x:Name="DropDownPopupBorder"
                                        BorderThickness="1"
                                        MinWidth="3"
                                        MinHeight="3"
                                        Width="{TemplateBinding DropDownWidth}"
                                        Height="{TemplateBinding DropDownHeight}"
                                        MaxWidth="{TemplateBinding DropDownMaxWidth}"
                                        MaxHeight="{TemplateBinding DropDownMaxHeight}"
                                        Background="{telerik1:Office2013Resource ResourceKey=MainBrush}"
                                        BorderBrush="{telerik1:Office2013Resource ResourceKey=HighDarkBrush}">
                                    <ContentControl x:Name="DropDownPopupContent"
                                            Foreground="{TemplateBinding Foreground}"
                                            Content="{TemplateBinding DropDownContent}"
                                            ContentTemplate="{TemplateBinding DropDownContentTemplate}"
                                            HorizontalContentAlignment="Stretch"
                                            VerticalContentAlignment="Stretch"
                                            HorizontalAlignment="Stretch"
                                            VerticalAlignment="Stretch"
                                            DataContext="{Binding RelativeSource={RelativeSource TemplatedParent}, Path=DataContext}"/>
                                </Border>
                            </Grid>
                        </Popup>
                    </Grid>
                    <ControlTemplate.Triggers>
                        <Trigger Property="DropDownPlacement" Value="Bottom"/>
                        <Trigger Property="DropDownPlacement" Value="Top">
                            <Setter TargetName="PART_DropDownIndicatorInner" Property="RenderTransform">
                                <Setter.Value>
                                    <TransformGroup>
                                        <RotateTransform Angle="180"/>
                                    </TransformGroup>
                                </Setter.Value>
                            </Setter>
                        </Trigger>
                        <Trigger Property="DropDownPlacement" Value="Left">
                            <Setter TargetName="PART_DropDownIndicatorInner" Property="RenderTransform">
                                <Setter.Value>
                                    <TransformGroup>
                                        <RotateTransform Angle="90"/>
                                    </TransformGroup>
                                </Setter.Value>
                            </Setter>
                        </Trigger>
                        <Trigger Property="DropDownPlacement" Value="Right">
                            <Setter TargetName="PART_DropDownIndicatorInner" Property="RenderTransform">
                                <Setter.Value>
                                    <TransformGroup>
                                        <RotateTransform Angle="-90"/>
                                    </TransformGroup>
                                </Setter.Value>
                            </Setter>
                        </Trigger>
                        <Trigger Property="IsKeyboardFocused" Value="False">
                            <Setter TargetName="FocusVisual" Property="Visibility" Value="Collapsed"/>
                        </Trigger>
                    </ControlTemplate.Triggers>
                </ControlTemplate>
            </Setter.Value>
        </Setter>
    </Style>
    <Style x:Key="RadHyperlinkButtonStyle" TargetType="telerik1:RadHyperlinkButton">
        <Setter Property="FontFamily" Value="{telerik1:Office2013Resource ResourceKey=FontFamily}"/>
        <Setter Property="FontSize" Value="{telerik1:Office2013Resource ResourceKey=FontSize}"/>
        <Setter Property="Foreground" Value="{telerik1:Office2013Resource ResourceKey=AccentMainBrush}"/>
        <Setter Property="VisitedForeground" Value="{StaticResource RadHyperlinkButtonVisitedBrush}"/>
        <Setter Property="VerticalContentAlignment" Value="Center"/>
        <Setter Property="Template">
            <Setter.Value>
                <ControlTemplate TargetType="telerik1:RadHyperlinkButton">
                    <Border x:Name="PART_Background"
                            BorderThickness="{TemplateBinding BorderThickness}"
                            BorderBrush="{TemplateBinding BorderBrush}"
                            Background="{TemplateBinding Background}"
                            CornerRadius="{TemplateBinding CornerRadius}">
                        <TextBlock VerticalAlignment="{TemplateBinding VerticalContentAlignment}" HorizontalAlignment="{TemplateBinding HorizontalContentAlignment}">
                            <Hyperlink x:Name="PART_Hyperlink"
                                    Command="{TemplateBinding Command}"
                                    AutomationProperties.Name="HyperlinkElement"
                                    CommandParameter="{TemplateBinding CommandParameter}"
                                    FontFamily="{TemplateBinding FontFamily}"
                                    FontSize="{TemplateBinding FontSize}"
                                    TextDecorations="{x:Null}"
                                    Cursor="Hand"
                                    TargetName="{TemplateBinding TargetName}"
                                    NavigateUri="{TemplateBinding NavigateUri}"
                                    Foreground="{TemplateBinding Foreground}">
                                <ContentPresenter x:Name="ContentPresenter"
                                        Content="{TemplateBinding Content}"
                                        ContentTemplate="{TemplateBinding ContentTemplate}"
                                        ContentTemplateSelector="{TemplateBinding ContentTemplateSelector}"
                                        ContentStringFormat="{TemplateBinding ContentStringFormat}"/>
                            </Hyperlink>
                        </TextBlock>
                    </Border>
                    <ControlTemplate.Triggers>
                        <Trigger SourceName="PART_Hyperlink" Property="IsMouseOver" Value="True">
                            <Setter Property="Foreground" Value="{StaticResource RadHyperlinkButtonHoverBrush}"/>
                            <Setter TargetName="PART_Hyperlink" Property="TextDecorations" Value="Underline"/>
                        </Trigger>
                        <Trigger Property="IsVisited" Value="True">
                            <Setter TargetName="PART_Hyperlink" Property="Foreground" Value="{Binding RelativeSource={RelativeSource TemplatedParent}, Path=VisitedForeground}"/>
                            <Setter TargetName="PART_Hyperlink" Property="TextDecorations" Value="Underline"/>
                        </Trigger>
                        <MultiTrigger>
                            <MultiTrigger.Conditions>
                                <Condition SourceName="PART_Hyperlink" Property="IsMouseOver" Value="True"/>
                                <Condition Property="IsVisited" Value="True"/>
                            </MultiTrigger.Conditions>
                            <Setter TargetName="PART_Hyperlink" Property="Foreground" Value="{StaticResource RadHyperlinkButtonHoverBrush}"/>
                        </MultiTrigger>
                        <Trigger Property="IsEnabled" Value="False">
                            <Setter TargetName="ContentPresenter" Property="Opacity" Value="0.5"/>
                        </Trigger>
                        <Trigger Property="IsBackgroundVisible" Value="False">
                            <Setter TargetName="PART_Background" Property="Background" Value="Transparent"/>
                        </Trigger>
                    </ControlTemplate.Triggers>
                </ControlTemplate>
            </Setter.Value>
        </Setter>
    </Style>
    <Style TargetType="telerik1:RadButton" BasedOn="{StaticResource RadButtonStyle}"/>
    <Style TargetType="telerik1:RadPathButton" BasedOn="{StaticResource RadPathButtonStyle}"/>
    <Style TargetType="telerik1:RadToggleButton" BasedOn="{StaticResource RadToggleButtonStyle}"/>
    <Style TargetType="telerik1:RadRadioButton" BasedOn="{StaticResource RadRadioButtonStyle}"/>
    <Style TargetType="telerik1:RadDropDownButton" BasedOn="{StaticResource RadDropDownButtonStyle}"/>
    <Style TargetType="telerik1:RadSplitButton" BasedOn="{StaticResource RadSplitButtonStyle}"/>
    <Style TargetType="telerik1:RadHyperlinkButton" BasedOn="{StaticResource RadHyperlinkButtonStyle}"/>
    <Style x:Key="WebCamButtonStyle" TargetType="telerik:RadButton" BasedOn="{StaticResource RadButtonStyle}">
        <Setter Property="Foreground" Value="{telerik:Office2013Resource ResourceKey=StrongBrush}"/>
        <Setter Property="MinWidth" Value="{StaticResource WebCam_ButtonMinSize}"/>
        <Setter Property="MinHeight" Value="{StaticResource WebCam_ButtonMinSize}"/>
        <Setter Property="Padding" Value="0"/>
        <Setter Property="Margin" Value="5 0"/>
        <Setter Property="VerticalAlignment" Value="Center"/>
        <Setter Property="VerticalContentAlignment" Value="Center"/>
        <Style.Triggers>
            <Trigger Property="IsMouseOver" Value="True">
                <Setter Property="Foreground" Value="{telerik:Office2013Resource ResourceKey=InvertedBrush}"/>
            </Trigger>
            <Trigger Property="IsPressed" Value="True">
                <Setter Property="Foreground" Value="{telerik:Office2013Resource ResourceKey=InvertedBrush}"/>
            </Trigger>
        </Style.Triggers>
    </Style>
    <ControlTemplate x:Key="RadWebCamTemplate" TargetType="telerikControls:RadWebCam">
        <Border x:Name="BorderVisual"
                Grid.RowSpan="2"
                Background="{TemplateBinding Background}"
                BorderThickness="{TemplateBinding BorderThickness}"
                BorderBrush="{TemplateBinding BorderBrush}"
                CornerRadius="{TemplateBinding mat:MaterialAssist.CornerRadius}"
                SnapsToDevicePixels="{TemplateBinding SnapsToDevicePixels}">
            <Grid x:Name="LayoutRoot" Background="{TemplateBinding Background}">
                <Grid.RowDefinitions>
                    <RowDefinition Height="*"/>
                    <RowDefinition Height="Auto"/>
                </Grid.RowDefinitions>
                <Image x:Name="PART_WebCamDisplay"
                        Stretch="Uniform"
                        VerticalAlignment="Center"
                        HorizontalAlignment="Center"
                        Visibility="{Binding HasError, RelativeSource={RelativeSource TemplatedParent}, Converter={StaticResource InvertedBooleanToVisibilityConverter}}"/>
                <Border x:Name="ToolbarPanel"
                        Grid.Row="1"
                        MinHeight="{StaticResource WebCam_ToolbarPanelMinHeight}"
                        BorderBrush="{TemplateBinding BorderBrush}"
                        BorderThickness="{StaticResource WebCam_ToolbarPanelBorderThickness}"
                        Visibility="{TemplateBinding ToolbarPanelVisibility}">
                    <Grid>
                        <StackPanel x:Name="RecordingLabelWrapper"
                                Margin="8 0 0 0"
                                Orientation="Horizontal"
                                HorizontalAlignment="Left"
                                VerticalAlignment="Center"
                                Visibility="{TemplateBinding RecordingLabelVisibility}">
                            <Border
                                    HorizontalAlignment="Left"
                                    VerticalAlignment="Center"
                                    Height="8"
                                    Width="8"
                                    Margin="8 0"
                                    CornerRadius="4"
                                    Background="{telerik:Office2013Resource ResourceKey=ValidationBrush}"/>
                            <TextBlock
                                    HorizontalAlignment="Left"
                                    VerticalAlignment="Center"
                                    Text="{Binding VideoRecordingElapsedTimeText, RelativeSource={RelativeSource TemplatedParent}}"
                                    FontSize="{telerik:Office2013Resource ResourceKey=FontSize}"
                                    Foreground="{TemplateBinding Foreground}"/>
                        </StackPanel>
                        <StackPanel HorizontalAlignment="Center" VerticalAlignment="Center" Orientation="Horizontal">
                            <telerik:RadButton x:Name="PART_SnapshotButton"
                                    Command="{x:Static telerikControls:RadWebCamCommands.TakeSnapshot}"
                                    Style="{StaticResource WebCamButtonStyle}"
                                    Visibility="{TemplateBinding SnapshotButtonVisibility}">
                                <ToolTipService.ToolTip>
                                    <ToolTip Content="{telerik:LocalizableResource Key=WebCam_TakeSnapshot}"/>
                                </ToolTipService.ToolTip>
                                <Path x:Name="SnapshotIcon" Data="{StaticResource SnapshotIcon}" Fill="{Binding Foreground, ElementName=PART_SnapshotButton}" Width="16" Height="16" Stretch="Fill"/>
                            </telerik:RadButton>
                            <telerik:RadToggleButton x:Name="PART_RecordButton"
                                    MinWidth="{StaticResource WebCam_ButtonMinSize}"
                                    MinHeight="{StaticResource WebCam_ButtonMinSize}"
                                    Padding="0"
                                    Margin="5 0"
                                    VerticalAlignment="Center"
                                    VerticalContentAlignment="Center"
                                    IsChecked="{Binding IsRecording, RelativeSource={RelativeSource TemplatedParent}, Mode=OneWay}"
                                    Command="{x:Static telerikControls:RadWebCamCommands.ToggleRecording}"
                                    CommandTarget="{Binding RelativeSource={RelativeSource TemplatedParent}}"
                                    Visibility="{TemplateBinding RecordingButtonVisibility}">
                                <ToolTipService.ToolTip>
                                    <ToolTip Content="{telerik:LocalizableResource Key=WebCam_StartRecording}"/>
                                </ToolTipService.ToolTip>
                                <Grid>
                                    <Path x:Name="RecordingIcon" Data="{StaticResource RecordingIcon}" Fill="{telerik:Office2013Resource ResourceKey=StrongBrush}" Width="16" Height="10" Stretch="Fill"/>
                                    <telerik:RadGlyph x:Name="StopRecordingIcon"
                                            Glyph="{StaticResource GlyphStop}"
                                            Foreground="{telerik:Office2013Resource ResourceKey=ValidationBrush}"
                                            FontSize="12"
                                            Visibility="Collapsed"/>
                                </Grid>
                            </telerik:RadToggleButton>
                            <telerik:RadButton x:Name="PART_SaveSnapshot"
                                    Command="{x:Static telerikControls:RadWebCamCommands.SaveSnapshot}"
                                    Style="{StaticResource WebCamButtonStyle}"
                                    Visibility="{Binding IsPreviewingSnapshot, RelativeSource={RelativeSource TemplatedParent}, Converter={StaticResource BooleanToVisibilityConverter}}">
                                <ToolTipService.ToolTip>
                                    <ToolTip Content="{telerik:LocalizableResource Key=WebCam_SaveSnapshot}"/>
                                </ToolTipService.ToolTip>
                                <telerik:RadGlyph x:Name="SaveIcon" Glyph="{StaticResource GlyphSave}" Foreground="{Binding Foreground, ElementName=PART_SaveSnapshot}" FontSize="16"/>
                            </telerik:RadButton>
                            <telerik:RadButton x:Name="PART_DiscardSnapshot"
                                    Command="{x:Static telerikControls:RadWebCamCommands.DiscardSnapshot}"
                                    Style="{StaticResource WebCamButtonStyle}"
                                    Visibility="{Binding IsPreviewingSnapshot, RelativeSource={RelativeSource TemplatedParent}, Converter={StaticResource BooleanToVisibilityConverter}}">
                                <ToolTipService.ToolTip>
                                    <ToolTip Content="{telerik:LocalizableResource Key=WebCam_DiscardSnapshot}"/>
                                </ToolTipService.ToolTip>
                                <telerik:RadGlyph x:Name="DiscardIcon" Glyph="{StaticResource GlyphCloseCircle}" Foreground="{Binding Foreground, ElementName=PART_DiscardSnapshot}" FontSize="16"/>
                            </telerik:RadButton>
                        </StackPanel>
                        <telerik:RadButton x:Name="PART_SettingsButton"
                                HorizontalAlignment="Right"
                                Margin="{StaticResource WebCam_SettingsButtonMargin}"
                                Style="{StaticResource WebCamButtonStyle}"
                                Command="{x:Static telerikControls:RadWebCamCommands.ShowSettingsDialog}"
                                CommandTarget="{Binding RelativeSource={RelativeSource TemplatedParent}}"
                                Visibility="{TemplateBinding SettingsButtonVisibility}">
                            <ToolTipService.ToolTip>
                                <ToolTip Content="{telerik:LocalizableResource Key=WebCam_SettingsDialogTitle}"/>
                            </ToolTipService.ToolTip>
                            <telerik:RadGlyph x:Name="SettingsIcon" Glyph="{StaticResource GlyphGear}" Foreground="{Binding Foreground, ElementName=PART_SettingsButton}" FontSize="16"/>
                        </telerik:RadButton>
                    </Grid>
                </Border>
                <Grid x:Name="CameraErrorWrapper" Grid.RowSpan="2" Background="{telerik:Office2013Resource ResourceKey=MainBrush}" Visibility="Collapsed">
                    <ContentPresenter x:Name="PART_CameraErrorContent"
                            IsHitTestVisible="False"
                            Margin="0 40"
                            ContentSource="CameraErrorContent"
                            HorizontalAlignment="{TemplateBinding HorizontalContentAlignment}"
                            VerticalAlignment="{TemplateBinding VerticalContentAlignment}"
                            TextBlock.Foreground="{TemplateBinding Foreground}"
                            TextBlock.FontSize="{telerik:Office2013Resource ResourceKey=FontSizeXL}"
                            Visibility="{Binding HasError, RelativeSource={RelativeSource TemplatedParent}, Converter={StaticResource BooleanToVisibilityConverter}}">
                        <ContentPresenter.Resources>
                            <Style TargetType="{x:Type TextBlock}">
                                <Setter Property="TextWrapping" Value="Wrap"/>
                            </Style>
                        </ContentPresenter.Resources>
                    </ContentPresenter>
                </Grid>
            </Grid>
        </Border>
        <ControlTemplate.Triggers>
            <Trigger SourceName="PART_RecordButton" Property="IsMouseOver" Value="True">
                <Setter TargetName="RecordingIcon" Property="Fill" Value="{telerik:Office2013Resource ResourceKey=InvertedBrush}"/>
            </Trigger>
            <Trigger SourceName="PART_RecordButton" Property="IsPressed" Value="True">
                <Setter TargetName="RecordingIcon" Property="Fill" Value="{telerik:Office2013Resource ResourceKey=InvertedBrush}"/>
            </Trigger>
            <Trigger SourceName="PART_RecordButton" Property="IsChecked" Value="True">
                <Setter TargetName="RecordingIcon" Property="Visibility" Value="Collapsed"/>
                <Setter TargetName="StopRecordingIcon" Property="Visibility" Value="Visible"/>
            </Trigger>
            <Trigger SourceName="PART_DiscardSnapshot" Property="IsMouseOver" Value="True">
                <Setter TargetName="DiscardIcon" Property="Foreground" Value="{telerik:Office2013Resource ResourceKey=ValidationBrush}"/>
            </Trigger>
            <Trigger SourceName="PART_DiscardSnapshot" Property="IsPressed" Value="True">
                <Setter TargetName="DiscardIcon" Property="Foreground" Value="{telerik:Office2013Resource ResourceKey=ValidationBrush}"/>
            </Trigger>
            <Trigger SourceName="PART_CameraErrorContent" Property="Visibility" Value="Visible">
                <Setter TargetName="CameraErrorWrapper" Property="Visibility" Value="Visible"/>
            </Trigger>
            <MultiTrigger>
                <MultiTrigger.Conditions>
                    <Condition Property="RecordingLabelVisibility" Value="Visible"/>
                    <Condition SourceName="PART_RecordButton" Property="IsChecked" Value="False"/>
                </MultiTrigger.Conditions>
                <Setter TargetName="RecordingLabelWrapper" Property="Visibility" Value="Collapsed"/>
            </MultiTrigger>
            <MultiTrigger>
                <MultiTrigger.Conditions>
                    <Condition Property="SnapshotButtonVisibility" Value="Visible"/>
                    <Condition Property="IsPreviewingSnapshot" Value="True"/>
                </MultiTrigger.Conditions>
                <Setter TargetName="PART_SnapshotButton" Property="Visibility" Value="Collapsed"/>
            </MultiTrigger>
            <MultiTrigger>
                <MultiTrigger.Conditions>
                    <Condition Property="RecordingButtonVisibility" Value="Visible"/>
                    <Condition Property="IsPreviewingSnapshot" Value="True"/>
                </MultiTrigger.Conditions>
                <Setter TargetName="PART_RecordButton" Property="Visibility" Value="Collapsed"/>
            </MultiTrigger>
            <MultiTrigger>
                <MultiTrigger.Conditions>
                    <Condition Property="SettingsButtonVisibility" Value="Visible"/>
                    <Condition Property="IsPreviewingSnapshot" Value="True"/>
                </MultiTrigger.Conditions>
                <Setter TargetName="PART_SettingsButton" Property="Visibility" Value="Collapsed"/>
            </MultiTrigger>
            <Trigger Property="IsEnabled" Value="False">
                <Setter TargetName="PART_WebCamDisplay" Property="Opacity" Value="{telerik1:Office2013Resource ResourceKey=DisabledOpacity}"/>
            </Trigger>
        </ControlTemplate.Triggers>
    </ControlTemplate>
    <Style x:Key="RadWebCamStyle" TargetType="telerikControls:RadWebCam">
        <Setter Property="Template" Value="{StaticResource RadWebCamTemplate}"/>
        <Setter Property="mat:MaterialAssist.CornerRadius" Value="{StaticResource WebCam_CornerRadius}"/>
        <Setter Property="Foreground" Value="{telerik:Office2013Resource ResourceKey=InvertedBrush}"/>
        <Setter Property="Background" Value="{telerik:Office2013Resource ResourceKey=MainBrush}"/>
        <Setter Property="BorderBrush" Value="{telerik:Office2013Resource ResourceKey=HighDarkBrush}"/>
        <Setter Property="BorderThickness" Value="{StaticResource WebCam_BorderThickness}"/>
        <Setter Property="MinWidth" Value="{StaticResource WebCam_MinWidth}"/>
        <Setter Property="MinHeight" Value="{StaticResource WebCam_MinHeight}"/>
        <Setter Property="HorizontalContentAlignment" Value="Center"/>
        <Setter Property="VerticalContentAlignment" Value="Center"/>
    </Style>
    <Style TargetType="telerikControls:RadWebCam" BasedOn="{StaticResource RadWebCamStyle}"/>
    <ControlTemplate x:Key="CameraSettingsControlTemplate" TargetType="telerikControls:CameraSettingsControl">
        <Grid>
            <telerik:RadTabControl
                    ItemsSource="{Binding SettingsViewModel}"
                    Margin="-1 0"
                    Padding="16 0"
                    SelectedItem="{Binding SelectedSettingsViewModelBase, Mode=TwoWay}"
                    IsContentPreserved="True"
                    BorderThickness="0"
                    Background="Transparent">
                <telerik:RadTabControl.ItemTemplate>
                    <DataTemplate>
                        <TextBlock Text="{Binding Title, Converter={StaticResource LocalizationConverter}}"/>
                    </DataTemplate>
                </telerik:RadTabControl.ItemTemplate>
                <telerik:RadTabControl.ContentTemplate>
                    <DataTemplate>
                        <Grid Grid.IsSharedSizeScope="True">
                            <Grid.ColumnDefinitions>
                                <ColumnDefinition Width="2*"/>
                                <ColumnDefinition Width="2*"/>
                                <ColumnDefinition Width="*"/>
                                <ColumnDefinition Width="Auto" SharedSizeGroup="A"/>
                            </Grid.ColumnDefinitions>
                            <Grid.RowDefinitions>
                                <RowDefinition Height="Auto"/>
                                <RowDefinition Height="Auto"/>
                                <RowDefinition Height="Auto"/>
                            </Grid.RowDefinitions>
                            <telerik:Label Grid.Column="3" Content="{telerik:LocalizableResource Key=WebCam_Settings_Auto}" Margin="0 4 8 4" HorizontalAlignment="Center"/>
                            <ItemsControl Grid.Row="1" Grid.ColumnSpan="4" ItemsSource="{Binding}">
                                <ItemsControl.ItemTemplate>
                                    <DataTemplate>
                                        <Grid IsEnabled="{Binding Enabled, Mode=OneWay}" Margin="8 4">
                                            <Grid.ColumnDefinitions>
                                                <ColumnDefinition Width="2*"/>
                                                <ColumnDefinition Width="2*"/>
                                                <ColumnDefinition Width="*"/>
                                                <ColumnDefinition Width="Auto" SharedSizeGroup="A"/>
                                            </Grid.ColumnDefinitions>
                                            <telerik:Label
                                                    Grid.Column="0"
                                                    Content="{Binding DisplayName, Converter={StaticResource LocalizationConverter}}"
                                                    Margin="8 0"
                                                    HorizontalAlignment="Left"
                                                    VerticalAlignment="Center"
                                                    VerticalContentAlignment="Center"/>
                                            <telerik:RadSlider
                                                    Grid.Column="1"
                                                    Margin="8 0"
                                                    VerticalAlignment="Center"
                                                    VerticalContentAlignment="Center"
                                                    IsEnabled="{Binding CheckBoxChecked, Converter={StaticResource InvertedBooleanConverter}}"
                                                    Minimum="{Binding MinValue}"
                                                    Maximum="{Binding MaxValue}"
                                                    SmallChange="{Binding Delta}"
                                                    Value="{Binding Value}"
                                                    IsSnapToTickEnabled="True"
                                                    TickFrequency="{Binding Delta}"/>
                                            <TextBlock Grid.Column="2" Margin="8 0" Text="{Binding Value}" VerticalAlignment="Center"/>
                                            <CheckBox Grid.Column="3" HorizontalAlignment="Center" IsChecked="{Binding CheckBoxChecked}" IsEnabled="{Binding CheckBoxEnabled, Mode=OneWay}">
                                                <ToolTipService.ToolTip>
                                                    <ToolTip Content="Auto"/>
                                                </ToolTipService.ToolTip>
                                            </CheckBox>
                                        </Grid>
                                    </DataTemplate>
                                </ItemsControl.ItemTemplate>
                            </ItemsControl>
                            <telerik:RadButton
                                    Grid.Column="1"
                                    Grid.Row="2"
                                    Command="{Binding DataContext.DefaultCommand,RelativeSource={RelativeSource AncestorType={x:Type telerikControls:CameraSettingsControl}}}"
                                    Margin="8 16"
                                    Content="{telerik:LocalizableResource Key=WebCam_Settings_Default}"
                                    HorizontalAlignment="Left"
                                    VerticalAlignment="Center"/>
                        </Grid>
                    </DataTemplate>
                </telerik:RadTabControl.ContentTemplate>
            </telerik:RadTabControl>
        </Grid>
    </ControlTemplate>
    <Style x:Key="CameraSettingsControlStyle" TargetType="telerikControls:CameraSettingsControl">
        <Setter Property="Template" Value="{StaticResource CameraSettingsControlTemplate}"/>
        <Setter Property="Width" Value="530"/>
        <Setter Property="Height" Value="460"/>
    </Style>
    <Style TargetType="telerikControls:CameraSettingsControl" BasedOn="{StaticResource CameraSettingsControlStyle}"/>
</ResourceDictionary>
