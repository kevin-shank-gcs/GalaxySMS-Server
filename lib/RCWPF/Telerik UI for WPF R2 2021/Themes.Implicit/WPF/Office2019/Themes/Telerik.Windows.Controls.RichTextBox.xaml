                                <telerikInput:RadColorPaletteView.ItemsPanel>
                                    <ItemsPanelTemplate>
                                        <primitives:RadUniformGrid Columns="10"/>
                                    </ItemsPanelTemplate>
                                </telerikInput:RadColorPaletteView.ItemsPanel>
                            </telerikInput:RadColorPaletteView>
                            <telerikInput:RadColorPaletteView x:Name="MainPalette"
                                    Grid.Row="3"
                                    Padding="3"
                                    BorderThickness="0"
                                    ItemTemplate="{TemplateBinding PaletteItemsTemplate}"
                                    ColorPropertyPath="{TemplateBinding ColorPropertyPath}"
                                    ItemContainerStyle="{TemplateBinding MainPaletteItemsStyle}"
                                    PaletteColumnsCount="{TemplateBinding MainPaletteColumnsCount}"/>
                            <Border x:Name="StandardPaletteBorder" Grid.Row="4" Background="{telerik1:Office2019Resource ResourceKey=AlternativeBackgroundBrush}" Margin="1 0" MinHeight="22">
                                <TextBlock x:Name="standardPaletteHeader"
                                        FontWeight="{TemplateBinding FontWeight}"
                                        Foreground="{TemplateBinding Foreground}"
                                        FontSize="{TemplateBinding FontSize}"
                                        VerticalAlignment="Center"
                                        Text="{TemplateBinding StandardPaletteHeaderText}"
                                        Margin="3 0"/>
                            </Border>
                            <telerikInput:RadColorPaletteView x:Name="StandartPalette"
                                    Grid.Row="5"
                                    Padding="3"
                                    BorderThickness="0"
                                    ItemTemplate="{TemplateBinding PaletteItemsTemplate}"
                                    ColorPropertyPath="{TemplateBinding ColorPropertyPath}">
                                <telerikInput:RadColorPaletteView.ItemsPanel>
                                    <ItemsPanelTemplate>
                                        <primitives:RadUniformGrid Columns="10"/>
                                    </ItemsPanelTemplate>
                                </telerikInput:RadColorPaletteView.ItemsPanel>
                            </telerikInput:RadColorPaletteView>
                            <Border x:Name="RecentColorsPaletteBorder"
                                    Grid.Row="6"
                                    Visibility="Collapsed"
                                    Padding="3"
                                    Margin="1 0"
                                    Background="{telerik1:Office2019Resource ResourceKey=AlternativeBackgroundBrush}"
                                    MinHeight="22">
                                <TextBlock x:Name="RecentColorsPaletteHeader"
                                        FontWeight="{TemplateBinding FontWeight}"
                                        Foreground="{TemplateBinding Foreground}"
                                        FontSize="{TemplateBinding FontSize}"
                                        VerticalAlignment="Center"
                                        Text="{TemplateBinding RecentColorsHeaderText}"
                                        Margin="3 0"/>
                            </Border>
                            <telerikInput:RadColorPaletteView x:Name="RecentColorsPalette"
                                    Grid.Row="7"
                                    BorderThickness="0"
                                    Padding="3"
                                    ItemTemplate="{TemplateBinding PaletteItemsTemplate}"
                                    ColorPropertyPath="{TemplateBinding ColorPropertyPath}"
                                    Visibility="Collapsed">
                                <telerikInput:RadColorPaletteView.ItemsPanel>
                                    <ItemsPanelTemplate>
                                        <primitives:RadUniformGrid Columns="10" Rows="1"/>
                                    </ItemsPanelTemplate>
                                </telerikInput:RadColorPaletteView.ItemsPanel>
                            </telerikInput:RadColorPaletteView>
                            <Border x:Name="AdditionalContentBorder" Grid.Row="8" Background="{telerik1:Office2019Resource ResourceKey=AlternativeBackgroundBrush}" Visibility="Collapsed">
                                <ContentPresenter Content="{TemplateBinding AdditionalContent}" ContentTemplate="{TemplateBinding AdditionalContentTemplate}"/>
