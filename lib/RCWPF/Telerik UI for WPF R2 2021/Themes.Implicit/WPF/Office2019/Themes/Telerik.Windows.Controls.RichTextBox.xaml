                                MinWidth="{StaticResource ColorPaletteViewItemMinSize}"
                                BorderBrush="{Binding RelativeSource={RelativeSource TemplatedParent}, Path=(helpers:ThemeHelper.FocusBrush)}"
                                BorderThickness="1"
