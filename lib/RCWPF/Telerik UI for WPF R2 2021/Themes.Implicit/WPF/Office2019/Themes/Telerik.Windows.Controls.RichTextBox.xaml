            <Trigger Property="IsMouseOver" SourceName="PopupButton" Value="True">
                <Setter Property="Foreground" TargetName="ArrowOverflow" Value="{telerik1:Office2019Resource ResourceKey=HighlightedForegroundBrush}"/>
            </Trigger>
            <Trigger Property="IsPressed" SourceName="ScrollUpButton" Value="True">
