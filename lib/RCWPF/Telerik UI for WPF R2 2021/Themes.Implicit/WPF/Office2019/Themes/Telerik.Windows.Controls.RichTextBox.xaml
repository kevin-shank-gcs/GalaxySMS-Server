    <ControlTemplate x:Key="RulerThumbControlTemplate" TargetType="telerikControlsRTBUI:RulerThumb">
        <Grid>
            <ToolTipService.ToolTip>
                <ToolTip Content="{TemplateBinding ToolTipText}"/>
            </ToolTipService.ToolTip>
            <Path x:Name="Indent"
                    Stretch="Fill"
                    Data="{StaticResource IndentIcon}"
                    Visibility="Collapsed"
                    RenderTransformOrigin="0.5 0.5"
                    Fill="{TemplateBinding Background}"
                    Height="8"
                    Width="9"
                    Stroke="{TemplateBinding Foreground}"
                    SnapsToDevicePixels="True"/>
            <Path x:Name="FirstLineIndent"
                    Stretch="Fill"
                    Data="{StaticResource FirstLineIndentIcon}"
                    SnapsToDevicePixels="True"
                    RenderTransformOrigin="0.5 0.5"
                    Height="8"
                    Width="9"
                    Visibility="Collapsed"
                    Fill="{TemplateBinding Background}"
                    Stroke="{TemplateBinding Foreground}"
                    VerticalAlignment="{TemplateBinding VerticalContentAlignment}"
                    HorizontalAlignment="{TemplateBinding HorizontalContentAlignment}"/>
            <Rectangle x:Name="LeftIndent"
                    Fill="{TemplateBinding Background}"
                    Visibility="Collapsed"
                    VerticalAlignment="{TemplateBinding VerticalContentAlignment}"
                    HorizontalAlignment="{TemplateBinding HorizontalContentAlignment}"
                    Height="5"
                    Width="9"
                    Stroke="{TemplateBinding Foreground}"/>
            <Path x:Name="LeftTab"
                    Data="{StaticResource LeftTabIcon}"
                    VerticalAlignment="{TemplateBinding VerticalContentAlignment}"
                    HorizontalAlignment="{TemplateBinding HorizontalContentAlignment}"
                    Visibility="Collapsed"
                    Fill="{TemplateBinding Foreground}"
                    Stretch="Fill"
                    UseLayoutRounding="True"
                    SnapsToDevicePixels="True"/>
            <Path x:Name="RightTab"
                    Data="{StaticResource RightTabIcon}"
                    Visibility="Collapsed"
                    Fill="{TemplateBinding Foreground}"
                    SnapsToDevicePixels="True"
                    VerticalAlignment="{TemplateBinding VerticalContentAlignment}"
                    HorizontalAlignment="{TemplateBinding HorizontalContentAlignment}"
                    UseLayoutRounding="True"
                    Stretch="Fill"/>
            <Path x:Name="CenterTab"
                    Data="{StaticResource CenterTabIcon}"
                    Visibility="Collapsed"
                    Fill="{TemplateBinding Foreground}"
                    SnapsToDevicePixels="True"
                    Stretch="Fill"
                    VerticalAlignment="{TemplateBinding VerticalContentAlignment}"
                    HorizontalAlignment="{TemplateBinding HorizontalContentAlignment}"
                    UseLayoutRounding="True"/>
            <Path x:Name="DecimalTab"
                    Data="{StaticResource DecimalTabIcon}"
                    Visibility="Collapsed"
                    Fill="{TemplateBinding Foreground}"
                    SnapsToDevicePixels="True"
                    Stretch="Fill"
                    VerticalAlignment="{TemplateBinding VerticalContentAlignment}"
                    HorizontalAlignment="{TemplateBinding HorizontalContentAlignment}"/>
            <Path x:Name="BarTab"
                    Visibility="Collapsed"
                    Data="{StaticResource BarTabIcon}"
                    Fill="{TemplateBinding Foreground}"
                    UseLayoutRounding="True"
                    VerticalAlignment="{TemplateBinding VerticalContentAlignment}"
                    HorizontalAlignment="{TemplateBinding HorizontalContentAlignment}"
                    Stretch="Fill"
                    Width="2"
                    Height="5"
                    SnapsToDevicePixels="True"/>
        </Grid>
        <ControlTemplate.Triggers>
            <Trigger Property="RulerThumbType" Value="LeftTabStop">
                <Setter Property="Visibility" TargetName="LeftTab" Value="Visible"/>
            </Trigger>
            <Trigger Property="RulerThumbType" Value="RightTabStop">
                <Setter Property="Visibility" TargetName="RightTab" Value="Visible"/>
            </Trigger>
            <Trigger Property="RulerThumbType" Value="CenterTabStop">
                <Setter Property="Visibility" TargetName="CenterTab" Value="Visible"/>
            </Trigger>
            <Trigger Property="RulerThumbType" Value="DecimalTabStop">
                <Setter Property="Visibility" TargetName="DecimalTab" Value="Visible"/>
            </Trigger>
            <Trigger Property="RulerThumbType" Value="Indent">
                <Setter Property="Visibility" TargetName="Indent" Value="Visible"/>
            </Trigger>
            <Trigger Property="RulerThumbType" Value="FirstLineIndent">
                <Setter Property="Visibility" TargetName="FirstLineIndent" Value="Visible"/>
            </Trigger>
            <Trigger Property="RulerThumbType" Value="LeftIndent">
                <Setter Property="Visibility" TargetName="LeftIndent" Value="Visible"/>
            </Trigger>
        </ControlTemplate.Triggers>
    </ControlTemplate>
