        <Setter Property="mat:MaterialAssist.CheckedBrush" Value="{telerik1:Office2019Resource ResourceKey=AccentPressedBackgroundBrush}"/>
        <Setter Property="BorderBrush" Value="{x:Null}"/>
        <Setter Property="BorderThickness" Value="2"/>
        <Setter Property="Padding" Value="2"/>
