    <Style x:Key="LinePropertiesSettingsControlStyle" TargetType="taskPaneControls:LinePropertiesSettingsControl">
        <Setter Property="VerticalContentAlignment" Value="Center"/>
        <Setter Property="Template">
            <Setter.Value>
                <ControlTemplate TargetType="taskPaneControls:LinePropertiesSettingsControl">
                    <Grid>
                        <Grid.RowDefinitions>
                            <RowDefinition Height="Auto"/>
                            <RowDefinition Height="Auto"/>
                        </Grid.RowDefinitions>
                        <Grid.ColumnDefinitions>
                            <ColumnDefinition Width="Auto"/>
                            <ColumnDefinition Width="*"/>
                            <ColumnDefinition Width="Auto"/>
                        </Grid.ColumnDefinitions>
                        <TextBlock Text="{Binding Manager.Culture, Source={StaticResource localizationManager}, Converter={StaticResource cultureConverter}, ConverterParameter=Documents_RadRichTextBox_Shapes_TaskPane_Width}" Style="{StaticResource FormatTextBlockStyle}"/>
                        <telerik:RadNumericUpDown x:Name="numericUpDown"
                                NullValue="0"
                                CustomUnit="pt"
                                SmallChange="0.25"
                                Grid.Column="2"
                                Minimum="0"
                                Maximum="1584"
                                Value="{Binding SelectedWidth, RelativeSource={RelativeSource TemplatedParent}, Mode=TwoWay}"
                                Width="105"
                                HorizontalAlignment="Right"
                                VerticalAlignment="{TemplateBinding VerticalContentAlignment}"/>
                        <TextBlock Text="{Binding Manager.Culture, Source={StaticResource localizationManager}, Converter={StaticResource cultureConverter}, ConverterParameter=Documents_RadRichTextBox_Shapes_TaskPane_DashType}" Grid.Row="1" Style="{StaticResource FormatTextBlockStyle}"/>
                        <telerik:RadDropDownButton Grid.Row="1" Grid.Column="2" HorizontalAlignment="Right" VerticalAlignment="{TemplateBinding VerticalContentAlignment}">
                            <telerik:RadDropDownButton.Content>
                                <Image Source="{telerik:IconResource IconRelativePath=dashes.png, IconSources={StaticResource IconSourcesShapes}}" Stretch="None"/>
                            </telerik:RadDropDownButton.Content>
                            <telerik:RadDropDownButton.DropDownContent>
                                <taskPaneControls:DashTypeGallery x:Name="dashTypeGallery" SelectedOutlineDashType="{Binding SelectedDashType, RelativeSource={RelativeSource TemplatedParent}, Mode=TwoWay}"/>
                            </telerik:RadDropDownButton.DropDownContent>
                        </telerik:RadDropDownButton>
                    </Grid>
                </ControlTemplate>
            </Setter.Value>
        </Setter>
    </Style>
