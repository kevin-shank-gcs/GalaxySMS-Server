    <ControlTemplate x:Key="ScreenTipTemplate" TargetType="telerikRibbonView:ScreenTip">
        <Grid x:Name="RootElement">
            <Border Background="{TemplateBinding Background}" BorderThickness="{TemplateBinding BorderThickness}" BorderBrush="{TemplateBinding BorderBrush}">
                <Grid Margin="{TemplateBinding Padding}">
                    <Grid.ColumnDefinitions>
                        <ColumnDefinition Width="Auto"/>
                        <ColumnDefinition Width="*"/>
                    </Grid.ColumnDefinitions>
                    <Grid.RowDefinitions>
                        <RowDefinition Height="Auto"/>
                        <RowDefinition Height="Auto"/>
                    </Grid.RowDefinitions>
                    <TextBlock x:Name="TitlePresenter" Grid.ColumnSpan="2" Text="{TemplateBinding Title}" Margin="0 0 0 5" TextWrapping="Wrap" FontWeight="Bold"/>
                    <Image x:Name="Image" Grid.Row="1" Source="{TemplateBinding Icon}" Stretch="None" Margin="0 5 10 6" VerticalAlignment="Top" Visibility="Collapsed"/>
                    <TextBlock x:Name="DescriptionPresenter"
                            Grid.Row="1"
                            Grid.Column="1"
                            TextWrapping="Wrap"
                            FontWeight="Normal"
                            Text="{TemplateBinding Description}"
                            VerticalAlignment="Top"
                            Visibility="Collapsed"/>
                </Grid>
            </Border>
        </Grid>
        <ControlTemplate.Triggers>
            <Trigger Property="ScreenTipState" Value="TitleDescriptionImage">
                <Setter TargetName="DescriptionPresenter" Property="Visibility" Value="Visible"/>
                <Setter TargetName="Image" Property="Visibility" Value="Visible"/>
                <Setter TargetName="RootElement" Property="Width" Value="318"/>
            </Trigger>
            <Trigger Property="ScreenTipState" Value="TitleDescription">
                <Setter TargetName="DescriptionPresenter" Property="Visibility" Value="Visible"/>
                <Setter TargetName="RootElement" Property="Width" Value="210"/>
            </Trigger>
            <Trigger Property="ScreenTipState" Value="Title">
                <Setter TargetName="RootElement" Property="Width" Value="210"/>
            </Trigger>
        </ControlTemplate.Triggers>
    </ControlTemplate>
