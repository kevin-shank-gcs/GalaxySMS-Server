                                            VerticalAlignment="Center"
                                            Focusable="False"
                                            IsBackgroundVisible="False">
                                        <telerik:RadGlyph FontSize="16" Glyph="{StaticResource GlyphRotate}"/>
                                        <ToolTipService.ToolTip>
