            </Trigger>
            <Trigger Property="IsOpen" Value="True">
                <Setter Property="Background" TargetName="BorderVisual" Value="{Binding RelativeSource={RelativeSource TemplatedParent}, Path=(mat:MaterialAssist.CheckedBrush), Mode=OneWay}"/>
                <Setter Property="BorderBrush" TargetName="BorderVisual" Value="{Binding RelativeSource={RelativeSource TemplatedParent}, Path=(mat:MaterialAssist.CheckedBrush), Mode=OneWay}"/>
            </Trigger>
            <Trigger Property="DropDownButtonPosition" Value="Right">
                <Setter TargetName="DropDownPanel" Property="Grid.Column" Value="2"/>
                <Setter TargetName="DropDownPanel" Property="Grid.RowSpan" Value="3"/>
                <Setter TargetName="DropDownPanel" Property="Grid.Row" Value="0"/>
                <Setter TargetName="DropDownPanel" Property="VerticalAlignment" Value="Center"/>
                <Setter TargetName="textRow2" Property="Visibility" Value="Collapsed"/>
                <Setter TargetName="textRow1" Property="Grid.RowSpan" Value="3"/>
                <Setter TargetName="Image" Property="Grid.RowSpan" Value="3"/>
                <Setter TargetName="ContentPresenter" Property="Grid.RowSpan" Value="3"/>
                <Setter TargetName="ContentPresenter" Property="Grid.Column" Value="1"/>
                <Setter TargetName="FirstRowContainer" Property="Grid.RowSpan" Value="3"/>
                <Setter TargetName="FirstRowContainer" Property="Orientation" Value="Horizontal"/>
                <Setter TargetName="FirstRowContainer" Property="Grid.Column" Value="1"/>
            </Trigger>
            <Trigger Property="DropDownPlacement" Value="Bottom"/>
            <Trigger Property="DropDownPlacement" Value="Top">
                <Setter TargetName="DropDownIndicator" Property="Glyph" Value="{StaticResource GlyphArrowChevronUp}"/>
                <Setter TargetName="PopupShadow" Property="CornerRadius" Value="{telerikControls:FluentResource ResourceKey=CornerRadiusTop}"/>
                <Setter TargetName="DropDownPopupBorder" Property="CornerRadius" Value="{telerikControls:FluentResource ResourceKey=CornerRadiusTop}"/>
            </Trigger>
            <Trigger Property="DropDownPlacement" Value="Left">
                <Setter TargetName="DropDownIndicator" Property="Glyph" Value="{StaticResource GlyphArrowChevronLeft}"/>
                <Setter TargetName="PopupShadow" Property="CornerRadius" Value="{telerikControls:FluentResource ResourceKey=CornerRadiusLeft}"/>
                <Setter TargetName="DropDownPopupBorder" Property="CornerRadius" Value="{telerikControls:FluentResource ResourceKey=CornerRadiusLeft}"/>
            </Trigger>
            <Trigger Property="DropDownPlacement" Value="Right">
                <Setter TargetName="DropDownIndicator" Property="Glyph" Value="{StaticResource GlyphArrowChevronRight}"/>
                <Setter TargetName="PopupShadow" Property="CornerRadius" Value="{telerikControls:FluentResource ResourceKey=CornerRadiusRight}"/>
                <Setter TargetName="DropDownPopupBorder" Property="CornerRadius" Value="{telerikControls:FluentResource ResourceKey=CornerRadiusRight}"/>
            </Trigger>
            <MultiTrigger>
                <MultiTrigger.Conditions>
                    <Condition Property="IsOpen" Value="True"/>
                    <Condition Property="DropDownPlacement" Value="Bottom"/>
                </MultiTrigger.Conditions>
                <Setter TargetName="DropDownIndicator" Property="Glyph" Value="{StaticResource GlyphArrowChevronUp}"/>
