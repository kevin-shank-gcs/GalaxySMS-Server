                            ContentStringFormat="{TemplateBinding HeaderStringFormat}"
                            ContentTemplateSelector="{TemplateBinding HeaderTemplateSelector}"/>
                </mat:FluentControl>
                <Border x:Name="SelectedVisual" BorderBrush="{telerikControls:FluentResource ResourceKey=AccentBrush}" BorderThickness="0 0 0 4" RenderTransformOrigin="0.5 1">
