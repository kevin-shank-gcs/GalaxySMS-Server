                                            Image="{telerik:IconResource IconRelativePath=16/highlight.png, IconSources={StaticResource IconSources}}"
                                            telerikControlsRTBUI:RadRichTextBoxRibbonUI.RichTextCommand="{Binding Path=ChangeFontHighlightColorCommand}"
                                            Focusable="False"
                                            Padding="5"
                                            MinHeight="28"
                                            VerticalAlignment="Center"
