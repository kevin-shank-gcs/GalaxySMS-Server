    <ControlTemplate x:Key="RadRibbonBackstageItemTemplate" TargetType="telerikRibbonView:RadRibbonBackstageItem">
        <Grid x:Name="wrapper" Background="Transparent">
            <Border Name="NormalVisual" BorderBrush="{TemplateBinding BorderBrush}" Background="{TemplateBinding Background}" BorderThickness="{TemplateBinding BorderThickness}"/>
            <mat:FluentControl x:Name="Fluent" IsSmartClipped="True">
                <StackPanel Orientation="Horizontal" x:Name="ContentPanel" Margin="20 10">
                    <Image x:Name="Image" Source="{TemplateBinding Icon}" MaxWidth="16" MaxHeight="16" Stretch="Fill" Margin="0 0 5 0" Visibility="Collapsed"/>
                    <ContentControl x:Name="HeaderElement"
                            Content="{TemplateBinding Header}"
                            IsTabStop="False"
                            Foreground="{TemplateBinding HeaderForeground}"
                            ContentTemplate="{TemplateBinding HeaderTemplate}"
                            HorizontalAlignment="{TemplateBinding HorizontalContentAlignment}"
                            VerticalAlignment="{TemplateBinding VerticalContentAlignment}"
                            ContentStringFormat="{TemplateBinding HeaderStringFormat}"
                            ContentTemplateSelector="{TemplateBinding HeaderTemplateSelector}"
                            SnapsToDevicePixels="True"/>
                </StackPanel>
            </mat:FluentControl>
        </Grid>
        <ControlTemplate.Triggers>
            <Trigger Property="IsEnabled" Value="False">
                <Setter TargetName="ContentPanel" Property="Opacity" Value="{telerikControls:FluentResource ResourceKey=DisabledOpacity}"/>
            </Trigger>
            <Trigger SourceName="Fluent" Property="IsMouseOver" Value="True">
                <Setter TargetName="NormalVisual" Property="Background" Value="{Binding RelativeSource={RelativeSource TemplatedParent}, Path=(mat:MaterialAssist.MouseOverBrush), Mode=OneWay}"/>
                <Setter TargetName="NormalVisual" Property="BorderBrush" Value="{Binding RelativeSource={RelativeSource TemplatedParent}, Path=(mat:MaterialAssist.MouseOverBrush), Mode=OneWay}"/>
            </Trigger>
            <Trigger SourceName="Fluent" Property="IsPressed" Value="True">
                <Setter TargetName="NormalVisual" Property="Background" Value="{Binding RelativeSource={RelativeSource TemplatedParent}, Path=(mat:MaterialAssist.PressedBrush), Mode=OneWay}"/>
                <Setter TargetName="NormalVisual" Property="BorderBrush" Value="{Binding RelativeSource={RelativeSource TemplatedParent}, Path=(mat:MaterialAssist.PressedBrush), Mode=OneWay}"/>
            </Trigger>
            <Trigger Property="IsSelected" Value="True">
                <Setter TargetName="NormalVisual" Property="Background" Value="{Binding RelativeSource={RelativeSource TemplatedParent}, Path=(mat:MaterialAssist.CheckedBrush), Mode=OneWay}"/>
                <Setter TargetName="NormalVisual" Property="BorderBrush" Value="{Binding RelativeSource={RelativeSource TemplatedParent}, Path=(mat:MaterialAssist.CheckedBrush), Mode=OneWay}"/>
            </Trigger>
        </ControlTemplate.Triggers>
    </ControlTemplate>
