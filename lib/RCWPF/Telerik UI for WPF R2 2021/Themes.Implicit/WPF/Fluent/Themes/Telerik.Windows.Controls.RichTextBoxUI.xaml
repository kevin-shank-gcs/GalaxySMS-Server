            <Trigger SourceName="ButtonPart" Property="IsMouseOver" Value="True">
                <Setter TargetName="DropDownPart" Property="Background" Value="{telerikControls:FluentResource ResourceKey=MainBrush}"/>
                <Setter TargetName="DropDownPart" Property="BorderBrush" Value="{telerikControls:FluentResource ResourceKey=MainBrush}"/>
            </Trigger>
            <Trigger SourceName="DropDownPart" Property="IsMouseOver" Value="True">
                <Setter TargetName="ButtonPart" Property="Background" Value="{telerikControls:FluentResource ResourceKey=MainBrush}"/>
                <Setter TargetName="ButtonPart" Property="BorderBrush" Value="{telerikControls:FluentResource ResourceKey=MainBrush}"/>
