                                <StackPanel Orientation="Horizontal">
                                    <telerik:RadToggleButton x:Name="buttonBold" Command="{Binding Path=ToggleBoldCommand}" IsTabStop="False" Focusable="False" IsBackgroundVisible="False" VerticalAlignment="Center">
                                        <Image Source="{telerik:IconResource IconRelativePath=16/bold.png, IconSources={StaticResource IconSources}}"/>
                                    </telerik:RadToggleButton>
                                    <telerik:RadToggleButton x:Name="buttonItalic" Command="{Binding Path=ToggleItalicCommand}" IsTabStop="False" Focusable="False" IsBackgroundVisible="False" VerticalAlignment="Center">
                                        <Image Source="{telerik:IconResource IconRelativePath=16/italic.png, IconSources={StaticResource IconSources}}"/>
                                    </telerik:RadToggleButton>
                                    <telerik:RadToggleButton x:Name="buttonUnderline" Command="{Binding Path=ToggleUnderlineCommand}" IsTabStop="False" Focusable="False" IsBackgroundVisible="False" VerticalAlignment="Center">
                                        <Image Source="{telerik:IconResource IconRelativePath=16/underline.png, IconSources={StaticResource IconSources}}"/>
                                    </telerik:RadToggleButton>
                                    <telerik:RadToggleButton x:Name="buttonAlignCenter"
                                            CommandParameter="Center"
                                            Command="{Binding Path=ChangeTextAlignmentCommand}"
                                            IsTabStop="False"
                                            Focusable="False"
                                            IsBackgroundVisible="False"
                                            VerticalAlignment="Center">
                                        <Image Source="{telerik:IconResource IconRelativePath=16/aligncenter.png, IconSources={StaticResource IconSources}}"/>
                                    </telerik:RadToggleButton>
                                    <telerikColorPickers:FormattingColorPicker x:Name="highlightColorPicker"
