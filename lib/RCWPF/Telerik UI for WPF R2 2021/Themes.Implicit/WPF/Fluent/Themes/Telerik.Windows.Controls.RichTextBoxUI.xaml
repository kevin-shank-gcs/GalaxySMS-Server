            <Trigger SourceName="DropDownPart" Property="IsChecked" Value="True">
                <Setter TargetName="ButtonPart" Property="Background" Value="{telerikControls:FluentResource ResourceKey=AccentBrush}"/>
                <Setter TargetName="ButtonPart" Property="BorderBrush" Value="{telerikControls:FluentResource ResourceKey=AccentBrush}"/>
                <Setter TargetName="textRow1" Property="Foreground" Value="{telerikControls:FluentResource ResourceKey=MarkerInvertedBrush}"/>
                <Setter TargetName="textRow2" Property="Foreground" Value="{telerikControls:FluentResource ResourceKey=MarkerInvertedBrush}"/>
                <Setter TargetName="DropDownIndicator" Property="Foreground" Value="{telerikControls:FluentResource ResourceKey=MarkerInvertedBrush}"/>
            </Trigger>
            <MultiTrigger>
