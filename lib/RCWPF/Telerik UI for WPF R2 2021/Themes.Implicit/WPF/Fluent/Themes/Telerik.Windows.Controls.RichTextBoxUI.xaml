                    <Condition Property="IsChecked" Value="True"/>
                    <Condition Property="IsOpen" Value="False"/>
                    <Condition Property="IsMouseOver" Value="True"/>
                </MultiTrigger.Conditions>
                <Setter Property="Background" Value="{telerikControls:FluentResource ResourceKey=AccentMouseOverBrush}"/>
                <Setter Property="BorderBrush" Value="{telerikControls:FluentResource ResourceKey=AccentMouseOverBrush}"/>
                <Setter TargetName="ButtonPart" Property="mat:MaterialAssist.MouseOverBrush" Value="{telerikControls:FluentResource ResourceKey=AccentMouseOverBrush}"/>
                <Setter TargetName="DropDownPart" Property="mat:MaterialAssist.MouseOverBrush" Value="{telerikControls:FluentResource ResourceKey=AccentMouseOverBrush}"/>
            </MultiTrigger>
            <MultiTrigger>
                <MultiTrigger.Conditions>
                    <Condition Property="IsToggle" Value="True"/>
                    <Condition Property="IsChecked" Value="True"/>
                    <Condition Property="IsOpen" Value="False"/>
                    <Condition SourceName="ButtonPart" Property="IsMouseOver" Value="True"/>
                </MultiTrigger.Conditions>
                <Setter Property="Background" Value="{telerikControls:FluentResource ResourceKey=AccentMouseOverBrush}"/>
                <Setter TargetName="DropDownPart" Property="Background" Value="{telerikControls:FluentResource ResourceKey=AccentMouseOverBrush}"/>
            </MultiTrigger>
            <MultiTrigger>
                <MultiTrigger.Conditions>
                    <Condition Property="IsToggle" Value="True"/>
                    <Condition Property="IsChecked" Value="True"/>
                    <Condition Property="IsOpen" Value="False"/>
                    <Condition SourceName="DropDownPart" Property="IsMouseOver" Value="True"/>
                </MultiTrigger.Conditions>
                <Setter Property="Background" Value="{telerikControls:FluentResource ResourceKey=AccentMouseOverBrush}"/>
                <Setter TargetName="ButtonPart" Property="Background" Value="{telerikControls:FluentResource ResourceKey=AccentMouseOverBrush}"/>
            </MultiTrigger>
            <MultiTrigger>
                <MultiTrigger.Conditions>
                    <Condition Property="IsToggle" Value="True"/>
                    <Condition Property="IsChecked" Value="True"/>
                    <Condition Property="IsOpen" Value="False"/>
                    <Condition SourceName="DropDownPart" Property="IsPressed" Value="True"/>
                </MultiTrigger.Conditions>
                <Setter Property="Foreground" Value="{telerikControls:FluentResource ResourceKey=MarkerBrush}"/>
                <Setter Property="Background" Value="{telerikControls:FluentResource ResourceKey=AccentMouseOverBrush}"/>
                <Setter TargetName="DropDownIndicator" Property="Foreground" Value="{telerikControls:FluentResource ResourceKey=IconBrush}"/>
                <Setter TargetName="ButtonPart" Property="Background" Value="{telerikControls:FluentResource ResourceKey=MainBrush}"/>
            </MultiTrigger>
            <MultiTrigger>
                <MultiTrigger.Conditions>
                    <Condition Property="IsToggle" Value="True"/>
                    <Condition Property="IsOpen" Value="True"/>
                </MultiTrigger.Conditions>
                <Setter Property="Foreground" Value="{telerikControls:FluentResource ResourceKey=MarkerInvertedBrush}"/>
            </MultiTrigger>
        </ControlTemplate.Triggers>
