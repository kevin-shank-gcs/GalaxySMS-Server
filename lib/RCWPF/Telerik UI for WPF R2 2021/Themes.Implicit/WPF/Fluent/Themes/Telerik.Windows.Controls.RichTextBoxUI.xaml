                                        </ToolTipService.ToolTip>
                                    </telerik:RadButton>
                                    <telerik:RadButton x:Name="btnCrop"
                                            Command="{Binding RelativeSource={RelativeSource TemplatedParent}, Path=ShowImageEditorDialogCommand}"
                                            CommandParameter="CropTool"
                                            IsTabStop="False"
                                            VerticalAlignment="Center"
                                            Focusable="False"
                                            IsBackgroundVisible="False">
                                        <telerik:RadGlyph FontSize="16" Glyph="{StaticResource GlyphCrop}"/>
                                        <ToolTipService.ToolTip>
                                            <TextBlock telerik:LocalizationManager.ResourceKey="Documents_ImageMiniToolBar_Crop"/>
                                        </ToolTipService.ToolTip>
                                    </telerik:RadButton>
                                    <telerik:RadButton x:Name="btnBlur"
                                            Command="{Binding RelativeSource={RelativeSource TemplatedParent}, Path=ShowImageEditorDialogCommand}"
                                            CommandParameter="BlurTool"
                                            IsTabStop="False"
                                            VerticalAlignment="Center"
                                            Focusable="False"
                                            IsBackgroundVisible="False">
                                        <telerik:RadGlyph FontSize="16" Glyph="{StaticResource GlyphBlur}"/>
                                        <ToolTipService.ToolTip>
                                            <TextBlock telerik:LocalizationManager.ResourceKey="Documents_ImageMiniToolBar_Blur"/>
                                        </ToolTipService.ToolTip>
                                    </telerik:RadButton>
                                    <telerik:RadButton x:Name="btnSharpen"
                                            Command="{Binding RelativeSource={RelativeSource TemplatedParent}, Path=ShowImageEditorDialogCommand}"
                                            CommandParameter="SharpenTool"
                                            IsTabStop="False"
                                            VerticalAlignment="Center"
                                            Focusable="False"
                                            IsBackgroundVisible="False">
                                        <telerik:RadGlyph FontSize="16" Glyph="{StaticResource GlyphSharpen}"/>
                                        <ToolTipService.ToolTip>
                                            <TextBlock telerik:LocalizationManager.ResourceKey="Documents_ImageMiniToolBar_Sharpen"/>
                                        </ToolTipService.ToolTip>
                                    </telerik:RadButton>
                                    <telerik:RadButton x:Name="btnFlipHorizontal"
                                            Command="{Binding RelativeSource={RelativeSource TemplatedParent}, Path=FlipCommand}"
                                            CommandParameter="Horizontal"
                                            IsTabStop="False"
                                            VerticalAlignment="Center"
                                            Focusable="False"
                                            IsBackgroundVisible="False">
                                        <telerik:RadGlyph FontSize="16" Glyph="{StaticResource GlyphFlipHorizontal}"/>
                                        <ToolTipService.ToolTip>
                                            <TextBlock telerik:LocalizationManager.ResourceKey="Documents_ImageMiniToolBar_FlipHorizontal"/>
                                        </ToolTipService.ToolTip>
                                    </telerik:RadButton>
                                    <telerik:RadButton x:Name="btnFlipVertical"
                                            Command="{Binding RelativeSource={RelativeSource TemplatedParent}, Path=FlipCommand}"
                                            CommandParameter="Vertical"
                                            IsTabStop="False"
                                            VerticalAlignment="Center"
                                            Focusable="False"
                                            IsBackgroundVisible="False">
                                        <telerik:RadGlyph FontSize="16" Glyph="{StaticResource GlyphFlipVertical}"/>
                                        <ToolTipService.ToolTip>
                                            <TextBlock telerik:LocalizationManager.ResourceKey="Documents_ImageMiniToolBar_FlipVertical"/>
                                        </ToolTipService.ToolTip>
                                    </telerik:RadButton>
                                </StackPanel>
                            </Grid>
