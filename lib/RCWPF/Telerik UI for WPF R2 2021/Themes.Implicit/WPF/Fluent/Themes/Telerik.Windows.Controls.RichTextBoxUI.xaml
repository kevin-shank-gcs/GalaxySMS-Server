        <Setter Property="MaxWidth" Value="350"/>
    </Style>
    <Style x:Key="ContentControlDropDownButtonStyle" TargetType="telerik:RadDropDownButton" BasedOn="{StaticResource RadDropDownButtonStyle}">
        <Setter Property="FontSize" Value="12"/>
        <Setter Property="BorderThickness" Value="1"/>
        <Setter Property="Padding" Value="0"/>
        <Setter Property="CornerRadius" Value="0"/>
        <Setter Property="VerticalContentAlignment" Value="Center"/>
        <Setter Property="HorizontalContentAlignment" Value="Center"/>
        <Setter Property="Cursor" Value="Arrow"/>
        <Setter Property="Template">
            <Setter.Value>
                <ControlTemplate TargetType="telerik:RadDropDownButton">
                    <Grid SnapsToDevicePixels="{TemplateBinding SnapsToDevicePixels}">
                        <Border x:Name="BorderVisual"
                                SnapsToDevicePixels="{TemplateBinding SnapsToDevicePixels}"
                                BorderThickness="{TemplateBinding BorderThickness}"
                                BorderBrush="{TemplateBinding BorderBrush}"
                                Background="{TemplateBinding Background}"/>
                        <mat:FluentControl
                                CornerRadius="{TemplateBinding CornerRadius}"
                                SnapsToDevicePixels="{TemplateBinding SnapsToDevicePixels}"
                                BorderThickness="{TemplateBinding BorderThickness}"
                                HorizontalContentAlignment="Stretch"
                                VerticalContentAlignment="Stretch">
                            <Grid x:Name="Content"
                                    Margin="{TemplateBinding Padding}"
                                    HorizontalAlignment="{TemplateBinding HorizontalContentAlignment}"
                                    VerticalAlignment="{TemplateBinding VerticalContentAlignment}">
                                <telerik:RadGlyph Glyph="{StaticResource GlyphArrowChevronDown}" FontSize="{TemplateBinding FontSize}" Foreground="{TemplateBinding Foreground}"/>
                            </Grid>
                        </mat:FluentControl>
                        <Popup x:Name="DropDownPopup"
                                AllowsTransparency="True"
                                StaysOpen="False"
                                Focusable="False"
                                Placement="Bottom"
                                PopupAnimation="{TemplateBinding PopupAnimation}"
                                HorizontalOffset="0"
                                VerticalOffset="-1">
                            <Border x:Name="DropDownPopupBorder"
                                    MinWidth="{TemplateBinding DropDownMinWidth}"
                                    MinHeight="{TemplateBinding DropDownMinHeight}"
                                    Width="{TemplateBinding DropDownWidth}"
                                    Height="{TemplateBinding DropDownHeight}"
                                    MaxWidth="{TemplateBinding DropDownMaxWidth}"
                                    MaxHeight="{TemplateBinding DropDownMaxHeight}"
                                    BorderThickness="1"
                                    Background="{telerikControls:FluentResource ResourceKey=AlternativeBrush}"
                                    BorderBrush="{telerikControls:FluentResource ResourceKey=BasicSolidBrush}">
                                <ContentControl x:Name="DropDownPopupContent"
                                        Content="{TemplateBinding DropDownContent}"
                                        HorizontalContentAlignment="Stretch"
                                        VerticalContentAlignment="Stretch"
                                        ContentTemplate="{TemplateBinding DropDownContentTemplate}"
                                        HorizontalAlignment="Stretch"
                                        VerticalAlignment="Stretch"
                                        DataContext="{Binding RelativeSource={RelativeSource TemplatedParent}, Path=DataContext}"
                                        ContentStringFormat="{TemplateBinding ContentStringFormat}"
                                        ContentTemplateSelector="{TemplateBinding ContentTemplateSelector}"/>
                            </Border>
                        </Popup>
                    </Grid>
                    <ControlTemplate.Triggers>

                        <!-- background is hidden -->
                        <MultiTrigger>
                            <MultiTrigger.Conditions>
                                <Condition Property="IsEnabled" Value="True"/>
                                <Condition Property="IsBackgroundVisible" Value="False"/>
                                <Condition Property="IsMouseOver" Value="False"/>
                                <Condition Property="IsOpen" Value="False"/>
                            </MultiTrigger.Conditions>
                            <Setter Property="Background" Value="Transparent"/>
                            <Setter Property="BorderBrush" Value="Transparent"/>
                        </MultiTrigger>
                    </ControlTemplate.Triggers>
                </ControlTemplate>
            </Setter.Value>
        </Setter>
    </Style>
    <Style x:Key="ContentControlBaseStyle" TargetType="{x:Type telerikControlsRTBUI:ContentControlBase}">
        <Setter Property="OutlineColor">
            <Setter.Value>
                <Binding Path="OutlineColor" FallbackValue="{StaticResource FallbackColor}"/>
            </Setter.Value>
        </Setter>
        <Setter Property="Foreground" Value="Black"/>
        <Setter Property="AliasFontSize" Value="12"/>
        <Setter Property="LineThickness" Value="1"/>
        <Setter Property="UseLayoutRounding" Value="True"/>
        <Setter Property="Template">
            <Setter.Value>
                <ControlTemplate TargetType="{x:Type telerikControlsRTBUI:ContentControlBase}">
                    <Canvas>
                        <Polygon x:Name="PART_MainBodyFill"
                                Points="{Binding RelativeSource={RelativeSource TemplatedParent}, Path=PolygonPoints}"
                                Fill="{Binding OutlineColor, RelativeSource={RelativeSource TemplatedParent}, Converter={StaticResource ColorToBrushConverter}}"
                                Opacity="0.2"/>
                        <Polygon x:Name="PART_MainBodyBorder"
                                RenderOptions.EdgeMode="Aliased"
                                Points="{Binding RelativeSource={RelativeSource TemplatedParent}, Path=PolygonPoints}"
                                Stroke="{Binding OutlineColor, RelativeSource={RelativeSource TemplatedParent}, Converter={StaticResource ColorToBrushConverter}}"
                                StrokeThickness="{Binding LineThickness, RelativeSource={RelativeSource TemplatedParent}}"
                                Visibility="{Binding Focused, RelativeSource={RelativeSource Mode=TemplatedParent}, Converter={StaticResource BooleanToVisibilityConverter}}"/>
                        <Border x:Name="PART_LeftThumbContainer" Background="White" Visibility="Hidden">
                            <telerik:RadPathButton x:Name="LeftThumb"
                                    Command="{TemplateBinding SelectCommand}"
                                    CommandParameter="{Binding}"
                                    Foreground="{TemplateBinding Foreground}"
                                    PathGeometry="{telerik:RadGlyph Type=Path, Glyph={StaticResource GlyphMoreVertical}}"
                                    Padding="5"
                                    Style="{StaticResource ContentControlPathButtonStyle}"
                                    Background="{Binding OutlineColor, RelativeSource={RelativeSource TemplatedParent}, Converter={StaticResource ColorToBrushWithOpacityConverter}, ConverterParameter=50}"
                                    BorderBrush="{Binding OutlineColor, RelativeSource={RelativeSource TemplatedParent}, Converter={StaticResource ColorToBrushConverter}}"/>
                        </Border>
                        <Border x:Name="PART_TopThumbContainer" Background="White" Visibility="Hidden">
                            <telerik:RadPathButton x:Name="TopThumb"
                                    Command="{TemplateBinding SelectCommand}"
                                    CommandParameter="{Binding}"
                                    Foreground="{TemplateBinding Foreground}"
                                    FontSize="{Binding RelativeSource={RelativeSource TemplatedParent}, Path=AliasFontSize}"
                                    Content="{Binding Alias, RelativeSource={RelativeSource TemplatedParent}}"
                                    ContentPlacement="Right"
                                    PathGeometry="{telerik:RadGlyph Type=Path, Glyph={StaticResource GlyphMoreVertical}}"
                                    Style="{StaticResource ContentControlTopThumbPathButtonStyle}"
                                    Background="{Binding OutlineColor, RelativeSource={RelativeSource TemplatedParent}, Converter={StaticResource ColorToBrushWithOpacityConverter}, ConverterParameter=50}"
                                    BorderBrush="{Binding OutlineColor, RelativeSource={RelativeSource TemplatedParent}, Converter={StaticResource ColorToBrushConverter}}"/>
                        </Border>
                        <Border x:Name="PART_ButtonContainer"/>
                    </Canvas>
                    <ControlTemplate.Triggers>
                        <Trigger Property="Focused" Value="True">
                            <Setter TargetName="PART_TopThumbContainer" Property="Visibility" Value="{Binding RelativeSource={RelativeSource TemplatedParent}, Path=ThumbShouldBeVisible, Converter={StaticResource BooleanToVisibilityConverter}}"/>
                            <Setter TargetName="PART_LeftThumbContainer" Property="Visibility" Value="{Binding RelativeSource={RelativeSource TemplatedParent}, Path=ThumbShouldBeVisible, Converter={StaticResource BooleanToVisibilityConverter}}"/>
                        </Trigger>
                        <DataTrigger Binding="{Binding Path=Alias, Converter={StaticResource IsStringNotEmptyToBoolConverter}}" Value="true">
                            <Setter TargetName="PART_LeftThumbContainer" Property="Visibility" Value="Hidden"/>
                        </DataTrigger>
                        <DataTrigger Binding="{Binding Path=Alias, Converter={StaticResource IsStringNotEmptyToBoolConverter}}" Value="false">
                            <Setter TargetName="PART_TopThumbContainer" Property="Visibility" Value="Hidden"/>
                        </DataTrigger>
                        <Trigger Property="TextFlowDirection" Value="RightToLeft">
                            <Setter TargetName="TopThumb" Property="ContentPlacement" Value="Left"/>
                        </Trigger>
                        <Trigger SourceName="LeftThumb" Property="IsMouseOver" Value="True">
                            <Setter TargetName="LeftThumb" Property="Background" Value="{Binding OutlineColor, RelativeSource={RelativeSource TemplatedParent}, Converter={StaticResource ColorToBrushWithOpacityConverter}, ConverterParameter=100}"/>
                        </Trigger>
                        <Trigger SourceName="TopThumb" Property="IsMouseOver" Value="True">
                            <Setter TargetName="TopThumb" Property="Background" Value="{Binding OutlineColor, RelativeSource={RelativeSource TemplatedParent}, Converter={StaticResource ColorToBrushWithOpacityConverter}, ConverterParameter=100}"/>
                        </Trigger>
                        <Trigger Property="Selected" Value="True">
                            <Setter TargetName="LeftThumb" Property="Background" Value="{Binding OutlineColor, RelativeSource={RelativeSource TemplatedParent}, Converter={StaticResource ColorToBrushConverter}}"/>
                            <Setter TargetName="TopThumb" Property="Background" Value="{Binding OutlineColor, RelativeSource={RelativeSource TemplatedParent}, Converter={StaticResource ColorToBrushConverter}}"/>
                            <Setter TargetName="PART_MainBodyFill" Property="Fill" Value="{Binding OutlineColor, RelativeSource={RelativeSource TemplatedParent}, Converter={StaticResource ColorToBrushConverter}}"/>
                        </Trigger>
                        <MultiTrigger>
                            <MultiTrigger.Conditions>
                                <Condition Property="Focused" Value="False"/>
                                <Condition Property="Hovered" Value="True"/>
                            </MultiTrigger.Conditions>
                            <Setter TargetName="PART_MainBodyFill" Property="Opacity" Value="0.2"/>
                        </MultiTrigger>
                        <MultiTrigger>
                            <MultiTrigger.Conditions>
                                <Condition Property="Focused" Value="True"/>
                                <Condition Property="Hovered" Value="True"/>
                            </MultiTrigger.Conditions>
                            <Setter TargetName="PART_MainBodyFill" Property="Opacity" Value="0.0"/>
                        </MultiTrigger>
                        <Trigger Property="Hovered" Value="False">
                            <Setter TargetName="PART_MainBodyFill" Property="Opacity" Value="0.0"/>
                        </Trigger>
                    </ControlTemplate.Triggers>
                </ControlTemplate>
            </Setter.Value>
        </Setter>
    </Style>
