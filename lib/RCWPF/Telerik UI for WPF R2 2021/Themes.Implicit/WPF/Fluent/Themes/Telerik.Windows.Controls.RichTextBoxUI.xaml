                    BorderThickness="{TemplateBinding BorderThickness}"
                    Opacity="0"
                    Background="{telerikControls:FluentResource ResourceKey=AccentFocusedBrush}"
                    BorderBrush="{telerikControls:FluentResource ResourceKey=AccentFocusedBrush}"
                    CornerRadius="{TemplateBinding CornerRadius}"/>
            <mat:FluentControl CornerRadius="{TemplateBinding CornerRadius}" EffectMode="Glow" BorderThickness="{TemplateBinding BorderThickness}" IsSmartClipped="True">
