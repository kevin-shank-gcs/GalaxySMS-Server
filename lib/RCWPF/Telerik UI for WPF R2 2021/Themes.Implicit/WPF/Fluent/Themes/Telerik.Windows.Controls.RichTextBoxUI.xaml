                <Setter Property="Foreground" Value="{telerikControls:FluentResource ResourceKey=MarkerInvertedBrush}"/>
                <Setter TargetName="DropDownIndicator" Property="Foreground" Value="{telerikControls:FluentResource ResourceKey=MarkerInvertedBrush}"/>
                <Setter Property="Background" Value="{telerikControls:FluentResource ResourceKey=AccentBrush}"/>
                <Setter Property="BorderBrush" Value="{telerikControls:FluentResource ResourceKey=AccentBrush}"/>
