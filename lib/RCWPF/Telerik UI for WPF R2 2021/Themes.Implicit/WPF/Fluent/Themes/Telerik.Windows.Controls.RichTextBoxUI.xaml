                <Setter TargetName="BorderVisual" Property="BorderBrush" Value="{telerikControls:FluentResource ResourceKey=MouseOverBrush}"/>
            </Trigger>
            <Trigger Property="IsPressed" Value="True">
                <Setter TargetName="BorderVisual" Property="Background" Value="{telerikControls:FluentResource ResourceKey=PressedBrush}"/>
                <Setter TargetName="BorderVisual" Property="BorderBrush" Value="{telerikControls:FluentResource ResourceKey=PressedBrush}"/>
            </Trigger>
            <Trigger Property="FlowDirection" Value="RightToLeft">
