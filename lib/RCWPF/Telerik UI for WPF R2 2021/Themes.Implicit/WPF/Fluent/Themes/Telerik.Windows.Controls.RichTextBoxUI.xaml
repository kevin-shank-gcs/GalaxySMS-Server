                    <telerikRibbonView:CustomContainerStyle TypeName="RadRibbonSplitButton" ContainerStyle="{StaticResource ApplicationRibbonSplitButton}"/>
                    <telerikRibbonView:CustomContainerStyle TypeName="Separator" ContainerStyle="{StaticResource ApplicationSeparatorStyle}"/>
