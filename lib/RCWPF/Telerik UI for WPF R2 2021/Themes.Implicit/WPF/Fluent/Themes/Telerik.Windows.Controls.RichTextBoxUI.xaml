                <Setter TargetName="Image" Property="Opacity" Value="{telerikControls:FluentResource ResourceKey=DisabledOpacity}"/>
                <Setter TargetName="DropDownIndicator" Property="Opacity" Value="{telerikControls:FluentResource ResourceKey=DisabledOpacity}"/>
