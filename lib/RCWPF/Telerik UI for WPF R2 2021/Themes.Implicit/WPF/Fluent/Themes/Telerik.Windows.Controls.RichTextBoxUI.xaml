    <ControlTemplate x:Key="RadRibbonGalleryTemplate" TargetType="telerikRibbonView:RadRibbonGallery">
        <Grid>
            <mat:Shadow ShadowDepth="{TemplateBinding mat:MaterialAssist.ShadowDepth}" Background="{TemplateBinding Background}" BorderThickness="{TemplateBinding BorderThickness}"/>
            <Border x:Name="ExpandedBorder" BorderThickness="{TemplateBinding BorderThickness}" BorderBrush="{TemplateBinding BorderBrush}" Background="{TemplateBinding Background}">
                <Grid>
                    <Grid.ColumnDefinitions>
                        <ColumnDefinition Width="*"/>
                        <ColumnDefinition Width="Auto"/>
                    </Grid.ColumnDefinitions>
                    <Grid.RowDefinitions>
                        <RowDefinition Height="*"/>
                        <RowDefinition Height="*"/>
                        <RowDefinition Height="*"/>
                    </Grid.RowDefinitions>
                    <ScrollViewer x:Name="ScrollViewer" Grid.RowSpan="3" VerticalScrollBarVisibility="Hidden" IsTabStop="False" BorderThickness="0" VerticalAlignment="Stretch" Padding="1 1 1 0">
                        <ContentPresenter x:Name="InRibbonHost" Width="{TemplateBinding ViewportWidth}" HorizontalAlignment="Left">
                            <ContentPresenter.Content>
                                <ItemsPresenter x:Name="ItemsPresenter"/>
                            </ContentPresenter.Content>
                        </ContentPresenter>
                    </ScrollViewer>
                    <telerikRibbonView:RadRibbonButton x:Name="ScrollUpButton"
                            HorizontalAlignment="Right"
                            telerikRibbonView:ScreenTip.Title="{TemplateBinding UpDownButtonTitle}"
                            Grid.Column="1"
                            Grid.Row="0"
                            IsTabStop="False">
                        <telerik:RadGlyph x:Name="ArrowUp" FontSize="16" Foreground="{telerikControls:FluentResource ResourceKey=IconBrush}" Glyph="{StaticResource GlyphArrowChevronUp}"/>
                    </telerikRibbonView:RadRibbonButton>
                    <telerikRibbonView:RadRibbonButton x:Name="ScrollDownButton"
                            HorizontalAlignment="Right"
                            telerikRibbonView:ScreenTip.Title="{TemplateBinding UpDownButtonTitle}"
                            Grid.Column="1"
                            Grid.Row="1"
                            IsTabStop="False">
                        <telerik:RadGlyph x:Name="ArrowDown" FontSize="16" Foreground="{telerikControls:FluentResource ResourceKey=IconBrush}" Glyph="{StaticResource GlyphArrowChevronDown}"/>
                    </telerikRibbonView:RadRibbonButton>
                    <telerikRibbonView:RadRibbonDropDownButton x:Name="PopupButton"
                            Grid.Column="1"
                            Grid.Row="2"
                            telerikRibbonView:ScreenTip.Title="{TemplateBinding MoreButtonTitle}"
                            DropDownIndicatorVisibility="Collapsed"
                            Visibility="{TemplateBinding PopupLaunchButtonVisibility}">
                        <telerik:RadGlyph x:Name="ArrowOverflow" FontSize="16" Foreground="{telerikControls:FluentResource ResourceKey=IconBrush}" Glyph="{StaticResource GlyphArrowOverflowDown}"/>
                    </telerikRibbonView:RadRibbonDropDownButton>
                </Grid>
            </Border>
            <telerikRibbonView:RadRibbonDropDownButton x:Name="CollapsedButton" HorizontalAlignment="Right" Size="Large" LargeImage="{TemplateBinding Icon}" Text="{TemplateBinding Title}" Visibility="Collapsed">
                <telerikRibbonView:RadRibbonDropDownButton.DropDownContent>
                    <Grid>
                        <Grid.RowDefinitions>
                            <RowDefinition Height="Auto"/>
                            <RowDefinition Height="*"/>
                        </Grid.RowDefinitions>
                        <ScrollViewer
                                Padding="0"
                                HorizontalScrollBarVisibility="Hidden"
                                IsTabStop="False"
                                Margin="0 0 -1 -2"
                                Height="{TemplateBinding PopupViewportHeight}"
                                VerticalScrollBarVisibility="Visible">
                            <ContentPresenter x:Name="PopupHost" Width="{TemplateBinding PopupViewportWidth}"/>
                        </ScrollViewer>
                        <ItemsControl
                                HorizontalContentAlignment="Stretch"
                                BorderThickness="0"
                                Margin="0 2 0 0"
                                Grid.Row="1"
                                ItemsSource="{TemplateBinding PopupMenuItems}"
                                Foreground="{TemplateBinding Foreground}"/>
                    </Grid>
                </telerikRibbonView:RadRibbonDropDownButton.DropDownContent>
            </telerikRibbonView:RadRibbonDropDownButton>
        </Grid>
        <ControlTemplate.Triggers>
            <Trigger Property="LayoutMode" Value="Simplified">
                <Setter TargetName="CollapsedButton" Property="Size" Value="Medium"/>
                <Setter TargetName="ScrollUpButton" Property="Visibility" Value="Collapsed"/>
                <Setter TargetName="ScrollDownButton" Property="Visibility" Value="Collapsed"/>
                <Setter TargetName="PopupButton" Property="Grid.Row" Value="0"/>
                <Setter TargetName="PopupButton" Property="Grid.RowSpan" Value="3"/>
                <Setter TargetName="PopupButton" Property="VerticalContentAlignment" Value="Center"/>
            </Trigger>
        </ControlTemplate.Triggers>
    </ControlTemplate>
