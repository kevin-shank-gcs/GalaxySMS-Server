        <Setter Property="ItemStyle" Value="{StaticResource RadDiagramPrintPreviewItemStyle}"/>
        <Setter Property="Template">
            <Setter.Value>
                <ControlTemplate>
                    <Grid Background="{TemplateBinding Background}">
                        <Grid.ColumnDefinitions>
                            <ColumnDefinition Width="*"/>
                            <ColumnDefinition MaxWidth="258"/>
                        </Grid.ColumnDefinitions>
                        <Grid>
                            <Grid.RowDefinitions>
                                <RowDefinition Height="*"/>
                                <RowDefinition Height="Auto"/>
                            </Grid.RowDefinitions>
                            <ScrollViewer Background="Transparent" VerticalScrollBarVisibility="Auto" HorizontalScrollBarVisibility="Auto" BorderThickness="0">
                                <telerik:RadUniformGrid x:Name="PART_PagesPanel" VerticalAlignment="Top" UseLayoutRounding="True" HorizontalAlignment="Left" Margin="15"/>
                            </ScrollViewer>
                            <StackPanel Grid.Row="1" MinHeight="36" HorizontalAlignment="Right" VerticalAlignment="Center" Orientation="Horizontal">
                                <TextBlock VerticalAlignment="Center" Text="{Binding Value, ElementName=ZoomSlider, Converter={StaticResource percentageConverter}}"/>
