        <Rectangle HorizontalAlignment="Center" StrokeThickness="2" Width="22" Height="22" Stroke="{Binding Path=Stroke, Mode=TwoWay}" Fill="{Binding Path=Fill, Mode=TwoWay}"/>
    </DataTemplate>
