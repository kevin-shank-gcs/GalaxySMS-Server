    <Style TargetType="extensions:SettingsPaneHomeControl" x:Key="SettingsPaneHomeControlStyle">
        <Setter Property="Template">
            <Setter.Value>
                <ControlTemplate TargetType="extensions:SettingsPaneHomeControl">
