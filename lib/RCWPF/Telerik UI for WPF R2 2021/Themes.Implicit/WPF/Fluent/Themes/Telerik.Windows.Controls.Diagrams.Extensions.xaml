    <extensions:BooleanToFontStyleConverter x:Key="BooleanToFontStyleConverter"/>
    <extensions:DoubleToSizeConverter x:Key="DoubleToSizeConverter"/>
    <extensions:PercentageConverter x:Key="PercentageConverter"/>
