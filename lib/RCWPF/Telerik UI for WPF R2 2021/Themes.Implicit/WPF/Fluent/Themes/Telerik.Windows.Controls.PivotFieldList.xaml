<ResourceDictionary
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:c="http://schemas.telerik.com/2008/xaml/compile"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        mc:Ignorable="c"
        xmlns:fieldList="clr-namespace:Telerik.Windows.Controls.FieldList;assembly=Telerik.Windows.Controls.PivotFieldList"
        xmlns:telerik="http://schemas.telerik.com/2008/xaml/presentation"
        xmlns:mat="clr-namespace:Telerik.Windows.Controls.MaterialControls;assembly=Telerik.Windows.Controls"
        xmlns:telerik1="clr-namespace:Telerik.Windows.Controls;assembly=Telerik.Windows.Controls"
        xmlns:sys="clr-namespace:System;assembly=mscorlib"
        xmlns:local="clr-namespace:Telerik.Windows.Controls;assembly=Telerik.Windows.Controls.PivotFieldList"
        xmlns:tree="clr-namespace:Telerik.Windows.Controls.Pivot;assembly=Telerik.Windows.Controls.Pivot"
        xmlns:editing="clr-namespace:Telerik.Windows.Controls.FieldList.Editing;assembly=Telerik.Windows.Controls.PivotFieldList"
        xmlns:ext="clr-namespace:Telerik.Windows.Controls.MarkupExtensions;assembly=Telerik.Windows.Controls">
    <telerik1:BooleanToVisibilityConverter x:Key="BooleanToVisibilityConverter"/>
    <telerik1:OpacityConverter x:Key="OpacityConverter"/>
    <telerik1:InvertedBooleanToVisibilityConverter x:Key="InvertedBooleanToVisibilityConverter"/>
    <Geometry x:Key="ArrowDown7x4">M0,0L7,0 3.5,4z</Geometry>
    <FontFamily x:Key="TelerikWebUI">/Telerik.Windows.Controls;component/Themes/Fonts/TelerikWebUI.ttf#TelerikWebUI</FontFamily>
    <sys:String x:Key="GlyphArrowChevronDown"></sys:String>
    <sys:String x:Key="GlyphFilter"></sys:String>
    <sys:String x:Key="GlyphThumbnailsUp"></sys:String>
    <sys:String x:Key="GlyphThumbnailsLeft"></sys:String>
    <sys:String x:Key="GlyphFullScreen"></sys:String>
    <sys:String x:Key="GlyphSum"></sys:String>
    <sys:String x:Key="GlyphFolderOpen"></sys:String>
    <sys:String x:Key="GlyphFieldsMore"></sys:String>
    <telerik1:ColorToBrushConverter x:Key="ColorToBrushConverter"/>
    <ext:DynamicResourceProxy x:Key="PrimaryBackgroundBrushProxy" ProxyValue="{Binding Source={x:Static telerik1:FluentPalette.Palette}, Path=PrimaryBackgroundColor, Converter={StaticResource ColorToBrushConverter}}"/>
    <editing:BooleanToSortOrderConverter x:Key="BooleanToSortOrderConverter"/>
    <editing:ComparisonToVisibleConverter x:Key="ComparisonToVisibleConverter"/>
    <editing:IntervalComparisonToVisibilityConverter x:Key="IntervalComparisonToVisibilityConverter"/>
    <editing:NamedToStringConverter x:Key="NamedToStringConverter"/>
    <editing:Top10FilterTypeToVisibilityConverter x:Key="Top10FilterTypeToVisibilityConverter"/>
    <Style x:Key="TextBlockTitleStyle" TargetType="TextBlock">
        <Setter Property="FontFamily" Value="{telerik:FluentResource ResourceKey=FontFamily}"/>
        <Setter Property="FontSize" Value="{telerik:FluentResource ResourceKey=FontSize}"/>
        <Setter Property="FontWeight" Value="Medium"/>
    </Style>
    <fieldList:FieldDragActionToSourceConverter x:Key="IconConverter" ColumnSource="" FilterSource="" RemoveSource="" RowSource="" ValueSource=""/>
    <fieldList:FieldListContextMenuBehavior x:Key="FieldListContextMenuBehavior"/>
    <fieldList:WindowDialogHostingBehavior x:Key="WindowDialogHostingBehavior"/>
    <fieldList:EditorsBehavior x:Key="EditorsBehavior">
        <fieldList:EditorsBehavior.Editors>
            <fieldList:SetSumAggregateEditor/>
            <fieldList:SetAverageAggregateEditor/>
            <fieldList:SetCountAggregateEditor/>
            <fieldList:SummarizationEditor/>
            <fieldList:LabelFilterEditor/>
            <fieldList:ValueFilterEditor/>
            <fieldList:Top10FilterEditor/>
            <fieldList:ClearFilterEditor/>
            <fieldList:SortingEditor/>
            <fieldList:SetIndexTotalFormatEditor/>
            <fieldList:SetPercentOfGrandTotalFormatEditor/>
            <fieldList:TotalFormatEditor/>
            <fieldList:ClearTotalFormatEditor/>
            <fieldList:StringFormatEditor/>
            <fieldList:ItemsSetFilterEditor/>
            <fieldList:DoubleGroupStepEditor/>
        </fieldList:EditorsBehavior.Editors>
    </fieldList:EditorsBehavior>
    <Style x:Key="DropIndicatorStyle" TargetType="fieldList:DropIndicator">
        <Setter Property="Background" Value="{x:Null}"/>
        <Setter Property="BorderBrush" Value="{x:Null}"/>
        <Setter Property="BorderThickness" Value="0"/>
        <Setter Property="HorizontalContentAlignment" Value="Stretch"/>
        <Setter Property="VerticalContentAlignment" Value="Stretch"/>
        <Setter Property="Template">
            <Setter.Value>
                <ControlTemplate TargetType="fieldList:DropIndicator">
                    <Border BorderThickness="{TemplateBinding BorderThickness}" BorderBrush="{TemplateBinding BorderBrush}" Background="{TemplateBinding Background}">
                        <VisualStateManager.VisualStateGroups>
                            <VisualStateGroup x:Name="DropPositions">
                                <VisualState x:Name="None">
                                    <Storyboard>
                                        <DoubleAnimation Duration="0:0:0" To="0" Storyboard.TargetName="PART_DropIndicator" Storyboard.TargetProperty="Opacity"/>
                                    </Storyboard>
                                </VisualState>
                                <VisualState x:Name="Over"/>
                                <VisualState x:Name="Top">
                                    <Storyboard>
                                        <ObjectAnimationUsingKeyFrames Storyboard.TargetName="DropLine" Storyboard.TargetProperty="VerticalAlignment">
                                            <DiscreteObjectKeyFrame KeyTime="0:0:0">
                                                <DiscreteObjectKeyFrame.Value>
                                                    <VerticalAlignment>Top</VerticalAlignment>
                                                </DiscreteObjectKeyFrame.Value>
                                            </DiscreteObjectKeyFrame>
                                        </ObjectAnimationUsingKeyFrames>
                                    </Storyboard>
                                </VisualState>
                                <VisualState x:Name="Bottom">
                                    <Storyboard>
                                        <ObjectAnimationUsingKeyFrames Storyboard.TargetName="DropLine" Storyboard.TargetProperty="VerticalAlignment">
                                            <DiscreteObjectKeyFrame KeyTime="0:0:0">
                                                <DiscreteObjectKeyFrame.Value>
                                                    <VerticalAlignment>Bottom</VerticalAlignment>
                                                </DiscreteObjectKeyFrame.Value>
                                            </DiscreteObjectKeyFrame>
                                        </ObjectAnimationUsingKeyFrames>
                                    </Storyboard>
                                </VisualState>
                            </VisualStateGroup>
                        </VisualStateManager.VisualStateGroups>
                        <Grid>
                            <ContentPresenter Margin="{TemplateBinding Padding}" HorizontalAlignment="{TemplateBinding HorizontalContentAlignment}" VerticalAlignment="{TemplateBinding VerticalContentAlignment}"/>
                            <Canvas x:Name="PART_Canvas" IsHitTestVisible="False">
                                <Border x:Name="PART_DropIndicator">
                                    <Border x:Name="DropLine" Height="4" Margin="0 -2" BorderThickness="2 0" BorderBrush="{telerik:FluentResource ResourceKey=AccentFocusedBrush}">
                                        <Rectangle Height="2" Fill="{telerik:FluentResource ResourceKey=AccentFocusedBrush}" SnapsToDevicePixels="True"/>
                                    </Border>
                                </Border>
                            </Canvas>
                        </Grid>
                    </Border>
                </ControlTemplate>
            </Setter.Value>
        </Setter>
    </Style>
    <Style x:Key="FieldListButtonStyle" TargetType="telerik:RadButton">
        <Setter Property="mat:MaterialAssist.MouseOverBrush" Value="{telerik1:FluentResource ResourceKey=MouseOverBrush}"/>
        <Setter Property="mat:MaterialAssist.PressedBrush" Value="{telerik1:FluentResource ResourceKey=PressedBrush}"/>
        <Setter Property="BorderThickness" Value="0"/>
        <Setter Property="FontFamily" Value="{telerik1:FluentResource ResourceKey=FontFamily}"/>
        <Setter Property="FontSize" Value="{telerik1:FluentResource ResourceKey=FontSize}"/>
        <Setter Property="Foreground" Value="{telerik1:FluentResource ResourceKey=MarkerBrush}"/>
        <Setter Property="Background" Value="{telerik1:FluentResource ResourceKey=MainBrush}"/>
        <Setter Property="BorderBrush" Value="{telerik1:FluentResource ResourceKey=BasicBrush}"/>
        <Setter Property="Padding" Value="5"/>
        <Setter Property="CornerRadius" Value="{telerik1:FluentResource ResourceKey=CornerRadius}"/>
        <Setter Property="FocusVisualStyle" Value="{x:Null}"/>
        <Setter Property="HorizontalContentAlignment" Value="Center"/>
        <Setter Property="VerticalContentAlignment" Value="Center"/>
        <Setter Property="Template">
            <Setter.Value>
                <ControlTemplate TargetType="telerik:RadButton">
                    <Grid SnapsToDevicePixels="True">
                        <Grid.ColumnDefinitions>
                            <ColumnDefinition Width="*"/>
                            <ColumnDefinition Width="Auto"/>
                        </Grid.ColumnDefinitions>
                        <Border x:Name="BorderVisual"
                                Grid.ColumnSpan="2"
                                BorderThickness="{TemplateBinding BorderThickness}"
                                BorderBrush="{TemplateBinding BorderBrush}"
                                Background="{TemplateBinding Background}"
                                CornerRadius="{TemplateBinding CornerRadius}"/>
                        <mat:FluentControl IsSmartClipped="True" BorderThickness="1" CornerRadius="{TemplateBinding CornerRadius}">
                            <Grid>
                                <Grid.ColumnDefinitions>
                                    <ColumnDefinition Width="*"/>
                                    <ColumnDefinition Width="Auto"/>
                                </Grid.ColumnDefinitions>
                                <ContentPresenter x:Name="Content"
                                        TextBlock.Foreground="{TemplateBinding Foreground}"
                                        Margin="{TemplateBinding Padding}"
                                        Content="{TemplateBinding Content}"
                                        ContentTemplate="{TemplateBinding ContentTemplate}"
                                        VerticalAlignment="{TemplateBinding VerticalContentAlignment}"
                                        HorizontalAlignment="{TemplateBinding HorizontalContentAlignment}"
                                        ContentTemplateSelector="{TemplateBinding ContentTemplateSelector}"
                                        ContentStringFormat="{TemplateBinding ContentStringFormat}"
                                        RecognizesAccessKey="True"/>
                                <telerik:RadGlyph x:Name="Arrow"
                                        Grid.Column="1"
                                        Foreground="{telerik:FluentResource ResourceKey=IconBrush}"
                                        FontSize="16"
                                        HorizontalAlignment="Right"
                                        VerticalAlignment="Center"
                                        Margin="0 0 5 0"
                                        Glyph="{StaticResource GlyphArrowChevronDown}"/>
                            </Grid>
                        </mat:FluentControl>
                    </Grid>
                    <ControlTemplate.Triggers>
                        <Trigger Property="IsMouseOver" Value="True">
                            <Setter Property="Background" Value="{Binding RelativeSource={RelativeSource Self}, Path=(mat:MaterialAssist.MouseOverBrush)}"/>
                        </Trigger>
                        <Trigger Property="IsPressed" Value="True">
                            <Setter Property="Background" Value="{Binding RelativeSource={RelativeSource Self}, Path=(mat:MaterialAssist.PressedBrush)}"/>
                        </Trigger>
                        <Trigger Property="IsEnabled" Value="False">
                            <Setter TargetName="Content" Property="Opacity" Value="{telerik1:FluentResource ResourceKey=DisabledOpacity}"/>
                            <Setter TargetName="Arrow" Property="Opacity" Value="{telerik1:FluentResource ResourceKey=DisabledOpacity}"/>
                        </Trigger>
                    </ControlTemplate.Triggers>
                </ControlTemplate>
            </Setter.Value>
        </Setter>
    </Style>
    <Style x:Key="FieldBoxStyle" TargetType="fieldList:FieldBox">
        <Setter Property="ScrollViewer.VerticalScrollBarVisibility" Value="Auto"/>
        <Setter Property="ScrollViewer.HorizontalScrollBarVisibility" Value="Disabled"/>
        <Setter Property="BorderBrush" Value="{telerik1:FluentResource ResourceKey=BasicBrush}"/>
        <Setter Property="BorderThickness" Value="1"/>
        <Setter Property="Background" Value="{telerik:FluentResource ResourceKey=PrimaryBackgroundBrush}"/>
        <Setter Property="Foreground" Value="{telerik:FluentResource ResourceKey=MarkerBrush}"/>
        <Setter Property="HorizontalContentAlignment" Value="Stretch"/>
        <Setter Property="VerticalContentAlignment" Value="Stretch"/>
        <Setter Property="ItemTemplateSelector">
            <Setter.Value>
                <fieldList:FieldContentTemplateSelector>
                    <fieldList:FieldContentTemplateSelector.Default>
                        <DataTemplate>
                            <TextBlock Text="{Binding Description.DisplayName}"/>
                        </DataTemplate>
                    </fieldList:FieldContentTemplateSelector.Default>
                    <fieldList:FieldContentTemplateSelector.ValuesPosition>
                        <DataTemplate>
                            <Grid>
                                <Grid.ColumnDefinitions>
                                    <ColumnDefinition Width="Auto"/>
                                    <ColumnDefinition Width="*"/>
                                </Grid.ColumnDefinitions>
                                <telerik:RadGlyph Foreground="{telerik:FluentResource ResourceKey=IconBrush}" FontSize="16" HorizontalAlignment="Left" VerticalAlignment="Center" Glyph="{StaticResource GlyphSum}"/>
                                <TextBlock Grid.Column="1" Text="{Binding Description.DisplayName}"/>
                            </Grid>
                        </DataTemplate>
                    </fieldList:FieldContentTemplateSelector.ValuesPosition>
                </fieldList:FieldContentTemplateSelector>
            </Setter.Value>
        </Setter>
        <Setter Property="ItemsPanel">
            <Setter.Value>
                <ItemsPanelTemplate>
                    <StackPanel Background="Transparent"/>
                </ItemsPanelTemplate>
            </Setter.Value>
        </Setter>
        <Setter Property="Template">
            <Setter.Value>
                <ControlTemplate TargetType="fieldList:FieldBox">
                    <Grid>
                        <Grid.RowDefinitions>
                            <RowDefinition Height="Auto"/>
                            <RowDefinition Height="*"/>
                        </Grid.RowDefinitions>
                        <ContentPresenter Content="{TemplateBinding Header}"/>
                        <Border
                                Grid.Row="1"
                                Background="{TemplateBinding Background}"
                                BorderBrush="{TemplateBinding BorderBrush}"
                                BorderThickness="{TemplateBinding BorderThickness}"
                                CornerRadius="{telerik1:FluentResource ResourceKey=CornerRadius}">
                            <fieldList:DropIndicator>
                                <ScrollViewer x:Name="ScrollViewer"
                                        Padding="1"
                                        BorderThickness="0"
                                        HorizontalScrollBarVisibility="{Binding Path=(ScrollViewer.HorizontalScrollBarVisibility), RelativeSource={RelativeSource Mode=TemplatedParent}}"
                                        VerticalScrollBarVisibility="{Binding Path=(ScrollViewer.VerticalScrollBarVisibility), RelativeSource={RelativeSource Mode=TemplatedParent}}">
                                    <ItemsPresenter AllowDrop="True"/>
                                </ScrollViewer>
                            </fieldList:DropIndicator>
                        </Border>
                    </Grid>
                </ControlTemplate>
            </Setter.Value>
        </Setter>
    </Style>
    <Style x:Key="FieldBoxItemStyle" TargetType="fieldList:FieldBoxItem">
        <Setter Property="telerik:DragDropManager.AllowCapturedDrag" Value="True"/>
        <Setter Property="Template">
            <Setter.Value>
                <ControlTemplate TargetType="fieldList:FieldBoxItem">
                    <Grid Background="Transparent">
                        <Grid.RowDefinitions>
                            <RowDefinition Height="Auto"/>
                            <RowDefinition Height="Auto"/>
                        </Grid.RowDefinitions>
                        <telerik:RadButton x:Name="Button"
                                AutomationProperties.Name="field box item"
                                DataContext="{Binding Description}"
                                Grid.Row="0"
                                Margin="2 1"
                                HorizontalContentAlignment="Stretch"
                                VerticalContentAlignment="Stretch"
                                Style="{StaticResource FieldListButtonStyle}">
                            <fieldList:RoutedContextMenuBehavior.Behavior>
                                <fieldList:RoutedContextMenuBehavior/>
                            </fieldList:RoutedContextMenuBehavior.Behavior>
                            <ContentPresenter Grid.Column="0"/>
                        </telerik:RadButton>
                        <ItemsControl Grid.Row="1" Margin="4 0 0 0" ItemsSource="{Binding Children}">
                            <ItemsControl.ItemTemplate>
                                <DataTemplate>
                                    <telerik:RadButton x:Name="SubButton"
                                            AutomationProperties.Name="sub field box item"
                                            DataContext="{Binding Description}"
                                            Grid.Row="0"
                                            Margin="1"
                                            HorizontalContentAlignment="Stretch"
                                            VerticalContentAlignment="Stretch">
                                        <fieldList:RoutedContextMenuBehavior.Behavior>
                                            <fieldList:RoutedContextMenuBehavior/>
                                        </fieldList:RoutedContextMenuBehavior.Behavior>
                                        <Grid>
                                            <Grid.ColumnDefinitions>
                                                <ColumnDefinition Width="*"/>
                                                <ColumnDefinition Width="Auto"/>
                                            </Grid.ColumnDefinitions>
                                            <ContentControl Grid.Column="0" Margin="2 0" Content="{Binding DisplayName}"/>
                                            <Path Grid.Column="2" Margin="1" Width="7" Height="4" Data="{StaticResource ArrowDown7x4}" Fill="Black"/>
                                        </Grid>
                                    </telerik:RadButton>
                                </DataTemplate>
                            </ItemsControl.ItemTemplate>
                        </ItemsControl>
                    </Grid>
                </ControlTemplate>
            </Setter.Value>
        </Setter>
    </Style>
    <DataTemplate x:Key="DimensionRoleTemplate">
        <telerik:RadDockPanel>
            <telerik:RadGlyph
                    Foreground="{telerik:FluentResource ResourceKey=IconBrush}"
                    FontSize="16"
                    HorizontalAlignment="Left"
                    VerticalAlignment="Center"
                    Margin="0 0 5 0"
                    Glyph="{StaticResource GlyphFullScreen}"/>
            <TextBlock Text="{Binding Data.DisplayName}" VerticalAlignment="Center" Style="{StaticResource TextBlockTitleStyle}"/>
        </telerik:RadDockPanel>
    </DataTemplate>
    <DataTemplate x:Key="FolderRoleTemplate">
        <telerik:RadDockPanel>
            <telerik:RadGlyph
                    Foreground="{telerik:FluentResource ResourceKey=IconBrush}"
                    FontSize="16"
                    HorizontalAlignment="Left"
                    VerticalAlignment="Center"
                    Margin="0 0 5 0"
                    Glyph="{StaticResource GlyphFolderOpen}"/>
            <TextBlock Text="{Binding Data.DisplayName}" VerticalAlignment="Center" Style="{StaticResource TextBlockTitleStyle}"/>
        </telerik:RadDockPanel>
    </DataTemplate>
    <DataTemplate x:Key="ValuesRoleTemplate">
        <telerik:RadDockPanel>
            <telerik:RadGlyph
                    Foreground="{telerik:FluentResource ResourceKey=IconBrush}"
                    FontSize="16"
                    HorizontalAlignment="Left"
                    VerticalAlignment="Center"
                    Margin="0 0 5 0"
                    Glyph="{StaticResource GlyphSum}"/>
            <TextBlock Text="{Binding Data.DisplayName}" VerticalAlignment="Center" Style="{StaticResource TextBlockTitleStyle}"/>
        </telerik:RadDockPanel>
    </DataTemplate>
    <DataTemplate x:Key="KpiRoleTemplate">
        <telerik:RadDockPanel>
            <Image Source="/Telerik.Windows.Controls.PivotFieldList;component/Themes/Icons/Green/KPI.png" Stretch="None" Margin="0 0 5 0"/>
            <TextBlock Text="{Binding Data.DisplayName}" VerticalAlignment="Center" Style="{StaticResource TextBlockTitleStyle}"/>
        </telerik:RadDockPanel>
    </DataTemplate>
    <DataTemplate x:Key="OtherRoleTemplate">
        <telerik:RadDockPanel>
            <telerik:RadGlyph
                    Foreground="{telerik:FluentResource ResourceKey=IconBrush}"
                    FontSize="16"
                    HorizontalAlignment="Left"
                    VerticalAlignment="Center"
                    Margin="0 0 5 0"
                    Glyph="{StaticResource GlyphFieldsMore}"/>
            <TextBlock Text="{Binding Data.DisplayName}" VerticalAlignment="Center" Style="{StaticResource TextBlockTitleStyle}"/>
        </telerik:RadDockPanel>
    </DataTemplate>
    <DataTemplate x:Key="SelectableRoleTemplate">
        <CheckBox telerik:DragDropManager.AllowCapturedDrag="True" IsChecked="{Binding Data.IsUsed, Mode=TwoWay}" Content="{Binding Data.DisplayName}"/>
    </DataTemplate>
    <DataTemplate x:Key="NoneRoleTemplate">
        <TextBlock Text="{Binding Data.DisplayName}"/>
    </DataTemplate>
    <fieldList:FieldBoxItemRolePresenterTemplateSelector x:Key="FieldBoxItemTemplateSelector"
            DimensionRoleTemplate="{StaticResource DimensionRoleTemplate}"
            FolderRoleTemplate="{StaticResource FolderRoleTemplate}"
            KpiRoleTemplate="{StaticResource KpiRoleTemplate}"
            NoneRoleTemplate="{StaticResource NoneRoleTemplate}"
            OtherRoleTemplate="{StaticResource OtherRoleTemplate}"
            SelectableRoleTemplate="{StaticResource SelectableRoleTemplate}"
            ValuesRoleTemplate="{StaticResource ValuesRoleTemplate}"/>
    <Style x:Key="FieldDragVisualStyle" TargetType="fieldList:FieldDragVisual">
        <Setter Property="ContentTemplate">
            <Setter.Value>
                <DataTemplate>
                    <Grid MinWidth="50">
                        <Border HorizontalAlignment="Center" VerticalAlignment="Top" Background="White">
                            <TextBlock Text="{Binding DraggedField.FieldInfo.DisplayName}"/>
                        </Border>
                        <TextBlock
                                Margin="37 25 0 0"
                                HorizontalAlignment="Left"
                                VerticalAlignment="Top"
                                Text="{Binding DragAction, Converter={StaticResource IconConverter}}"
                                FontSize="16"
                                Foreground="{telerik:FluentResource ResourceKey=IconBrush}"
                                FontFamily="{StaticResource TelerikWebUI}"/>
                    </Grid>
                </DataTemplate>
            </Setter.Value>
        </Setter>
        <Setter Property="Template">
            <Setter.Value>
                <ControlTemplate TargetType="fieldList:FieldDragVisual">
                    <Border x:Name="BorderVisual"
                            Background="{TemplateBinding Background}"
                            BorderBrush="{TemplateBinding BorderBrush}"
                            BorderThickness="{TemplateBinding BorderThickness}"
                            CornerRadius="1.5">
                        <ContentPresenter Margin="{TemplateBinding Padding}" HorizontalAlignment="{TemplateBinding HorizontalContentAlignment}" VerticalAlignment="{TemplateBinding VerticalContentAlignment}"/>
                    </Border>
                </ControlTemplate>
            </Setter.Value>
        </Setter>
    </Style>
    <ControlTemplate x:Key="RadPivotFieldListTemplate" TargetType="local:RadPivotFieldList">
        <Grid>
            <mat:Shadow Background="{TemplateBinding Background}" ShadowDepth="{TemplateBinding mat:MaterialAssist.ShadowDepth}"/>
            <Border
                    Background="{TemplateBinding Background}"
                    BorderBrush="{TemplateBinding BorderBrush}"
                    BorderThickness="{TemplateBinding BorderThickness}"
                    DataContext="{Binding ViewModel, RelativeSource={RelativeSource TemplatedParent}}"
                    CornerRadius="{telerik1:FluentResource ResourceKey=CornerRadius}">
                <Grid>
                    <telerik:RadBusyIndicator IsBusy="{TemplateBinding Property=IsBusy}">
                        <Grid>
                            <Grid.ColumnDefinitions>
                                <ColumnDefinition Width="*" MinWidth="120"/>
                                <ColumnDefinition Width="*" MinWidth="120"/>
                            </Grid.ColumnDefinitions>
                            <Grid.RowDefinitions>
                                <RowDefinition Height="Auto"/>
                                <RowDefinition Height="2*"/>
                                <RowDefinition Height="Auto"/>
                                <RowDefinition Height="*"/>
                                <RowDefinition Height="*"/>
                                <RowDefinition Height="Auto"/>
                            </Grid.RowDefinitions>
                            <TextBlock
                                    Grid.ColumnSpan="2"
                                    Foreground="{TemplateBinding Foreground}"
                                    telerik:LocalizationManager.ResourceKey="PivotFieldList_ChooseFieldsToAddToReport"
                                    Margin="10 10 10 0"/>
                            <tree:TreeGrid x:Name="PART_Fields"
                                    Grid.Row="1"
                                    Grid.ColumnSpan="2"
                                    QuickStyle="{x:Null}"
                                    ItemsSource="{Binding Fields}"
                                    RowHeaderTemplateSelector="{StaticResource FieldBoxItemTemplateSelector}"
                                    FontWeight="Medium"
                                    Margin="10">
                                <fieldList:DragDropBehavior.Behavior>
                                    <fieldList:FieldsTreeDragDropBehavior/>
                                </fieldList:DragDropBehavior.Behavior>
                                <tree:TreeGrid.HierarchyAdapter>
                                    <fieldList:FieldAdapter/>
                                </tree:TreeGrid.HierarchyAdapter>
                            </tree:TreeGrid>
                            <TextBlock
                                    Grid.Row="2"
                                    Grid.ColumnSpan="2"
                                    Foreground="{TemplateBinding Foreground}"
                                    telerik:LocalizationManager.ResourceKey="PivotFieldList_DragFieldsBetweenAreasBelow"
                                    Margin="10 5 5 0"/>
                            <fieldList:FieldBox x:Name="ReportFilter" Grid.Row="3" ItemsSource="{Binding Filters}" Margin="10 5 5 5">
                                <fieldList:DragDropBehavior.Behavior>
                                    <fieldList:FieldBoxDragDropBehavior/>
                                </fieldList:DragDropBehavior.Behavior>
                                <fieldList:FieldBox.Header>
                                    <StackPanel Margin="0 5" Orientation="Horizontal">
                                        <telerik:RadGlyph Foreground="{telerik:FluentResource ResourceKey=IconBrush}" FontSize="16" HorizontalAlignment="Left" VerticalAlignment="Center" Glyph="{StaticResource GlyphFilter}"/>
                                        <TextBlock Margin="4 0 0 0" telerik:LocalizationManager.ResourceKey="PivotFieldList_ReportFilter" Style="{StaticResource TextBlockTitleStyle}"/>
                                    </StackPanel>
                                </fieldList:FieldBox.Header>
                            </fieldList:FieldBox>
                            <fieldList:FieldBox x:Name="ColumnLabels" Grid.Row="3" Grid.Column="1" ItemsSource="{Binding ColumnLabels}" Margin="5 5 10 5">
                                <fieldList:DragDropBehavior.Behavior>
                                    <fieldList:FieldBoxDragDropBehavior/>
                                </fieldList:DragDropBehavior.Behavior>
                                <fieldList:FieldBox.Header>
                                    <StackPanel Margin="0 5" Orientation="Horizontal">
                                        <telerik:RadGlyph
                                                Foreground="{telerik:FluentResource ResourceKey=IconBrush}"
                                                FontSize="16"
                                                HorizontalAlignment="Left"
                                                VerticalAlignment="Center"
                                                Glyph="{StaticResource GlyphThumbnailsUp}"/>
                                        <TextBlock Margin="4 0 0 0" telerik:LocalizationManager.ResourceKey="PivotFieldList_ColumnLabels" Style="{StaticResource TextBlockTitleStyle}"/>
                                    </StackPanel>
                                </fieldList:FieldBox.Header>
                            </fieldList:FieldBox>
                            <fieldList:FieldBox x:Name="RowLabels" Grid.Row="4" ItemsSource="{Binding RowLabels}" Margin="10 5 5 5">
                                <fieldList:DragDropBehavior.Behavior>
                                    <fieldList:FieldBoxDragDropBehavior/>
                                </fieldList:DragDropBehavior.Behavior>
                                <fieldList:FieldBox.Header>
                                    <StackPanel Margin="0 5" Orientation="Horizontal">
                                        <telerik:RadGlyph
                                                Foreground="{telerik:FluentResource ResourceKey=IconBrush}"
                                                FontSize="16"
                                                HorizontalAlignment="Left"
                                                VerticalAlignment="Center"
                                                Glyph="{StaticResource GlyphThumbnailsLeft}"/>
                                        <TextBlock Margin="4 0 0 0" telerik:LocalizationManager.ResourceKey="PivotFieldList_RowLabels" Style="{StaticResource TextBlockTitleStyle}"/>
                                    </StackPanel>
                                </fieldList:FieldBox.Header>
                            </fieldList:FieldBox>
                            <fieldList:FieldBox x:Name="Values" Grid.Row="4" Grid.Column="1" ItemsSource="{Binding Values}" Margin="5 5 10 5">
                                <fieldList:DragDropBehavior.Behavior>
                                    <fieldList:FieldBoxDragDropBehavior/>
                                </fieldList:DragDropBehavior.Behavior>
                                <fieldList:FieldBox.Header>
                                    <StackPanel Margin="0 5" Orientation="Horizontal">
                                        <telerik:RadGlyph Foreground="{telerik:FluentResource ResourceKey=IconBrush}" FontSize="16" HorizontalAlignment="Left" VerticalAlignment="Center" Glyph="{StaticResource GlyphSum}"/>
                                        <TextBlock Margin="4 0 0 0" telerik:LocalizationManager.ResourceKey="PivotFiledList_Values" Style="{StaticResource TextBlockTitleStyle}"/>
                                    </StackPanel>
                                </fieldList:FieldBox.Header>
                            </fieldList:FieldBox>
                            <CheckBox x:Name="DeferUpdatesCheckBox"
                                    Grid.Row="5"
                                    Grid.ColumnSpan="2"
                                    VerticalAlignment="Center"
                                    IsChecked="{Binding DeferUpdates, Mode=TwoWay}"
                                    telerik:LocalizationManager.ResourceKey="PivotFieldList_DeferLayoutUpdate"
                                    Margin="10 5 5 10"/>
                            <telerik:RadButton x:Name="PART_UpdateButton"
                                    Grid.Row="5"
                                    Grid.ColumnSpan="2"
                                    HorizontalAlignment="Right"
                                    VerticalAlignment="Center"
                                    MinWidth="80"
                                    Command="{Binding UpdateCommand}"
                                    telerik:LocalizationManager.ResourceKey="PivotFieldList_Update"
                                    Margin="5 5 10 10"/>
                        </Grid>
                    </telerik:RadBusyIndicator>
                </Grid>
            </Border>
        </Grid>
    </ControlTemplate>
    <Style x:Key="PivotFieldListStyle" TargetType="local:RadPivotFieldList">
        <Setter Property="mat:MaterialAssist.ShadowDepth" Value="Depth1"/>
        <Setter Property="fieldList:EditorsBehavior.Behavior" Value="{StaticResource EditorsBehavior}"/>
        <Setter Property="fieldList:WindowDialogHostingBehavior.Behavior" Value="{StaticResource WindowDialogHostingBehavior}"/>
        <Setter Property="fieldList:FieldListContextMenuBehavior.Behavior" Value="{StaticResource FieldListContextMenuBehavior}"/>
        <Setter Property="UseLayoutRounding" Value="True"/>
        <Setter Property="BorderBrush" Value="{telerik:FluentResource ResourceKey=BasicBrush}"/>
        <Setter Property="Background" Value="{telerik:FluentResource ResourceKey=PrimaryBackgroundBrush}"/>
        <Setter Property="Foreground" Value="{telerik:FluentResource ResourceKey=MarkerBrush}"/>
        <Setter Property="BorderThickness" Value="0"/>
        <Setter Property="Template" Value="{StaticResource RadPivotFieldListTemplate}"/>
    </Style>
    <DataTemplate x:Key="NamedObjectTemplate">
        <TextBlock Text="{Binding Converter={StaticResource NamedToStringConverter}}"/>
    </DataTemplate>
    <Style x:Key="ConfirmationFrameStyle" TargetType="editing:ConfirmationFrame">
        <Setter Property="Background" Value="{Binding Source={StaticResource PrimaryBackgroundBrushProxy}, Path=ProxyValue, Converter={StaticResource OpacityConverter}, ConverterParameter=204}"/>
        <Setter Property="Foreground" Value="{telerik1:FluentResource ResourceKey=MarkerBrush}"/>
        <Setter Property="FontSize" Value="{telerik1:FluentResource ResourceKey=FontSize}"/>
        <Setter Property="FontFamily" Value="{telerik1:FluentResource ResourceKey=FontFamily}"/>
        <Setter Property="HorizontalContentAlignment" Value="Stretch"/>
        <Setter Property="VerticalContentAlignment" Value="Stretch"/>
        <Setter Property="Padding" Value="10 10 10 0"/>
        <Setter Property="IsTabStop" Value="False"/>
        <Setter Property="Template">
            <Setter.Value>
                <ControlTemplate TargetType="editing:ConfirmationFrame">
                    <Border Background="{TemplateBinding Background}" BorderBrush="{TemplateBinding BorderBrush}" BorderThickness="{TemplateBinding BorderThickness}">
                        <Grid>
                            <Grid.RowDefinitions>
                                <RowDefinition Height="*"/>
                                <RowDefinition Height="Auto"/>
                            </Grid.RowDefinitions>
                            <telerik:RadBusyIndicator
                                    IsTabStop="False"
                                    IsEnabled="{Binding IsAvailable}"
                                    IsBusy="{Binding IsWorking}"
                                    Visibility="{Binding RequiresRefresh, Converter={StaticResource InvertedBooleanToVisibilityConverter}}">
                                <ContentPresenter Margin="{TemplateBinding Padding}" HorizontalAlignment="{TemplateBinding HorizontalContentAlignment}" VerticalAlignment="{TemplateBinding VerticalContentAlignment}"/>
                            </telerik:RadBusyIndicator>
                            <ContentControl Visibility="{Binding RequiresRefresh, Converter={StaticResource BooleanToVisibilityConverter}}" IsEnabled="{Binding IsAvailable}" IsTabStop="False">
                                <StackPanel VerticalAlignment="Center" Margin="{TemplateBinding Padding}">
                                    <TextBlock
                                            HorizontalAlignment="Center"
                                            telerik:LocalizationManager.ResourceKey="PivotFieldList_TheActionRequiresMoreRecentInformation"
                                            TextWrapping="Wrap"
                                            TextAlignment="Center"/>
                                    <TextBlock HorizontalAlignment="Center" telerik:LocalizationManager.ResourceKey="PivotFieldList_PleaseRefreshThePivot" TextWrapping="Wrap" Margin="0 8" TextAlignment="Center"/>
                                    <telerik:RadButton Margin="10" HorizontalAlignment="Center" Command="{Binding Refresh}" telerik:LocalizationManager.ResourceKey="PivotFieldList_Refresh"/>
                                </StackPanel>
                            </ContentControl>
                            <Border Grid.Row="1" Background="{telerik1:FluentResource ResourceKey=AlternativeBrush}" Margin="0 5 0 0">
                                <StackPanel Orientation="Horizontal" HorizontalAlignment="Right" VerticalAlignment="Bottom" Margin="10 5">
                                    <telerik:RadButton Margin="5 0" Command="{Binding Ok}" telerik:LocalizationManager.ResourceKey="Ok" IsDefault="True" MinWidth="80"/>
                                    <telerik:RadButton Command="{Binding Cancel}" telerik:LocalizationManager.ResourceKey="Cancel" MinWidth="80" IsCancel="True"/>
                                </StackPanel>
                            </Border>
                        </Grid>
                    </Border>
                </ControlTemplate>
            </Setter.Value>
        </Setter>
    </Style>
    <Style x:Key="DialogBaseStyle" TargetType="Control">
        <Setter Property="HorizontalContentAlignment" Value="Stretch"/>
        <Setter Property="VerticalContentAlignment" Value="Top"/>
        <Setter Property="Foreground" Value="{telerik:FluentResource ResourceKey=MarkerBrush}"/>
        <Setter Property="IsTabStop" Value="False"/>
        <Setter Property="Background" Value="{telerik1:FluentResource ResourceKey=AlternativeBrush}"/>
    </Style>
    <Style x:Key="SortingOptionsDialogStyle" TargetType="editing:SortingOptionsDialog" BasedOn="{StaticResource DialogBaseStyle}">
        <Setter Property="Template">
            <Setter.Value>
                <ControlTemplate TargetType="editing:SortingOptionsDialog">
                    <editing:ConfirmationFrame>
                        <telerik:GroupBox telerik:LocalizationManager.ResourceKey="PivotFieldList_SortOptions" IsTabStop="False">
                            <StackPanel>
                                <RadioButton x:Name="None"
                                        AutomationProperties.Name="None"
                                        IsChecked="{Binding SortOrder, Converter={StaticResource BooleanToSortOrderConverter}, ConverterParameter=None, Mode=TwoWay}"
                                        Margin="5"
                                        telerik:LocalizationManager.ResourceKey="PivotFieldList_None"/>
                                <RadioButton x:Name="Ascending"
                                        AutomationProperties.Name="Ascending"
                                        IsChecked="{Binding SortOrder, Converter={StaticResource BooleanToSortOrderConverter}, ConverterParameter=Ascending, Mode=TwoWay}"
                                        Margin="5"
                                        telerik:LocalizationManager.ResourceKey="PivotFieldList_AscendingBy"/>
                                <telerik:RadComboBox
                                        Margin="36 4 4 4"
                                        AutomationProperties.Name="Ascending Comparers"
                                        ItemsSource="{Binding AvailableComparers}"
                                        DisplayMemberPath="ComparerDescription"
                                        SelectedItem="{Binding SelectedAscendingComparer, Mode=TwoWay}"
                                        IsEnabled="{Binding SortOrder, Converter={StaticResource BooleanToSortOrderConverter}}"/>
                                <RadioButton x:Name="Descending"
                                        AutomationProperties.Name="Descending"
                                        IsChecked="{Binding SortOrder, Converter={StaticResource BooleanToSortOrderConverter}, ConverterParameter=Descending, Mode=TwoWay}"
                                        Margin="5"
                                        telerik:LocalizationManager.ResourceKey="PivotFieldList_DescendingBy"/>
                                <telerik:RadComboBox
                                        Margin="36 4 4 4"
                                        AutomationProperties.Name="Descending Comparers"
                                        ItemsSource="{Binding AvailableComparers}"
                                        DisplayMemberPath="ComparerDescription"
                                        SelectedItem="{Binding SelectedDescendingComparer, Mode=TwoWay}"
                                        IsEnabled="{Binding SortOrder, Converter={StaticResource BooleanToSortOrderConverter}}"/>
                            </StackPanel>
                        </telerik:GroupBox>
                    </editing:ConfirmationFrame>
                </ControlTemplate>
            </Setter.Value>
        </Setter>
    </Style>
    <Style x:Key="LabelFilterDialogStyle" TargetType="editing:LabelFilterDialog" BasedOn="{StaticResource DialogBaseStyle}">
        <Setter Property="Template">
            <Setter.Value>
                <ControlTemplate TargetType="editing:LabelFilterDialog">
                    <editing:ConfirmationFrame>
                        <Grid>
                            <ContentPresenter x:Name="PART_FilterControlPresenter"/>
                        </Grid>
                    </editing:ConfirmationFrame>
                </ControlTemplate>
            </Setter.Value>
        </Setter>
    </Style>
    <Style x:Key="ValueFilterDialogStyle" TargetType="editing:ValueFilterDialog" BasedOn="{StaticResource DialogBaseStyle}">
        <Setter Property="Template">
            <Setter.Value>
                <ControlTemplate TargetType="editing:ValueFilterDialog">
                    <editing:ConfirmationFrame>
                        <telerik:GroupBox telerik:LocalizationManager.ResourceKey="PivotFieldList_ShowItemsForWhich" IsTabStop="False">
                            <Grid>
                                <Grid.ColumnDefinitions>
                                    <ColumnDefinition Width="160"/>
                                    <ColumnDefinition Width="180"/>
                                    <ColumnDefinition Width="*"/>
                                </Grid.ColumnDefinitions>
                                <telerik:RadComboBox
                                        Grid.Column="0"
                                        AutomationProperties.Name="Value Sources"
                                        ItemsSource="{Binding AvailableValueSources}"
                                        SelectedItem="{Binding SelectedValueSource, Mode=TwoWay}"
                                        DisplayMemberPath="DisplayName"
                                        Margin="4"/>
                                <telerik:RadComboBox
                                        Grid.Column="1"
                                        AutomationProperties.Name="Conditions"
                                        ItemsSource="{Binding AvailableConditions}"
                                        ItemTemplate="{StaticResource NamedObjectTemplate}"
                                        SelectedItem="{Binding SelectedCondition, Mode=TwoWay}"
                                        Margin="4"/>
                                <telerik:RadNumericUpDown x:Name="Than"
                                        AutomationProperties.Name="Than"
                                        Value="{Binding Than, Mode=TwoWay}"
                                        Grid.Column="2"
                                        Grid.ColumnSpan="3"
                                        Margin="4"
                                        Visibility="{Binding SelectedCondition.Condition, Converter={StaticResource ComparisonToVisibleConverter}}"/>
                                <Grid Grid.Column="2" Visibility="{Binding SelectedCondition.Condition, Converter={StaticResource IntervalComparisonToVisibilityConverter}}">
                                    <Grid.ColumnDefinitions>
                                        <ColumnDefinition Width="*"/>
                                        <ColumnDefinition Width="Auto"/>
                                        <ColumnDefinition Width="*"/>
                                    </Grid.ColumnDefinitions>
                                    <telerik:RadNumericUpDown x:Name="From" AutomationProperties.Name="From" Value="{Binding From, Mode=TwoWay}" Grid.Column="0" Margin="4"/>
                                    <TextBlock x:Name="And" Grid.Column="1" VerticalAlignment="Center" telerik:LocalizationManager.ResourceKey="PivotFieldList_And"/>
                                    <telerik:RadNumericUpDown x:Name="To" AutomationProperties.Name="To" Value="{Binding To, Mode=TwoWay}" Grid.Column="2" Margin="4"/>
                                </Grid>
                            </Grid>
                        </telerik:GroupBox>
                    </editing:ConfirmationFrame>
                </ControlTemplate>
            </Setter.Value>
        </Setter>
    </Style>
    <Style x:Key="Top10FilterDialogStyle" TargetType="editing:Top10FilterDialog" BasedOn="{StaticResource DialogBaseStyle}">
        <Setter Property="Template">
            <Setter.Value>
                <ControlTemplate TargetType="editing:Top10FilterDialog">
                    <editing:ConfirmationFrame>
                        <telerik:GroupBox telerik:LocalizationManager.ResourceKey="PivotFieldList_Show" IsTabStop="False">
                            <Grid>
                                <Grid.ColumnDefinitions>
                                    <ColumnDefinition Width="105"/>
                                    <ColumnDefinition Width="*"/>
                                    <ColumnDefinition Width="105"/>
                                    <ColumnDefinition Width="Auto"/>
                                    <ColumnDefinition Width="130"/>
                                </Grid.ColumnDefinitions>
                                <telerik:RadComboBox
                                        Grid.Column="0"
                                        AutomationProperties.Name="Available Selections"
                                        ItemsSource="{Binding AvailableSelections}"
                                        ItemTemplate="{StaticResource NamedObjectTemplate}"
                                        SelectedItem="{Binding SelectedSelection, Mode=TwoWay}"
                                        Margin="4"/>
                                <Grid Grid.Column="1" Margin="4">
                                    <telerik:RadNumericUpDown x:Name="Items"
                                            AutomationProperties.Name="Items"
                                            Value="{Binding Items, Mode=TwoWay}"
                                            Minimum="0"
                                            IsInteger="True"
                                            SmallChange="1"
                                            LargeChange="10"
                                            Visibility="{Binding SelectedFilterType, Converter={StaticResource Top10FilterTypeToVisibilityConverter}, ConverterParameter=Items}"/>
                                    <telerik:RadNumericUpDown x:Name="Percent"
                                            AutomationProperties.Name="Percent"
                                            Value="{Binding Percent, Mode=TwoWay}"
                                            Minimum="0"
                                            Maximum="1"
                                            SmallChange="0.01"
                                            LargeChange="0.1"
                                            ValueFormat="Percentage"
                                            NumberDecimalDigits="2"
                                            IsInteger="False"
                                            Visibility="{Binding SelectedFilterType, Converter={StaticResource Top10FilterTypeToVisibilityConverter}, ConverterParameter=Percent}"/>
                                    <telerik:RadNumericUpDown x:Name="Sum"
                                            AutomationProperties.Name="Sum"
                                            Value="{Binding Sum, Mode=TwoWay}"
                                            Minimum="0"
                                            NumberDecimalDigits="2"
                                            SmallChange="10"
                                            LargeChange="100"
                                            Visibility="{Binding SelectedFilterType, Converter={StaticResource Top10FilterTypeToVisibilityConverter}, ConverterParameter=Sum}"/>
                                </Grid>
                                <telerik:RadComboBox
                                        Grid.Column="2"
                                        AutomationProperties.Name="Filter Types"
                                        ItemsSource="{Binding AvailableFilterTypes}"
                                        ItemTemplate="{StaticResource NamedObjectTemplate}"
                                        SelectedItem="{Binding SelectedFilterType, Mode=TwoWay}"
                                        Margin="4"/>
                                <TextBlock Grid.Column="3" VerticalAlignment="Center" Margin="4" telerik:LocalizationManager.ResourceKey="PivotFieldList_By"/>
                                <telerik:RadComboBox
                                        Grid.Column="4"
                                        AutomationProperties.Name="Value Sources"
                                        ItemsSource="{Binding AvailableValueSources}"
                                        SelectedItem="{Binding SelectedValueSource, Mode=TwoWay}"
                                        DisplayMemberPath="DisplayName"
                                        Margin="4"/>
                            </Grid>
                        </telerik:GroupBox>
                    </editing:ConfirmationFrame>
                </ControlTemplate>
            </Setter.Value>
        </Setter>
    </Style>
    <Style x:Key="ValueSummarizationDialogStyle" TargetType="editing:ValueSummarizationDialog" BasedOn="{StaticResource DialogBaseStyle}">
        <Setter Property="Template">
            <Setter.Value>
                <ControlTemplate TargetType="editing:ValueSummarizationDialog">
                    <editing:ConfirmationFrame>
                        <telerik:GroupBox telerik:LocalizationManager.ResourceKey="PivotFieldList_SummarizeValuesBy" IsTabStop="False">
                            <Grid>
                                <Grid.RowDefinitions>
                                    <RowDefinition Height="Auto"/>
                                    <RowDefinition Height="*"/>
                                </Grid.RowDefinitions>
                                <TextBlock TextWrapping="Wrap" Margin="4 8" telerik:LocalizationManager.ResourceKey="PivotFieldList_ChooseAggregateFunction"/>
                                <telerik:RadListBox
                                        Grid.Row="1"
                                        ItemsSource="{Binding AvailableAggregateFunctions}"
                                        AutomationProperties.Name="Aggregate Functions"
                                        ItemTemplate="{StaticResource NamedObjectTemplate}"
                                        SelectedItem="{Binding SelectedAggregateFunction, Mode=TwoWay}"
                                        MaxWidth="230"
                                        Margin="4"
                                        HorizontalAlignment="Stretch"/>
                            </Grid>
                        </telerik:GroupBox>
                    </editing:ConfirmationFrame>
                </ControlTemplate>
            </Setter.Value>
        </Setter>
    </Style>
    <Style x:Key="ValueCalculationsDialogStyle" TargetType="editing:ValueCalculationsDialog" BasedOn="{StaticResource DialogBaseStyle}">
        <Setter Property="VerticalContentAlignment" Value="Stretch"/>
        <Setter Property="Template">
            <Setter.Value>
                <ControlTemplate TargetType="editing:ValueCalculationsDialog">
                    <editing:ConfirmationFrame>
                        <telerik:GroupBox telerik:LocalizationManager.ResourceKey="PivotFieldList_ShowValuesAs" IsTabStop="False">
                            <Grid>
                                <Grid.RowDefinitions>
                                    <RowDefinition Height="Auto"/>
                                    <RowDefinition Height="Auto"/>
                                    <RowDefinition Height="*"/>
                                </Grid.RowDefinitions>
                                <Grid.ColumnDefinitions>
                                    <ColumnDefinition Width="*"/>
                                    <ColumnDefinition Width="*"/>
                                </Grid.ColumnDefinitions>
                                <telerik:RadComboBox
                                        Grid.ColumnSpan="2"
                                        ItemsSource="{Binding AvailableCalculationOptions}"
                                        SelectedItem="{Binding SelectedCalculationOption, Mode=TwoWay}"
                                        AutomationProperties.Name="Calculation Options"
                                        DisplayMemberPath="DisplayName"
                                        Margin="4"/>
                                <telerik:Label Grid.Row="1" Grid.Column="0" IsEnabled="{Binding RequiresGroupDescription}" telerik:LocalizationManager.ResourceKey="PivotFieldList_BaseField"/>
                                <telerik:RadListBox
                                        Grid.Row="2"
                                        Grid.Column="0"
                                        IsEnabled="{Binding RequiresGroupDescription}"
                                        AutomationProperties.Name="Group Descriptions"
                                        ItemsSource="{Binding AvailableGroupDescriptions}"
                                        SelectedItem="{Binding SelectedGroupDescription, Mode=TwoWay}"
                                        DisplayMemberPath="DisplayName"
                                        Margin="4"/>
                                <telerik:Label Grid.Row="1" Grid.Column="1" IsEnabled="{Binding RequiresGroupName}" telerik:LocalizationManager.ResourceKey="PivotFieldList_BaseItem"/>
                                <telerik:RadListBox
                                        Grid.Row="2"
                                        Grid.Column="1"
                                        IsEnabled="{Binding RequiresGroupName}"
                                        AutomationProperties.Name="Group Names"
                                        ItemsSource="{Binding AvailableGroupNames}"
                                        ItemTemplate="{StaticResource NamedObjectTemplate}"
                                        SelectedItem="{Binding SelectedGroupName, Mode=TwoWay}"
                                        Margin="4"/>
                            </Grid>
                        </telerik:GroupBox>
                    </editing:ConfirmationFrame>
                </ControlTemplate>
            </Setter.Value>
        </Setter>
    </Style>
    <Style x:Key="StringFormatsDialogStyle" TargetType="editing:StringFormatsDialog" BasedOn="{StaticResource DialogBaseStyle}">
        <Setter Property="Template">
            <Setter.Value>
                <ControlTemplate TargetType="editing:StringFormatsDialog">
                    <editing:ConfirmationFrame>
                        <telerik:GroupBox telerik:LocalizationManager.ResourceKey="PivotFieldList_GeneralFormat" IsTabStop="False">
                            <Grid>
                                <Grid.ColumnDefinitions>
                                    <ColumnDefinition Width="130"/>
                                    <ColumnDefinition Width="*"/>
                                </Grid.ColumnDefinitions>
                                <Grid.RowDefinitions>
                                    <RowDefinition Height="Auto"/>
                                    <RowDefinition Height="Auto"/>
                                </Grid.RowDefinitions>
                                <telerik:Label Margin="4" Padding="0" telerik:LocalizationManager.ResourceKey="PivotFieldList_Format"/>
                                <telerik:RadComboBox
                                        Grid.Column="1"
                                        Margin="4"
                                        AutomationProperties.Name="Format Options"
                                        ItemsSource="{Binding AvailableFormatOptions}"
                                        Text="{Binding SelectedFormat, Mode=TwoWay}"
                                        IsEditable="True"
                                        SelectedValuePath="StringFormat"
                                        DisplayMemberPath="StringFormat"/>
                                <TextBlock Grid.Row="1" Grid.ColumnSpan="2" Margin="4" TextWrapping="Wrap" telerik:LocalizationManager.ResourceKey="PivotFieldList_StringFormatDescription"/>
                            </Grid>
                        </telerik:GroupBox>
                    </editing:ConfirmationFrame>
                </ControlTemplate>
            </Setter.Value>
        </Setter>
    </Style>
    <Style x:Key="ItemsSetFilterDialogStyle" TargetType="editing:ItemsSetFilterDialog" BasedOn="{StaticResource DialogBaseStyle}">
        <Setter Property="Template">
            <Setter.Value>
                <ControlTemplate TargetType="editing:ItemsSetFilterDialog">
                    <editing:ConfirmationFrame>
                        <Grid>
                            <ContentPresenter x:Name="PART_FilterControlPresenter"/>
                        </Grid>
                    </editing:ConfirmationFrame>
                </ControlTemplate>
            </Setter.Value>
        </Setter>
    </Style>
    <Style x:Key="PivotItemsFilteringControlStyle" TargetType="editing:PivotItemsFilteringControl">
        <Setter Property="Template">
            <Setter.Value>
                <ControlTemplate TargetType="editing:PivotItemsFilteringControl">
                    <Grid>
                        <Grid.RowDefinitions>
                            <RowDefinition Height="*"/>
                            <RowDefinition Height="Auto"/>
                        </Grid.RowDefinitions>
                        <telerik:GroupBox Grid.Row="0" telerik:LocalizationManager.ResourceKey="PivotFieldList_SelectItem">
                            <ItemsControl x:Name="PART_DistinctFilter" ItemsSource="{Binding Selection, FallbackValue={x:Null}}">
                                <ItemsControl.Template>
                                    <ControlTemplate TargetType="ItemsControl">
                                        <ScrollViewer BorderBrush="{TemplateBinding BorderBrush}" BorderThickness="{TemplateBinding BorderThickness}" Background="{TemplateBinding Background}" CanContentScroll="True">
                                            <ItemsPresenter/>
                                        </ScrollViewer>
                                    </ControlTemplate>
                                </ItemsControl.Template>
                                <ItemsControl.ItemsPanel>
                                    <ItemsPanelTemplate>
                                        <VirtualizingStackPanel/>
                                    </ItemsPanelTemplate>
                                </ItemsControl.ItemsPanel>
                                <ItemsControl.ItemTemplate>
                                    <DataTemplate>
                                        <CheckBox
                                                Content="{Binding FallbackValue={x:Null}}"
                                                ContentTemplate="{StaticResource NamedObjectTemplate}"
                                                Margin="5"
                                                IsChecked="{Binding IsSelected, Mode=TwoWay, FallbackValue={x:Null}}"/>
                                    </DataTemplate>
                                </ItemsControl.ItemTemplate>
                            </ItemsControl>
                        </telerik:GroupBox>
                        <telerik:GroupBox Grid.Row="1" telerik:LocalizationManager.ResourceKey="PivotFieldList_ItemFilterConditionCaption">
                            <ContentPresenter x:Name="PART_OtherFilterPlaceholder"/>
                        </telerik:GroupBox>
                    </Grid>
                </ControlTemplate>
            </Setter.Value>
        </Setter>
    </Style>
    <Style x:Key="OperatorValueFilterControlStyle" TargetType="editing:OperatorValueFilterControl">
        <Setter Property="Template">
            <Setter.Value>
                <ControlTemplate TargetType="editing:OperatorValueFilterControl">
                    <Grid>
                        <Grid.RowDefinitions>
                            <RowDefinition Height="Auto"/>
                            <RowDefinition Height="Auto"/>
                        </Grid.RowDefinitions>
                        <Grid.ColumnDefinitions>
                            <ColumnDefinition Width="170"/>
                            <ColumnDefinition Width="*"/>
                        </Grid.ColumnDefinitions>
                        <telerik:RadComboBox
                                ItemsSource="{Binding AvailableConditions}"
                                AutomationProperties.Name="Available Conditions"
                                ItemTemplate="{StaticResource NamedObjectTemplate}"
                                SelectedItem="{Binding SelectedCondition, Mode=TwoWay}"
                                Margin="4"/>
                        <ContentPresenter x:Name="PART_EditorPresenterThan"
                                Grid.Column="1"
                                Grid.ColumnSpan="3"
                                Margin="4"
                                Visibility="{Binding SelectedCondition.Condition, Converter={StaticResource ComparisonToVisibleConverter}}"/>
                        <Grid Grid.Column="1" Visibility="{Binding SelectedCondition.Condition, Converter={StaticResource IntervalComparisonToVisibilityConverter}}">
                            <Grid.ColumnDefinitions>
                                <ColumnDefinition Width="*"/>
                                <ColumnDefinition Width="Auto"/>
                                <ColumnDefinition Width="*"/>
                            </Grid.ColumnDefinitions>
                            <ContentPresenter x:Name="PART_EditorPresenterFrom" Grid.Column="0" Margin="4"/>
                            <TextBlock x:Name="And" Grid.Column="1" VerticalAlignment="Center" telerik:LocalizationManager.ResourceKey="PivotFieldList_And"/>
                            <ContentPresenter x:Name="PART_EditorPresenterTo" Grid.Column="2" Margin="4"/>
                        </Grid>
                        <CheckBox Grid.Row="1" Grid.Column="1" Margin="4" IsChecked="{Binding IgnoreCase, Mode=TwoWay}" telerik:LocalizationManager.ResourceKey="PivotFieldList_IgnoreCase"/>
                    </Grid>
                </ControlTemplate>
            </Setter.Value>
        </Setter>
    </Style>
    <Style x:Key="DoubleGroupStepDialogStyle" TargetType="editing:DoubleGroupStepDialog" BasedOn="{StaticResource DialogBaseStyle}">
        <Setter Property="Template">
            <Setter.Value>
                <ControlTemplate TargetType="editing:DoubleGroupStepDialog">
                    <editing:ConfirmationFrame>
                        <telerik:GroupBox telerik:LocalizationManager.ResourceKey="PivotFieldList_SelectStep" IsTabStop="False">
                            <Grid>
                                <telerik:RadNumericUpDown
                                        AutomationProperties.Name="Selected Step"
                                        Value="{Binding SelectedStep, Mode=TwoWay, ValidatesOnDataErrors=True, NotifyOnValidationError=True}"
                                        UpdateValueEvent="PropertyChanged"
                                        NumberDecimalDigits="10"
                                        HideTrailingZeros="True"
                                        SmallChange="10"
                                        LargeChange="100"/>
                            </Grid>
                        </telerik:GroupBox>
                    </editing:ConfirmationFrame>
                </ControlTemplate>
            </Setter.Value>
        </Setter>
    </Style>
