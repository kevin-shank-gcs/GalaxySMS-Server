    <Style x:Key="HelpButtonStyle" TargetType="telerikRibbonView:RadRibbonButton" BasedOn="{StaticResource RadRibbonButtonStyle}">
        <Setter Property="IsTabStop" Value="False"/>
        <Setter Property="VerticalAlignment" Value="Center"/>
    </Style>
    <ControlTemplate x:Key="MinimizeButtonTemplate" TargetType="telerikRibbonView:RadRibbonToggleButton">
        <Grid x:Name="RootElement">
            <VisualStateManager.VisualStateGroups>
                <VisualStateGroup x:Name="KeyboardNavigationStates">
                    <VisualState x:Name="KeyboardUnfocused"/>
                    <VisualState x:Name="KeyboardFocused">
                        <Storyboard>
                            <DoubleAnimation Duration="0" To="1" Storyboard.TargetProperty="(UIElement.Opacity)" Storyboard.TargetName="KeyboardNavigationVisual"/>
                        </Storyboard>
                    </VisualState>
                </VisualStateGroup>
            </VisualStateManager.VisualStateGroups>
            <Border x:Name="OuterBorder"
                    CornerRadius="{telerik1:GreenResource ResourceKey=CornerRadius}"
                    BorderThickness="{TemplateBinding BorderThickness}"
                    BorderBrush="{TemplateBinding BorderBrush}"
                    Background="{TemplateBinding Background}"/>
            <Border x:Name="KeyboardNavigationVisual"
                    BorderThickness="{TemplateBinding BorderThickness}"
                    Opacity="0"
                    Background="{telerik1:GreenResource ResourceKey=MouseOverBrush}"
                    BorderBrush="{telerik1:GreenResource ResourceKey=MouseOverBrush}"/>
            <Path x:Name="arrow"
                    VerticalAlignment="Center"
                    HorizontalAlignment="Center"
                    Data="{StaticResource ThinArrowTop8x5}"
                    RenderTransformOrigin="0.5 0.5"
                    Width="8"
                    Height="5"
                    Stretch="None"
                    Fill="{telerik1:GreenResource ResourceKey=StrongBrush}"
                    StrokeThickness="0"/>
        </Grid>
        <ControlTemplate.Triggers>
            <Trigger Property="IsMouseOver" Value="True">
                <Setter Property="Fill" TargetName="arrow" Value="{telerik1:GreenResource ResourceKey=MarkerBrush}"/>
                <Setter Property="Background" TargetName="OuterBorder" Value="{telerik1:GreenResource ResourceKey=MouseOverBrush}"/>
            </Trigger>
            <Trigger Property="IsPressed" Value="True">
                <Setter Property="Fill" TargetName="arrow" Value="{telerik1:GreenResource ResourceKey=SelectedBrush}"/>
                <Setter Property="Background" TargetName="OuterBorder" Value="{telerik1:GreenResource ResourceKey=AccentHighBrush}"/>
            </Trigger>
            <Trigger Property="IsEnabled" Value="False">
                <Setter Property="Opacity" TargetName="arrow" Value="{telerik1:GreenResource ResourceKey=DisabledOpacity}"/>
                <Setter Property="Fill" TargetName="arrow" Value="{telerik1:GreenResource ResourceKey=MarkerBrush}"/>
            </Trigger>
            <Trigger Property="IsChecked" Value="True">
                <Setter TargetName="arrow" Property="LayoutTransform">
                    <Setter.Value>
                        <TransformGroup>
                            <RotateTransform Angle="180"/>
                        </TransformGroup>
                    </Setter.Value>
                </Setter>
                <Setter TargetName="RootElement" Property="telerikRibbonView:ScreenTip.Title" Value="{telerik:LocalizableResource Key=RibbonViewExpandRibbon}"/>
            </Trigger>
            <Trigger Property="IsChecked" Value="False">
                <Setter TargetName="RootElement" Property="telerikRibbonView:ScreenTip.Title" Value="{telerik:LocalizableResource Key=RibbonViewMinimizeRibbon}"/>
            </Trigger>
        </ControlTemplate.Triggers>
    </ControlTemplate>
    <Style x:Key="MinimizeButtonStyle" TargetType="telerikRibbonView:RadRibbonToggleButton">
        <Setter Property="IsTabStop" Value="False"/>
        <Setter Property="BorderBrush" Value="Transparent"/>
        <Setter Property="Background" Value="Transparent"/>
        <Setter Property="HorizontalContentAlignment" Value="Center"/>
        <Setter Property="VerticalContentAlignment" Value="Center"/>
        <Setter Property="VerticalAlignment" Value="Center"/>
        <Setter Property="Size" Value="Small"/>
        <Setter Property="FocusVisualStyle" Value="{x:Null}"/>
        <Setter Property="MinWidth" Value="18"/>
        <Setter Property="MinHeight" Value="18"/>
        <Setter Property="Template" Value="{StaticResource MinimizeButtonTemplate}"/>
    </Style>
    <ControlTemplate x:Key="RadRibbonViewTemplate" TargetType="telerikRibbonView:RadRibbonView">
        <Grid x:Name="RootPanel">
            <Grid.Resources>
                <Style x:Key="SelectedTabContentStyle" TargetType="ContentPresenter">
                    <Style.Triggers>
                        <DataTrigger Binding="{Binding RelativeSource={RelativeSource AncestorType=telerikRibbonView:RadRibbonView}, Path=LayoutMode}" Value="Simplified">
                            <Setter Property="Height" Value="{Binding RelativeSource={RelativeSource AncestorType=telerikRibbonView:RadRibbonView}, Path=SimplifiedContentHeight}"/>
                        </DataTrigger>
                        <DataTrigger Binding="{Binding RelativeSource={RelativeSource AncestorType=telerikRibbonView:RadRibbonView}, Path=LayoutMode}" Value="Default">
                            <Setter Property="Height" Value="{Binding RelativeSource={RelativeSource AncestorType=telerikRibbonView:RadRibbonView}, Path=ContentHeight}"/>
                        </DataTrigger>
                    </Style.Triggers>
                </Style>
            </Grid.Resources>
            <Grid.RowDefinitions>
                <RowDefinition Height="Auto"/>
                <RowDefinition Height="Auto"/>
                <RowDefinition Height="Auto"/>
            </Grid.RowDefinitions>
            <Rectangle Grid.Row="1" Grid.RowSpan="2" Fill="{TemplateBinding Background}"/>
            <ContentPresenter x:Name="SelectedTabContent" Grid.Row="1" Style="{StaticResource SelectedTabContentStyle}"/>
            <Grid x:Name="TopGrid">
                <Grid.ColumnDefinitions>
                    <ColumnDefinition Width="Auto"/>
                    <ColumnDefinition Width="*"/>
                    <ColumnDefinition Width="Auto"/>
                </Grid.ColumnDefinitions>
                <Grid.RowDefinitions>
                    <RowDefinition Height="*"/>
                    <RowDefinition Height="Auto"/>
                </Grid.RowDefinitions>
                <Border x:Name="titleBarBackground" Background="{TemplateBinding TitleBarBackground}" IsHitTestVisible="False" Grid.ColumnSpan="3"/>
                <Rectangle x:Name="TitleBarRectangleBackground" Grid.Row="1" Grid.ColumnSpan="3" Fill="{TemplateBinding TitleBarBackground}"/>
                <telerikRibbonViewPrimitives:RibbonTitleBarPanel x:Name="RibbonTitleBarPanel" Grid.ColumnSpan="3" Visibility="{TemplateBinding TitleBarVisibility}">
                    <ContentPresenter x:Name="AboveToolBarHost" VerticalAlignment="Center" telerikRibbonViewPrimitives:RibbonTitleBarPanel.Element="QuickAccessToolBar"/>
                    <telerikRibbonViewPrimitives:WindowTitle x:Name="WindowTitle"
                            Title="{TemplateBinding Title}"
                            telerikRibbonViewPrimitives:RibbonTitleBarPanel.Element="WindowTitle"
                            ApplicationName="{TemplateBinding ApplicationName}"
                            Style="{TemplateBinding WindowTitleStyle}"/>
                    <StackPanel x:Name="ContextualGroupsHeaders" Margin="1 0 0 0" Orientation="Horizontal" telerikRibbonViewPrimitives:RibbonTitleBarPanel.Element="ContextualGroupsPanel"/>
                </telerikRibbonViewPrimitives:RibbonTitleBarPanel>
                <telerikRibbonViewPrimitives:RadRibbonApplicationDropDownButton x:Name="ApplicationButton"
                        Grid.Row="1"
                        IsEnabled="{TemplateBinding IsApplicationButtonEnabled}"
                        IsOpen="{Binding RelativeSource={RelativeSource TemplatedParent}, Path=IsApplicationMenuOpen, Mode=TwoWay}"
                        SmallImage="{TemplateBinding ApplicationButtonImageSource}"
                        Content="{TemplateBinding ApplicationButtonContent}"
                        Visibility="{TemplateBinding ApplicationButtonVisibility}"
                        Style="{TemplateBinding ApplicationButtonStyle}"/>
                <telerikRibbonViewPrimitives:RibbonScrollViewer Grid.Column="1" Grid.Row="1" Margin="0 0 0 -1" x:Name="TabsScrollViewer">
                    <ItemsPresenter/>
                </telerikRibbonViewPrimitives:RibbonScrollViewer>
                <StackPanel Orientation="Horizontal" Grid.Column="2" Grid.Row="1">
                    <telerik:RadToggleSwitchButton x:Name="LayoutModeButton"
                            VerticalAlignment="Center"
                            ContentPosition="Right"
                            CheckedContent="{TemplateBinding LayoutModeButtonContent}"
                            UncheckedContent="{TemplateBinding LayoutModeButtonContent}"
                            IsChecked="{Binding RelativeSource={RelativeSource AncestorType=telerikRibbonView:RadRibbonView}, Path=LayoutMode , Converter={StaticResource BooleanToRibbonLayoutConverter}, Mode=TwoWay}"
                            HorizontalAlignment="Center"
                            Visibility="{Binding RelativeSource={RelativeSource TemplatedParent}, Path=ShowLayoutModeButton, Converter={StaticResource RibbonBooleanToVisibilityConverter}}"/>
                    <telerikRibbonView:RadRibbonToggleButton x:Name="MinimizeButton"
                            Style="{TemplateBinding MinimizeButtonStyle}"
                            Visibility="{TemplateBinding MinimizeButtonVisibility}"
                            IsEnabled="{TemplateBinding IsMinimizable}"
                            IsChecked="{Binding RelativeSource={RelativeSource TemplatedParent}, Path=IsMinimized, Mode=TwoWay}"/>
                    <telerikRibbonView:RadRibbonButton
                            Command="{TemplateBinding HelpCommand}"
                            CommandParameter="{TemplateBinding HelpCommandParameter}"
                            CommandTarget="{TemplateBinding HelpCommandTarget}"
                            x:Name="HelpButton"
                            Visibility="{TemplateBinding HelpButtonVisibility}"
                            SmallImage="{TemplateBinding HelpButtonImageSource}"
                            Style="{TemplateBinding HelpButtonStyle}"
                            telerikRibbonView:ScreenTip.Title="{telerik:LocalizableResource Key=RibbonViewHelp}"/>
                    <ContentPresenter Content="{TemplateBinding TabStripAdditionalContent}"/>
                </StackPanel>
                <Rectangle Fill="Transparent" VerticalAlignment="Bottom" Height="2" Margin="0 0 0 -1" Grid.ColumnSpan="3" Grid.RowSpan="2"/>
            </Grid>
            <ContentPresenter x:Name="BelowToolBarHost" Grid.Row="2"/>
            <Popup x:Name="TabContentPopup" VerticalOffset="-1" PlacementTarget="{Binding ElementName=TopGrid}" AllowsTransparency="True">
                <Popup.Resources>
                    <Style x:Key="SelectedTabContentPopupStyle" TargetType="ContentPresenter">
                        <Style.Triggers>
                            <DataTrigger Binding="{Binding RelativeSource={RelativeSource AncestorType=telerikRibbonView:RadRibbonView}, Path=LayoutMode}" Value="Simplified">
                                <Setter Property="Height" Value="{Binding RelativeSource={RelativeSource AncestorType=telerikRibbonView:RadRibbonView}, Path=SimplifiedContentHeight}"/>
                            </DataTrigger>
                            <DataTrigger Binding="{Binding RelativeSource={RelativeSource AncestorType=telerikRibbonView:RadRibbonView}, Path=LayoutMode}" Value="Default">
                                <Setter Property="Height" Value="{Binding RelativeSource={RelativeSource AncestorType=telerikRibbonView:RadRibbonView}, Path=ContentHeight}"/>
                            </DataTrigger>
                        </Style.Triggers>
                    </Style>
                </Popup.Resources>
                <ContentPresenter x:Name="SelectedTabContentPopup" Style="{StaticResource SelectedTabContentPopupStyle}"/>
            </Popup>
        </Grid>
        <ControlTemplate.Triggers>
            <Trigger Property="IsHostedInRibbonWindow" Value="True">
                <Setter TargetName="titleBarBackground" Property="Visibility" Value="Collapsed"/>
            </Trigger>
        </ControlTemplate.Triggers>
    </ControlTemplate>
    <Style x:Key="RadRibbonViewStyle" TargetType="telerikRibbonView:RadRibbonView">
