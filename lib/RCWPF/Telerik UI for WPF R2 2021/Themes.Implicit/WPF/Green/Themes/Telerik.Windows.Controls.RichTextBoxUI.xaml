    <Style TargetType="taskPaneControls:GradientLineSettingsControl" BasedOn="{StaticResource GradientLineSettingsControlStyle}"/>
    <Style TargetType="taskPaneControls:ColorAndTransparencyPicker" BasedOn="{StaticResource ColorAndTransparencyPickerStyle}"/>
    <Style TargetType="taskPaneControls:GradientSettingsControl" BasedOn="{StaticResource GradientSettingsControlStyle}"/>
    <Style TargetType="taskPaneControls:PresetGradientsPopupGallery" BasedOn="{StaticResource PresetGradientsPopupGalleryStyle}"/>
    <Style TargetType="taskPaneControls:GradientsDirectionPopupGallery" BasedOn="{StaticResource GradientsDirectionPopupGalleryStyle}"/>
