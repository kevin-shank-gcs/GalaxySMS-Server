    <Style x:Key="RadRibbonWindowMinimizeButtonStyle" TargetType="telerikRibbonView:RadRibbonButton" BasedOn="{StaticResource RadRibbonButtonStyle}">
        <Setter Property="BorderBrush" Value="Transparent"/>
        <Setter Property="Background" Value="Transparent"/>
        <Setter Property="HorizontalContentAlignment" Value="Center"/>
        <Setter Property="VerticalContentAlignment" Value="Center"/>
        <Setter Property="BorderThickness" Value="0"/>
        <Setter Property="Height" Value="22"/>
        <Setter Property="Width" Value="22"/>
        <Setter Property="Foreground" Value="{telerik1:GreenResource ResourceKey=StrongBrush}"/>
    </Style>
    <Style x:Key="RadRibbonWindowToggleButtonStyle" TargetType="telerik:RadToggleButton">
        <Setter Property="Foreground" Value="{telerik1:GreenResource ResourceKey=StrongBrush}"/>
        <Setter Property="BorderBrush" Value="Transparent"/>
        <Setter Property="Background" Value="Transparent"/>
        <Setter Property="HorizontalContentAlignment" Value="Center"/>
        <Setter Property="VerticalContentAlignment" Value="Center"/>
        <Setter Property="BorderThickness" Value="0"/>
        <Setter Property="Height" Value="22"/>
        <Setter Property="Width" Value="22"/>
        <Setter Property="Template">
            <Setter.Value>
                <ControlTemplate TargetType="telerik:RadToggleButton">
                    <Grid>
                        <Border x:Name="BorderVisual"
                                BorderThickness="{TemplateBinding BorderThickness}"
                                Background="{TemplateBinding Background}"
                                BorderBrush="{TemplateBinding BorderBrush}"
                                CornerRadius="{TemplateBinding CornerRadius}"/>
                        <Grid x:Name="Restore" Background="Transparent" ToolTipService.ToolTip="Maximize" HorizontalAlignment="Center" VerticalAlignment="Center">
                            <Grid.ToolTip>
                                <TextBlock Text="{telerik:LocalizableResource Key=RibbonWindowMaximize}"/>
                            </Grid.ToolTip>
                            <Path x:Name="MaximizedPath"
                                    Data="{StaticResource MaximizeIcon}"
                                    Fill="{Binding Foreground, RelativeSource={RelativeSource AncestorType={x:Type telerik:RadToggleButton}}}"
                                    HorizontalAlignment="Center"
                                    VerticalAlignment="Center"/>
                        </Grid>
                        <Grid Visibility="Collapsed" x:Name="Collapse" Background="Transparent" HorizontalAlignment="Center" VerticalAlignment="Center" ToolTipService.ToolTip="Restore Down">
                            <Grid.ToolTip>
                                <TextBlock Text="{telerik:LocalizableResource Key=RibbonWindowRestoreDown}"/>
                            </Grid.ToolTip>
                            <Path x:Name="RestoredPath"
                                    Data="{StaticResource RestoreIcon}"
                                    Fill="{Binding Foreground, RelativeSource={RelativeSource AncestorType={x:Type telerik:RadToggleButton}}}"
                                    HorizontalAlignment="Center"
                                    VerticalAlignment="Center"/>
                        </Grid>
                    </Grid>
                    <ControlTemplate.Triggers>
                        <Trigger Property="IsMouseOver" Value="True">
                            <Setter TargetName="MaximizedPath" Property="Fill" Value="{telerik1:GreenResource ResourceKey=SelectedBrush}"/>
                            <Setter TargetName="RestoredPath" Property="Fill" Value="{telerik1:GreenResource ResourceKey=SelectedBrush}"/>
                            <Setter TargetName="BorderVisual" Property="Background" Value="{telerik1:GreenResource ResourceKey=MouseOverBrush}"/>
                        </Trigger>
                        <Trigger Property="IsPressed" Value="True">
                            <Setter TargetName="MaximizedPath" Property="Fill" Value="{telerik1:GreenResource ResourceKey=SelectedBrush}"/>
                            <Setter TargetName="RestoredPath" Property="Fill" Value="{telerik1:GreenResource ResourceKey=SelectedBrush}"/>
                            <Setter TargetName="BorderVisual" Property="Background" Value="{telerik1:GreenResource ResourceKey=AccentHighBrush}"/>
                        </Trigger>
                        <Trigger Property="IsChecked" Value="True">
                            <Setter TargetName="Restore" Property="Visibility" Value="Collapsed"/>
                            <Setter TargetName="Collapse" Property="Visibility" Value="Visible"/>
                        </Trigger>
                        <Trigger Property="IsEnabled" Value="False">
                            <Setter TargetName="Restore" Property="Opacity" Value="{telerik1:GreenResource ResourceKey=DisabledOpacity}"/>
                            <Setter TargetName="Collapse" Property="Opacity" Value="{telerik1:GreenResource ResourceKey=DisabledOpacity}"/>
                        </Trigger>
                    </ControlTemplate.Triggers>
                </ControlTemplate>
            </Setter.Value>
        </Setter>
