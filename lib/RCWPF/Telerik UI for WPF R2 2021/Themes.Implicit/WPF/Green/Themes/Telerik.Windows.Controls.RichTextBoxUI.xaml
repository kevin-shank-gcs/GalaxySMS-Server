    <Style TargetType="taskPaneControls:LineSettingsControl" BasedOn="{StaticResource LineSettingsControlStyle}"/>
    <Style TargetType="taskPaneControls:LinePropertiesSettingsControl" BasedOn="{StaticResource LinePropertiesSettingsControlStyle}"/>
    <Style TargetType="taskPaneControls:DashTypeGallery" BasedOn="{StaticResource DashTypeGalleryStyle}"/>
