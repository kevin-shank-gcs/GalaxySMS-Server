                            <Setter TargetName="TopThumb" Property="ContentPlacement" Value="Left"/>
                            <Setter TargetName="TopThumb" Property="DockPanel.Dock" Value="Right"/>
                            <Setter TargetName="PART_TopThumbImage" Property="DockPanel.Dock" Value="Left"/>
                        </Trigger>
                        <Trigger SourceName="TopThumb" Property="IsMouseOver" Value="True">
                            <Setter TargetName="TopThumb" Property="Background" Value="{Binding OutlineColor, RelativeSource={RelativeSource TemplatedParent}, Converter={StaticResource ColorToBrushWithOpacityConverter}, ConverterParameter=100}"/>
                        </Trigger>
                        <Trigger SourceName="PART_TopThumbImage" Property="IsMouseOver" Value="True">
                            <Setter TargetName="PART_TopThumbImage" Property="Background" Value="{Binding OutlineColor, RelativeSource={RelativeSource TemplatedParent}, Converter={StaticResource ColorToBrushWithOpacityConverter}, ConverterParameter=100}"/>
                        </Trigger>
                        <Trigger Property="Selected" Value="True">
                            <Setter TargetName="TopThumb" Property="Background" Value="{Binding OutlineColor, RelativeSource={RelativeSource TemplatedParent}, Converter={StaticResource ColorToBrushConverter}}"/>
                            <Setter TargetName="PART_TopThumbImage" Property="Background" Value="{Binding OutlineColor, RelativeSource={RelativeSource TemplatedParent}, Converter={StaticResource ColorToBrushConverter}}"/>
                            <Setter TargetName="PART_MainBodyFill" Property="Fill" Value="{Binding OutlineColor, RelativeSource={RelativeSource TemplatedParent}, Converter={StaticResource ColorToBrushConverter}}"/>
                        </Trigger>
                        <MultiTrigger>
                            <MultiTrigger.Conditions>
                                <Condition Property="Focused" Value="False"/>
                                <Condition Property="Hovered" Value="True"/>
                            </MultiTrigger.Conditions>
                            <Setter TargetName="PART_MainBodyFill" Property="Opacity" Value="0.2"/>
                        </MultiTrigger>
                        <MultiTrigger>
                            <MultiTrigger.Conditions>
                                <Condition Property="Focused" Value="True"/>
                                <Condition Property="Hovered" Value="True"/>
                            </MultiTrigger.Conditions>
                            <Setter TargetName="PART_MainBodyFill" Property="Opacity" Value="0.0"/>
                        </MultiTrigger>
                        <Trigger Property="Hovered" Value="False">
                            <Setter TargetName="PART_MainBodyFill" Property="Opacity" Value="0.0"/>
                        </Trigger>
                        <Trigger Property="TopThumbButtonShouldBeVisible" Value="False">
                            <Setter TargetName="PART_TopThumbImage" Property="Visibility" Value="Collapsed"/>
                        </Trigger>
                    </ControlTemplate.Triggers>
                </ControlTemplate>
            </Setter.Value>
        </Setter>
    </Style>
    <Style x:Key="RepeatingSectionControlStyle" TargetType="telerikControlsRTBUI:RepeatingSectionControl" BasedOn="{StaticResource ContentControlBaseStyle}">
        <Setter Property="ThumbShouldBeVisible" Value="True"/>
