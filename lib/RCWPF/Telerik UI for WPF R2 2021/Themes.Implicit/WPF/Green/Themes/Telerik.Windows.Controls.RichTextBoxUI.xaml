    <Style x:Key="TaskPaneStyle" TargetType="telerikControlsRTBUI:TaskPane">
        <Setter Property="Background" Value="{telerik1:GreenResource ResourceKey=PrimaryBrush}"/>
        <Setter Property="BorderBrush" Value="{telerik1:GreenResource ResourceKey=BasicBrush}"/>
        <Setter Property="Foreground" Value="{telerik1:GreenResource ResourceKey=MarkerBrush}"/>
        <Setter Property="Padding" Value="10 8"/>
        <Setter Property="FocusVisualStyle" Value="{x:Null}"/>
        <Setter Property="MinWidth" Value="310"/>
        <Setter Property="Template">
            <Setter.Value>
                <ControlTemplate TargetType="telerikControlsRTBUI:TaskPane">
                    <Border BorderBrush="{TemplateBinding BorderBrush}" BorderThickness="{TemplateBinding BorderThickness}" Background="{TemplateBinding Background}">
                        <Grid>
                            <Grid.RowDefinitions>
                                <RowDefinition Height="Auto"/>
                                <RowDefinition Height="*"/>
                            </Grid.RowDefinitions>
                            <Grid.ColumnDefinitions>
                                <ColumnDefinition Width="*"/>
                                <ColumnDefinition Width="Auto"/>
                            </Grid.ColumnDefinitions>
                            <TextBlock Text="{Binding Manager.Culture, Source={StaticResource localizationManager}, Converter={StaticResource cultureConverter}, ConverterParameter=Documents_RadRichTextBox_Shapes_TaskPane_FormatShape}" Style="{StaticResource TaskPaneTextBlockStyle}"/>
                            <telerik:RadButton
                                    Grid.Column="1"
                                    x:Name="closeButton"
                                    Background="Transparent"
                                    BorderBrush="Transparent"
                                    Padding="4"
                                    Margin="0 0 5 0"
                                    VerticalAlignment="Center"
                                    HorizontalAlignment="Center">
                                <telerik:RadButton.Content>
                                    <telerik:RadGlyph Foreground="{TemplateBinding Foreground}" FontSize="16" HorizontalAlignment="Center" VerticalAlignment="Center" Glyph="{StaticResource GlyphClose}"/>
                                </telerik:RadButton.Content>
                            </telerik:RadButton>
                            <telerik:RadTabControl x:Name="taskPane" Grid.Row="1" Grid.ColumnSpan="2" BorderThickness="1 0" Padding="{TemplateBinding Padding}">
                                <telerik:RadTabControl.Items>
                                    <telerik:RadTabItem Margin="10 0 0 0">
                                        <telerik:RadTabItem.Header>
                                            <Image Source="{telerik:IconResource IconRelativePath='FormatShapesBucket.png', IconSources={StaticResource IconSourcesShapes}}" ToolTipService.ToolTip="{Binding Manager.Culture, Source={StaticResource localizationManager}, Converter={StaticResource cultureConverter}, ConverterParameter=Documents_RadRichTextBox_Shapes_TaskPane_FillAndLine}" Stretch="None"/>
                                        </telerik:RadTabItem.Header>
                                        <telerik:RadTabItem.Content>
                                            <ScrollViewer BorderThickness="0">
                                                <Grid>
                                                    <Grid.RowDefinitions>
                                                        <RowDefinition Height="Auto"/>
                                                        <RowDefinition Height="Auto"/>
                                                        <RowDefinition Height="*"/>
                                                    </Grid.RowDefinitions>
                                                    <telerik:RadExpander x:Name="fillExpander">
                                                        <telerik:RadExpander.Header>
                                                            <TextBlock Text="{Binding Manager.Culture, Source={StaticResource localizationManager}, Converter={StaticResource cultureConverter}, ConverterParameter=Documents_RadRichTextBox_Shapes_TaskPane_Fill}" Style="{StaticResource TaskPaneHeaderTextBlockStyle}"/>
                                                        </telerik:RadExpander.Header>
                                                        <telerik:RadExpander.Content>
                                                            <taskPaneControls:FillSettingsControl AssociatedRichTextBox="{Binding AssociatedRichTextBox, RelativeSource={RelativeSource Mode=FindAncestor, AncestorType=telerikControlsRTBUI:TaskPane}}"/>
                                                        </telerik:RadExpander.Content>
                                                    </telerik:RadExpander>
                                                    <telerik:RadExpander x:Name="lineExpander" Grid.Row="1">
                                                        <telerik:RadExpander.Header>
                                                            <TextBlock Text="{Binding Manager.Culture, Source={StaticResource localizationManager}, Converter={StaticResource cultureConverter}, ConverterParameter=Documents_RadRichTextBox_Shapes_TaskPane_Line}" Style="{StaticResource TaskPaneHeaderTextBlockStyle}"/>
                                                        </telerik:RadExpander.Header>
                                                        <telerik:RadExpander.Content>
                                                            <taskPaneControls:LineSettingsControl AssociatedRichTextBox="{Binding AssociatedRichTextBox, RelativeSource={RelativeSource Mode=FindAncestor, AncestorType=telerikControlsRTBUI:TaskPane}}"/>
                                                        </telerik:RadExpander.Content>
                                                    </telerik:RadExpander>
                                                </Grid>
                                            </ScrollViewer>
                                        </telerik:RadTabItem.Content>
                                    </telerik:RadTabItem>
                                </telerik:RadTabControl.Items>
                            </telerik:RadTabControl>
                        </Grid>
                    </Border>
                </ControlTemplate>
            </Setter.Value>
        </Setter>
    </Style>
    <Style x:Key="FillSettingsControlStyle" TargetType="taskPaneControls:FillSettingsControl">
        <Setter Property="BorderBrush" Value="{telerik1:GreenResource ResourceKey=BasicBrush}"/>
        <Setter Property="Padding" Value="10 8"/>
        <Setter Property="Template">
            <Setter.Value>
                <ControlTemplate TargetType="taskPaneControls:FillSettingsControl">
                    <Grid>
                        <Grid.RowDefinitions>
                            <RowDefinition Height="Auto"/>
                            <RowDefinition Height="Auto"/>
                            <RowDefinition Height="Auto"/>
                        </Grid.RowDefinitions>
                        <StackPanel Orientation="Vertical" Margin="28 0 0 0">
                            <RadioButton x:Name="noFillButton" Content="{Binding Manager.Culture, Source={StaticResource localizationManager}, Converter={StaticResource cultureConverter}, ConverterParameter=Documents_RadRichTextBox_Shapes_TaskPane_NoFill}" Margin="0 0 0 6"/>
                            <RadioButton x:Name="solidFillButton"
                                    Content="{Binding Manager.Culture, Source={StaticResource localizationManager}, Converter={StaticResource cultureConverter}, ConverterParameter=Documents_RadRichTextBox_Shapes_TaskPane_SolidFill}"
                                    Margin="0 0 0 6"
                                    IsChecked="True"/>
                            <RadioButton x:Name="gradientFillButton" Content="{Binding Manager.Culture, Source={StaticResource localizationManager}, Converter={StaticResource cultureConverter}, ConverterParameter=Documents_RadRichTextBox_Shapes_TaskPane_GradientFill}" Margin="0 0 0 6"/>
                            <RadioButton x:Name="patternFillButton" Content="{Binding Manager.Culture, Source={StaticResource localizationManager}, Converter={StaticResource cultureConverter}, ConverterParameter=Documents_RadRichTextBox_Shapes_TaskPane_PatternFill}" Margin="0 0 0 6"/>
                        </StackPanel>
                        <Separator HorizontalAlignment="Stretch" Margin="0 15 0 8" Height="1" Background="{TemplateBinding BorderBrush}" Grid.Row="1"/>
                        <taskPaneControls:SolidFillSettingsControl x:Name="solidFillSettingsControl" Margin="{TemplateBinding Padding}" Grid.Row="2"/>
                        <taskPaneControls:GradientSettingsControl x:Name="gradientFillSettingsControl" Margin="{TemplateBinding Padding}" Grid.Row="2" Visibility="Collapsed"/>
                        <taskPaneControls:PatternFillSettingsControl x:Name="patternFillSettingsControl"
                                Margin="{Binding Padding, RelativeSource={RelativeSource TemplatedParent}, Converter={StaticResource ThicknessToOrientedThicknessConverter}, ConverterParameter=LeftRightBottom}"
                                Grid.Row="2"
                                Visibility="Collapsed"/>
                    </Grid>
                </ControlTemplate>
            </Setter.Value>
        </Setter>
    </Style>
    <Style x:Key="SolidFillSettingsControlStyle" TargetType="taskPaneControls:SolidFillSettingsControl">
        <Setter Property="Template">
            <Setter.Value>
                <ControlTemplate TargetType="taskPaneControls:SolidFillSettingsControl">
                    <Grid>
                        <taskPaneControls:ColorAndTransparencyPicker SelectedColor="{Binding SelectedColor, RelativeSource={RelativeSource TemplatedParent}, Mode=TwoWay}"/>
                    </Grid>
                </ControlTemplate>
            </Setter.Value>
        </Setter>
    </Style>
    <Style x:Key="ColorAndTransparencyPickerStyle" TargetType="taskPaneControls:ColorAndTransparencyPicker">
        <Setter Property="VerticalContentAlignment" Value="Center"/>
        <Setter Property="Template">
            <Setter.Value>
                <ControlTemplate TargetType="taskPaneControls:ColorAndTransparencyPicker">
                    <Grid>
                        <Grid.ColumnDefinitions>
                            <ColumnDefinition Width="Auto"/>
                            <ColumnDefinition Width="*"/>
                            <ColumnDefinition Width="Auto"/>
                        </Grid.ColumnDefinitions>
                        <Grid.RowDefinitions>
                            <RowDefinition Height="Auto"/>
                            <RowDefinition Height="Auto"/>
                        </Grid.RowDefinitions>
                        <TextBlock Text="{Binding Manager.Culture, Source={StaticResource localizationManager}, Converter={StaticResource cultureConverter}, ConverterParameter=Documents_RadRichTextBox_Shapes_TaskPane_Color}" HorizontalAlignment="Left" Style="{StaticResource FormatTextBlockStyle}"/>
                        <telerik:RadColorPicker x:Name="colorPicker"
                                SelectedColor="{Binding SelectedColor, RelativeSource={RelativeSource Mode=FindAncestor, AncestorType=taskPaneControls:ColorAndTransparencyPicker}, Mode=TwoWay}"
                                Grid.Column="2"
                                HorizontalAlignment="Right"
                                VerticalAlignment="{TemplateBinding VerticalContentAlignment}">
                            <telerik:RadColorPicker.ContentTemplate>
                                <DataTemplate>
                                    <Grid Width="16" Height="16" Margin="3 0">
                                        <Image Stretch="None" HorizontalAlignment="Center" Source="{telerik:IconResource IconRelativePath=bucket.png, IconSources={StaticResource IconSourcesShapes}}"/>
                                        <Rectangle HorizontalAlignment="Stretch" VerticalAlignment="Bottom" Height="4" Fill="{Binding Converter={StaticResource converter}}"/>
                                    </Grid>
                                </DataTemplate>
                            </telerik:RadColorPicker.ContentTemplate>
                        </telerik:RadColorPicker>
                        <TextBlock
                                Text="{Binding Manager.Culture, Source={StaticResource localizationManager}, Converter={StaticResource cultureConverter}, ConverterParameter=Documents_RadRichTextBox_Shapes_TaskPane_Transparency}"
                                Grid.Row="1"
                                HorizontalAlignment="Left"
                                Style="{StaticResource FormatTextBlockStyle}"/>
                        <telerik:RadSlider
                                Minimum="0"
                                Maximum="100"
                                Value="{Binding SelectedTransparency, RelativeSource={RelativeSource Mode=FindAncestor, AncestorType=taskPaneControls:ColorAndTransparencyPicker}, Mode=TwoWay}"
                                SmallChange="1"
                                Grid.Row="1"
                                Grid.Column="1"
                                Margin="8 0"
                                x:Name="slider"
                                VerticalAlignment="{TemplateBinding VerticalContentAlignment}"/>
                        <telerik:RadNumericUpDown x:Name="numericUpDown"
                                NullValue="0"
                                CustomUnit="%"
                                SmallChange="1"
                                Minimum="0"
                                Maximum="100"
                                Grid.Row="1"
                                Grid.Column="2"
                                Width="75"
                                HorizontalAlignment="Right"
                                VerticalAlignment="{TemplateBinding VerticalContentAlignment}"
                                Value="100"/>
                    </Grid>
                </ControlTemplate>
            </Setter.Value>
        </Setter>
    </Style>
    <Style x:Key="PresetGradientsPopupGalleryStyle" TargetType="taskPaneControls:PresetGradientsPopupGallery">
        <Setter Property="Template">
            <Setter.Value>
                <ControlTemplate TargetType="taskPaneControls:PresetGradientsPopupGallery">
                    <telerik:RadListBox x:Name="PopupListBox" ItemContainerStyle="{StaticResource ShapesPopupGalleryListBoxItemStyle}" BorderThickness="0" Padding="0">
                        <telerik:RadListBox.ItemTemplate>
                            <DataTemplate>
                                <telerikControlsRTBUI:GradientFillControl GradientFill="{Binding GradientFill}" Width="50" Height="50">
                                    <telerik:EventToCommandBehavior.EventBindings>
                                        <telerik:EventBinding EventName="MouseLeftButtonUp" RaiseOnHandledEvents="True" Command="{x:Static telerik:PopupCommands.CloseAll}"/>
                                    </telerik:EventToCommandBehavior.EventBindings>
                                </telerikControlsRTBUI:GradientFillControl>
                            </DataTemplate>
                        </telerik:RadListBox.ItemTemplate>
                        <telerik:RadListBox.ItemsPanel>
                            <ItemsPanelTemplate>
                                <telerik:RadWrapPanel Orientation="Vertical" Width="336" Height="280" Margin="1"/>
                            </ItemsPanelTemplate>
                        </telerik:RadListBox.ItemsPanel>
                    </telerik:RadListBox>
                </ControlTemplate>
            </Setter.Value>
        </Setter>
    </Style>
    <Style x:Key="GradientSettingsControlStyle" TargetType="taskPaneControls:GradientSettingsControl">
        <Setter Property="VerticalContentAlignment" Value="Center"/>
        <Setter Property="Template">
            <Setter.Value>
                <ControlTemplate TargetType="taskPaneControls:GradientSettingsControl">
                    <Grid>
                        <Grid.RowDefinitions>
                            <RowDefinition Height="Auto"/>
                            <RowDefinition Height="Auto"/>
                            <RowDefinition Height="Auto"/>
                            <RowDefinition Height="Auto"/>
                        </Grid.RowDefinitions>
                        <Grid.ColumnDefinitions>
                            <ColumnDefinition Width="Auto"/>
                            <ColumnDefinition Width="*"/>
                            <ColumnDefinition Width="Auto"/>
                        </Grid.ColumnDefinitions>
                        <TextBlock Text="{Binding Manager.Culture, Source={StaticResource localizationManager}, Converter={StaticResource cultureConverter}, ConverterParameter=Documents_RadRichTextBox_Shapes_TaskPane_PresetGradients}" Style="{StaticResource FormatTextBlockStyle}"/>
                        <telerik:RadDropDownButton Grid.Column="2" HorizontalAlignment="Right" VerticalAlignment="{TemplateBinding VerticalContentAlignment}">
                            <telerik:RadDropDownButton.Content>
                                <Image Source="{telerik:IconResource IconRelativePath=gradient.png, IconSources={StaticResource IconSourcesShapes}}" Stretch="None"/>
                            </telerik:RadDropDownButton.Content>
                            <telerik:RadDropDownButton.DropDownContent>
                                <taskPaneControls:PresetGradientsPopupGallery x:Name="presetGradientsGallery"/>
                            </telerik:RadDropDownButton.DropDownContent>
                        </telerik:RadDropDownButton>
                        <TextBlock Text="{Binding Manager.Culture, Source={StaticResource localizationManager}, Converter={StaticResource cultureConverter}, ConverterParameter=Documents_RadRichTextBox_Shapes_TaskPane_Type}" Grid.Row="1" Style="{StaticResource FormatTextBlockStyle}"/>
                        <telerik:RadComboBox x:Name="gradientType" Grid.Row="1" Grid.Column="2" VerticalAlignment="{TemplateBinding VerticalContentAlignment}">
                            <telerik:RadComboBoxItem Content="{Binding Manager.Culture, Source={StaticResource localizationManager}, Converter={StaticResource cultureConverter}, ConverterParameter=Documents_RadRichTextBox_Shapes_TaskPane_Linear}" DataContext="Linear"/>
                            <telerik:RadComboBoxItem Content="{Binding Manager.Culture, Source={StaticResource localizationManager}, Converter={StaticResource cultureConverter}, ConverterParameter=Documents_RadRichTextBox_Shapes_TaskPane_Radial}" DataContext="Radial"/>
                        </telerik:RadComboBox>
                        <TextBlock Text="{Binding Manager.Culture, Source={StaticResource localizationManager}, Converter={StaticResource cultureConverter}, ConverterParameter=Documents_RadRichTextBox_Shapes_TaskPane_Direction}" Grid.Row="2" Style="{StaticResource FormatTextBlockStyle}"/>
                        <telerik:RadDropDownButton Grid.Row="2" Grid.Column="2" HorizontalAlignment="Right" VerticalAlignment="{TemplateBinding VerticalContentAlignment}">
                            <telerik:RadDropDownButton.Content>
                                <Image Source="{telerik:IconResource IconRelativePath=gradient.png, IconSources={StaticResource IconSourcesShapes}}" Width="16" Height="16"/>
                            </telerik:RadDropDownButton.Content>
                            <telerik:RadDropDownButton.DropDownContent>
                                <taskPaneControls:GradientsDirectionPopupGallery x:Name="gradientsDicrectionsGallery"/>
                            </telerik:RadDropDownButton.DropDownContent>
                        </telerik:RadDropDownButton>
                        <TextBlock Text="{Binding Manager.Culture, Source={StaticResource localizationManager}, Converter={StaticResource cultureConverter}, ConverterParameter=Documents_RadRichTextBox_Shapes_TaskPane_Angle}" Grid.Row="3" Style="{StaticResource FormatTextBlockStyle}"/>
                        <telerik:RadNumericUpDown x:Name="angleNumericUpDown"
                                NullValue="0"
                                Grid.Row="4"
                                Grid.Column="2"
                                CustomUnit="°"
                                SmallChange="10"
                                Minimum="0"
                                Maximum="359.9"
                                Width="75"
                                HorizontalAlignment="Right"
                                VerticalAlignment="{TemplateBinding VerticalContentAlignment}"/>
                    </Grid>
                </ControlTemplate>
            </Setter.Value>
        </Setter>
    </Style>
    <Style x:Key="GradientsDirectionPopupGalleryStyle" TargetType="taskPaneControls:GradientsDirectionPopupGallery">
        <Setter Property="Template">
            <Setter.Value>
                <ControlTemplate TargetType="taskPaneControls:GradientsDirectionPopupGallery">
                    <telerik:RadListBox x:Name="PopupListBox" ItemContainerStyle="{StaticResource ShapesPopupGalleryListBoxItemStyle}" BorderThickness="0" Padding="0">
                        <telerik:RadListBox.ItemTemplate>
                            <DataTemplate>
                                <telerikControlsRTBUI:GradientFillControl GradientFill="{Binding GradientFill}" ToolTipService.ToolTip="{Binding Path=Tooltip}" Width="50" Height="50">
                                    <telerik:EventToCommandBehavior.EventBindings>
                                        <telerik:EventBinding EventName="MouseLeftButtonUp" RaiseOnHandledEvents="True" Command="{x:Static telerik:PopupCommands.CloseAll}"/>
                                    </telerik:EventToCommandBehavior.EventBindings>
                                </telerikControlsRTBUI:GradientFillControl>
                            </DataTemplate>
                        </telerik:RadListBox.ItemTemplate>
                        <telerik:RadListBox.ItemsPanel>
                            <ItemsPanelTemplate>
                                <telerik:RadWrapPanel Orientation="Horizontal" Width="282" Margin="1"/>
                            </ItemsPanelTemplate>
                        </telerik:RadListBox.ItemsPanel>
                    </telerik:RadListBox>
                </ControlTemplate>
            </Setter.Value>
        </Setter>
    </Style>
    <Style x:Key="PatternFillControlStyle" TargetType="telerikControlsRTBUI:PatternFillControl">
        <Setter Property="HorizontalAlignment" Value="Stretch"/>
        <Setter Property="VerticalAlignment" Value="Stretch"/>
        <Setter Property="HorizontalContentAlignment" Value="Stretch"/>
        <Setter Property="VerticalContentAlignment" Value="Stretch"/>
        <Setter Property="Template">
            <Setter.Value>
                <ControlTemplate TargetType="telerikControlsRTBUI:PatternFillControl">
                    <Canvas x:Name="Container" HorizontalAlignment="{TemplateBinding HorizontalContentAlignment}" VerticalAlignment="{TemplateBinding VerticalContentAlignment}"/>
                </ControlTemplate>
            </Setter.Value>
        </Setter>
    </Style>
    <Style x:Key="PatternFillListBoxItemStyle" TargetType="telerik:RadListBoxItem">
        <Setter Property="helpers:ThemeHelper.MouseOverBrush" Value="{telerik1:GreenResource ResourceKey=BasicBrush}"/>
        <Setter Property="helpers:ThemeHelper.CheckedBrush" Value="{telerik1:GreenResource ResourceKey=MouseOverBrush}"/>
        <Setter Property="BorderThickness" Value="3"/>
        <Setter Property="BorderBrush" Value="Transparent"/>
        <Setter Property="ToolTipService.ToolTip" Value="{Binding Converter={StaticResource PatternTypeToTooltipStringConverter}}"/>
        <Setter Property="Template">
            <Setter.Value>
                <ControlTemplate TargetType="telerik:RadListBoxItem">
                    <Grid Width="{TemplateBinding Width}" Height="{TemplateBinding Height}">
                        <Border x:Name="BorderVisual" Background="{TemplateBinding Background}" BorderBrush="{TemplateBinding BorderBrush}" BorderThickness="{TemplateBinding BorderThickness}">
                            <telerikControlsRTBUI:PatternFillControl BackgroundColor="{Binding BackgroundColor, RelativeSource={RelativeSource AncestorType=taskPaneControls:PatternFillSettingsControl}, Mode=TwoWay}" PatternColor="{Binding PatternColor, RelativeSource={RelativeSource AncestorType=taskPaneControls:PatternFillSettingsControl}, Mode=TwoWay}" PatternType="{Binding}"/>
                        </Border>
                    </Grid>
                    <ControlTemplate.Triggers>
                        <Trigger Property="IsMouseOver" Value="True">
                            <Setter TargetName="BorderVisual" Property="BorderBrush" Value="{Binding RelativeSource={RelativeSource TemplatedParent}, Path=(helpers:ThemeHelper.MouseOverBrush)}"/>
                        </Trigger>
                        <Trigger Property="IsSelected" Value="True">
                            <Setter TargetName="BorderVisual" Property="BorderBrush" Value="{Binding RelativeSource={RelativeSource TemplatedParent}, Path=(helpers:ThemeHelper.CheckedBrush)}"/>
                        </Trigger>
                    </ControlTemplate.Triggers>
                </ControlTemplate>
            </Setter.Value>
        </Setter>
    </Style>
    <Style x:Key="PatternFillSettingsControlStyle" TargetType="taskPaneControls:PatternFillSettingsControl">
        <Setter Property="VerticalContentAlignment" Value="Center"/>
        <Setter Property="Template">
            <Setter.Value>
                <ControlTemplate TargetType="taskPaneControls:PatternFillSettingsControl">
                    <Grid>
                        <Grid.RowDefinitions>
                            <RowDefinition Height="Auto"/>
                            <RowDefinition Height="Auto"/>
                            <RowDefinition Height="Auto"/>
                            <RowDefinition Height="Auto"/>
                        </Grid.RowDefinitions>
                        <Grid.ColumnDefinitions>
                            <ColumnDefinition Width="Auto"/>
                            <ColumnDefinition Width="*"/>
                            <ColumnDefinition Width="Auto"/>
                        </Grid.ColumnDefinitions>
                        <TextBlock Text="{Binding Manager.Culture, Source={StaticResource localizationManager}, Converter={StaticResource cultureConverter}, ConverterParameter=Documents_RadRichTextBox_Shapes_TaskPane_Pattern}" Style="{StaticResource FormatTextBlockStyle}"/>
                        <telerik:RadListBox x:Name="patternFillsListBox"
                                Grid.Row="1"
                                Grid.ColumnSpan="3"
                                Height="200"
                                Width="208"
                                HorizontalAlignment="Left"
                                ItemContainerStyle="{StaticResource PatternFillListBoxItemStyle}"
                                Margin="0 10"
                                SelectedValue="{Binding PatternType, RelativeSource={RelativeSource AncestorType=taskPaneControls:PatternFillSettingsControl}, Mode=TwoWay}">
                            <telerik:RadListBox.ItemsPanel>
                                <ItemsPanelTemplate>
                                    <telerik:VirtualizingWrapPanel ItemWidth="34" ItemHeight="24"/>
                                </ItemsPanelTemplate>
                            </telerik:RadListBox.ItemsPanel>
                        </telerik:RadListBox>
                        <TextBlock Text="{Binding Manager.Culture, Source={StaticResource localizationManager}, Converter={StaticResource cultureConverter}, ConverterParameter=Documents_RadRichTextBox_Shapes_TaskPane_Foreground}" Grid.Row="2" Style="{StaticResource FormatTextBlockStyle}"/>
                        <telerik:RadColorPicker x:Name="foregroundColorPicker"
                                Grid.Row="2"
                                Grid.Column="2"
                                SelectedColor="{Binding PatternColor, RelativeSource={RelativeSource AncestorType=taskPaneControls:PatternFillSettingsControl}, Mode=TwoWay}"
                                VerticalAlignment="{TemplateBinding VerticalContentAlignment}">
                            <telerik:RadColorPicker.ContentTemplate>
                                <DataTemplate>
                                    <Grid Width="16" Height="16" Margin="3 0">
                                        <Image Stretch="None" HorizontalAlignment="Center" Source="{telerik:IconResource IconRelativePath=bucket.png, IconSources={StaticResource IconSourcesShapes}}"/>
                                        <Rectangle HorizontalAlignment="Stretch" VerticalAlignment="Bottom" Height="4" Fill="{Binding Converter={StaticResource converter}}"/>
                                    </Grid>
                                </DataTemplate>
                            </telerik:RadColorPicker.ContentTemplate>
                        </telerik:RadColorPicker>
                        <TextBlock Text="{Binding Manager.Culture, Source={StaticResource localizationManager}, Converter={StaticResource cultureConverter}, ConverterParameter=Documents_RadRichTextBox_Shapes_TaskPane_Background}" Grid.Row="3" Style="{StaticResource FormatTextBlockStyle}"/>
                        <telerik:RadColorPicker x:Name="backgroundColorPicker"
                                SelectedColor="{Binding BackgroundColor, RelativeSource={RelativeSource AncestorType=taskPaneControls:PatternFillSettingsControl}, Mode=TwoWay}"
                                Grid.Row="3"
                                Grid.Column="2"
                                VerticalAlignment="{TemplateBinding VerticalContentAlignment}">
                            <telerik:RadColorPicker.ContentTemplate>
                                <DataTemplate>
                                    <Grid Width="16" Height="16" Margin="3 0">
                                        <Image Stretch="None" HorizontalAlignment="Center" Source="{telerik:IconResource IconRelativePath=bucket.png, IconSources={StaticResource IconSourcesShapes}}"/>
                                        <Rectangle HorizontalAlignment="Stretch" VerticalAlignment="Bottom" Height="4" Fill="{Binding Converter={StaticResource converter}}"/>
                                    </Grid>
                                </DataTemplate>
                            </telerik:RadColorPicker.ContentTemplate>
                        </telerik:RadColorPicker>
                    </Grid>
                </ControlTemplate>
            </Setter.Value>
        </Setter>
    </Style>
    <Style x:Key="LineSettingsControlStyle" TargetType="taskPaneControls:LineSettingsControl">
        <Setter Property="BorderBrush" Value="{telerik1:GreenResource ResourceKey=BasicBrush}"/>
        <Setter Property="Padding" Value="10 8"/>
        <Setter Property="Template">
            <Setter.Value>
                <ControlTemplate TargetType="taskPaneControls:LineSettingsControl">
                    <Grid>
                        <Grid.RowDefinitions>
                            <RowDefinition Height="Auto"/>
                            <RowDefinition Height="Auto"/>
                            <RowDefinition Height="Auto"/>
                        </Grid.RowDefinitions>
                        <StackPanel Orientation="Vertical" Margin="28 0 0 0">
                            <RadioButton x:Name="noLineButton" Content="{Binding Manager.Culture, Source={StaticResource localizationManager}, Converter={StaticResource cultureConverter}, ConverterParameter=Documents_RadRichTextBox_Shapes_TaskPane_NoLine}" Margin="0 0 0 6"/>
                            <RadioButton x:Name="solidLineButton"
                                    Content="{Binding Manager.Culture, Source={StaticResource localizationManager}, Converter={StaticResource cultureConverter}, ConverterParameter=Documents_RadRichTextBox_Shapes_TaskPane_SolidLine}"
                                    Margin="0 0 0 6"
                                    IsChecked="True"/>
                            <RadioButton x:Name="gradientLineButton" Content="{Binding Manager.Culture, Source={StaticResource localizationManager}, Converter={StaticResource cultureConverter}, ConverterParameter=Documents_RadRichTextBox_Shapes_TaskPane_GradientLine}" Margin="0 0 0 6"/>
                        </StackPanel>
                        <Separator HorizontalAlignment="Stretch" Margin="0 15 0 8" Height="1" Background="{TemplateBinding BorderBrush}" Grid.Row="1"/>
                        <taskPaneControls:SolidLineSettingsControl x:Name="solidLineSettingsControl" Margin="{TemplateBinding Padding}" Grid.Row="2"/>
                        <taskPaneControls:GradientLineSettingsControl x:Name="gradientLineSettingsControl" Margin="{TemplateBinding Padding}" Grid.Row="2" Visibility="Collapsed"/>
                    </Grid>
                </ControlTemplate>
            </Setter.Value>
        </Setter>
    </Style>
    <Style x:Key="SolidLineSettingsControlStyle" TargetType="taskPaneControls:SolidLineSettingsControl">
        <Setter Property="VerticalContentAlignment" Value="Center"/>
        <Setter Property="Template">
            <Setter.Value>
                <ControlTemplate TargetType="taskPaneControls:SolidLineSettingsControl">
                    <Grid>
                        <Grid.RowDefinitions>
                            <RowDefinition Height="Auto"/>
                            <RowDefinition Height="Auto"/>
                            <RowDefinition Height="Auto"/>
                        </Grid.RowDefinitions>
                        <Grid.ColumnDefinitions>
                            <ColumnDefinition Width="Auto"/>
                            <ColumnDefinition Width="*"/>
                            <ColumnDefinition Width="Auto"/>
                        </Grid.ColumnDefinitions>
                        <taskPaneControls:ColorAndTransparencyPicker Grid.ColumnSpan="3" SelectedColor="{Binding SelectedColor, RelativeSource={RelativeSource TemplatedParent}, Mode=TwoWay}"/>
                        <TextBlock Text="{Binding Manager.Culture, Source={StaticResource localizationManager}, Converter={StaticResource cultureConverter}, ConverterParameter=Documents_RadRichTextBox_Shapes_TaskPane_Width}" Grid.Row="1" Style="{StaticResource FormatTextBlockStyle}"/>
                        <telerik:RadNumericUpDown x:Name="numericUpDown"
                                NullValue="0"
                                CustomUnit="pt"
                                SmallChange="0.25"
                                Grid.Row="1"
                                Grid.Column="2"
                                Minimum="0"
                                Maximum="1584"
                                Value="{Binding SelectedWidth, RelativeSource={RelativeSource TemplatedParent}, Mode=TwoWay}"
                                Width="75"
                                HorizontalAlignment="Right"
                                VerticalAlignment="{TemplateBinding VerticalContentAlignment}"/>
                        <TextBlock Text="{Binding Manager.Culture, Source={StaticResource localizationManager}, Converter={StaticResource cultureConverter}, ConverterParameter=Documents_RadRichTextBox_Shapes_TaskPane_DashType}" Grid.Row="2" Style="{StaticResource FormatTextBlockStyle}"/>
                        <telerik:RadDropDownButton Grid.Row="2" Grid.Column="2" HorizontalAlignment="Right" VerticalAlignment="{TemplateBinding VerticalContentAlignment}">
                            <telerik:RadDropDownButton.Content>
                                <Image Source="{telerik:IconResource IconRelativePath=dashes.png, IconSources={StaticResource IconSourcesShapes}}" Stretch="None"/>
                            </telerik:RadDropDownButton.Content>
                            <telerik:RadDropDownButton.DropDownContent>
                                <taskPaneControls:DashTypeGallery x:Name="dashTypeGallery" SelectedOutlineDashType="{Binding SelectedDashType, RelativeSource={RelativeSource TemplatedParent}, Mode=TwoWay}"/>
                            </telerik:RadDropDownButton.DropDownContent>
                        </telerik:RadDropDownButton>
                    </Grid>
                </ControlTemplate>
            </Setter.Value>
        </Setter>
    </Style>
    <Style x:Key="GradientLineSettingsControlStyle" TargetType="taskPaneControls:GradientLineSettingsControl">
        <Setter Property="VerticalContentAlignment" Value="Center"/>
        <Setter Property="Template">
            <Setter.Value>
                <ControlTemplate TargetType="taskPaneControls:GradientLineSettingsControl">
                    <Grid>
                        <Grid.RowDefinitions>
                            <RowDefinition Height="Auto"/>
                            <RowDefinition Height="Auto"/>
                        </Grid.RowDefinitions>
                        <taskPaneControls:GradientSettingsControl x:Name="gradientFillSettingsControl" Margin="{TemplateBinding Padding}" SelectedBrush="{Binding SelectedBrush, RelativeSource={RelativeSource TemplatedParent}, Mode=TwoWay}"/>
                        <taskPaneControls:LinePropertiesSettingsControl Grid.Row="1" SelectedWidth="{Binding SelectedWidth, RelativeSource={RelativeSource TemplatedParent}, Mode=TwoWay}" SelectedDashType="{Binding SelectedDashType, RelativeSource={RelativeSource TemplatedParent}, Mode=TwoWay}"/>
                    </Grid>
                </ControlTemplate>
            </Setter.Value>
        </Setter>
    </Style>
    <Style x:Key="DashTypeGalleryStyle" TargetType="taskPaneControls:DashTypeGallery">
        <Setter Property="Template">
            <Setter.Value>
                <ControlTemplate TargetType="taskPaneControls:DashTypeGallery">
                    <telerik:RadListBox x:Name="PART_OutlineDashTypeListBox"
                            ItemContainerStyle="{StaticResource ShapesPopupGalleryListBoxItemStyle}"
                            BorderThickness="0"
                            ScrollViewer.VerticalScrollBarVisibility="Disabled"
                            ScrollViewer.HorizontalScrollBarVisibility="Disabled"
                            ItemsSource="{Binding OutlineDashTypeItems, RelativeSource={RelativeSource Mode=FindAncestor, AncestorType=taskPaneControls:DashTypeGallery}}"
                            SelectedValuePath="LineDashType"
                            SelectedValue="{Binding SelectedOutlineDashType, RelativeSource={RelativeSource Mode=FindAncestor, AncestorType=taskPaneControls:DashTypeGallery}, Mode=TwoWay}">
                        <telerik:EventToCommandBehavior.EventBindings>
                            <telerik:EventBinding EventName="MouseLeftButtonUp" RaiseOnHandledEvents="True" Command="{x:Static telerik:PopupCommands.CloseAll}"/>
                        </telerik:EventToCommandBehavior.EventBindings>
                        <telerik:RadListBox.ItemsPanel>
                            <ItemsPanelTemplate>
                                <VirtualizingStackPanel/>
                            </ItemsPanelTemplate>
                        </telerik:RadListBox.ItemsPanel>
                        <telerik:RadListBox.ItemTemplate>
                            <DataTemplate>
                                <Line
                                        Style="{StaticResource ShapesOutlinePickerLineStyle}"
                                        Stroke="{Binding Foreground, RelativeSource={RelativeSource AncestorType={x:Type telerik:RadListBoxItem}}}"
                                        StrokeDashArray="{Binding LineDashType, Converter={StaticResource LineDashTypeToDoubleCollectionConverter}}"
                                        StrokeThickness="2"
                                        ToolTipService.ToolTip="{Binding Path=TooltipText, Mode=OneTime}"/>
                            </DataTemplate>
                        </telerik:RadListBox.ItemTemplate>
                    </telerik:RadListBox>
                </ControlTemplate>
            </Setter.Value>
        </Setter>
    </Style>
    <Style x:Key="LinePropertiesSettingsControlStyle" TargetType="taskPaneControls:LinePropertiesSettingsControl">
        <Setter Property="VerticalContentAlignment" Value="Center"/>
        <Setter Property="Template">
            <Setter.Value>
                <ControlTemplate TargetType="taskPaneControls:LinePropertiesSettingsControl">
                    <Grid>
                        <Grid.RowDefinitions>
                            <RowDefinition Height="Auto"/>
                            <RowDefinition Height="Auto"/>
                        </Grid.RowDefinitions>
                        <Grid.ColumnDefinitions>
                            <ColumnDefinition Width="Auto"/>
                            <ColumnDefinition Width="*"/>
                            <ColumnDefinition Width="Auto"/>
                        </Grid.ColumnDefinitions>
                        <TextBlock Text="{Binding Manager.Culture, Source={StaticResource localizationManager}, Converter={StaticResource cultureConverter}, ConverterParameter=Documents_RadRichTextBox_Shapes_TaskPane_Width}" Style="{StaticResource FormatTextBlockStyle}"/>
                        <telerik:RadNumericUpDown x:Name="numericUpDown"
                                NullValue="0"
                                CustomUnit="pt"
                                SmallChange="0.25"
                                Grid.Column="2"
                                Minimum="0"
                                Maximum="1584"
                                Value="{Binding SelectedWidth, RelativeSource={RelativeSource TemplatedParent}, Mode=TwoWay}"
                                Width="75"
                                HorizontalAlignment="Right"
                                VerticalAlignment="{TemplateBinding VerticalContentAlignment}"/>
                        <TextBlock Text="{Binding Manager.Culture, Source={StaticResource localizationManager}, Converter={StaticResource cultureConverter}, ConverterParameter=Documents_RadRichTextBox_Shapes_TaskPane_DashType}" Grid.Row="1" Style="{StaticResource FormatTextBlockStyle}"/>
                        <telerik:RadDropDownButton Grid.Row="1" Grid.Column="2" HorizontalAlignment="Right" VerticalAlignment="{TemplateBinding VerticalContentAlignment}">
                            <telerik:RadDropDownButton.Content>
                                <Image Source="{telerik:IconResource IconRelativePath=dashes.png, IconSources={StaticResource IconSourcesShapes}}" Stretch="None"/>
                            </telerik:RadDropDownButton.Content>
                            <telerik:RadDropDownButton.DropDownContent>
                                <taskPaneControls:DashTypeGallery x:Name="dashTypeGallery" SelectedOutlineDashType="{Binding SelectedDashType, RelativeSource={RelativeSource TemplatedParent}, Mode=TwoWay}"/>
                            </telerik:RadDropDownButton.DropDownContent>
                        </telerik:RadDropDownButton>
                    </Grid>
                </ControlTemplate>
            </Setter.Value>
        </Setter>
    </Style>
    <Style TargetType="telerikControlsRTBUI:RulerThumb" BasedOn="{StaticResource RulerThumbStyle}"/>
