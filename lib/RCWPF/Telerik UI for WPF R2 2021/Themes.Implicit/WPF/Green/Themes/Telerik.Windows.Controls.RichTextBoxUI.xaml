                    <VisualState x:Name="ButtonNormal"/>
                    <VisualState x:Name="ButtonMouseLeave"/>
                    <VisualState x:Name="ButtonMouseOver">
                        <Storyboard>
                            <ObjectAnimationUsingKeyFrames Storyboard.TargetName="OuterBorder" Storyboard.TargetProperty="Background" Duration="0">
                                <DiscreteObjectKeyFrame KeyTime="0" Value="{telerik1:GreenResource ResourceKey=MouseOverBrush}"/>
                            </ObjectAnimationUsingKeyFrames>
