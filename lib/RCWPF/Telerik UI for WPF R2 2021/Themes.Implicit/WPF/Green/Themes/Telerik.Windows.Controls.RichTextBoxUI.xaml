    <Style TargetType="Thumb" x:Key="SectionColumnThumbStyle">
        <Setter Property="Cursor" Value="SizeWE"/>
        <Setter Property="Background" Value="{telerik:GreenResource ResourceKey=ComplementaryBrush}"/>
        <Setter Property="Foreground" Value="{telerik:GreenResource ResourceKey=StrongBrush}"/>
        <Setter Property="Width" Value="10"/>
        <Setter Property="Height" Value="14"/>
        <Setter Property="Template">
            <Setter.Value>
                <ControlTemplate TargetType="Thumb">
                    <Grid Background="{TemplateBinding Background}" Width="{TemplateBinding Width}" Margin="-5 1 0 1">
                        <Border BorderBrush="{telerik:GreenResource ResourceKey=BasicBrush}" BorderThickness="1">
                            <Path
                                    Data="M0,0 L3,0 L3,1 L1,1 L1,8 L0,8 z"
                                    HorizontalAlignment="Left"
                                    Height="8"
                                    Width="3"
                                    Margin="2"
                                    Stretch="Fill"
                                    UseLayoutRounding="False"
                                    VerticalAlignment="Top"
                                    Fill="{TemplateBinding Foreground}"/>
                        </Border>
                    </Grid>
                </ControlTemplate>
            </Setter.Value>
        </Setter>
    </Style>
    <Style x:Key="DocumentRulerBaseStyle" TargetType="telerikControlsRTBUI:DocumentRulerBase">
        <Setter Property="Focusable" Value="False"/>
        <Setter Property="IsTabStop" Value="False"/>
        <Setter Property="MarkersBrush" Value="{telerik:GreenResource ResourceKey=StrongBrush}"/>
        <Setter Property="HorizontalThumbStyle" Value="{StaticResource HorizontalThumbStyle}"/>
        <Setter Property="VerticalThumbStyle" Value="{StaticResource VerticalThumbStyle}"/>
        <Setter Property="ThumbBackgroundStyle" Value="{StaticResource ThumbBackgroundStyle}"/>
        <Setter Property="TableColumnThumbStyle" Value="{StaticResource TableColumnThumbStyle}"/>
        <Setter Property="SectionColumnThumbStyle" Value="{StaticResource SectionColumnThumbStyle}"/>
        <Setter Property="Template">
            <Setter.Value>
                <ControlTemplate TargetType="telerikControlsRTBUI:DocumentRulerBase">
                    <Grid x:Name="PART_LayoutRoot">
                        <Grid.ColumnDefinitions>
                            <ColumnDefinition Width="24"/>
                            <ColumnDefinition/>
                        </Grid.ColumnDefinitions>
                        <Grid.RowDefinitions>
                            <RowDefinition Height="24"/>
                            <RowDefinition/>
                        </Grid.RowDefinitions>
                        <ContentPresenter Grid.Column="1" Grid.Row="1"/>
                        <Canvas x:Name="PART_HorizontalCanvas"
                                Grid.Column="1"
                                Background="{telerik:GreenResource ResourceKey=AlternativeBrush}"
                                HorizontalAlignment="Stretch"
                                VerticalAlignment="Stretch">
                            <Grid x:Name="PART_HorizontalRulerContainer" Height="19" Margin="0 5">
                                <Grid.RowDefinitions>
                                    <RowDefinition/>
                                    <RowDefinition Height="5"/>
                                </Grid.RowDefinitions>
                                <Canvas>
                                    <Rectangle x:Name="PART_LeftMarginRectangle" Style="{StaticResource PageMarginStyle}" Height="14"/>
                                    <Rectangle x:Name="PART_PageHorizontalRectangle"
                                            Fill="{telerik:GreenResource ResourceKey=MainBrush}"
                                            Canvas.Left="0"
                                            Canvas.Top="0"
                                            Height="14"
                                            Stroke="{telerik:GreenResource ResourceKey=BasicBrush}"
                                            StrokeThickness="1"/>
                                    <Rectangle x:Name="PART_RightMarginRectangle" Style="{StaticResource PageMarginStyle}" Height="14"/>
                                </Canvas>
                                <Canvas x:Name="PART_HorizontalMarkersContainer"/>
                                <Canvas x:Name="PART_HorizontalThumbBackgroundsContainer"/>
                                <Canvas x:Name="PART_HorizontalThumbsContainer" Canvas.ZIndex="10">
                                    <telerikControlsRTBUI:RulerThumb x:Name="PART_ParagraphFirstLineIndent"
                                            RulerThumbType="FirstLineIndent"
                                            Canvas.ZIndex="10"
                                            ToolTipText="{Binding Manager.Culture, Source={StaticResource localizationManager}, Converter={StaticResource cultureConverter}, ConverterParameter=Documents_DocumentRuler_FirstLineIndent}"
                                            Style="{StaticResource FirstLineIndentThumbStyle}"/>
                                    <telerikControlsRTBUI:RulerThumb x:Name="PART_ParagraphHangingIndent"
                                            RulerThumbType="Indent"
                                            Canvas.ZIndex="10"
                                            ToolTipText="{Binding Manager.Culture, Source={StaticResource localizationManager}, Converter={StaticResource cultureConverter}, ConverterParameter=Documents_DocumentRuler_HangingIndent}"
                                            Style="{StaticResource HangingIndentThumbStyle}"/>
                                    <telerikControlsRTBUI:RulerThumb x:Name="PART_ParagraphLeftIndent"
                                            RulerThumbType="LeftIndent"
                                            Canvas.ZIndex="10"
                                            ToolTipText="{Binding Manager.Culture, Source={StaticResource localizationManager}, Converter={StaticResource cultureConverter}, ConverterParameter=Documents_DocumentRuler_LeftIndent}"
                                            Style="{StaticResource LeftIndentThumbStyle}"/>
                                    <telerikControlsRTBUI:RulerThumb x:Name="PART_ParagraphRightIndent"
                                            Canvas.ZIndex="10"
                                            ToolTipText="{Binding Manager.Culture, Source={StaticResource localizationManager}, Converter={StaticResource cultureConverter}, ConverterParameter=Documents_DocumentRuler_RightIndent}"
                                            Style="{StaticResource RightIndentThumbStyle}"
                                            RulerThumbType="Indent"/>
                                    <telerikControlsRTBUI:RulerThumb x:Name="PART_TabStopMarker" Canvas.ZIndex="0" Foreground="{telerik:GreenResource ResourceKey=BasicBrush}"/>
                                    <telerikControlsRTBUI:RulerThumb x:Name="PART_FirstLineIndentMarker" RulerThumbType="FirstLineIndent" Canvas.ZIndex="0" Style="{StaticResource FirstLineIndentThumbStyle}"/>
                                    <telerikControlsRTBUI:RulerThumb x:Name="PART_IndentMarker" RulerThumbType="Indent" Canvas.ZIndex="0" Style="{StaticResource HangingIndentThumbStyle}"/>
                                    <telerikControlsRTBUI:RulerThumb x:Name="PART_LeftIndentMarker" RulerThumbType="LeftIndent" Canvas.ZIndex="0" Style="{StaticResource LeftIndentThumbStyle}"/>
                                </Canvas>
                                <Canvas x:Name="PART_TabStopMarkersContainer" Grid.Row="1" Height="5"/>
                            </Grid>
                        </Canvas>
                        <Canvas x:Name="PART_VerticalCanvas"
                                Grid.Row="1"
                                Background="{telerik:GreenResource ResourceKey=AlternativeBrush}"
                                HorizontalAlignment="Stretch"
                                VerticalAlignment="Stretch">
                            <Grid Grid.Row="1" x:Name="PART_VerticalRulerContainer" Margin="5 0" Width="14">
                                <Canvas>
                                    <Rectangle x:Name="PART_TopMarginRectangle" Style="{StaticResource PageMarginStyle}" Width="14"/>
                                    <Rectangle x:Name="PART_PageVerticalRectangle"
                                            Fill="{telerik:GreenResource ResourceKey=MainBrush}"
                                            Canvas.Left="0"
                                            Canvas.Top="0"
                                            Width="14"
                                            Stroke="{telerik:GreenResource ResourceKey=BasicBrush}"
                                            StrokeThickness="1"/>
                                    <Rectangle x:Name="PART_BottomMarginRectangle" Style="{StaticResource PageMarginStyle}" Width="14"/>
                                </Canvas>
                                <Canvas x:Name="PART_VerticalMarkersContainer"/>
                                <Canvas x:Name="PART_VerticalThumbBackgroundsContainer"/>
                                <Canvas x:Name="PART_VerticalThumbsContainer"/>
                            </Grid>
                        </Canvas>
                        <Border
                                Background="{telerik:GreenResource ResourceKey=AlternativeBrush}"
                                BorderBrush="{telerik:GreenResource ResourceKey=BasicBrush}"
                                BorderThickness="1"
                                Margin="0 -1 0 0">
                            <Border x:Name="PART_TabStops"
                                    Background="{telerik:GreenResource ResourceKey=MainBrush}"
                                    BorderBrush="{telerik:GreenResource ResourceKey=BasicBrush}"
                                    BorderThickness="1"
                                    Width="14"
                                    Height="14">
                                <telerikControlsRTBUI:RulerThumb x:Name="PART_RulerThumbTypeSelector" HorizontalAlignment="Center" VerticalAlignment="Center" Margin="0" IsHitTestVisible="False"/>
                            </Border>
                        </Border>
                        <Line x:Name="PART_MarkLine"
                                Grid.Row="1"
                                Grid.Column="1"
                                Stroke="{telerik:GreenResource ResourceKey=StrongBrush}"
                                StrokeThickness="1"
                                StrokeDashArray="1 1 1 1 1 3"
                                Margin="0 2 0 0"/>
                    </Grid>
                </ControlTemplate>
            </Setter.Value>
        </Setter>
    </Style>
    <Style x:Key="LayoutModeButtonStyle" TargetType="telerik:RadToggleButton">
        <Setter Property="FontFamily" Value="{telerik:GreenResource ResourceKey=FontFamily}"/>
        <Setter Property="FontSize" Value="{telerik:GreenResource ResourceKey=FontSize}"/>
        <Setter Property="Foreground" Value="{telerik1:GreenResource ResourceKey=SelectedBrush}"/>
        <Setter Property="Background" Value="{telerik1:GreenResource ResourceKey=AccentHighBrush}"/>
        <Setter Property="BorderBrush" Value="{x:Null}"/>
        <Setter Property="BorderThickness" Value="1"/>
        <Setter Property="Padding" Value="3"/>
        <Setter Property="CornerRadius" Value="{telerik1:GreenResource ResourceKey=CornerRadius}"/>
        <Setter Property="HorizontalContentAlignment" Value="Center"/>
        <Setter Property="VerticalContentAlignment" Value="Center"/>
        <Setter Property="Template">
            <Setter.Value>
                <ControlTemplate TargetType="telerik:RadToggleButton">
                    <Border
                            SnapsToDevicePixels="True"
                            x:Name="BorderVisual"
                            BorderThickness="{TemplateBinding BorderThickness}"
                            BorderBrush="{TemplateBinding BorderBrush}"
                            Background="{TemplateBinding Background}"
                            CornerRadius="{TemplateBinding CornerRadius}">
                        <ContentPresenter x:Name="Content"
                                Margin="{TemplateBinding Padding}"
                                Content="{TemplateBinding Content}"
                                ContentTemplate="{TemplateBinding ContentTemplate}"
                                VerticalAlignment="{TemplateBinding VerticalContentAlignment}"
                                HorizontalAlignment="{TemplateBinding HorizontalContentAlignment}"
                                ContentTemplateSelector="{TemplateBinding ContentTemplateSelector}"
                                ContentStringFormat="{TemplateBinding ContentStringFormat}"
                                RecognizesAccessKey="True"/>
                    </Border>
                    <ControlTemplate.Triggers>
                        <Trigger Property="IsEnabled" Value="False">
                            <Setter TargetName="Content" Property="Opacity" Value="{telerik1:GreenResource ResourceKey=DisabledOpacity}"/>
                            <Setter TargetName="BorderVisual" Property="BorderBrush" Value="{telerik1:GreenResource ResourceKey=BasicBrush}"/>
                            <Setter TargetName="BorderVisual" Property="Background" Value="{telerik1:GreenResource ResourceKey=AlternativeBrush}"/>
                        </Trigger>
                        <MultiTrigger>
                            <MultiTrigger.Conditions>
                                <Condition Property="IsEnabled" Value="False"/>
                                <Condition Property="IsChecked" Value="True"/>
                            </MultiTrigger.Conditions>
                            <Setter TargetName="Content" Property="Opacity" Value="{telerik1:GreenResource ResourceKey=DisabledOpacity}"/>
                            <Setter TargetName="BorderVisual" Property="BorderBrush" Value="{telerik1:GreenResource ResourceKey=AccentHighBrush}"/>
                            <Setter TargetName="BorderVisual" Property="BorderBrush" Value="{telerik1:GreenResource ResourceKey=AccentHighBrush}"/>
                        </MultiTrigger>
                        <MultiTrigger>
                            <MultiTrigger.Conditions>
                                <Condition Property="IsBackgroundVisible" Value="False"/>
                                <Condition Property="IsMouseOver" Value="False"/>
                                <Condition Property="IsChecked" Value="False"/>
                            </MultiTrigger.Conditions>
                            <Setter TargetName="BorderVisual" Property="Background" Value="Transparent"/>
                            <Setter TargetName="BorderVisual" Property="BorderBrush" Value="Transparent"/>
                        </MultiTrigger>
                        <Trigger Property="IsChecked" Value="True">
                            <Setter TargetName="BorderVisual" Property="Background" Value="{telerik1:GreenResource ResourceKey=AccentHighBrush}"/>
                            <Setter TargetName="BorderVisual" Property="BorderBrush" Value="{telerik1:GreenResource ResourceKey=AccentHighBrush}"/>
                        </Trigger>
                        <Trigger Property="IsFocused" Value="True">
                            <Setter TargetName="BorderVisual" Property="BorderBrush" Value="{telerik1:GreenResource ResourceKey=AccentLowBrush}"/>
                        </Trigger>
                        <Trigger Property="IsMouseOver" Value="True">
                            <Setter TargetName="BorderVisual" Property="Background" Value="{telerik1:GreenResource ResourceKey=MouseOverBrush}"/>
                            <Setter TargetName="BorderVisual" Property="BorderBrush" Value="{telerik1:GreenResource ResourceKey=MouseOverBrush}"/>
                        </Trigger>
                        <Trigger Property="IsPressed" Value="True">
                            <Setter TargetName="BorderVisual" Property="Background" Value="{telerik1:GreenResource ResourceKey=AccentHighBrush}"/>
                            <Setter TargetName="BorderVisual" Property="BorderBrush" Value="{telerik1:GreenResource ResourceKey=AccentHighBrush}"/>
                        </Trigger>
                        <MultiTrigger>
                            <MultiTrigger.Conditions>
                                <Condition Property="IsChecked" Value="True"/>
                                <Condition Property="IsMouseOver" Value="True"/>
                                <Condition Property="IsPressed" Value="False"/>
                            </MultiTrigger.Conditions>
                            <Setter TargetName="BorderVisual" Property="BorderBrush" Value="{telerik1:GreenResource ResourceKey=MouseOverBrush}"/>
                            <Setter TargetName="BorderVisual" Property="Background" Value="{telerik1:GreenResource ResourceKey=MouseOverBrush}"/>
                        </MultiTrigger>
                    </ControlTemplate.Triggers>
                </ControlTemplate>
            </Setter.Value>
        </Setter>
    </Style>
