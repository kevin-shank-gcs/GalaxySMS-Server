    <Style x:Key="ImageMiniToolBarStyle" TargetType="telerikControlsRTBUI:ImageMiniToolBar" BasedOn="{StaticResource MiniToolbarBaseStyle}">
        <Setter Property="Width" Value="40"/>
        <Setter Property="Template">
            <Setter.Value>
                <ControlTemplate TargetType="telerikControlsRTBUI:ImageMiniToolBar">
                    <Border
                            Background="{TemplateBinding Background}"
                            BorderBrush="{TemplateBinding BorderBrush}"
                            BorderThickness="{TemplateBinding BorderThickness}"
                            CornerRadius="{StaticResource ImageMiniToolBar_CornerRadius}">
                        <Grid>
                            <Grid.RowDefinitions>
                                <RowDefinition Height="Auto" MinHeight="32"/>
                                <RowDefinition Height="*"/>
                                <RowDefinition Height="Auto"/>
                            </Grid.RowDefinitions>
                            <Border Background="{telerik1:GreenResource ResourceKey=AccentHighBrush}" Margin="-1"/>
                            <StackPanel Margin="2" Orientation="Vertical">
                                <telerik:RadButton
                                        HorizontalAlignment="Right"
                                        Command="{Binding RelativeSource={RelativeSource TemplatedParent}, Path=CloseCommand}"
                                        Width="10"
                                        Height="10"
                                        Padding="0"
                                        IsBackgroundVisible="False">
                                    <Path
                                            Data="{StaticResource MiniToolBarIcon}"
                                            Fill="{telerik1:GreenResource ResourceKey=FixedBrush}"
                                            HorizontalAlignment="Center"
                                            VerticalAlignment="Center"
                                            Stretch="None"
                                            SnapsToDevicePixels="True"/>
                                </telerik:RadButton>
                                <TextBlock
                                        telerik:LocalizationManager.ResourceKey="Documents_ImageMiniToolBar_Header"
                                        Foreground="{telerik1:GreenResource ResourceKey=FixedBrush}"
                                        HorizontalAlignment="Center"
                                        Margin="2"/>
                                <Grid HorizontalAlignment="Center" Margin="0 2">
