    <Style x:Key="RepeatingSectionItemControlStyle" TargetType="telerikControlsRTBUI:RepeatingSectionItemControl" BasedOn="{StaticResource ContentControlBaseStyle}">
        <Setter Property="ButtonHeight" Value="18"/>
        <Setter Property="ButtonWidth" Value="18"/>
        <Setter Property="ButtonShouldBeVisible" Value="True"/>
        <Setter Property="Template">
            <Setter.Value>
                <ControlTemplate TargetType="{x:Type telerikControlsRTBUI:RepeatingSectionItemControl}">
                    <Canvas>
                        <Polygon x:Name="PART_MainBodyFill"
                                Points="{Binding RelativeSource={RelativeSource TemplatedParent}, Path=PolygonPoints}"
                                Fill="{Binding OutlineColor, RelativeSource={RelativeSource TemplatedParent}, Converter={StaticResource ColorToBrushConverter}}"
                                Opacity="0.2"/>
                        <Polygon x:Name="PART_MainBodyBorder"
                                RenderOptions.EdgeMode="Aliased"
                                Points="{Binding RelativeSource={RelativeSource TemplatedParent}, Path=PolygonPoints}"
                                Stroke="{Binding OutlineColor, RelativeSource={RelativeSource TemplatedParent}, Converter={StaticResource ColorToBrushConverter}}"
                                StrokeThickness="{Binding LineThickness, RelativeSource={RelativeSource TemplatedParent}}"
                                Visibility="{Binding Focused, RelativeSource={RelativeSource Mode=TemplatedParent}, Converter={StaticResource BooleanToVisibilityConverter}}"/>
                        <Border x:Name="PART_LeftThumbContainer"/>
                        <Border x:Name="PART_TopThumbContainer"/>
                        <Border x:Name="PART_ButtonContainer"
                                Background="White"
                                Height="{Binding RelativeSource={RelativeSource TemplatedParent}, Path=ButtonHeight}"
                                Width="{Binding RelativeSource={RelativeSource TemplatedParent}, Path=ButtonWidth}"
                                Visibility="Hidden"
                                BorderBrush="{Binding OutlineColor, RelativeSource={RelativeSource TemplatedParent}, Converter={StaticResource ColorToBrushConverter}}"
                                BorderThickness="0 1 1 1">
                            <telerik:RadPathButton x:Name="Button"
                                    Visibility="{Binding ButtonShouldBeVisible, Converter={StaticResource BooleanToVisibilityConverter}}"
                                    Command="{TemplateBinding RepeatItemCommand}"
                                    CommandParameter="{Binding}"
                                    Foreground="{TemplateBinding Foreground}"
                                    PathGeometry="{telerik:RadGlyph Type=Path, Glyph={StaticResource GlyphPlus}}"
                                    Padding="0"
                                    PathStyle="{StaticResource ContentControlPathButtonPathStyle}"
                                    Style="{StaticResource ContentControlPathButtonStyle}"
                                    Background="{Binding OutlineColor, RelativeSource={RelativeSource TemplatedParent}, Converter={StaticResource ColorToBrushWithOpacityConverter}, ConverterParameter=50}"/>
                        </Border>
                    </Canvas>
                    <ControlTemplate.Triggers>
                        <Trigger Property="Focused" Value="True">
                            <Setter TargetName="PART_ButtonContainer" Property="Visibility" Value="Visible"/>
                        </Trigger>
                        <Trigger Property="TextFlowDirection" Value="RightToLeft">
                            <Setter TargetName="PART_ButtonContainer" Property="BorderThickness" Value="1 1 0 1"/>
                        </Trigger>
                        <Trigger SourceName="Button" Property="IsMouseOver" Value="True">
                            <Setter TargetName="Button" Property="Background" Value="{Binding OutlineColor, RelativeSource={RelativeSource TemplatedParent}, Converter={StaticResource ColorToBrushWithOpacityConverter}, ConverterParameter=100}"/>
                        </Trigger>
                        <Trigger Property="Selected" Value="True">
                            <Setter TargetName="Button" Property="Background" Value="{Binding OutlineColor, RelativeSource={RelativeSource TemplatedParent}, Converter={StaticResource ColorToBrushConverter}}"/>
                            <Setter TargetName="PART_MainBodyFill" Property="Fill" Value="{Binding OutlineColor, RelativeSource={RelativeSource TemplatedParent}, Converter={StaticResource ColorToBrushConverter}}"/>
                        </Trigger>
                        <MultiTrigger>
                            <MultiTrigger.Conditions>
                                <Condition Property="Focused" Value="False"/>
                                <Condition Property="Hovered" Value="True"/>
                            </MultiTrigger.Conditions>
                            <Setter TargetName="PART_MainBodyFill" Property="Opacity" Value="0.2"/>
                        </MultiTrigger>
                        <MultiTrigger>
                            <MultiTrigger.Conditions>
                                <Condition Property="Focused" Value="True"/>
                                <Condition Property="Hovered" Value="True"/>
                            </MultiTrigger.Conditions>
                            <Setter TargetName="PART_MainBodyFill" Property="Opacity" Value="0.0"/>
                        </MultiTrigger>
                        <Trigger Property="Hovered" Value="False">
                            <Setter TargetName="PART_MainBodyFill" Property="Opacity" Value="0.0"/>
                        </Trigger>
                        <Trigger Property="ButtonShouldBeVisible" Value="False">
                            <Setter TargetName="PART_ButtonContainer" Property="Visibility" Value="Hidden"/>
                        </Trigger>
                    </ControlTemplate.Triggers>
                </ControlTemplate>
            </Setter.Value>
        </Setter>
    </Style>
