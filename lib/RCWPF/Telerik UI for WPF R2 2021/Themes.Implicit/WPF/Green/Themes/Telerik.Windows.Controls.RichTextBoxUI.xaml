                <Setter TargetName="ButtonBackgroundDecorator" Property="BorderBrush" Value="{telerik1:GreenResource ResourceKey=AccentHighBrush}"/>
                <Setter Property="Foreground" TargetName="textRow1" Value="{telerik1:GreenResource ResourceKey=SelectedBrush}"/>
