                                <Path.Resources>
                                    <Style TargetType="Path">
                                        <Setter Property="Stroke" Value="{telerik:GreenResource ResourceKey=StrongBrush}"/>
                                        <Style.Triggers>
                                            <DataTrigger Binding="{Binding IsFocused, ElementName=buttonZoomOut}" Value="True">
                                                <Setter Property="Stroke" Value="{telerik1:GreenResource ResourceKey=SelectedBrush}"/>
                                            </DataTrigger>
                                            <DataTrigger Binding="{Binding IsMouseOver, ElementName=buttonZoomOut}" Value="True">
                                                <Setter Property="Stroke" Value="{telerik1:GreenResource ResourceKey=SelectedBrush}"/>
                                            </DataTrigger>
                                            <DataTrigger Binding="{Binding IsPressed, ElementName=buttonZoomOut}" Value="True">
                                                <Setter Property="Stroke" Value="{telerik1:GreenResource ResourceKey=SelectedBrush}"/>
                                            </DataTrigger>
                                        </Style.Triggers>
                                    </Style>
                                </Path.Resources>
                            </Path>
                        </telerik:RadButton>
                        <telerikControlsRTBUIStatusBar:NonLinearSlider x:Name="sliderZoom" VerticalAlignment="Center" Grid.Column="3"/>
                        <telerik:RadButton Grid.Column="4" x:Name="buttonZoomIn" Width="16" Height="16" BorderThickness="0" Padding="3" Margin="8 0 0 0">
                            <Path Data="{StaticResource PlusIcon8x8}" Stretch="Fill" Width="8" Height="8">
                                <Path.Resources>
                                    <Style TargetType="Path">
                                        <Setter Property="Fill" Value="{telerik:GreenResource ResourceKey=StrongBrush}"/>
                                        <Style.Triggers>
                                            <DataTrigger Binding="{Binding IsFocused, ElementName=buttonZoomIn}" Value="True">
                                                <Setter Property="Fill" Value="{telerik1:GreenResource ResourceKey=SelectedBrush}"/>
                                            </DataTrigger>
                                            <DataTrigger Binding="{Binding IsMouseOver, ElementName=buttonZoomIn}" Value="True">
                                                <Setter Property="Fill" Value="{telerik1:GreenResource ResourceKey=SelectedBrush}"/>
                                            </DataTrigger>
                                            <DataTrigger Binding="{Binding IsPressed, ElementName=buttonZoomIn}" Value="True">
                                                <Setter Property="Fill" Value="{telerik1:GreenResource ResourceKey=SelectedBrush}"/>
                                            </DataTrigger>
                                        </Style.Triggers>
                                    </Style>
                                </Path.Resources>
                            </Path>
                        </telerik:RadButton>
                    </Grid>
                </ControlTemplate>
            </Setter.Value>
        </Setter>
    </Style>
    <Style x:Key="RadRichTextBoxStatusBarStyle" TargetType="telerikControlsRTBUI:RadRichTextBoxStatusBar">
