                                Header="{telerik:LocalizableResource Key=RibbonViewQATShowBelow}"
                                Foreground="{TemplateBinding Foreground}"
                                Command="{x:Static telerikRibbonView:RibbonCommands.ToggleQATPositionCommand}"/>
