    <Style TargetType="telerikControlsRTBUI:ShapesGallery" BasedOn="{StaticResource ShapesGalleryStyle}"/>
    <Style TargetType="telerikControlsRTBUI:ShapesRibbonGallery" BasedOn="{StaticResource ShapesRibbonGalleryStyle}"/>
    <Style TargetType="telerikControlsRTBUI:ShapesColorPicker" BasedOn="{StaticResource ShapesColorPickerStyle}"/>
    <Style TargetType="telerikControlsRTBUI:ShapesOutlinePicker" BasedOn="{StaticResource ShapesOutlinePickerStyle}"/>
    <Style TargetType="telerikControlsRTBUI:GradientFillControl" BasedOn="{StaticResource GradientFillControlStyle}"/>
    <Style TargetType="telerikControlsRTBUI:GradientsGallery" BasedOn="{StaticResource GradientsGalleryStyle}"/>
    <Style TargetType="telerikControlsRTBUI:TaskPane" BasedOn="{StaticResource TaskPaneStyle}"/>
    <Style TargetType="taskPaneControls:FillSettingsControl" BasedOn="{StaticResource FillSettingsControlStyle}"/>
    <Style TargetType="taskPaneControls:SolidFillSettingsControl" BasedOn="{StaticResource SolidFillSettingsControlStyle}"/>
