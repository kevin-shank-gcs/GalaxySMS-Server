    <ControlTemplate x:Key="RulerThumbControlTemplate" TargetType="telerikControlsRTBUI:RulerThumb">
        <Grid>
            <ToolTipService.ToolTip>
                <ToolTip Content="{TemplateBinding ToolTipText}"/>
            </ToolTipService.ToolTip>
            <Path x:Name="Indent"
                    Stretch="Fill"
                    Data="{StaticResource IndentIcon}"
                    Visibility="Collapsed"
                    RenderTransformOrigin="0.5 0.5"
                    Fill="{TemplateBinding Background}"
                    Height="8"
                    Width="9"
                    Stroke="{TemplateBinding Foreground}"
                    SnapsToDevicePixels="True"/>
            <Path x:Name="FirstLineIndent"
                    Stretch="Fill"
                    Data="{StaticResource FirstLineIndentIcon}"
                    SnapsToDevicePixels="True"
                    RenderTransformOrigin="0.5 0.5"
                    Height="8"
                    Width="9"
                    Visibility="Collapsed"
                    Fill="{TemplateBinding Background}"
                    Stroke="{TemplateBinding Foreground}"
                    VerticalAlignment="{TemplateBinding VerticalContentAlignment}"
                    HorizontalAlignment="{TemplateBinding HorizontalContentAlignment}"/>
            <Rectangle x:Name="LeftIndent"
                    Fill="{TemplateBinding Background}"
                    Visibility="Collapsed"
                    VerticalAlignment="{TemplateBinding VerticalContentAlignment}"
                    HorizontalAlignment="{TemplateBinding HorizontalContentAlignment}"
                    Height="5"
                    Width="9"
                    Stroke="{TemplateBinding Foreground}"/>
            <Path x:Name="LeftTab"
                    Data="{StaticResource LeftTabIcon}"
                    VerticalAlignment="{TemplateBinding VerticalContentAlignment}"
                    HorizontalAlignment="{TemplateBinding HorizontalContentAlignment}"
                    Visibility="Collapsed"
                    Fill="{TemplateBinding Foreground}"
                    Stretch="Fill"
                    UseLayoutRounding="True"
                    Width="5"
                    Height="5"
                    SnapsToDevicePixels="True"/>
            <Path x:Name="RightTab"
                    Data="{StaticResource RightTabIcon}"
                    Visibility="Collapsed"
                    Fill="{TemplateBinding Foreground}"
                    Width="5"
                    Height="5"
                    SnapsToDevicePixels="True"
                    VerticalAlignment="{TemplateBinding VerticalContentAlignment}"
                    HorizontalAlignment="{TemplateBinding HorizontalContentAlignment}"
                    UseLayoutRounding="True"
                    Stretch="Fill"/>
            <Path x:Name="CenterTab"
                    Data="{StaticResource CenterTabIcon}"
                    Visibility="Collapsed"
                    Fill="{TemplateBinding Foreground}"
                    Width="8"
                    Height="5"
                    SnapsToDevicePixels="True"
                    Stretch="Fill"
                    VerticalAlignment="{TemplateBinding VerticalContentAlignment}"
                    HorizontalAlignment="{TemplateBinding HorizontalContentAlignment}"
                    UseLayoutRounding="True"/>
            <Path x:Name="DecimalTab"
                    Data="{StaticResource DecimalTabIcon}"
                    Visibility="Collapsed"
                    Fill="{TemplateBinding Foreground}"
                    Width="8"
                    Height="5"
                    SnapsToDevicePixels="True"
                    Stretch="Fill"
                    VerticalAlignment="{TemplateBinding VerticalContentAlignment}"
                    HorizontalAlignment="{TemplateBinding HorizontalContentAlignment}"/>
            <Path x:Name="BarTab"
                    Visibility="Collapsed"
                    Data="{StaticResource BarTabIcon}"
                    Fill="{TemplateBinding Foreground}"
                    UseLayoutRounding="True"
                    VerticalAlignment="{TemplateBinding VerticalContentAlignment}"
                    HorizontalAlignment="{TemplateBinding HorizontalContentAlignment}"
                    Stretch="Fill"
                    Width="2"
                    Height="5"
                    SnapsToDevicePixels="True"/>
        </Grid>
        <ControlTemplate.Triggers>
            <Trigger Property="RulerThumbType" Value="LeftTabStop">
                <Setter Property="Visibility" TargetName="LeftTab" Value="Visible"/>
            </Trigger>
            <Trigger Property="RulerThumbType" Value="RightTabStop">
                <Setter Property="Visibility" TargetName="RightTab" Value="Visible"/>
            </Trigger>
            <Trigger Property="RulerThumbType" Value="CenterTabStop">
                <Setter Property="Visibility" TargetName="CenterTab" Value="Visible"/>
            </Trigger>
            <Trigger Property="RulerThumbType" Value="DecimalTabStop">
                <Setter Property="Visibility" TargetName="DecimalTab" Value="Visible"/>
            </Trigger>
            <Trigger Property="RulerThumbType" Value="Indent">
                <Setter Property="Visibility" TargetName="Indent" Value="Visible"/>
            </Trigger>
            <Trigger Property="RulerThumbType" Value="FirstLineIndent">
                <Setter Property="Visibility" TargetName="FirstLineIndent" Value="Visible"/>
            </Trigger>
            <Trigger Property="RulerThumbType" Value="LeftIndent">
                <Setter Property="Visibility" TargetName="LeftIndent" Value="Visible"/>
            </Trigger>
        </ControlTemplate.Triggers>
    </ControlTemplate>
    <Style x:Key="RulerThumbBaseStyle" TargetType="telerikControlsRTBUI:RulerThumb">
        <Setter Property="Foreground" Value="{telerik:GreenResource ResourceKey=StrongBrush}"/>
        <Setter Property="Background" Value="{telerik1:GreenResource ResourceKey=MainBrush}"/>
        <Setter Property="Cursor" Value="Arrow"/>
        <Setter Property="Margin" Value="-4 9 0 0"/>
        <Setter Property="VerticalContentAlignment" Value="Center"/>
        <Setter Property="HorizontalContentAlignment" Value="Center"/>
        <Setter Property="Template" Value="{StaticResource RulerThumbControlTemplate}"/>
    </Style>
    <Style x:Key="RulerThumbStyle" TargetType="telerikControlsRTBUI:RulerThumb" BasedOn="{StaticResource RulerThumbBaseStyle}"/>
    <Style x:Key="HangingIndentThumbStyle" TargetType="telerikControlsRTBUI:RulerThumb" BasedOn="{StaticResource RulerThumbBaseStyle}">
        <Setter Property="Margin" Value="-4 7 0 0"/>
    </Style>
    <Style x:Key="FirstLineIndentThumbStyle" TargetType="telerikControlsRTBUI:RulerThumb" BasedOn="{StaticResource RulerThumbBaseStyle}">
        <Setter Property="Margin" Value="-4 0 0 0"/>
    </Style>
    <Style x:Key="LeftIndentThumbStyle" TargetType="telerikControlsRTBUI:RulerThumb" BasedOn="{StaticResource RulerThumbBaseStyle}">
        <Setter Property="Margin" Value="-4 13 0 0"/>
    </Style>
    <Style x:Key="RightIndentThumbStyle" TargetType="telerikControlsRTBUI:RulerThumb" BasedOn="{StaticResource RulerThumbBaseStyle}">
        <Setter Property="Margin" Value="-4 7 0 0"/>
    </Style>
    <Style TargetType="Thumb" x:Key="TableColumnThumbStyle">
        <Setter Property="Cursor" Value="SizeWE"/>
        <Setter Property="Template">
            <Setter.Value>
                <ControlTemplate TargetType="Thumb">
                    <Grid Background="{TemplateBinding Background}" Margin="-3 3 0 0">
                        <Border Background="{telerik:GreenResource ResourceKey=MainBrush}"/>
                        <Border Height="1" Margin="0 1 0 0" VerticalAlignment="Top" Background="{telerik:GreenResource ResourceKey=StrongBrush}" UseLayoutRounding="True" SnapsToDevicePixels="True"/>
                        <Border Height="1" Margin="0 3 0 0" VerticalAlignment="Top" Background="{telerik:GreenResource ResourceKey=StrongBrush}" UseLayoutRounding="True" SnapsToDevicePixels="True"/>
                        <Border Height="1" Margin="0 5 0 0" VerticalAlignment="Top" Background="{telerik:GreenResource ResourceKey=StrongBrush}" UseLayoutRounding="True" SnapsToDevicePixels="True"/>
                        <Border Height="1" Margin="0 7 0 0" VerticalAlignment="Top" Background="{telerik:GreenResource ResourceKey=StrongBrush}" UseLayoutRounding="True" SnapsToDevicePixels="True"/>
                        <Border
                                HorizontalAlignment="Left"
                                Margin="1 0 0 0"
                                Width="1"
                                Background="{telerik:GreenResource ResourceKey=StrongBrush}"
                                UseLayoutRounding="False"
                                SnapsToDevicePixels="True"/>
                        <Border
                                HorizontalAlignment="Left"
                                Margin="3 0 0 0"
                                Width="1"
                                Background="{telerik:GreenResource ResourceKey=StrongBrush}"
                                UseLayoutRounding="True"
                                SnapsToDevicePixels="True"/>
                        <Border
                                HorizontalAlignment="Left"
                                Margin="5 0 0 0"
                                Width="1"
                                Background="{telerik:GreenResource ResourceKey=StrongBrush}"
                                UseLayoutRounding="True"
                                SnapsToDevicePixels="True"/>
                    </Grid>
                </ControlTemplate>
            </Setter.Value>
        </Setter>
