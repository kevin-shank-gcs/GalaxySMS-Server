        <Setter Property="SubmenuHeaderTemplateKey" Value="{StaticResource DropDownMenuItemTemplate}"/>
        <Setter Property="TopLevelHeaderTemplateKey" Value="{StaticResource DropDownMenuItemTemplate}"/>
