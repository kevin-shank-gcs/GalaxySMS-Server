        <Setter Property="helpers:ThemeHelper.CheckedBrush" Value="{telerik:VisualStudio2013Resource ResourceKey=HeaderBrush}"/>
        <Setter Property="helpers:ThemeHelper.CornerRadius" Value="0"/>
        <Setter Property="RenderTransformOrigin" Value="0.5 0.5"/>
        <Setter Property="IsTabStop" Value="False"/>
        <Setter Property="Foreground" Value="{telerik:VisualStudio2013Resource ResourceKey=MarkerBrush}"/>
