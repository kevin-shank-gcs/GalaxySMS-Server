    <telerik:InvertedBooleanToOpacityConverter x:Key="InvertedBooleanToOpacityConverter"/>
    <ControlTemplate x:Key="CardViewGroupPanelItemTemplate" TargetType="cardView:CardViewGroupPanelItem">
        <StackPanel x:Name="PART_GroupPanelItemStackPanel"
                Orientation="Horizontal"
                VerticalAlignment="{TemplateBinding VerticalContentAlignment}"
                Background="{TemplateBinding Background}">
            <Grid HorizontalAlignment="{TemplateBinding HorizontalContentAlignment}">
                <Grid.RowDefinitions>
                    <RowDefinition Height="Auto" MinHeight="4"/>
                    <RowDefinition/>
                </Grid.RowDefinitions>
                <Path x:Name="PART_InsertionPoint"
                        Data="M4,0L4,7 0,3.35z"
                        Stretch="Fill"
                        RenderTransformOrigin="0.5,0.5"
                        Fill="{TemplateBinding Foreground}"
                        HorizontalAlignment="Center"
                        Width="4"
                        Height="6"
                        VerticalAlignment="Top"
                        Margin="3 0"
                        SnapsToDevicePixels="True"
                        Visibility="{TemplateBinding InsertionPointVisibility}">
                    <Path.RenderTransform>
                        <TransformGroup>
                            <ScaleTransform/>
                            <SkewTransform/>
                            <RotateTransform Angle="-90"/>
                            <TranslateTransform/>
                        </TransformGroup>
                    </Path.RenderTransform>
                </Path>
                <Grid x:Name="PART_ConnectingLine" Opacity="{Binding IsFirst, RelativeSource={RelativeSource TemplatedParent}, Converter={StaticResource InvertedBooleanToOpacityConverter}}" Grid.Row="1">
                    <Path
                            Fill="{TemplateBinding Foreground}"
                            HorizontalAlignment="Center"
                            VerticalAlignment="Center"
                            RenderTransformOrigin="0.5,0.5"
                            Stretch="Fill"
                            Stroke="{x:Null}"
                            Width="4"
                            Data="M4,0L4,7 0,3.35z"
                            Height="6"
                            Margin="6 -3 6 0"
                            SnapsToDevicePixels="True">
                        <Path.RenderTransform>
                            <TransformGroup>
                                <ScaleTransform/>
                                <SkewTransform/>
                                <RotateTransform Angle="180"/>
                            </TransformGroup>
                        </Path.RenderTransform>
                    </Path>
                </Grid>
            </Grid>
            <cardView:CardViewGroupPanelCell Content="{TemplateBinding Content}" SortDirection="{Binding Path=SortDirection}" x:Name="PART_Cell"/>
            <Path x:Name="PART_RightInsertionPoint"
                    Data="M0,0 L6,0 L3,4 z"
                    Stretch="Fill"
                    RenderTransformOrigin="0.5,0.5"
                    Fill="{TemplateBinding Foreground}"
                    HorizontalAlignment="Center"
                    Width="6"
                    Height="4"
                    VerticalAlignment="Top"
                    Margin="1 0"
                    Opacity="0.7"
                    SnapsToDevicePixels="True"
                    Visibility="{TemplateBinding RightInsertionPointVisibility}"/>
        </StackPanel>
    </ControlTemplate>
