                <TextBlock x:Name="FlagEnumEditor_InputTextBlock_Pressed"
                        Foreground="{telerik:VisualStudio2013Resource ResourceKey=SelectedBrush}"
                        Visibility="{Binding IsPressed,ElementName=PART_EnumValuesDropDownButton,Converter={StaticResource BooleanToVisibilityConverter}}"
                        MinHeight="0"
                        Padding="{TemplateBinding Padding}"
                        Text="{Binding Value, RelativeSource={RelativeSource TemplatedParent}, Mode=TwoWay, ValidatesOnExceptions=True, ValidatesOnDataErrors=True}"/>
