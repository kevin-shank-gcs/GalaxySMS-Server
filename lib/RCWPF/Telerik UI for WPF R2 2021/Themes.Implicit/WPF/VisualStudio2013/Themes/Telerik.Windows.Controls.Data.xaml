                        Fill="{telerik:VisualStudio2013Resource ResourceKey=StrongBrush}"
                        Height="8"
                        Stretch="Fill"
                        HorizontalAlignment="Center"
                        VerticalAlignment="Center"
                        Width="8"/>
                <Path x:Name="path_pressed"
