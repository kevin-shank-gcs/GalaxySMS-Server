                        HorizontalAlignment="Center"
                        VerticalAlignment="Top"
                        Margin="14 -2 0 0"/>
                <cardView:CardViewFilteringDropDown x:Name="PART_DistinctFilterControl" Grid.Column="2" Visibility="{TemplateBinding FilteringUIVisibility}" Margin="0 0 4 0"/>
                <telerik:RadButton x:Name="PART_SortButton"
                        Grid.Column="1"
                        Opacity="0"
                        MinHeight="0"
                        Visibility="{Binding AllowSorting, Converter={StaticResource BooleanToVisibilityConverter}}"
                        Style="{StaticResource CardDataFieldDescriptorHeaderCellButtonStyle}"
                        telerik:TouchManager.TouchMode="None">
                    <telerik:RadGlyph x:Name="GlyphSorting" Glyph="{StaticResource GlyphSortDescSmall}" FontSize="16" HorizontalAlignment="Center" VerticalAlignment="Center"/>
                </telerik:RadButton>
            </Grid>
        </Grid>
        <ControlTemplate.Triggers>

            <!-- IsFilteringAllowed: False -->
            <DataTrigger Binding="{Binding Path=IsFilteringAllowed, RelativeSource={RelativeSource AncestorType={x:Type controls:RadCardView}, Mode=FindAncestor}}" Value="False">
                <Setter TargetName="PART_DistinctFilterControl" Property="Visibility" Value="Collapsed"/>
            </DataTrigger>

            <!-- CanUserSortDescriptors: False -->
            <DataTrigger Binding="{Binding Path=CanUserSortDescriptors, RelativeSource={RelativeSource AncestorType={x:Type controls:RadCardView}, Mode=FindAncestor}}" Value="False">
                <Setter TargetName="PART_SortButton" Property="Visibility" Value="Collapsed"/>
            </DataTrigger>

            <!-- IsSortingButtonVisible: False -->
            <DataTrigger Binding="{Binding IsSortingButtonVisible, RelativeSource={RelativeSource Mode=Self}}" Value="False">
                <Setter TargetName="PART_SortButton" Property="Visibility" Value="Collapsed"/>
            </DataTrigger>

            <!-- SortButton IsPressed -->
            <Trigger SourceName="PART_SortButton" Property="IsPressed" Value="True">
                <Setter TargetName="GlyphSorting" Property="Foreground" Value="{telerik:VisualStudio2013Resource ResourceKey=SelectedBrush}"/>
            </Trigger>

            <!-- IsMouseOver -->
            <Trigger Property="IsMouseOver" Value="True">
                <Setter TargetName="MouseOverVisual" Property="Opacity" Value="1"/>
                <Setter TargetName="PART_SortButton" Property="Opacity" Value="1"/>
            </Trigger>

            <!-- SortingState: Ascending -->
            <Trigger Property="SortingState" Value="Ascending">
                <Setter TargetName="PART_SortButton" Property="Opacity" Value="1"/>
                <Setter TargetName="GlyphSorting" Property="Glyph" Value="{StaticResource GlyphSortAscSmall}"/>
                <Setter TargetName="GlyphSorting" Property="Foreground" Value="{telerik:VisualStudio2013Resource ResourceKey=MarkerBrush}"/>
                <Setter TargetName="SelectedVisual" Property="Opacity" Value="1"/>
                <Setter TargetName="PART_DistinctFilterControl" Property="Foreground" Value="{telerik:VisualStudio2013Resource ResourceKey=SelectedBrush}"/>
                <Setter Property="Foreground" Value="{telerik:VisualStudio2013Resource ResourceKey=SelectedBrush}"/>
            </Trigger>

            <!-- SortingState: Descending -->
            <Trigger Property="SortingState" Value="Descending">
                <Setter TargetName="PART_SortButton" Property="Opacity" Value="1"/>
                <Setter TargetName="SelectedVisual" Property="Opacity" Value="1"/>
                <Setter TargetName="GlyphSorting" Property="Foreground" Value="{telerik:VisualStudio2013Resource ResourceKey=MarkerBrush}"/>
                <Setter Property="Foreground" Value="{telerik:VisualStudio2013Resource ResourceKey=SelectedBrush}"/>
                <Setter TargetName="PART_DistinctFilterControl" Property="Foreground" Value="{telerik:VisualStudio2013Resource ResourceKey=SelectedBrush}"/>
            </Trigger>
        </ControlTemplate.Triggers>
    </ControlTemplate>
