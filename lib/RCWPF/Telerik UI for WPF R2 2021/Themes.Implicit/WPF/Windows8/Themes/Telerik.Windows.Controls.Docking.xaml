    <Style x:Key="DockingNavigator_ListBoxScrollButtonStyle" TargetType="RepeatButton" BasedOn="{StaticResource RepeatButtonStyle}">
        <Setter Property="helpers:ThemeHelper.CornerRadius" Value="0"/>
        <Setter Property="Background" Value="{telerik:Windows8Resource ResourceKey=BasicBrush}"/>
        <Setter Property="BorderBrush" Value="{telerik:Windows8Resource ResourceKey=MainBrush}"/>
        <Setter Property="BorderThickness" Value="0"/>
        <Setter Property="Interval" Value="50"/>
        <Setter Property="IsTabStop" Value="False"/>
        <Setter Property="MinWidth" Value="0"/>
        <Setter Property="MinHeight" Value="0"/>
        <Setter Property="Height" Value="22"/>
        <Setter Property="Focusable" Value="False"/>
    </Style>
    <Style x:Key="DockingNavigator_ListBoxScrollViewerStyle" TargetType="{x:Type ScrollViewer}">
        <Setter Property="Padding" Value="0"/>
        <Setter Property="HorizontalScrollBarVisibility" Value="Hidden"/>
        <Setter Property="VerticalScrollBarVisibility" Value="Auto"/>
        <Setter Property="CanContentScroll" Value="True"/>
        <Setter Property="Template">
            <Setter.Value>
                <ControlTemplate TargetType="{x:Type ScrollViewer}">
                    <Grid SnapsToDevicePixels="true">
                        <Grid.RowDefinitions>
                            <RowDefinition Height="Auto"/>
                            <RowDefinition Height="*"/>
                            <RowDefinition Height="Auto"/>
                        </Grid.RowDefinitions>
                        <RepeatButton
                                Command="{x:Static ScrollBar.LineUpCommand}"
                                VerticalAlignment="Top"
                                Style="{StaticResource DockingNavigator_ListBoxScrollButtonStyle}"
                                CommandTarget="{Binding RelativeSource={RelativeSource TemplatedParent}}">
                            <RepeatButton.Content>
                                <telerik:RadGlyph HorizontalAlignment="Center" VerticalAlignment="Center" FontSize="16" Glyph="{StaticResource GlyphArrow60Up}"/>
                            </RepeatButton.Content>
                            <RepeatButton.Visibility>
                                <MultiBinding FallbackValue="Visibility.Collapsed" Converter="{StaticResource MenuScrollingVisibilityConverter}" ConverterParameter="0">
                                    <Binding RelativeSource="{RelativeSource TemplatedParent}" Path="ComputedVerticalScrollBarVisibility"/>
                                    <Binding RelativeSource="{RelativeSource TemplatedParent}" Path="VerticalOffset"/>
                                    <Binding RelativeSource="{RelativeSource TemplatedParent}" Path="ExtentHeight"/>
                                    <Binding RelativeSource="{RelativeSource TemplatedParent}" Path="ViewportHeight"/>
                                </MultiBinding>
                            </RepeatButton.Visibility>
                        </RepeatButton>
                        <RepeatButton
                                Grid.Row="2"
                                VerticalAlignment="Bottom"
                                Style="{StaticResource DockingNavigator_ListBoxScrollButtonStyle}"
                                Command="{x:Static ScrollBar.LineDownCommand}"
                                CommandTarget="{Binding RelativeSource={RelativeSource TemplatedParent}}">
                            <RepeatButton.Content>
                                <telerik:RadGlyph HorizontalAlignment="Center" VerticalAlignment="Center" FontSize="16" Glyph="{StaticResource GlyphArrow60Down}"/>
                            </RepeatButton.Content>
                            <RepeatButton.Visibility>
                                <MultiBinding FallbackValue="Visibility.Collapsed" Converter="{StaticResource MenuScrollingVisibilityConverter}" ConverterParameter="100">
                                    <Binding RelativeSource="{RelativeSource TemplatedParent}" Path="ComputedVerticalScrollBarVisibility"/>
                                    <Binding RelativeSource="{RelativeSource TemplatedParent}" Path="VerticalOffset"/>
                                    <Binding RelativeSource="{RelativeSource TemplatedParent}" Path="ExtentHeight"/>
                                    <Binding RelativeSource="{RelativeSource TemplatedParent}" Path="ViewportHeight"/>
                                </MultiBinding>
                            </RepeatButton.Visibility>
                        </RepeatButton>
                        <ScrollContentPresenter x:Name="PART_ScrollContentPresenter" Grid.Row="1" Margin="{TemplateBinding Padding}" CanContentScroll="{TemplateBinding CanContentScroll}"/>
                    </Grid>
                </ControlTemplate>
            </Setter.Value>
        </Setter>
        <Setter Property="PanningMode" Value="None"/>
    </Style>
    <Style x:Key="DockingNavigatorListBoxStyle" TargetType="dock:DockingNavigatorListBox" BasedOn="{StaticResource RadListBoxStyle}">
        <Setter Property="helpers:ThemeHelper.CornerRadius" Value="0"/>
        <Setter Property="telerik:ScrollingSettingsBehavior.IsEnabled" Value="True"/>
        <Setter Property="telerik:ScrollingSettingsBehavior.ScrollAreaPadding" Value="30"/>
        <Setter Property="telerik:ScrollingSettingsBehavior.ScrollStep" Value="28"/>
        <Setter Property="telerik:ScrollingSettingsBehavior.ScrollStepTime" Value="0:0:0.05"/>
        <Setter Property="ScrollViewer.VerticalScrollBarVisibility" Value="Auto"/>
        <Setter Property="ScrollViewer.HorizontalScrollBarVisibility" Value="Disabled"/>
        <Setter Property="ScrollViewer.CanContentScroll" Value="True"/>
        <Setter Property="KeyboardNavigation.ControlTabNavigation" Value="Cycle"/>
        <Setter Property="KeyboardNavigation.TabNavigation" Value="Cycle"/>
        <Setter Property="Width" Value="170"/>
        <Setter Property="Height" Value="265"/>
        <Setter Property="Margin" Value="0 0 20 0"/>
        <Setter Property="HorizontalAlignment" Value="Left"/>
        <Setter Property="VerticalAlignment" Value="Stretch"/>
        <Setter Property="Template">
            <Setter.Value>
                <ControlTemplate TargetType="dock:DockingNavigatorListBox">
                    <Grid UseLayoutRounding="True">
                        <Border x:Name="Border"
                                BorderBrush="{TemplateBinding BorderBrush}"
                                BorderThickness="{TemplateBinding BorderThickness}"
                                Background="{TemplateBinding Background}"
                                CornerRadius="{TemplateBinding helpers:ThemeHelper.CornerRadius}">
                            <ScrollViewer x:Name="PART_ScrollViewer"
                                    IsTabStop="False"
                                    Style="{StaticResource DockingNavigator_ListBoxScrollViewerStyle}"
                                    HorizontalScrollBarVisibility="{TemplateBinding ScrollViewer.HorizontalScrollBarVisibility}"
                                    VerticalScrollBarVisibility="{TemplateBinding ScrollViewer.VerticalScrollBarVisibility}">
                                <ItemsPresenter ClipToBounds="True"/>
                            </ScrollViewer>
                        </Border>
                        <ContentPresenter x:Name="dragVisualPlaceholder" HorizontalAlignment="Stretch" Visibility="Collapsed" VerticalAlignment="Stretch"/>
                    </Grid>
                </ControlTemplate>
            </Setter.Value>
        </Setter>
    </Style>
    <DataTemplate x:Key="DockingNavigatorItemTemplate">
        <Grid>
            <Grid.ColumnDefinitions>
                <ColumnDefinition Width="Auto"/>
                <ColumnDefinition Width="*"/>
            </Grid.ColumnDefinitions>
            <telerik:RadGlyph Glyph="{Binding Icon}" FontSize="16" Margin="0 0 5 0" VerticalAlignment="Center" Visibility="{Binding Icon, Converter={StaticResource NullToVisibilityConverter}}"/>
            <TextBlock Grid.Column="1" Text="{Binding Header}" VerticalAlignment="Center" TextTrimming="CharacterEllipsis"/>
        </Grid>
    </DataTemplate>
    <local:DockingNavigatorItemTemplateSelector x:Key="DefaultDockingNavigatorItemTemplateSelector" ActivePaneTemplate="{StaticResource DockingNavigatorItemTemplate}" ActiveDocumentTemplate="{StaticResource DockingNavigatorItemTemplate}"/>
    <ControlTemplate x:Key="DockingNavigatorTemplate" TargetType="local:DockingNavigator">
        <Grid x:Name="LayoutRoot">
            <Grid.RowDefinitions>
                <RowDefinition Height="Auto"/>
                <RowDefinition Height="*"/>
                <RowDefinition Height="Auto"/>
            </Grid.RowDefinitions>
            <Border x:Name="BorderVisual"
                    Grid.RowSpan="3"
                    Background="{TemplateBinding Background}"
                    BorderBrush="{TemplateBinding BorderBrush}"
                    BorderThickness="{TemplateBinding BorderThickness}"
                    CornerRadius="{Binding RelativeSource={RelativeSource TemplatedParent}, Path=(helpers:ThemeHelper.CornerRadius)}"/>
            <StackPanel Orientation="Vertical">
                <Grid MinHeight="30">
                    <Border x:Name="DockingNavigatorHeader"
                            Background="Transparent"
                            BorderBrush="Transparent"
                            BorderThickness="{TemplateBinding BorderThickness}"
                            CornerRadius="{Binding RelativeSource={RelativeSource TemplatedParent}, Path=(helpers:ThemeHelper.CornerRadius), Converter={StaticResource CornerRadiusConverter}, ConverterParameter=TopLeftRight}"/>
                    <Grid VerticalAlignment="Center" Margin="10 0">
                        <Grid.ColumnDefinitions>
                            <ColumnDefinition Width="Auto"/>
                            <ColumnDefinition Width="*"/>
                        </Grid.ColumnDefinitions>
                        <ContentPresenter x:Name="PART_HeaderIcon"
                                Content="{Binding SelectedModel.Icon}"
                                ContentTemplate="{TemplateBinding IconTemplate}"
                                ContentTemplateSelector="{TemplateBinding IconTemplateSelector}"/>
                        <ContentPresenter x:Name="PART_Header"
                                Grid.Column="1"
                                Content="{Binding SelectedModel.Header}"
                                ContentStringFormat="{TemplateBinding HeaderStringFormat}"
                                ContentTemplate="{TemplateBinding HeaderTemplate}"
                                ContentTemplateSelector="{TemplateBinding HeaderTemplateSelector}"
                                TextBlock.Foreground="{telerik:Windows8Resource ResourceKey=MarkerBrush}"
                                TextBlock.FontSize="{telerik:Windows8Resource ResourceKey=FontSizeL}"/>
                    </Grid>
                </Grid>
                <ContentPresenter
                        Content="{Binding SelectedModel.Description}"
                        ContentStringFormat="{TemplateBinding DescriptionStringFormat}"
                        ContentTemplate="{TemplateBinding DescriptionTemplate}"
                        ContentTemplateSelector="{TemplateBinding DescriptionTemplateSelector}"
                        VerticalAlignment="Top"
                        Margin="10 0"
                        MinHeight="16"/>
            </StackPanel>
            <Grid x:Name="ContentGrid" Grid.Row="1" Margin="{TemplateBinding Padding}" VerticalAlignment="Top">
                <Grid.RowDefinitions>
                    <RowDefinition Height="Auto"/>
                    <RowDefinition Height="*"/>
                </Grid.RowDefinitions>
                <Grid.ColumnDefinitions>
                    <ColumnDefinition Width="Auto"/>
                    <ColumnDefinition Width="Auto"/>
                    <ColumnDefinition Width="Auto"/>
                </Grid.ColumnDefinitions>
                <TextBlock
                        Text="{TemplateBinding ActivePanesListBoxHeader}"
                        Foreground="{telerik:Windows8Resource ResourceKey=AccentBrush}"
                        FontSize="{telerik:Windows8Resource ResourceKey=FontSizeL}"
                        Margin="0 0 0 15"/>
                <dock:DockingNavigatorListBox x:Name="PART_ActivePanesList"
                        Grid.Row="1"
                        AutomationProperties.Name="ActivePanesListBox"
                        ItemsSource="{Binding ActivePanesModels}"
                        DisplayMemberPath="Header"
                        KeyboardNavigation.TabNavigation="Cycle"
                        SelectedItem="{Binding SelectedModel, Mode=TwoWay}"
                        Style="{TemplateBinding ActivePanesListBoxStyle}"/>
                <TextBlock
                        Grid.Column="1"
                        Text="{TemplateBinding ActiveDocumentsListBoxHeader}"
                        Foreground="{telerik:Windows8Resource ResourceKey=AccentBrush}"
                        FontSize="{telerik:Windows8Resource ResourceKey=FontSizeL}"
                        Margin="0 0 0 15"/>
                <dock:DockingNavigatorListBox x:Name="PART_ActiveDocumentsList"
                        Grid.Column="1"
                        Grid.Row="1"
                        AutomationProperties.Name="ActiveDocumentsListBox"
                        ItemsSource="{Binding ActiveDocumentsModels}"
                        DisplayMemberPath="Header"
                        KeyboardNavigation.TabNavigation="Cycle"
                        SelectedItem="{Binding SelectedModel, Mode=TwoWay}"
                        Style="{TemplateBinding ActiveDocumentsListBoxStyle}"/>
                <TextBlock Grid.Column="4" Text="{TemplateBinding PreviewHeader}" FontSize="{telerik:Windows8Resource ResourceKey=FontSizeL}" Margin="0 0 0 15"/>
                <Grid x:Name="PreviewGrid"
                        Grid.Column="3"
                        Grid.Row="1"
                        Width="{TemplateBinding PreviewWidth}"
                        Height="{TemplateBinding PreviewHeight}"
                        Margin="0"
                        HorizontalAlignment="Left"
                        VerticalAlignment="Top">
                    <Border Background="Transparent" BorderBrush="{telerik:Windows8Resource ResourceKey=BasicBrush}" BorderThickness="1" SnapsToDevicePixels="True">
                        <Rectangle x:Name="PART_Preview" SnapsToDevicePixels="True" ClipToBounds="True" Margin="5"/>
                    </Border>
                </Grid>
            </Grid>
            <Grid Grid.Row="3" Margin="10 0 10 10" VerticalAlignment="Bottom">
                <ContentPresenter
                        Content="{Binding SelectedModel.Footer}"
                        ContentStringFormat="{TemplateBinding FooterStringFormat}"
                        ContentTemplate="{TemplateBinding FooterTemplate}"
                        ContentTemplateSelector="{TemplateBinding FooterTemplateSelector}"
                        TextBlock.FontSize="{TemplateBinding FontSize}"
                        MinHeight="20">
                    <ContentPresenter.Resources>
                        <Style TargetType="TextBlock">
                            <Setter Property="VerticalAlignment" Value="Bottom"/>
                            <Setter Property="HorizontalAlignment" Value="Left"/>
                        </Style>
                    </ContentPresenter.Resources>
                </ContentPresenter>
            </Grid>
        </Grid>
    </ControlTemplate>
    <Style x:Key="DockingNavigatorStyle" TargetType="local:DockingNavigator">
        <Setter Property="helpers:ThemeHelper.CornerRadius" Value="0"/>
        <Setter Property="FontFamily" Value="{telerik1:Windows8Resource ResourceKey=FontFamily}"/>
        <Setter Property="FontSize" Value="{telerik1:Windows8Resource ResourceKey=FontSize}"/>
        <Setter Property="Foreground" Value="{telerik:Windows8Resource ResourceKey=MarkerBrush}"/>
        <Setter Property="Background" Value="{telerik:Windows8Resource ResourceKey=MainBrush}"/>
        <Setter Property="BorderBrush" Value="{telerik:Windows8Resource ResourceKey=BasicBrush}"/>
        <Setter Property="BorderThickness" Value="1"/>
        <Setter Property="HorizontalAlignment" Value="Left"/>
        <Setter Property="VerticalAlignment" Value="Top"/>
        <Setter Property="PreviewWidth" Value="300"/>
        <Setter Property="PreviewHeight" Value="265"/>
        <Setter Property="Padding" Value="10 14"/>
        <Setter Property="ActivePanesListBoxHeader" Value="{Binding Manager.Culture, Source={StaticResource localizationManagerProvider}, Converter={StaticResource cultureToStringConverter}, ConverterParameter=Docking_ActivePanes}"/>
        <Setter Property="ActiveDocumentsListBoxHeader" Value="{Binding Manager.Culture, Source={StaticResource localizationManagerProvider}, Converter={StaticResource cultureToStringConverter}, ConverterParameter=Docking_ActiveDocuments}"/>
        <Setter Property="PreviewHeader" Value="{Binding Manager.Culture, Source={StaticResource localizationManagerProvider}, Converter={StaticResource cultureToStringConverter}, ConverterParameter=Docking_PreviewHeader}"/>
        <Setter Property="Focusable" Value="False"/>
        <Setter Property="ActivePanesListBoxStyle" Value="{StaticResource DockingNavigatorListBoxStyle}"/>
        <Setter Property="ActiveDocumentsListBoxStyle" Value="{StaticResource DockingNavigatorListBoxStyle}"/>
        <Setter Property="ItemTemplateSelector" Value="{StaticResource DefaultDockingNavigatorItemTemplateSelector}"/>
        <Setter Property="Template" Value="{StaticResource DockingNavigatorTemplate}"/>
    </Style>
    <Style TargetType="dock:DockingNavigatorListBoxItem" BasedOn="{StaticResource RadListBoxItemStyle}"/>
    <Style TargetType="dock:DockingNavigatorListBox" BasedOn="{StaticResource DockingNavigatorListBoxStyle}"/>
    <Style TargetType="local:DockingNavigator" BasedOn="{StaticResource DockingNavigatorStyle}"/>
</ResourceDictionary>
