                    BorderBrush="{TemplateBinding BorderBrush}"
                    BorderThickness="{TemplateBinding BorderThickness}"
                    Padding="{TemplateBinding Padding}">
                <telerik:RadDockPanel x:Name="RootDockPanel">
                    <dock:AutoHideArea x:Name="LeftAutoHide" telerik:RadDockPanel.Dock="Left" TabStripPlacement="Left" BackgroundVisibility="Collapsed" TabOrientation="Vertical"/>
                    <dock:AutoHideArea x:Name="RightAutoHide" telerik:RadDockPanel.Dock="Right" TabStripPlacement="Right" BackgroundVisibility="Collapsed" TabOrientation="Vertical"/>
                    <dock:AutoHideArea x:Name="TopAutoHide" telerik:RadDockPanel.Dock="Top" TabStripPlacement="Top" BackgroundVisibility="Collapsed" TabOrientation="Horizontal"/>
                    <dock:AutoHideArea x:Name="BottomAutoHide" telerik:RadDockPanel.Dock="Bottom" TabStripPlacement="Bottom" BackgroundVisibility="Collapsed" TabOrientation="Horizontal"/>
                    <local:DockingPanel x:Name="ContentDockPanel" Margin="0">
                        <dock:DocumentHost x:Name="DocumentHost" IsTabStop="{TemplateBinding IsTabStop}"/>
                    </local:DockingPanel>
                </telerik:RadDockPanel>
            </Border>
        </Grid>
    </ControlTemplate>
    <animation:AnimationSelector x:Key="RadDockingAnimationSelector">
        <animation:AnimationGroup AnimationName="Move">
            <animation:MoveAnimation/>
            <animation:ResizeAnimation/>
            <animation:FadeAnimation Direction="In"/>
            <animation:ScaleAnimation Direction="In" MinScale="0.7" MaxScale="1.0"/>
        </animation:AnimationGroup>
        <animation:AnimationGroup AnimationName="Show">
            <animation:FadeAnimation Direction="In"/>
            <animation:ScaleAnimation Direction="In" MinScale="0.95" MaxScale="1.0"/>
        </animation:AnimationGroup>
        <animation:AnimationGroup AnimationName="Hide" SpeedRatio="0.7">
            <animation:FadeAnimation Direction="Out"/>
            <animation:ScaleAnimation Direction="Out" MinScale="0.7" MaxScale="1.0"/>
        </animation:AnimationGroup>
        <animation:AnimationGroup AnimationName="LeftIn">
            <animation:SlideAnimation SlideMode="Top" Duration="{StaticResource AutoHideInAnimationDuration}" TargetElementName="PopupChild" Orientation="Horizontal" Direction="In"/>
        </animation:AnimationGroup>
        <animation:AnimationGroup AnimationName="LeftOut">
            <animation:SlideAnimation TargetElementName="PopupChild" SlideMode="Top" Duration="{StaticResource AutoHideOutAnimationDuration}" Orientation="Horizontal" Direction="Out"/>
        </animation:AnimationGroup>
        <animation:AnimationGroup AnimationName="TopIn">
            <animation:SlideAnimation Orientation="Vertical" Duration="{StaticResource AutoHideInAnimationDuration}" TargetElementName="PopupChild" Direction="In"/>
        </animation:AnimationGroup>
        <animation:AnimationGroup AnimationName="TopOut">
            <animation:SlideAnimation TargetElementName="PopupChild" Duration="{StaticResource AutoHideOutAnimationDuration}" Orientation="Vertical" Direction="Out"/>
        </animation:AnimationGroup>
        <animation:AnimationGroup AnimationName="RightIn">
            <animation:SlideAnimation SlideMode="Bottom" TargetElementName="PopupChild" Duration="{StaticResource AutoHideInAnimationDuration}" Orientation="Horizontal" Direction="In"/>
        </animation:AnimationGroup>
        <animation:AnimationGroup AnimationName="RightOut">
            <animation:SlideAnimation TargetElementName="PopupChild" SlideMode="Bottom" Duration="{StaticResource AutoHideOutAnimationDuration}" Orientation="Horizontal" Direction="Out"/>
        </animation:AnimationGroup>
        <animation:AnimationGroup AnimationName="BottomIn">
            <animation:SlideAnimation SlideMode="Bottom" Duration="{StaticResource AutoHideInAnimationDuration}" Orientation="Vertical" TargetElementName="PopupChild" Direction="In"/>
        </animation:AnimationGroup>
        <animation:AnimationGroup AnimationName="BottomOut">
            <animation:SlideAnimation Duration="{StaticResource AutoHideOutAnimationDuration}" TargetElementName="PopupChild" Direction="Out" Orientation="Vertical" SlideMode="Bottom"/>
        </animation:AnimationGroup>
    </animation:AnimationSelector>
    <Style x:Key="RadDockingStyle" TargetType="local:RadDocking">
        <Setter Property="Foreground" Value="{telerik:MaterialResource ResourceKey=MarkerBrush}"/>
        <Setter Property="Background" Value="{telerik:MaterialResource ResourceKey=PrimaryBrush}"/>
        <Setter Property="BorderBrush" Value="{telerik:MaterialResource ResourceKey=DividerBrush}"/>
        <Setter Property="BorderThickness" Value="0"/>
        <Setter Property="Padding" Value="10"/>
        <Setter Property="VisualCueStyle" Value="{StaticResource VisualCueStyle}"/>
        <Setter Property="Template" Value="{StaticResource RadDockingControlTemplate}"/>
        <Setter Property="CompassStyle" Value="{StaticResource CompassStyle}"/>
        <Setter Property="RootCompassStyle" Value="{StaticResource RootCompassStyle}"/>
        <Setter Property="animation:AnimationManager.AnimationSelector" Value="{StaticResource RadDockingAnimationSelector}"/>
    </Style>
    <Style TargetType="dock:AutoHideArea" BasedOn="{StaticResource AutoHideAreaStyle}"/>
    <Style TargetType="local:RadPane" BasedOn="{StaticResource RadPaneStyle}"/>
    <Style TargetType="local:RadDocumentPane" BasedOn="{StaticResource RadPaneStyle}"/>
    <Style TargetType="dock:RadGridResizer" BasedOn="{StaticResource RadGridResizerStyle}"/>
    <Style TargetType="local:RadSplitContainer" BasedOn="{StaticResource RadSplitContainerStyle}"/>
    <Style TargetType="dock:PaneHeader" BasedOn="{StaticResource PaneHeaderStyle}"/>
    <Style TargetType="local:RadPaneGroup" BasedOn="{StaticResource RadPaneGroupStyle}"/>
    <Style TargetType="dock:ToolWindow" BasedOn="{StaticResource ToolWindowStyle}"/>
    <Style TargetType="dock:Compass" BasedOn="{StaticResource CompassStyle}"/>
    <Style TargetType="dock:RootCompass" BasedOn="{StaticResource RootCompassStyle}"/>
    <Style TargetType="dock:VisualCue" BasedOn="{StaticResource VisualCueStyle}"/>
    <Style TargetType="dock:DocumentHost" BasedOn="{StaticResource DocumentHostStyle}"/>
    <Style TargetType="local:RadDocking" BasedOn="{StaticResource RadDockingStyle}"/>
    <MenuScrollingVisibilityConverter x:Key="MenuScrollingVisibilityConverter"/>
    <telerik:NullToVisibilityConverter x:Key="NullToVisibilityConverter"/>
    <Style x:Key="DockingNavigator_ListBoxScrollButtonStyle" TargetType="RepeatButton" BasedOn="{StaticResource RepeatButtonStyle}">
        <Setter Property="mat:MaterialAssist.IsShadowDisabled" Value="True"/>
        <Setter Property="helpers:ThemeHelper.CornerRadius" Value="0"/>
        <Setter Property="Foreground" Value="{telerik:MaterialResource ResourceKey=IconBrush}"/>
        <Setter Property="Background" Value="{telerik:MaterialResource ResourceKey=MainBrush}"/>
        <Setter Property="BorderBrush" Value="{telerik:MaterialResource ResourceKey=DividerBrush}"/>
        <Setter Property="BorderThickness" Value="0"/>
        <Setter Property="Interval" Value="50"/>
        <Setter Property="IsTabStop" Value="False"/>
        <Setter Property="MinWidth" Value="0"/>
        <Setter Property="MinHeight" Value="0"/>
        <Setter Property="Height" Value="36"/>
        <Setter Property="Focusable" Value="False"/>
    </Style>
    <Style x:Key="DockingNavigator_ListBoxScrollViewerStyle" TargetType="{x:Type ScrollViewer}">
        <Setter Property="Padding" Value="0"/>
        <Setter Property="HorizontalScrollBarVisibility" Value="Hidden"/>
        <Setter Property="VerticalScrollBarVisibility" Value="Auto"/>
        <Setter Property="CanContentScroll" Value="True"/>
        <Setter Property="Template">
            <Setter.Value>
                <ControlTemplate TargetType="{x:Type ScrollViewer}">
                    <Grid SnapsToDevicePixels="true">
                        <Grid.RowDefinitions>
                            <RowDefinition Height="Auto"/>
                            <RowDefinition Height="*"/>
                            <RowDefinition Height="Auto"/>
                        </Grid.RowDefinitions>
                        <RepeatButton
                                Command="{x:Static ScrollBar.LineUpCommand}"
                                VerticalAlignment="Top"
                                Style="{StaticResource DockingNavigator_ListBoxScrollButtonStyle}"
                                CommandTarget="{Binding RelativeSource={RelativeSource TemplatedParent}}">
                            <RepeatButton.Content>
                                <telerik:RadGlyph HorizontalAlignment="Center" VerticalAlignment="Center" FontSize="16" Glyph="{StaticResource GlyphArrow60Up}"/>
                            </RepeatButton.Content>
                            <RepeatButton.Visibility>
                                <MultiBinding FallbackValue="Visibility.Collapsed" Converter="{StaticResource MenuScrollingVisibilityConverter}" ConverterParameter="0">
                                    <Binding RelativeSource="{RelativeSource TemplatedParent}" Path="ComputedVerticalScrollBarVisibility"/>
                                    <Binding RelativeSource="{RelativeSource TemplatedParent}" Path="VerticalOffset"/>
                                    <Binding RelativeSource="{RelativeSource TemplatedParent}" Path="ExtentHeight"/>
                                    <Binding RelativeSource="{RelativeSource TemplatedParent}" Path="ViewportHeight"/>
                                </MultiBinding>
                            </RepeatButton.Visibility>
                        </RepeatButton>
                        <RepeatButton
                                Grid.Row="2"
                                VerticalAlignment="Bottom"
                                Style="{StaticResource DockingNavigator_ListBoxScrollButtonStyle}"
                                Command="{x:Static ScrollBar.LineDownCommand}"
                                CommandTarget="{Binding RelativeSource={RelativeSource TemplatedParent}}">
                            <RepeatButton.Content>
                                <telerik:RadGlyph HorizontalAlignment="Center" VerticalAlignment="Center" FontSize="16" Glyph="{StaticResource GlyphArrow60Down}"/>
                            </RepeatButton.Content>
                            <RepeatButton.Visibility>
                                <MultiBinding FallbackValue="Visibility.Collapsed" Converter="{StaticResource MenuScrollingVisibilityConverter}" ConverterParameter="100">
                                    <Binding RelativeSource="{RelativeSource TemplatedParent}" Path="ComputedVerticalScrollBarVisibility"/>
                                    <Binding RelativeSource="{RelativeSource TemplatedParent}" Path="VerticalOffset"/>
                                    <Binding RelativeSource="{RelativeSource TemplatedParent}" Path="ExtentHeight"/>
                                    <Binding RelativeSource="{RelativeSource TemplatedParent}" Path="ViewportHeight"/>
                                </MultiBinding>
                            </RepeatButton.Visibility>
                        </RepeatButton>
                        <ScrollContentPresenter x:Name="PART_ScrollContentPresenter" Grid.Row="1" Margin="{TemplateBinding Padding}" CanContentScroll="{TemplateBinding CanContentScroll}"/>
                    </Grid>
                </ControlTemplate>
            </Setter.Value>
        </Setter>
        <Setter Property="PanningMode" Value="None"/>
    </Style>
    <Style x:Key="DockingNavigatorListBoxStyle" TargetType="dock:DockingNavigatorListBox" BasedOn="{StaticResource RadListBoxStyle}">
        <Setter Property="mat:MaterialAssist.ShadowDepth" Value="Depth1"/>
        <Setter Property="helpers:ThemeHelper.CornerRadius" Value="0"/>
        <Setter Property="telerik:ScrollingSettingsBehavior.IsEnabled" Value="True"/>
        <Setter Property="telerik:ScrollingSettingsBehavior.ScrollAreaPadding" Value="30"/>
        <Setter Property="telerik:ScrollingSettingsBehavior.ScrollStep" Value="28"/>
        <Setter Property="telerik:ScrollingSettingsBehavior.ScrollStepTime" Value="0:0:0.05"/>
        <Setter Property="ScrollViewer.VerticalScrollBarVisibility" Value="Auto"/>
        <Setter Property="ScrollViewer.HorizontalScrollBarVisibility" Value="Disabled"/>
        <Setter Property="ScrollViewer.CanContentScroll" Value="True"/>
        <Setter Property="KeyboardNavigation.ControlTabNavigation" Value="Cycle"/>
        <Setter Property="KeyboardNavigation.TabNavigation" Value="Cycle"/>
        <Setter Property="Width" Value="190"/>
        <Setter Property="Height" Value="251"/>
        <Setter Property="Margin" Value="0 0 20 0"/>
        <Setter Property="HorizontalAlignment" Value="Left"/>
        <Setter Property="VerticalAlignment" Value="Stretch"/>
        <Setter Property="Template">
            <Setter.Value>
                <ControlTemplate TargetType="dock:DockingNavigatorListBox">
                    <Grid UseLayoutRounding="True">
                        <mat:Shadow x:Name="Shadow"
                                ShadowDepth="{Binding RelativeSource={RelativeSource TemplatedParent}, Path=(mat:MaterialAssist.ShadowDepth)}"
                                CornerRadius="{Binding RelativeSource={RelativeSource TemplatedParent}, Path=(helpers:ThemeHelper.CornerRadius)}"
                                Background="{TemplateBinding Background}"/>
                        <Border x:Name="Border"
                                BorderBrush="{TemplateBinding BorderBrush}"
                                BorderThickness="{TemplateBinding BorderThickness}"
                                Background="{TemplateBinding Background}"
                                CornerRadius="{TemplateBinding helpers:ThemeHelper.CornerRadius}">
                            <ScrollViewer x:Name="PART_ScrollViewer"
                                    IsTabStop="False"
                                    Style="{StaticResource DockingNavigator_ListBoxScrollViewerStyle}"
                                    HorizontalScrollBarVisibility="{TemplateBinding ScrollViewer.HorizontalScrollBarVisibility}"
                                    VerticalScrollBarVisibility="{TemplateBinding ScrollViewer.VerticalScrollBarVisibility}">
                                <ItemsPresenter ClipToBounds="True"/>
                            </ScrollViewer>
                        </Border>
                        <ContentPresenter x:Name="dragVisualPlaceholder" HorizontalAlignment="Stretch" Visibility="Collapsed" VerticalAlignment="Stretch"/>
                    </Grid>
                </ControlTemplate>
            </Setter.Value>
        </Setter>
    </Style>
    <DataTemplate x:Key="DockingNavigatorItemTemplate">
        <Grid>
            <Grid.ColumnDefinitions>
                <ColumnDefinition Width="Auto"/>
                <ColumnDefinition Width="*"/>
            </Grid.ColumnDefinitions>
            <telerik:RadGlyph Glyph="{Binding Icon}" FontSize="16" Margin="0 0 5 0" VerticalAlignment="Center" Visibility="{Binding Icon, Converter={StaticResource NullToVisibilityConverter}}"/>
            <TextBlock Grid.Column="1" Text="{Binding Header}" VerticalAlignment="Center" TextTrimming="CharacterEllipsis"/>
        </Grid>
    </DataTemplate>
    <local:DockingNavigatorItemTemplateSelector x:Key="DefaultDockingNavigatorItemTemplateSelector" ActivePaneTemplate="{StaticResource DockingNavigatorItemTemplate}" ActiveDocumentTemplate="{StaticResource DockingNavigatorItemTemplate}"/>
    <ControlTemplate x:Key="DockingNavigatorTemplate" TargetType="local:DockingNavigator">
        <Grid x:Name="LayoutRoot" Margin="5">
            <Grid.RowDefinitions>
                <RowDefinition Height="Auto"/>
                <RowDefinition Height="*"/>
                <RowDefinition Height="Auto"/>
            </Grid.RowDefinitions>
            <mat:Shadow x:Name="Shadow"
                    Grid.RowSpan="3"
                    ShadowDepth="{Binding RelativeSource={RelativeSource TemplatedParent}, Path=(mat:MaterialAssist.ShadowDepth)}"
                    CornerRadius="{Binding RelativeSource={RelativeSource TemplatedParent}, Path=(helpers:ThemeHelper.CornerRadius)}"
                    Background="{TemplateBinding Background}"/>
            <Border x:Name="BorderVisual"
                    Grid.RowSpan="3"
                    Background="{TemplateBinding Background}"
                    BorderBrush="{TemplateBinding BorderBrush}"
                    BorderThickness="{TemplateBinding BorderThickness}"
                    CornerRadius="{Binding RelativeSource={RelativeSource TemplatedParent}, Path=(helpers:ThemeHelper.CornerRadius)}"/>
            <StackPanel Orientation="Vertical">
                <Grid MinHeight="36">
                    <Border x:Name="DockingNavigatorHeader"
                            Background="Transparent"
                            BorderBrush="Transparent"
                            BorderThickness="{TemplateBinding BorderThickness}"
                            CornerRadius="{Binding RelativeSource={RelativeSource TemplatedParent}, Path=(helpers:ThemeHelper.CornerRadius), Converter={StaticResource CornerRadiusConverter}, ConverterParameter=TopLeftRight}"/>
                    <Grid VerticalAlignment="Center" Margin="10 0">
                        <Grid.ColumnDefinitions>
                            <ColumnDefinition Width="Auto"/>
                            <ColumnDefinition Width="*"/>
                        </Grid.ColumnDefinitions>
                        <ContentPresenter x:Name="PART_HeaderIcon"
                                Content="{Binding SelectedModel.Icon}"
                                ContentTemplate="{TemplateBinding IconTemplate}"
                                ContentTemplateSelector="{TemplateBinding IconTemplateSelector}"/>
                        <ContentPresenter x:Name="PART_Header"
                                Grid.Column="1"
                                Content="{Binding SelectedModel.Header}"
                                ContentStringFormat="{TemplateBinding HeaderStringFormat}"
                                ContentTemplate="{TemplateBinding HeaderTemplate}"
                                ContentTemplateSelector="{TemplateBinding HeaderTemplateSelector}"
                                TextBlock.Foreground="{telerik:MaterialResource ResourceKey=MarkerBrush}"
                                TextBlock.FontSize="{telerik:MaterialResource ResourceKey=FontSize}"/>
                    </Grid>
                </Grid>
                <ContentPresenter
                        Content="{Binding SelectedModel.Description}"
                        ContentStringFormat="{TemplateBinding DescriptionStringFormat}"
                        ContentTemplate="{TemplateBinding DescriptionTemplate}"
                        ContentTemplateSelector="{TemplateBinding DescriptionTemplateSelector}"
                        VerticalAlignment="Top"
                        Margin="10 0"
                        MinHeight="16"
                        TextBlock.FontSize="{telerik:MaterialResource ResourceKey=FontSizeS}"/>
            </StackPanel>
            <Grid x:Name="ContentGrid" Grid.Row="1" Margin="{TemplateBinding Padding}" VerticalAlignment="Top">
                <Grid.RowDefinitions>
                    <RowDefinition Height="Auto"/>
                    <RowDefinition Height="*"/>
                </Grid.RowDefinitions>
                <Grid.ColumnDefinitions>
                    <ColumnDefinition Width="Auto"/>
                    <ColumnDefinition Width="Auto"/>
                    <ColumnDefinition Width="Auto"/>
                </Grid.ColumnDefinitions>
                <TextBlock
                        Text="{TemplateBinding ActivePanesListBoxHeader}"
                        Foreground="{telerik:MaterialResource ResourceKey=PrimaryNormalBrush}"
                        FontSize="{telerik:MaterialResource ResourceKey=FontSize}"
                        Margin="0 0 0 10"/>
                <dock:DockingNavigatorListBox x:Name="PART_ActivePanesList"
                        Grid.Row="1"
                        AutomationProperties.Name="ActivePanesListBox"
                        ItemsSource="{Binding ActivePanesModels}"
                        DisplayMemberPath="Header"
                        KeyboardNavigation.TabNavigation="Cycle"
                        SelectedItem="{Binding SelectedModel, Mode=TwoWay}"
                        Style="{TemplateBinding ActivePanesListBoxStyle}"/>
                <TextBlock
                        Grid.Column="1"
                        Text="{TemplateBinding ActiveDocumentsListBoxHeader}"
                        Foreground="{telerik:MaterialResource ResourceKey=PrimaryNormalBrush}"
                        FontSize="{telerik:MaterialResource ResourceKey=FontSize}"
                        Margin="0 0 0 10"/>
                <dock:DockingNavigatorListBox x:Name="PART_ActiveDocumentsList"
                        Grid.Column="1"
                        Grid.Row="1"
                        AutomationProperties.Name="ActiveDocumentsListBox"
                        ItemsSource="{Binding ActiveDocumentsModels}"
                        DisplayMemberPath="Header"
                        KeyboardNavigation.TabNavigation="Cycle"
                        SelectedItem="{Binding SelectedModel, Mode=TwoWay}"
                        Style="{TemplateBinding ActiveDocumentsListBoxStyle}"/>
                <TextBlock Grid.Column="4" Text="{TemplateBinding PreviewHeader}" FontSize="{telerik:MaterialResource ResourceKey=FontSize}" Margin="0 0 0 10"/>
                <Grid x:Name="PreviewGrid"
                        Grid.Column="3"
                        Grid.Row="1"
                        Width="{TemplateBinding PreviewWidth}"
                        Height="{TemplateBinding PreviewHeight}"
                        Margin="0"
                        HorizontalAlignment="Left"
                        VerticalAlignment="Top">
                    <mat:Shadow ShadowDepth="Depth1" Background="{TemplateBinding Background}"/>
                    <Border Background="{telerik:MaterialResource ResourceKey=MainBrush}" BorderBrush="{x:Null}" BorderThickness="1" SnapsToDevicePixels="True">
                        <Rectangle x:Name="PART_Preview" SnapsToDevicePixels="True" ClipToBounds="True" Margin="5"/>
                    </Border>
                </Grid>
            </Grid>
            <Grid Grid.Row="3" Margin="10 0 10 10" VerticalAlignment="Bottom">
                <ContentPresenter
                        Content="{Binding SelectedModel.Footer}"
                        ContentStringFormat="{TemplateBinding FooterStringFormat}"
                        ContentTemplate="{TemplateBinding FooterTemplate}"
                        ContentTemplateSelector="{TemplateBinding FooterTemplateSelector}"
                        TextBlock.FontSize="{TemplateBinding FontSize}"
                        MinHeight="30">
                    <ContentPresenter.Resources>
                        <Style TargetType="TextBlock">
                            <Setter Property="VerticalAlignment" Value="Bottom"/>
                            <Setter Property="HorizontalAlignment" Value="Left"/>
                        </Style>
                    </ContentPresenter.Resources>
                </ContentPresenter>
            </Grid>
        </Grid>
    </ControlTemplate>
    <Style x:Key="DockingNavigatorStyle" TargetType="local:DockingNavigator">
        <Setter Property="mat:MaterialAssist.ShadowDepth" Value="Depth1"/>
        <Setter Property="helpers:ThemeHelper.CornerRadius" Value="{telerik:MaterialResource ResourceKey=CornerRadius}"/>
        <Setter Property="FontFamily" Value="{telerik:MaterialResource ResourceKey=FontFamily}"/>
        <Setter Property="FontSize" Value="{telerik:MaterialResource ResourceKey=FontSize}"/>
        <Setter Property="Foreground" Value="{telerik:MaterialResource ResourceKey=MarkerBrush}"/>
        <Setter Property="Background" Value="{telerik:MaterialResource ResourceKey=MainBrush}"/>
        <Setter Property="BorderBrush" Value="{x:Null}"/>
        <Setter Property="BorderThickness" Value="1"/>
        <Setter Property="HorizontalAlignment" Value="Left"/>
        <Setter Property="VerticalAlignment" Value="Top"/>
        <Setter Property="PreviewWidth" Value="300"/>
        <Setter Property="PreviewHeight" Value="251"/>
        <Setter Property="Padding" Value="10 15 10 10"/>
        <Setter Property="ActivePanesListBoxHeader" Value="{Binding Manager.Culture, Source={StaticResource localizationManagerProvider}, Converter={StaticResource cultureToStringConverter}, ConverterParameter=Docking_ActivePanes}"/>
        <Setter Property="ActiveDocumentsListBoxHeader" Value="{Binding Manager.Culture, Source={StaticResource localizationManagerProvider}, Converter={StaticResource cultureToStringConverter}, ConverterParameter=Docking_ActiveDocuments}"/>
        <Setter Property="PreviewHeader" Value="{Binding Manager.Culture, Source={StaticResource localizationManagerProvider}, Converter={StaticResource cultureToStringConverter}, ConverterParameter=Docking_PreviewHeader}"/>
        <Setter Property="Focusable" Value="False"/>
        <Setter Property="ActivePanesListBoxStyle" Value="{StaticResource DockingNavigatorListBoxStyle}"/>
        <Setter Property="ActiveDocumentsListBoxStyle" Value="{StaticResource DockingNavigatorListBoxStyle}"/>
        <Setter Property="ItemTemplateSelector" Value="{StaticResource DefaultDockingNavigatorItemTemplateSelector}"/>
        <Setter Property="Template" Value="{StaticResource DockingNavigatorTemplate}"/>
    </Style>
    <Style TargetType="dock:DockingNavigatorListBoxItem" BasedOn="{StaticResource RadListBoxItemStyle}"/>
    <Style TargetType="dock:DockingNavigatorListBox" BasedOn="{StaticResource DockingNavigatorListBoxStyle}"/>
    <Style TargetType="local:DockingNavigator" BasedOn="{StaticResource DockingNavigatorStyle}"/>
</ResourceDictionary>
