    <ControlTemplate x:Key="PaneHeaderControlTemplate" TargetType="dock:PaneHeader">
        <Border x:Name="LayoutRoot" Background="{TemplateBinding Background}" BorderThickness="{TemplateBinding BorderThickness}" BorderBrush="{TemplateBinding BorderBrush}">
            <Grid x:Name="Content">
                <Grid.ColumnDefinitions>
                    <ColumnDefinition Width="*"/>
                    <ColumnDefinition Width="Auto"/>
                    <ColumnDefinition Width="Auto"/>
                    <ColumnDefinition Width="Auto"/>
                    <ColumnDefinition Width="0"/>
                </Grid.ColumnDefinitions>
                <ContentControl x:Name="TitleElement"
                        VerticalAlignment="{TemplateBinding VerticalAlignment}"
                        FontFamily="{TemplateBinding FontFamily}"
                        FontSize="{TemplateBinding FontSize}"
                        Foreground="{TemplateBinding Foreground}"
                        Margin="{TemplateBinding Padding}"
                        Content="{Binding SelectedPane.Title, RelativeSource={RelativeSource TemplatedParent}}"
                        ContentTemplate="{Binding SelectedPane.TitleTemplate, RelativeSource={RelativeSource TemplatedParent}}"
                        HorizontalContentAlignment="Stretch"
                        IsTabStop="False"/>
                <telerik:RadDropDownButton x:Name="HeaderDropDownMenu"
                        Grid.Column="1"
                        Command="local:RadDockingCommands.ContextMenuOpen"
                        Style="{StaticResource DropDownButtonDropDownStyle}"
                        CommandParameter="{TemplateBinding SelectedPane}"
                        IsTabStop="{TemplateBinding IsTabStop}"
                        Margin="0"/>
                <Grid Grid.Column="2" Visibility="{Binding SelectedPane.PinButtonVisibility, RelativeSource={RelativeSource TemplatedParent}}" MinWidth="48" Margin="0">
                    <telerik:RadButton x:Name="HeaderPinButton"
                            Command="local:RadDockingCommands.Pin"
                            CommandParameter="{TemplateBinding SelectedPane}"
                            Style="{StaticResource RadButtonPinStyle}"
                            IsTabStop="{TemplateBinding IsTabStop}"/>
                    <telerik:RadButton x:Name="HeaderUnPinButton"
                            Visibility="Collapsed"
                            Command="local:RadDockingCommands.Pin"
                            CommandParameter="{TemplateBinding SelectedPane}"
                            Style="{StaticResource RadButtonUnpinStyle}"
                            IsTabStop="{TemplateBinding IsTabStop}"/>
                </Grid>
                <telerik:RadButton x:Name="HeaderCloseButton"
                        Grid.Column="3"
                        Command="local:RadDockingCommands.Close"
                        CommandParameter="{TemplateBinding SelectedPane}"
                        Style="{StaticResource RadButtonCloseStyle}"
                        IsTabStop="{TemplateBinding IsTabStop}"
                        VerticalAlignment="Center"
                        Margin="0"/>
            </Grid>
        </Border>
        <ControlTemplate.Triggers>
            <DataTrigger Binding="{Binding SelectedPane.IsPinned, RelativeSource={RelativeSource TemplatedParent}}" Value="False">
                <Setter TargetName="HeaderUnPinButton" Property="Visibility" Value="Visible"/>
                <Setter TargetName="HeaderPinButton" Property="Visibility" Value="Collapsed"/>
            </DataTrigger>
            <DataTrigger Binding="{Binding Path=SelectedPane.(telerikNavigation:RadContextMenu.ContextMenu).HasItems, RelativeSource={RelativeSource TemplatedParent}}" Value="False">
                <Setter TargetName="HeaderDropDownMenu" Property="Visibility" Value="Collapsed"/>
            </DataTrigger>
            <DataTrigger Binding="{Binding Path=SelectedPane.ContextMenuTemplate, RelativeSource={RelativeSource TemplatedParent}}" Value="{x:Null}">
                <Setter TargetName="HeaderDropDownMenu" Property="Visibility" Value="Collapsed"/>
            </DataTrigger>
            <Trigger Property="IsHighlighted" Value="True">
                <Setter TargetName="LayoutRoot" Property="Background" Value="{telerik:MaterialResource ResourceKey=AccentNormalBrush}"/>
                <Setter TargetName="HeaderPinButton" Property="Foreground" Value="{telerik:MaterialResource ResourceKey=IconBrush}"/>
                <Setter TargetName="HeaderUnPinButton" Property="Foreground" Value="{telerik:MaterialResource ResourceKey=IconBrush}"/>
                <Setter TargetName="HeaderCloseButton" Property="Foreground" Value="{telerik:MaterialResource ResourceKey=IconBrush}"/>
                <Setter TargetName="HeaderDropDownMenu" Property="Foreground" Value="{telerik:MaterialResource ResourceKey=IconBrush}"/>
                <Setter TargetName="TitleElement" Property="Foreground" Value="{telerik:MaterialResource ResourceKey=MarkerBrush}"/>
                <Setter TargetName="HeaderPinButton" Property="mat:MaterialAssist.MouseOverBrush" Value="{telerik:MaterialResource ResourceKey=AccentHoverBrush}"/>
                <Setter TargetName="HeaderPinButton" Property="mat:MaterialAssist.PressedBrush" Value="{telerik:MaterialResource ResourceKey=AccentPressedBrush}"/>
                <Setter TargetName="HeaderUnPinButton" Property="mat:MaterialAssist.MouseOverBrush" Value="{telerik:MaterialResource ResourceKey=AccentHoverBrush}"/>
                <Setter TargetName="HeaderUnPinButton" Property="mat:MaterialAssist.PressedBrush" Value="{telerik:MaterialResource ResourceKey=AccentPressedBrush}"/>
                <Setter TargetName="HeaderCloseButton" Property="mat:MaterialAssist.MouseOverBrush" Value="{telerik:MaterialResource ResourceKey=AccentHoverBrush}"/>
                <Setter TargetName="HeaderCloseButton" Property="mat:MaterialAssist.PressedBrush" Value="{telerik:MaterialResource ResourceKey=AccentPressedBrush}"/>
                <Setter TargetName="HeaderDropDownMenu" Property="mat:MaterialAssist.MouseOverBrush" Value="{telerik:MaterialResource ResourceKey=AccentHoverBrush}"/>
                <Setter TargetName="HeaderDropDownMenu" Property="mat:MaterialAssist.PressedBrush" Value="{telerik:MaterialResource ResourceKey=AccentPressedBrush}"/>
            </Trigger>
            <Trigger Property="IsEnabled" Value="False">
                <Setter TargetName="TitleElement" Property="Opacity" Value="{telerik:MaterialResource ResourceKey=DisabledOpacity}"/>
            </Trigger>
        </ControlTemplate.Triggers>
    </ControlTemplate>
