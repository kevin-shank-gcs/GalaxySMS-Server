    <Style x:Key="RadGridResizerStyle" TargetType="dock:RadGridResizer">
        <Setter Property="Background" Value="Transparent"/>
        <Setter Property="BorderBrush" Value="{x:Null}"/>
        <Setter Property="BorderThickness" Value="0"/>
        <Setter Property="HorizontalAlignment" Value="Stretch"/>
        <Setter Property="VerticalAlignment" Value="Stretch"/>
        <Setter Property="IsTabStop" Value="True"/>
        <Setter Property="MinWidth" Value="2"/>
        <Setter Property="MinHeight" Value="2"/>
        <Setter Property="PreviewStyle" Value="{StaticResource GridResizerPreviewStyle}"/>
        <Setter Property="Template" Value="{StaticResource RadGridResizerControlTemplate}"/>
        <Setter Property="Panel.ZIndex" Value="100"/>
        <Style.Triggers>
            <Trigger Property="Placement" Value="Top">
                <Setter Property="Cursor" Value="SizeNS"/>
            </Trigger>
            <Trigger Property="Placement" Value="Right">
                <Setter Property="Cursor" Value="SizeWE"/>
            </Trigger>
            <Trigger Property="Placement" Value="Bottom">
                <Setter Property="Cursor" Value="SizeNS"/>
            </Trigger>
            <Trigger Property="Placement" Value="Left">
                <Setter Property="Cursor" Value="SizeWE"/>
            </Trigger>
        </Style.Triggers>
    </Style>
    <Style x:Key="RadSplitContainerStyle" TargetType="local:RadSplitContainer">
        <Setter Property="Template">
            <Setter.Value>
                <ControlTemplate TargetType="local:RadSplitContainer">
                    <Grid>
                        <Border x:Name="BorderVisual"
                                Background="{TemplateBinding Background}"
                                BorderBrush="{TemplateBinding BorderBrush}"
                                BorderThickness="{TemplateBinding BorderThickness}"
                                Padding="{TemplateBinding Padding}">
                            <ItemsPresenter/>
                        </Border>
                        <dock:RadGridResizer x:Name="DockResizer" Placement="Left" ShowsPreview="True" Visibility="Collapsed" VerticalAlignment="Stretch" HorizontalAlignment="Left"/>
                    </Grid>
                    <ControlTemplate.Triggers>
                        <Trigger Property="SplitterPosition">
                            <Trigger.Value>
                                <Dock>Left</Dock>
                            </Trigger.Value>
                            <Setter TargetName="DockResizer" Property="Visibility" Value="Visible"/>
                            <Setter TargetName="DockResizer" Property="Width" Value="10"/>
                            <Setter TargetName="BorderVisual" Property="Margin" Value="10 0 0 0"/>
                            <Setter TargetName="DockResizer" Property="Cursor" Value="SizeWE"/>
                        </Trigger>
                        <Trigger Property="SplitterPosition">
                            <Trigger.Value>
                                <Dock>Top</Dock>
                            </Trigger.Value>
                            <Setter TargetName="DockResizer" Property="Visibility" Value="Visible"/>
                            <Setter TargetName="DockResizer" Property="VerticalAlignment" Value="Top"/>
                            <Setter TargetName="DockResizer" Property="HorizontalAlignment" Value="Stretch"/>
                            <Setter TargetName="DockResizer" Property="Cursor" Value="SizeNS"/>
                            <Setter TargetName="DockResizer" Property="Placement">
                                <Setter.Value>
                                    <Dock>Top</Dock>
                                </Setter.Value>
                            </Setter>
                            <Setter TargetName="DockResizer" Property="Height" Value="10"/>
                            <Setter TargetName="BorderVisual" Property="Margin" Value="0 10 0 0"/>
                        </Trigger>
                        <Trigger Property="SplitterPosition">
                            <Trigger.Value>
                                <Dock>Right</Dock>
                            </Trigger.Value>
                            <Setter TargetName="DockResizer" Property="Visibility" Value="Visible"/>
                            <Setter TargetName="DockResizer" Property="Width" Value="10"/>
                            <Setter TargetName="BorderVisual" Property="Margin" Value="0 0 10 0"/>
                            <Setter TargetName="DockResizer" Property="HorizontalAlignment" Value="Right"/>
                            <Setter TargetName="DockResizer" Property="Cursor" Value="SizeWE"/>
                            <Setter TargetName="DockResizer" Property="Placement">
                                <Setter.Value>
                                    <Dock>Right</Dock>
                                </Setter.Value>
                            </Setter>
                        </Trigger>
                        <Trigger Property="SplitterPosition">
                            <Trigger.Value>
                                <Dock>Bottom</Dock>
                            </Trigger.Value>
                            <Setter TargetName="DockResizer" Property="Cursor" Value="SizeNS"/>
                            <Setter TargetName="DockResizer" Property="Visibility" Value="Visible"/>
                            <Setter TargetName="DockResizer" Property="VerticalAlignment" Value="Bottom"/>
                            <Setter TargetName="DockResizer" Property="HorizontalAlignment" Value="Stretch"/>
                            <Setter TargetName="DockResizer" Property="Height" Value="10"/>
                            <Setter TargetName="DockResizer" Property="Placement">
                                <Setter.Value>
                                    <Dock>Bottom</Dock>
                                </Setter.Value>
                            </Setter>
                            <Setter TargetName="BorderVisual" Property="Margin" Value="0 0 0 10"/>
                        </Trigger>
                    </ControlTemplate.Triggers>
                </ControlTemplate>
            </Setter.Value>
        </Setter>
