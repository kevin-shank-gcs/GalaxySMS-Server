                <Setter TargetName="HeaderElement" Property="helpers:ThemeHelper.CheckedBrush" Value="{Binding RelativeSource={RelativeSource TemplatedParent}, Path=(helpers:ThemeHelper.CheckedBrush)}"/>
            </Trigger>
