        <Setter Property="VerticalContentAlignment" Value="Center"/>
        <Setter Property="Template" Value="{StaticResource RadPaneTemplate}"/>
        <Setter Property="LeftTemplate" Value="{StaticResource RadPaneTemplate}"/>
        <Setter Property="TopTemplate" Value="{StaticResource RadPaneTemplate}"/>
        <Setter Property="RightTemplate" Value="{StaticResource RadPaneTemplate}"/>
        <Setter Property="BottomTemplate" Value="{StaticResource RadPaneTemplate}"/>
        <Setter Property="DocumentHostTemplate" Value="{StaticResource RadPaneDocumentHostTemplate}"/>
        <Setter Property="ContextMenuTemplate" Value="{StaticResource RadPaneContextMenuTemplate}"/>
        <Style.Triggers>
            <Trigger Property="IsInDocumentHost" Value="True">
                <Setter Property="Padding" Value="5 0"/>
                <Setter Property="Margin" Value="0 0 1 0"/>
                <Setter Property="BorderThickness" Value="0"/>
            </Trigger>
            <MultiTrigger>
                <MultiTrigger.Conditions>
                    <Condition Property="IsInDocumentHost" Value="True"/>
                    <Condition Property="IsSelected" Value="False"/>
                </MultiTrigger.Conditions>
                <Setter Property="Foreground" Value="{telerik:VisualStudio2019Resource ResourceKey=MarkerInvertedBrush}"/>
                <Setter Property="Background" Value="{telerik:VisualStudio2019Resource ResourceKey=HeaderBrush}"/>
            </MultiTrigger>
            <MultiTrigger>
                <MultiTrigger.Conditions>
                    <Condition Property="IsInDocumentHost" Value="False"/>
                    <Condition Property="IsPinned" Value="True"/>
                </MultiTrigger.Conditions>
                <Setter Property="Padding" Value="5 0"/>
                <Setter Property="Background" Value="{telerik:VisualStudio2019Resource ResourceKey=HeaderBrush}"/>
