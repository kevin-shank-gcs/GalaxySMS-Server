    <Style x:Key="RadGridResizerStyle" TargetType="dock:RadGridResizer">
        <Setter Property="Background" Value="#01FFFFFF"/>
        <Setter Property="BorderThickness" Value="0"/>
        <Setter Property="BorderBrush" Value="{x:Null}"/>
        <Setter Property="HorizontalAlignment" Value="Stretch"/>
        <Setter Property="VerticalAlignment" Value="Stretch"/>
        <Setter Property="MinWidth" Value="2"/>
        <Setter Property="MinHeight" Value="2"/>
        <Setter Property="IsTabStop" Value="True"/>
        <Setter Property="Panel.ZIndex" Value="100"/>
        <Setter Property="PreviewStyle" Value="{StaticResource GridResizerPreviewStyle}"/>
        <Setter Property="Template" Value="{StaticResource RadGridResizerControlTemplate}"/>
        <Style.Triggers>
            <Trigger Property="Placement" Value="Top">
                <Setter Property="Cursor" Value="SizeNS"/>
            </Trigger>
            <Trigger Property="Placement" Value="Right">
                <Setter Property="Cursor" Value="SizeWE"/>
            </Trigger>
            <Trigger Property="Placement" Value="Bottom">
                <Setter Property="Cursor" Value="SizeNS"/>
            </Trigger>
            <Trigger Property="Placement" Value="Left">
                <Setter Property="Cursor" Value="SizeWE"/>
            </Trigger>
        </Style.Triggers>
