            <Setter.Value>
                <InputBindingCollection>
                    <MouseBinding Command="telerikDocking:RadDockingCommands.PaneHeaderMenuOpen">
                        <MouseBinding.Gesture>
                            <MouseGesture MouseAction="RightClick"/>
                        </MouseBinding.Gesture>
                    </MouseBinding>
                </InputBindingCollection>
            </Setter.Value>
        </Setter>
