    <ControlTemplate x:Key="RadPaneDocumentHostTemplate" TargetType="telerikDocking:RadPane">
        <Grid x:Name="wrapper">
            <Grid.ColumnDefinitions>
                <ColumnDefinition Width="*"/>
                <ColumnDefinition Width="Auto"/>
            </Grid.ColumnDefinitions>
            <Border x:Name="BorderVisual"
                    BorderThickness="{TemplateBinding BorderThickness}"
                    BorderBrush="{TemplateBinding BorderBrush}"
                    Background="{TemplateBinding Background}"
                    Grid.ColumnSpan="2"/>
            <telerik:RadButton x:Name="RadPaneCloseButton"
                    Grid.Column="1"
                    IsBackgroundVisible="False"
                    Style="{StaticResource RadButtonDocumentCloseStyle}"
                    Foreground="{TemplateBinding Foreground}"
                    VerticalAlignment="Center"
                    Command="telerikDocking:RadDockingCommands.Close"
                    Visibility="{Binding IsEnabled, RelativeSource={RelativeSource Self}, Converter={StaticResource BooleanToVisibilityConverter}}"/>
            <telerikNavigationPrimitives:TabItemContentPresenter x:Name="HeaderElement"
                    Foreground="{TemplateBinding Foreground}"
                    Content="{TemplateBinding Header}"
                    ContentTemplate="{TemplateBinding HeaderTemplate}"
                    Padding="{TemplateBinding Padding}"
                    HorizontalContentAlignment="{TemplateBinding HorizontalContentAlignment}"
                    VerticalContentAlignment="{TemplateBinding VerticalContentAlignment}"
                    Style="{StaticResource TabItemContentPresenterStyle}"
                    MinHeight="16"/>
        </Grid>
        <ControlTemplate.Triggers>
            <Trigger Property="IsMouseOver" Value="True">
                <Setter TargetName="HeaderElement" Property="Foreground" Value="{telerik:VisualStudio2019Resource ResourceKey=MarkerBrush}"/>
                <Setter TargetName="RadPaneCloseButton" Property="Foreground" Value="{telerik:VisualStudio2019Resource ResourceKey=AccentDarkBrush}"/>
                <Setter TargetName="BorderVisual" Property="Background" Value="{Binding RelativeSource={RelativeSource TemplatedParent}, Path=(helpers:ThemeHelper.MouseOverBrush), Mode=OneWay}"/>
            </Trigger>
            <Trigger Property="IsSelected" Value="True">
                <Setter TargetName="HeaderElement" Property="Foreground" Value="{telerik:VisualStudio2019Resource ResourceKey=MarkerBrush}"/>
                <Setter TargetName="RadPaneCloseButton" Property="Foreground" Value="{telerik:VisualStudio2019Resource ResourceKey=IconBrush}"/>
                <Setter TargetName="BorderVisual" Property="Background" Value="{Binding RelativeSource={RelativeSource TemplatedParent}, Path=(helpers:ThemeHelper.CheckedBrush), Mode=OneWay}"/>
            </Trigger>
            <MultiTrigger>
                <MultiTrigger.Conditions>
                    <Condition Property="IsSelected" Value="True"/>
                    <Condition Property="IsActive" Value="False"/>
                </MultiTrigger.Conditions>
                <Setter TargetName="HeaderElement" Property="Foreground" Value="{telerik:VisualStudio2019Resource ResourceKey=MarkerBrush}"/>
                <Setter TargetName="BorderVisual" Property="Background" Value="{telerik:VisualStudio2019Resource ResourceKey=BasicBrush}"/>
            </MultiTrigger>
            <Trigger Property="IsEnabled" Value="False">
                <Setter TargetName="BorderVisual" Property="Background" Value="{x:Null}"/>
                <Setter TargetName="HeaderElement" Property="Foreground" Value="{telerik:VisualStudio2019Resource ResourceKey=MarkerBrush}"/>
                <Setter TargetName="HeaderElement" Property="Opacity" Value="{telerik:VisualStudio2019Resource ResourceKey=DisabledOpacity}"/>
                <Setter TargetName="RadPaneCloseButton" Property="Opacity" Value="{telerik:VisualStudio2019Resource ResourceKey=DisabledOpacity}"/>
            </Trigger>
        </ControlTemplate.Triggers>
    </ControlTemplate>
    <DataTemplate x:Key="RadPaneContextMenuTemplate">
        <telerikNavigation:RadContextMenu InheritDataContext="False">
            <telerikNavigation:RadMenuItem IsChecked="{Binding IsFloatingOnly}" Command="telerikDocking:RadDockingCommands.Floating" CommandParameter="{Binding}" CommandTarget="{Binding}"/>
            <telerikNavigation:RadMenuItem IsChecked="{Binding IsDockableOptionChecked}" Command="telerikDocking:RadDockingCommands.Dockable" CommandParameter="{Binding}" CommandTarget="{Binding}"/>
            <telerikNavigation:RadMenuItem IsChecked="{Binding IsInDocumentHost}" Command="telerikDocking:RadDockingCommands.TabbedDocument" CommandParameter="{Binding}" CommandTarget="{Binding}"/>
            <telerikNavigation:RadMenuItem
                    IsChecked="{Binding IsPinned, Converter={StaticResource InvertedBooleanConverter}}"
                    Command="telerikDocking:RadDockingCommands.Pin"
                    CommandParameter="{Binding}"
                    CommandTarget="{Binding}"/>
            <telerikNavigation:RadMenuItem IsChecked="{Binding IsHidden}" Command="telerikDocking:RadDockingCommands.Close" CommandParameter="{Binding}" CommandTarget="{Binding}"/>
        </telerikNavigation:RadContextMenu>
    </DataTemplate>
    <ControlTemplate x:Key="RadPaneTemplate" TargetType="telerikDocking:RadPane">
        <Grid x:Name="wrapper">
            <Border x:Name="BorderVisual"
                    BorderThickness="{TemplateBinding BorderThickness}"
                    BorderBrush="{TemplateBinding BorderBrush}"
                    Background="{TemplateBinding Background}"
                    Margin="{TemplateBinding Margin}"/>
            <Border x:Name="UnpinnedVisual"
                    BorderBrush="{telerik:VisualStudio2019Resource ResourceKey=HeaderBrush}"
                    Margin="{TemplateBinding Margin}"
                    Visibility="{Binding RelativeSource={RelativeSource TemplatedParent}, Path=IsPinned, Converter={StaticResource InvertedBooleanToVisibilityConverter}}"/>
            <telerikNavigationPrimitives:TabItemContentPresenter x:Name="HeaderElement"
                    Foreground="{telerik:VisualStudio2019Resource ResourceKey=MarkerInvertedBrush}"
                    Content="{TemplateBinding Header}"
                    ContentTemplate="{TemplateBinding HeaderTemplate}"
                    Padding="{TemplateBinding Padding}"
                    HorizontalContentAlignment="{TemplateBinding HorizontalContentAlignment}"
                    VerticalContentAlignment="{TemplateBinding VerticalContentAlignment}"
                    Style="{StaticResource TabItemContentPresenterStyle}"
                    MinHeight="18"/>
        </Grid>
        <ControlTemplate.Triggers>
            <Trigger Property="TabStripPlacement" Value="Left">
                <Setter TargetName="UnpinnedVisual" Property="BorderThickness" Value="4 0 0 0"/>
            </Trigger>
            <Trigger Property="TabStripPlacement" Value="Top">
                <Setter TargetName="UnpinnedVisual" Property="BorderThickness" Value="0 4 0 0"/>
            </Trigger>
            <Trigger Property="TabStripPlacement" Value="Right">
                <Setter TargetName="UnpinnedVisual" Property="BorderThickness" Value="0 0 4 0"/>
            </Trigger>
            <Trigger Property="TabStripPlacement" Value="Bottom">
                <Setter TargetName="UnpinnedVisual" Property="BorderThickness" Value="0 0 0 4"/>
            </Trigger>
            <Trigger Property="IsPinned" Value="False">
                <Setter Property="Margin" Value="0 0 0 1"/>
            </Trigger>
            <MultiTrigger>
                <MultiTrigger.Conditions>
                    <Condition Property="IsPinned" Value="False"/>
                    <Condition Property="TabStripPlacement" Value="Top"/>
                </MultiTrigger.Conditions>
                <Setter Property="Margin" Value="0 0 1 0"/>
            </MultiTrigger>
            <Trigger Property="IsMouseOver" Value="True">
                <Setter TargetName="UnpinnedVisual" Property="BorderBrush" Value="{Binding RelativeSource={RelativeSource TemplatedParent}, Path=(helpers:ThemeHelper.MouseOverBrush)}"/>
            </Trigger>
            <Trigger Property="IsPinned" Value="True">
                <Setter TargetName="HeaderElement" Property="Foreground" Value="{telerik:VisualStudio2019Resource ResourceKey=MarkerInvertedBrush}"/>
            </Trigger>
            <MultiTrigger>
                <MultiTrigger.Conditions>
                    <Condition Property="IsPinned" Value="True"/>
                    <Condition Property="IsMouseOver" Value="True"/>
                </MultiTrigger.Conditions>
                <Setter TargetName="HeaderElement" Property="Foreground" Value="{telerik:VisualStudio2019Resource ResourceKey=MarkerBrush}"/>
                <Setter TargetName="BorderVisual" Property="Background" Value="{Binding RelativeSource={RelativeSource TemplatedParent}, Path=(helpers:ThemeHelper.MouseOverBrush)}"/>
                <Setter TargetName="BorderVisual" Property="BorderBrush" Value="{Binding RelativeSource={RelativeSource AncestorType=telerikDocking:RadPaneGroup}, Path=BorderBrush}"/>
                <Setter TargetName="BorderVisual" Property="BorderThickness" Value="{Binding RelativeSource={RelativeSource AncestorType=telerikDocking:RadPaneGroup}, Path=BorderThickness, Converter={StaticResource ThicknessToOrientedThicknessConverter}, ConverterParameter=LeftRightBottom}"/>
            </MultiTrigger>
            <MultiTrigger>
                <MultiTrigger.Conditions>
                    <Condition Property="IsPinned" Value="True"/>
                    <Condition Property="IsSelected" Value="True"/>
                </MultiTrigger.Conditions>
                <Setter TargetName="HeaderElement" Property="Foreground" Value="{telerik:VisualStudio2019Resource ResourceKey=MarkerBrush}"/>
                <Setter TargetName="BorderVisual" Property="Background" Value="{Binding RelativeSource={RelativeSource AncestorType=telerikDocking:RadPaneGroup}, Path=Background}"/>
                <Setter TargetName="BorderVisual" Property="BorderBrush" Value="{Binding RelativeSource={RelativeSource AncestorType=telerikDocking:RadPaneGroup}, Path=BorderBrush}"/>
                <Setter TargetName="BorderVisual" Property="BorderThickness" Value="{Binding RelativeSource={RelativeSource AncestorType=telerikDocking:RadPaneGroup}, Path=BorderThickness, Converter={StaticResource ThicknessToOrientedThicknessConverter}, ConverterParameter=LeftRightBottom}"/>
            </MultiTrigger>
            <MultiTrigger>
                <MultiTrigger.Conditions>
                    <Condition Property="IsPinned" Value="True"/>
                    <Condition Property="IsActive" Value="True"/>
                </MultiTrigger.Conditions>
                <Setter TargetName="HeaderElement" Property="Foreground" Value="{telerik:VisualStudio2019Resource ResourceKey=AccentDarkBrush}"/>
            </MultiTrigger>
            <Trigger Property="IsEnabled" Value="False">
                <Setter TargetName="HeaderElement" Property="Foreground" Value="{telerik:VisualStudio2019Resource ResourceKey=MarkerBrush}"/>
                <Setter TargetName="HeaderElement" Property="Opacity" Value="{telerik:VisualStudio2019Resource ResourceKey=DisabledOpacity}"/>
            </Trigger>
        </ControlTemplate.Triggers>
    </ControlTemplate>
    <Style x:Key="RadPaneStyle" TargetType="telerikDocking:RadPane">
