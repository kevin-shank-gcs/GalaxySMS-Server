                            <Setter Property="Foreground" Value="{telerik1:VisualStudio2019Resource ResourceKey=AccentFocusedBrush}"/>
                        </Trigger>
                        <Trigger Property="IsPressed" Value="True">
                            <Setter Property="Foreground" Value="{telerik1:VisualStudio2019Resource ResourceKey=AccentFocusedBrush}"/>
