                                    telerik:TouchManager.ShouldSuspendMousePromotion="True"
                                    IsReadOnly="True"
                                    TextWrapping="Wrap"
                                    ScrollViewer.PanningMode="None"
                                    AcceptsReturn="True"
                                    Text="{TemplateBinding Text}">
                                <telerik:RadContextMenu.ContextMenu>
