                                </telerik:RadContextMenu.ContextMenu>
                                <TextBox.Resources>
                                    <Style TargetType="ScrollViewer" BasedOn="{StaticResource ScrollViewerStyle}">
                                        <Setter Property="BorderThickness" Value="0"/>
                                    </Style>
