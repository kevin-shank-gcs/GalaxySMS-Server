                                HorizontalAlignment="Left"
                                VerticalAlignment="Stretch"
                                HorizontalContentAlignment="Center"
                                VerticalContentAlignment="Center"
                                Width="{StaticResource PdfViewer_ToolbarButtonWidth}"
                                Margin="1"
                                Padding="0">
                            <telerik:RadGlyph Glyph="{StaticResource GlyphArrowChevronUp}" FontSize="16" Foreground="{telerik1:Office2019Resource ResourceKey=IconBrush}"/>
                        </telerik:RadButton>
                        <telerik:RadButton x:Name="PART_PageDownButton"
                                Command="{Binding PageDownCommandDescriptor.Command}"
                                ToolTipService.ToolTip="Next page"
