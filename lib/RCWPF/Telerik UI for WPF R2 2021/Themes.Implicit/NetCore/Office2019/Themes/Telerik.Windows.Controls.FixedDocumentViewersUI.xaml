                                HorizontalAlignment="Left"
                                VerticalAlignment="Stretch"
                                HorizontalContentAlignment="Center"
                                VerticalContentAlignment="Center"
                                Width="{StaticResource PdfViewer_ToolbarButtonWidth}"
                                Margin="1"
                                Padding="0">
                            <telerik:RadGlyph Glyph="{StaticResource GlyphArrowChevronDown}" FontSize="16" Foreground="{telerik1:Office2019Resource ResourceKey=IconBrush}"/>
                        </telerik:RadButton>
                        <fixedDocumentViewersUI:CurrentPageTextBox x:Name="PART_CurrentPageNumberTextBox"
