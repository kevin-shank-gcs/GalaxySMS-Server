        <Setter Property="helpers:ThemeHelper.PressedBrush" Value="{telerik1:Office2019Resource ResourceKey=PressedBackgroundBrush}"/>
        <Setter Property="Foreground" Value="{telerik1:Office2019Resource ResourceKey=IconBrush}"/>
        <Setter Property="Background" Value="Transparent"/>
        <Setter Property="BorderBrush" Value="{x:Null}"/>
        <Setter Property="BorderThickness" Value="0"/>
        <Setter Property="MinHeight" Value="26"/>
        <Setter Property="MinWidth" Value="26"/>
        <Setter Property="Padding" Value="0"/>
        <Setter Property="VerticalAlignment" Value="Center"/>
        <Setter Property="HorizontalAlignment" Value="Center"/>
        <Setter Property="HorizontalContentAlignment" Value="Center"/>
        <Setter Property="VerticalContentAlignment" Value="Center"/>
        <Setter Property="telerik:RadDockPanel.Dock" Value="Left"/>
