    <ControlTemplate x:Key="CardViewItemExpanderHeaderButtonTemplate" TargetType="telerik:RadToggleButton">
        <Border x:Name="BorderVisual"
                CornerRadius="{Binding RelativeSource={RelativeSource AncestorType={x:Type telerik:RadExpander}}, Path=(helpers:ThemeHelper.CornerRadius)}"
                BorderBrush="{TemplateBinding BorderBrush}"
                BorderThickness="{TemplateBinding BorderThickness}"
                Background="{TemplateBinding Background}">
            <telerik:RadGlyph x:Name="chevron"
                    Grid.Column="1"
                    HorizontalAlignment="Center"
                    VerticalAlignment="Center"
                    FontSize="16"
                    Foreground="{TemplateBinding Foreground}"
                    Glyph="{StaticResource GlyphArrow60Down}"/>
        </Border>
        <ControlTemplate.Triggers>

            <!-- IsMouseOver -->
            <Trigger Property="IsMouseOver" Value="True">
                <Setter TargetName="BorderVisual" Property="Background" Value="{telerik:Office2013Resource ResourceKey=EffectAccentLowBrush}"/>
                <Setter Property="BorderBrush" Value="{telerik:Office2013Resource ResourceKey=AccentBrush}"/>
                <Setter TargetName="chevron" Property="Foreground" Value="{telerik:Office2013Resource ResourceKey=InvertedBrush}"/>
            </Trigger>

            <!-- IsPressed -->
            <Trigger Property="IsPressed" Value="True">
                <Setter TargetName="BorderVisual" Property="Background" Value="Transparent"/>
            </Trigger>

            <!-- IsMouseOver and IsSelected -->
            <MultiDataTrigger>
                <MultiDataTrigger.Conditions>
                    <Condition Binding="{Binding IsMouseOver, RelativeSource={RelativeSource Self}}" Value="True"/>
                    <Condition Binding="{Binding IsSelected, RelativeSource={RelativeSource AncestorType={x:Type controls:RadCardViewItem}}}" Value="True"/>
                </MultiDataTrigger.Conditions>
                <Setter TargetName="BorderVisual" Property="Background" Value="{telerik:Office2013Resource ResourceKey=EffectHighBrush}"/>
                <Setter TargetName="chevron" Property="Foreground" Value="{telerik:Office2013Resource ResourceKey=MainBrush}"/>
            </MultiDataTrigger>

            <!-- IsPressed and IsSelected -->
            <MultiDataTrigger>
                <MultiDataTrigger.Conditions>
                    <Condition Binding="{Binding IsPressed, RelativeSource={RelativeSource Self}}" Value="True"/>
                    <Condition Binding="{Binding IsSelected, RelativeSource={RelativeSource AncestorType={x:Type controls:RadCardViewItem}}}" Value="True"/>
                </MultiDataTrigger.Conditions>
                <Setter TargetName="BorderVisual" Property="Background" Value="{telerik:Office2013Resource ResourceKey=EffectHighBrush}"/>
            </MultiDataTrigger>
        </ControlTemplate.Triggers>
    </ControlTemplate>
    <Style x:Key="CardViewItemExpanderHeaderButtonStyle" TargetType="telerik:RadToggleButton">
        <Setter Property="Background" Value="Transparent"/>
        <Setter Property="BorderBrush" Value="Transparent"/>
        <Setter Property="BorderThickness" Value="1"/>
        <Setter Property="Foreground" Value="{telerik:Office2013Resource ResourceKey=StrongBrush}"/>
        <Setter Property="FocusVisualStyle" Value="{x:Null}"/>
        <Setter Property="Width" Value="18"/>
        <Setter Property="Height" Value="18"/>
        <Setter Property="Template" Value="{StaticResource CardViewItemExpanderHeaderButtonTemplate}"/>
    </Style>
    <ControlTemplate x:Key="CardViewItemExpanderTemplate" TargetType="telerik:RadExpander">
        <Border x:Name="BorderVisual"
                SnapsToDevicePixels="{TemplateBinding SnapsToDevicePixels}"
                BorderBrush="{TemplateBinding BorderBrush}"
                BorderThickness="{TemplateBinding BorderThickness}"
                Background="{TemplateBinding Background}"
                CornerRadius="{Binding RelativeSource={RelativeSource TemplatedParent}, Path=(helpers:ThemeHelper.CornerRadius)}">
            <Grid>
                <Grid.RowDefinitions>
                    <RowDefinition x:Name="row0" Height="Auto"/>
                    <RowDefinition x:Name="row1" Height="Auto"/>
                </Grid.RowDefinitions>
                <Grid.ColumnDefinitions>
                    <ColumnDefinition x:Name="col0" Width="Auto"/>
                    <ColumnDefinition x:Name="col1" Width="Auto"/>
                </Grid.ColumnDefinitions>
                <ContentPresenter x:Name="Content"
                        Margin="{TemplateBinding Padding}"
                        Visibility="Collapsed"
                        UseLayoutRounding="True"
                        Content="{TemplateBinding Content}"
                        ContentTemplate="{TemplateBinding ContentTemplate}"
                        ContentTemplateSelector="{TemplateBinding ContentTemplateSelector}"
                        HorizontalAlignment="{TemplateBinding HorizontalContentAlignment}"
                        VerticalAlignment="{TemplateBinding VerticalContentAlignment}"/>
                <telerikPrimitives:LayoutTransformControl x:Name="HeaderContentTransform" RenderTransformOrigin=".5 .5">
                    <Border x:Name="HeaderPanel"
                            MinHeight="27"
                            Background="{telerik:Office2013Resource ResourceKey=MediumLightBrush}"
                            CornerRadius="{Binding RelativeSource={RelativeSource TemplatedParent}, Path=(helpers:ThemeHelper.CornerRadius), Converter={StaticResource CornerRadiusConverter}, ConverterParameter=TopLeftRight}"
                            BorderBrush="{TemplateBinding BorderBrush}"
                            BorderThickness="{Binding BorderThickness, RelativeSource={RelativeSource TemplatedParent}, Converter={StaticResource ThicknessToOrientedThicknessConverter}, ConverterParameter=Bottom}">
                        <Grid Background="Transparent" Margin="14 0 4 0">
                            <Grid.ColumnDefinitions>
                                <ColumnDefinition Width="*"/>
                                <ColumnDefinition Width="Auto"/>
                            </Grid.ColumnDefinitions>
                            <ContentPresenter x:Name="HeaderContent"
                                    FocusVisualStyle="{x:Null}"
                                    TextElement.Foreground="{telerik:Office2013Resource ResourceKey=InvertedBrush}"
                                    Content="{TemplateBinding Header}"
                                    ContentTemplate="{TemplateBinding HeaderTemplate}"
                                    ContentTemplateSelector="{TemplateBinding HeaderTemplateSelector}"
                                    ContentStringFormat="{TemplateBinding HeaderStringFormat}"
                                    VerticalAlignment="{TemplateBinding VerticalHeaderAlignment}"
                                    HorizontalAlignment="{TemplateBinding HorizontalHeaderAlignment}"/>
                            <telerik:RadToggleButton x:Name="HeaderButton"
                                    Grid.Column="1"
                                    IsChecked="{Binding Path=IsExpanded, RelativeSource={RelativeSource TemplatedParent}, Mode=TwoWay}"
                                    ClickMode="{TemplateBinding ClickMode}"
                                    Content="{TemplateBinding Header}"
                                    ContentTemplate="{TemplateBinding HeaderTemplate}"
                                    Style="{TemplateBinding HeaderButtonStyle}"
                                    ContentStringFormat="{TemplateBinding HeaderStringFormat}"
                                    ContentTemplateSelector="{TemplateBinding HeaderTemplateSelector}"/>
                        </Grid>
                    </Border>
                </telerikPrimitives:LayoutTransformControl>
            </Grid>
        </Border>
        <ControlTemplate.Triggers>

            <!-- CardViewItem Focused -->
            <Trigger Property="IsKeyboardFocusWithin" Value="True">
                <Setter TargetName="BorderVisual" Property="BorderBrush" Value="{Binding RelativeSource={RelativeSource TemplatedParent}, Path=(helpers:ThemeHelper.FocusBrush)}"/>
                <Setter TargetName="HeaderPanel" Property="BorderBrush" Value="{Binding RelativeSource={RelativeSource TemplatedParent}, Path=(helpers:ThemeHelper.FocusBrush)}"/>
            </Trigger>
            <DataTrigger Binding="{Binding IsFocused, RelativeSource={RelativeSource AncestorType={x:Type controls:RadCardViewItem}}}" Value="True">
                <Setter TargetName="BorderVisual" Property="BorderBrush" Value="{Binding RelativeSource={RelativeSource TemplatedParent}, Path=(helpers:ThemeHelper.FocusBrush)}"/>
                <Setter TargetName="HeaderPanel" Property="BorderBrush" Value="{Binding RelativeSource={RelativeSource TemplatedParent}, Path=(helpers:ThemeHelper.FocusBrush)}"/>
            </DataTrigger>

            <!-- CardViewItem not expanded -->
            <Trigger Property="IsExpanded" Value="False">
                <Setter TargetName="HeaderPanel" Property="CornerRadius" Value="{Binding RelativeSource={RelativeSource TemplatedParent}, Path=(helpers:ThemeHelper.CornerRadius)}"/>
                <Setter TargetName="HeaderPanel" Property="BorderThickness" Value="0"/>
                <Setter TargetName="HeaderPanel" Property="MinHeight" Value="{Binding Path=CollapsedLength, RelativeSource={RelativeSource AncestorType={x:Type controls:RadCardView}, Mode=FindAncestor}}"/>
            </Trigger>

            <!-- CardViewItem Selected -->
            <DataTrigger Binding="{Binding IsSelected, RelativeSource={RelativeSource AncestorType={x:Type controls:RadCardViewItem}}, Mode=TwoWay}" Value="True">
                <Setter TargetName="HeaderPanel" Property="Background" Value="{Binding RelativeSource={RelativeSource TemplatedParent}, Path=(helpers:ThemeHelper.CheckedBrush)}"/>
                <Setter TargetName="HeaderButton" Property="Foreground" Value="{telerik:Office2013Resource ResourceKey=MainBrush}"/>
                <Setter TargetName="HeaderContent" Property="TextElement.Foreground" Value="{telerik:Office2013Resource ResourceKey=MainBrush}"/>
            </DataTrigger>

            <!-- Disabled -->
            <Trigger Property="IsEnabled" Value="False">
                <Setter TargetName="HeaderContent" Property="Opacity" Value="{telerik1:Office2013Resource ResourceKey=DisabledOpacity}"/>
                <Setter TargetName="HeaderButton" Property="Opacity" Value="{telerik1:Office2013Resource ResourceKey=DisabledOpacity}"/>
            </Trigger>
            <Trigger Property="ResultOrientation" Value="Vertical">
                <Setter TargetName="HeaderContentTransform" Property="LayoutTransform">
                    <Setter.Value>
                        <RotateTransform Angle="90"/>
                    </Setter.Value>
                </Setter>
                <Setter TargetName="HeaderContentTransform" Property="Grid.Row" Value="1"/>
            </Trigger>
            <Trigger Property="ResultOrientation" Value="Horizontal">
                <Setter TargetName="HeaderContentTransform" Property="LayoutTransform">
                    <Setter.Value>
                        <RotateTransform Angle="0"/>
                    </Setter.Value>
                </Setter>
                <Setter TargetName="HeaderContentTransform" Property="Grid.Column" Value="1"/>
            </Trigger>

            <!-- ExpandDirection: Down -->
            <Trigger Property="ExpandDirection" Value="Down">
                <Setter TargetName="col0" Property="Width" Value="*"/>
                <Setter TargetName="row1" Property="Height" Value="*"/>
                <Setter TargetName="Content" Property="Grid.Row" Value="1"/>
                <Setter TargetName="Content" Property="Grid.Column" Value="0"/>
                <Setter TargetName="HeaderContentTransform" Property="Grid.Row" Value="0"/>
                <Setter TargetName="HeaderContentTransform" Property="Grid.Column" Value="0"/>
            </Trigger>
        </ControlTemplate.Triggers>
    </ControlTemplate>
    <Style x:Key="CardViewItemExpanderStyle" TargetType="telerik:RadExpander">
        <Setter Property="helpers:ThemeHelper.FocusBrush" Value="{telerik:Office2013Resource ResourceKey=AccentBrush}"/>
        <Setter Property="helpers:ThemeHelper.CheckedBrush" Value="{telerik:Office2013Resource ResourceKey=AccentBrush}"/>
