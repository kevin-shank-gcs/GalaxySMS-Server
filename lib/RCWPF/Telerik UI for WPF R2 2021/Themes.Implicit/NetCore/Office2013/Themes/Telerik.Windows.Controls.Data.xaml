            <telerik:RadToggleButton x:Name="PART_CardViewGroupItemToggleButton"
                    Content="{TemplateBinding Header}"
                    Foreground="{TemplateBinding Foreground}"
                    Style="{StaticResource CardViewGroupItem_ToggleButtonStyle}"
                    VerticalContentAlignment="{TemplateBinding VerticalContentAlignment}"
                    Padding="{TemplateBinding Padding}"
                    IsChecked="{Binding IsExpanded, RelativeSource={RelativeSource Mode=TemplatedParent}, Mode=TwoWay}"/>
        </Grid>
        <ControlTemplate.Triggers>

            <!-- IsMouseOver -->
            <Trigger Property="IsMouseOver" Value="True">
                <Setter TargetName="BackgroundVisual" Property="Background" Value="{Binding RelativeSource={RelativeSource TemplatedParent}, Path=(helpers:ThemeHelper.MouseOverBrush)}"/>
            </Trigger>

            <!-- Focused -->
            <Trigger Property="IsKeyboardFocusWithin" Value="True">
                <Setter TargetName="BackgroundVisual" Property="Background" Value="{Binding RelativeSource={RelativeSource TemplatedParent}, Path=(helpers:ThemeHelper.FocusBrush)}"/>
            </Trigger>

            <!-- Disabled -->
            <Trigger Property="IsEnabled" Value="False">
