                            Background="{telerik:Office2013Resource ResourceKey=MainBrush}"
                            BorderThickness="1"
                            Visibility="{Binding DropDownContent,RelativeSource={RelativeSource TemplatedParent},Converter={StaticResource ControlPanelItemButtonTooltipToVisibilityConverter}}"
                            CornerRadius="0"
                            MinWidth="3"
                            MinHeight="3"
                            Width="{TemplateBinding DropDownWidth}"
                            Height="{TemplateBinding DropDownHeight}"
                            MaxWidth="{TemplateBinding DropDownMaxWidth}"
