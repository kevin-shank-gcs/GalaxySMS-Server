                            MinHeight="0"
                            AutomationProperties.Name="{TemplateBinding AutomationProperties.Name}"
                            AutomationProperties.HelpText="{TemplateBinding AutomationProperties.HelpText}"
                            AutomationProperties.LabeledBy="{TemplateBinding AutomationProperties.LabeledBy}"
                            TabIndex="{TemplateBinding TabIndex}"
                            IsReadOnly="{TemplateBinding IsReadOnly}"
                            Visibility="{TemplateBinding EditorVisibility}"
                            Text="{Binding Text, RelativeSource={RelativeSource TemplatedParent}, Mode=TwoWay, UpdateSourceTrigger=PropertyChanged}"
                            WatermarkContent="{TemplateBinding WatermarkContent}"
                            WatermarkTemplate="{TemplateBinding WatermarkTemplate}"
                            VerticalContentAlignment="{TemplateBinding VerticalContentAlignment}"
                            HorizontalAlignment="{TemplateBinding HorizontalContentAlignment}"
                            Foreground="{TemplateBinding Foreground}"
                            FontSize="{TemplateBinding FontSize}"
                            Background="{x:Null}"
                            BorderBrush="{x:Null}"
                            BorderThickness="0"
                            ReadOnlyBackground="{x:Null}"
                            DisabledBackground="{x:Null}"
                            helpers:ThemeHelper.FocusBrush="Transparent"
                            helpers:ThemeHelper.MouseOverBrush="Transparent">
                        <telerik:RadWatermarkTextBox.AdditionalContent>
                            <StackPanel Orientation="Horizontal">
                                <telerik:RadButton x:Name="ClearButton"
                                        Margin="0"
                                        Visibility="{Binding ElementName=WatermarkBox, Path=Text, Converter={StaticResource NullToVisibilityConverter}}"
                                        Content="{TemplateBinding ClearButtonContent}"
                                        ContentTemplate="{TemplateBinding ClearButtonTemplate}"
                                        Command="historyNavigationPane:RadFilePathPickerCommands.Clear"
                                        CommandTarget="{Binding RelativeSource={RelativeSource TemplatedParent}}"
                                        helpers:ThemeHelper.FocusBrush="Transparent"
                                        helpers:ThemeHelper.MouseOverBrush="Transparent"
                                        helpers:ThemeHelper.PressedBrush="Transparent"
                                        helpers:ThemeHelper.CornerRadius="0"
                                        CornerRadius="0"
                                        Style="{StaticResource ClearButtonStyle}"/>
                            </StackPanel>
                        </telerik:RadWatermarkTextBox.AdditionalContent>
                    </telerik:RadWatermarkTextBox>
                    <telerik:RadButton x:Name="ShowDialogButton"
                            Grid.Column="2"
                            Padding="4 0"
                            MinHeight="0"
                            BorderBrush="{TemplateBinding BorderBrush}"
                            BorderThickness="{Binding RelativeSource={RelativeSource TemplatedParent}, Path=BorderThickness, Converter={StaticResource ThicknessToOrientedThicknessConverter}, ConverterParameter=Left}"
                            CornerRadius="{Binding RelativeSource={RelativeSource TemplatedParent}, Path=(helpers:ThemeHelper.CornerRadius), Converter={StaticResource CornerRadiusConverter}, ConverterParameter=TopBottomRight}"
                            Content="{TemplateBinding ShowDialogButtonContent}"
                            ContentTemplate="{TemplateBinding ShowDialogButtonTemplate}"
                            Command="historyNavigationPane:RadFilePathPickerCommands.ShowDialog"
                            CommandTarget="{Binding RelativeSource={RelativeSource TemplatedParent}}"
                            helpers:ThemeHelper.FocusBrush="Transparent"
                            mat:MaterialAssist.IsShadowDisabled="True"/>
                </Grid>
            </Border>
            <Border x:Name="FocusVisual"
                    Visibility="Collapsed"
                    BorderBrush="{TemplateBinding helpers:ThemeHelper.FocusBrush}"
                    BorderThickness="{TemplateBinding BorderThickness}"
                    CornerRadius="{TemplateBinding helpers:ThemeHelper.CornerRadius}"/>
        </Grid>
        <ControlTemplate.Triggers>
            <MultiTrigger>
                <MultiTrigger.Conditions>
                    <Condition Property="EditorVisibility" Value="Collapsed"/>
                    <Condition Property="IconVisibility" Value="Collapsed"/>
                </MultiTrigger.Conditions>
                <Setter TargetName="ShowDialogButton" Property="BorderThickness" Value="0"/>
                <Setter TargetName="ShowDialogButton" Property="CornerRadius" Value="{Binding RelativeSource={RelativeSource TemplatedParent}, Path=(helpers:ThemeHelper.CornerRadius)}"/>
                <Setter TargetName="ShowDialogButtonColumn" Property="Width" Value="*"/>
                <Setter TargetName="IconColumn" Property="Width" Value="0"/>
                <Setter TargetName="FilePathColumn" Property="Width" Value="0"/>
            </MultiTrigger>
            <MultiTrigger>
                <MultiTrigger.Conditions>
                    <Condition Property="IsMouseOver" Value="True"/>
                    <Condition SourceName="ClearButton" Property="IsMouseOver" Value="False"/>
                </MultiTrigger.Conditions>
                <Setter TargetName="OuterBorderVisual" Property="BorderBrush" Value="{Binding RelativeSource={RelativeSource TemplatedParent}, Path=(helpers:ThemeHelper.MouseOverBrush), Mode=OneWay}"/>
                <Setter TargetName="ShowDialogButton" Property="BorderBrush" Value="{Binding RelativeSource={RelativeSource TemplatedParent}, Path=(helpers:ThemeHelper.MouseOverBrush), Mode=OneWay}"/>
            </MultiTrigger>
            <Trigger Property="IsKeyboardFocusWithin" Value="True">
                <Setter TargetName="FocusVisual" Property="Visibility" Value="Visible"/>
            </Trigger>
            <MultiTrigger>
                <MultiTrigger.Conditions>
                    <Condition SourceName="ShowDialogButton" Property="IsKeyboardFocusWithin" Value="True"/>
                    <Condition SourceName="ShowDialogButton" Property="IsPressed" Value="False"/>
                </MultiTrigger.Conditions>
                <Setter TargetName="ShowDialogButton" Property="Foreground" Value="{telerik:Office2013Resource ResourceKey=AccentMainBrush}"/>
            </MultiTrigger>
            <MultiTrigger>
                <MultiTrigger.Conditions>
                    <Condition SourceName="ClearButton" Property="IsKeyboardFocusWithin" Value="True"/>
                    <Condition SourceName="ClearButton" Property="IsPressed" Value="False"/>
                </MultiTrigger.Conditions>
                <Setter TargetName="ClearButton" Property="Foreground" Value="{telerik:Office2013Resource ResourceKey=AccentMainBrush}"/>
            </MultiTrigger>
            <MultiTrigger>
                <MultiTrigger.Conditions>
                    <Condition Property="IconVisibility" Value="Visible"/>
                    <Condition SourceName="ClearButton" Property="Visibility" Value="Collapsed"/>
                </MultiTrigger.Conditions>
                <Setter TargetName="WatermarkBox" Property="Padding" Value="5 0"/>
            </MultiTrigger>
            <Trigger SourceName="ClearButton" Property="Visibility" Value="Visible">
                <Setter TargetName="WatermarkBox" Property="Padding" Value="5 0 0 0"/>
            </Trigger>
            <Trigger Property="IsEnabled" Value="False">
                <Setter TargetName="Icon" Property="Opacity" Value="{telerik:Office2013Resource ResourceKey=DisabledOpacity}"/>
                <Setter TargetName="InnerBackgroundVisual" Property="Background" Value="{telerik:Office2013Resource ResourceKey=BasicBrush}"/>
                <Setter TargetName="OuterBorderVisual" Property="BorderBrush" Value="{telerik:Office2013Resource ResourceKey=MediumDarkBrush}"/>
                <Setter TargetName="ShowDialogButton" Property="BorderBrush" Value="{telerik:Office2013Resource ResourceKey=MediumDarkBrush}"/>
            </Trigger>
        </ControlTemplate.Triggers>
    </ControlTemplate>
