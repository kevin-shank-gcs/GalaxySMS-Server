                                    MaxWidth="650"
                                    MaxHeight="500"
                                    MinWidth="150"
                                    ItemContainerStyle="{StaticResource BreadcrumbListBoxItemStyle}"
                                    SelectedItem="{Binding RelativeSource={RelativeSource TemplatedParent}, Path=SelectedAutocompleteItem, Mode=TwoWay}"
                                    Background="{telerik1:Office2013Resource ResourceKey=MainBrush}"
                                    BorderBrush="{telerik1:Office2013Resource ResourceKey=HighDarkBrush}"
                                    MinHeight="28"
                                    Padding="1"/>
                        </Popup>
                    </Grid>
                </ControlTemplate>
            </Setter.Value>
        </Setter>
    </Style>
