                    CornerRadius="{TemplateBinding CornerRadius}"/>
            <Border x:Name="disabledBorder"
                    BorderThickness="{TemplateBinding BorderThickness}"
                    BorderBrush="{telerik:Office2013Resource ResourceKey=HighDarkBrush}"
                    CornerRadius="{TemplateBinding CornerRadius}"
                    Opacity="0.5"
                    Background="{telerik:Office2013Resource ResourceKey=MainBrush}"
                    Visibility="Collapsed"/>
            <Border x:Name="CheckedVisual"
                    Opacity="0"
                    Background="{telerik:Office2013Resource ResourceKey=EffectAccentHighBrush}"
                    BorderBrush="{telerik:Office2013Resource ResourceKey=AccentMainBrush}"
                    BorderThickness="{TemplateBinding BorderThickness}"
                    CornerRadius="{TemplateBinding CornerRadius}"/>
            <Grid>
                <Grid.RowDefinitions>
                    <RowDefinition Height="Auto"/>
                    <RowDefinition Height="*"/>
                    <RowDefinition Height="Auto"/>
                </Grid.RowDefinitions>
                <Grid.ColumnDefinitions>
                    <ColumnDefinition Width="Auto"/>
                    <ColumnDefinition Width="*"/>
                    <ColumnDefinition Width="Auto"/>
                </Grid.ColumnDefinitions>
                <ContentPresenter x:Name="Content"
                        Grid.Row="1"
                        Grid.Column="1"
                        TextBlock.Foreground="{TemplateBinding Foreground}"
                        Margin="{TemplateBinding Padding}"
                        Content="{TemplateBinding Content}"
                        ContentTemplate="{TemplateBinding ContentTemplate}"
                        VerticalAlignment="{TemplateBinding VerticalContentAlignment}"
                        HorizontalAlignment="{TemplateBinding HorizontalContentAlignment}"
                        ContentTemplateSelector="{TemplateBinding ContentTemplateSelector}"
                        ContentStringFormat="{TemplateBinding ContentStringFormat}"
                        RecognizesAccessKey="True"/>
            </Grid>
            <Popup x:Name="DropDownPopup" SnapsToDevicePixels="True" AllowsTransparency="True" StaysOpen="False" Focusable="False" Placement="Bottom">
                <Grid>
                    <Border x:Name="DropDownPopupBorder"
