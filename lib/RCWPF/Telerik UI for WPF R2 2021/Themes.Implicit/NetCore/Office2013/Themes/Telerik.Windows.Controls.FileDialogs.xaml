        <Setter Property="Margin" Value="2 5 4 3"/>
        <Setter Property="Padding" Value="8 0"/>
        <Setter Property="Foreground" Value="{telerik:Office2013Resource ResourceKey=InvertedBrush}"/>
        <Setter Property="Content" Value=""/>
