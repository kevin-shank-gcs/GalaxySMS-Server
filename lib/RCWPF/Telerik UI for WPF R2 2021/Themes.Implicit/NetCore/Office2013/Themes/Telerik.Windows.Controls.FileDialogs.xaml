    <ControlTemplate x:Key="DialogWindowTemplate" TargetType="telerikControls:DialogWindowBase">
        <Grid x:Name="LayoutRoot">
            <Border x:Name="WindowOuterBorder" Grid.RowSpan="2" BorderBrush="{TemplateBinding BorderBrush}" BorderThickness="0">
                <AdornerDecorator>
                    <Grid x:Name="AdornerLayoutRoot">
                        <Grid.RowDefinitions>
                            <RowDefinition x:Name="WindowHeaderRow" Height="Auto" MinHeight="30"/>
                            <RowDefinition Height="*"/>
                        </Grid.RowDefinitions>
                        <Grid x:Name="Header" Margin="1 0" Background="{telerik:Office2013Resource ResourceKey=AccentBrush}">
                            <Grid.ColumnDefinitions>
                                <ColumnDefinition Width="*"/>
                                <ColumnDefinition Width="Auto"/>
                            </Grid.ColumnDefinitions>
                            <Grid Grid.Column="0" Margin="8 0">
                                <Grid.ColumnDefinitions>
                                    <ColumnDefinition Width="Auto"/>
                                    <ColumnDefinition Width="*"/>
                                </Grid.ColumnDefinitions>
                                <ContentPresenter x:Name="IconContent"
                                        Grid.Column="0"
                                        HorizontalAlignment="Left"
                                        VerticalAlignment="Center"
                                        Content="{TemplateBinding Icon}"
                                        ContentTemplate="{TemplateBinding IconTemplate}"
                                        Margin="2 2 6 2"/>
                                <ContentControl x:Name="HeaderContent"
                                        Grid.Column="1"
                                        IsTabStop="False"
                                        HorizontalAlignment="Stretch"
                                        VerticalAlignment="Center"
                                        HorizontalContentAlignment="Stretch"
                                        Content="{TemplateBinding Header}"
                                        ContentTemplate="{TemplateBinding HeaderTemplate}"
                                        ContentTemplateSelector="{TemplateBinding HeaderTemplateSelector}"
                                        SnapsToDevicePixels="True"
                                        FontSize="{telerik:Office2013Resource ResourceKey=FontSizeL}"
                                        FontFamily="{telerik:Office2013Resource ResourceKey=FontFamily}"
                                        Foreground="{telerik:Office2013Resource ResourceKey=MainBrush}"/>
                            </Grid>
                            <Border x:Name="PART_HeaderButtonsBorder" Grid.Column="1" Margin="0 0 6 0">
                                <telerik:RadButton x:Name="PART_CloseButton"
                                        Command="{x:Static telerik:WindowCommands.Close}"
                                        Style="{StaticResource WindowButtonStyle}"
                                        Visibility="{Binding IsEnabled, RelativeSource={RelativeSource Self}, Converter={StaticResource BooleanToVisibilityConverter}}">
                                    <ToolTipService.ToolTip>
                                        <ToolTip telerik:LocalizationManager.ResourceKey="Close"/>
                                    </ToolTipService.ToolTip>
                                    <Path
                                            Data="M2.5,0 L4.5,0 4.5,2.5 7,2.5 7,4.5 4.5,4.5 4.5,7 2.5,7 2.5,4.5 0,4.5 0,2.5 2.5,2.5 z"
                                            Fill="{telerik:Office2013Resource ResourceKey=MainBrush}"
                                            Width="10"
                                            Height="10"
                                            RenderTransformOrigin="0.5,0.5"
                                            Stretch="Uniform"
                                            VerticalAlignment="Center">
                                        <Path.RenderTransform>
                                            <RotateTransform Angle="-135"/>
                                        </Path.RenderTransform>
                                    </Path>
                                </telerik:RadButton>
                            </Border>
                        </Grid>
                        <Border x:Name="ContentOuterBorder"
                                Grid.Row="1"
                                Margin="{TemplateBinding BorderThickness}"
                                BorderThickness="1 0 1 1"
                                BorderBrush="{TemplateBinding BorderBrush}"
                                Background="{TemplateBinding Background}">
                            <ContentPresenter x:Name="ContentElement" Margin="{TemplateBinding Padding}" Content="{TemplateBinding Content}" ContentTemplate="{TemplateBinding ContentTemplate}"/>
                        </Border>
                        <Path x:Name="ResizeIcon"
                                Grid.Row="1"
                                Fill="{TemplateBinding BorderBrush}"
                                HorizontalAlignment="Right"
                                VerticalAlignment="Bottom"
                                Margin="0 0 3 3"
                                Stretch="None"
                                Stroke="{x:Null}"
                                Data="M8,8 L10,8 10,10 8,10 z M4,8 L6,8 6,10 4,10 z M0,8 L2,8 2,10 0,10 z M8,4 L10,4 10,6 8,6 z M4,4 L6,4 6,6 4,6 z M8,0 L10,0 10,2 8,2 z"/>
                    </Grid>
                </AdornerDecorator>
            </Border>
        </Grid>
        <ControlTemplate.Triggers>
            <Trigger Property="IsInActiveState" Value="False">
                <Setter TargetName="HeaderContent" Property="Opacity" Value="0.7"/>
                <Setter TargetName="PART_HeaderButtonsBorder" Property="Opacity" Value="0.7"/>
                <Setter TargetName="ResizeIcon" Property="Opacity" Value="0.5"/>
                <Setter TargetName="WindowOuterBorder" Property="BorderBrush" Value="{Binding RelativeSource={RelativeSource TemplatedParent}, Path=BorderBrush, Converter={StaticResource OpacityConverter}, ConverterParameter=127}"/>
            </Trigger>
            <Trigger Property="WindowState" Value="Maximized">
                <Setter Property="BorderThickness" Value="0"/>
                <Setter TargetName="WindowOuterBorder" Property="BorderThickness" Value="0"/>
                <Setter TargetName="Header" Property="Margin" Value="-1 0"/>
                <Setter TargetName="PART_HeaderButtonsBorder" Property="Margin" Value="0 0 3 0"/>
                <Setter TargetName="ResizeIcon" Property="Visibility" Value="Collapsed"/>
            </Trigger>
            <Trigger Property="WindowState" Value="Minimized">
                <Setter TargetName="WindowHeaderRow" Property="Height" Value="28"/>
                <Setter TargetName="WindowHeaderRow" Property="MinHeight" Value="28"/>
                <Setter TargetName="HeaderContent" Property="Margin" Value="0"/>
                <Setter TargetName="LayoutRoot" Property="HorizontalAlignment" Value="Left"/>
                <Setter TargetName="LayoutRoot" Property="VerticalAlignment" Value="Top"/>
                <Setter TargetName="PART_CloseButton" Property="Padding" Value="0"/>
                <Setter TargetName="AdornerLayoutRoot" Property="Width" Value="145"/>
            </Trigger>
        </ControlTemplate.Triggers>
    </ControlTemplate>
