                <Grid Grid.Column="0" HorizontalAlignment="Left" Width="{TemplateBinding IconColumnWidth}"/>
                <Grid Grid.Column="1" Margin="{StaticResource MenuItemSeparatorMargin}" Height="2">
                    <Rectangle Height="1" VerticalAlignment="Top" Fill="{StaticResource MenuItemSeparatorTop}"/>
                    <Rectangle Height="1" VerticalAlignment="Bottom" Fill="{StaticResource MenuItemSeparatorBottom}"/>
                </Grid>
            </Grid>
            <Grid x:Name="VerticalSeparator" Visibility="Collapsed" Width="2" Margin="2">
                <Rectangle Width="1" HorizontalAlignment="Left" Fill="{StaticResource MenuItemSeparatorLeft}"/>
                <Rectangle Width="1" HorizontalAlignment="Right" Fill="{StaticResource MenuItemSeparatorRight}"/>
