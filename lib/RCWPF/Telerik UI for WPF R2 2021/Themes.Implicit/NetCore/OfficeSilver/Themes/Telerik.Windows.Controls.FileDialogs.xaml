            <Trigger Property="IsSelected" Value="False">
                <Setter TargetName="PinButton" Property="Margin" Value="0 2 4 0"/>
                <Setter TargetName="CloseButton" Property="Margin" Value="0 2 4 0"/>
            </Trigger>
            <Trigger Property="IsSelected" Value="True"/>
        </ControlTemplate.Triggers>
    </ControlTemplate>
