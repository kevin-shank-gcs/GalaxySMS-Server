                                <Image x:Name="image" Source="{Binding SmallImage}" Width="16" Height="16" Stretch="Uniform"/>
                                <TextBlock
                                        Margin="4 0 0 0"
                                        Text="{Binding Label}"
                                        MaxWidth="200"
                                        TextTrimming="CharacterEllipsis"
                                        VerticalAlignment="Center"
                                        Visibility="{Binding IsInEditMode, Converter={StaticResource ResourceKey=InvertedBooleanToVisibilityConverter}}"/>
                                <historyNavigationPane:FileBrowserEditTextBox
                                        Margin="4 0 0 0"
                                        Text="{Binding Name, Mode=TwoWay}"
                                        MaxWidth="200"
                                        VerticalAlignment="Center"
                                        Padding="2 0 0 0"
                                        Visibility="{Binding IsInEditMode, Converter={StaticResource ResourceKey=BooleanToVisibilityConverter}}"/>
                            </StackPanel>
                            <DataTemplate.Triggers>
                                <DataTrigger Binding="{Binding DataContext.IsCut, ElementName=image}" Value="True">
                                    <Setter TargetName="image" Property="Opacity" Value="0.4"/>
                                </DataTrigger>
                                <DataTrigger Binding="{Binding DataContext.IsHidden, ElementName=image}" Value="True">
                                    <Setter TargetName="image" Property="Opacity" Value="0.4"/>
                                </DataTrigger>
                            </DataTemplate.Triggers>
                        </DataTemplate>
                    </historyNavigationPane:FileBrowserListBox.ItemTemplate>
                </historyNavigationPane:FileBrowserListBox>
            </DataTemplate>
        </historyNavigationPane:LayoutConfiguratorTemplateSelector.ListDataTemplate>
        <historyNavigationPane:LayoutConfiguratorTemplateSelector.TilesDataTemplate>
            <DataTemplate>
                <historyNavigationPane:FileBrowserListBox
                        ItemsSource="{Binding CurrentFileSystemObjects}"
                        SelectedItem="{Binding SelectedFileSystemObject, Mode=TwoWay}"
                        x:Name="Listbox_Tiles"
                        AutomationProperties.Name="tiles layout"
                        DataContext="{Binding DataContext, RelativeSource={RelativeSource AncestorType={x:Type historyNavigationPane:FileBrowserTabControl}}}">
                    <historyNavigationPane:FileBrowserListBox.ItemsPanel>
                        <ItemsPanelTemplate>
                            <telerik:VirtualizingWrapPanel Orientation="Horizontal" ItemWidth="250" ItemHeight="58" ScrollStep="174"/>
                        </ItemsPanelTemplate>
                    </historyNavigationPane:FileBrowserListBox.ItemsPanel>
                    <historyNavigationPane:FileBrowserListBox.Resources>
                        <DataTemplate DataType="{x:Type historyNavigationPane:FileInfoWrapper}">
                            <StackPanel Orientation="Horizontal" VerticalAlignment="Center">
                                <Image x:Name="image" Source="{Binding MediumImage}" Width="32" Height="32" VerticalAlignment="Center"/>
                                <StackPanel Orientation="Vertical" Margin="4 0 0 0" VerticalAlignment="Center">
                                    <StackPanel Orientation="Horizontal">
                                        <TextBlock
                                                Text="{Binding Name}"
                                                MaxWidth="190"
                                                TextTrimming="CharacterEllipsis"
                                                Visibility="{Binding IsInEditMode, Converter={StaticResource ResourceKey=InvertedBooleanToVisibilityConverter}}"/>
                                        <historyNavigationPane:FileBrowserEditTextBox
                                                Text="{Binding Name, Mode=TwoWay}"
                                                MaxWidth="190"
                                                HorizontalAlignment="Center"
                                                TextAlignment="Center"
                                                TextWrapping="Wrap"
                                                VerticalAlignment="Center"
                                                Visibility="{Binding IsInEditMode, Converter={StaticResource ResourceKey=BooleanToVisibilityConverter}}"/>
                                    </StackPanel>
                                    <TextBlock Text="{Binding TypeText}" MaxWidth="190" TextTrimming="CharacterEllipsis" Opacity="0.8"/>
                                    <TextBlock Text="{Binding Size, Converter={StaticResource roundFileSizeConverter}}" MaxWidth="190" TextTrimming="CharacterEllipsis" Opacity="0.8"/>
                                </StackPanel>
                            </StackPanel>
                            <DataTemplate.Triggers>
                                <DataTrigger Binding="{Binding DataContext.IsCut, ElementName=image}" Value="True">
                                    <Setter TargetName="image" Property="Opacity" Value="0.4"/>
                                </DataTrigger>
                                <DataTrigger Binding="{Binding DataContext.IsHidden, ElementName=image}" Value="True">
                                    <Setter TargetName="image" Property="Opacity" Value="0.4"/>
                                </DataTrigger>
                            </DataTemplate.Triggers>
                        </DataTemplate>
                        <DataTemplate DataType="{x:Type historyNavigationPane:DirectoryBaseWrapper}">
                            <StackPanel Orientation="Horizontal" VerticalAlignment="Stretch">
                                <Image x:Name="image" Source="{Binding MediumImage}" Width="32" Height="32" VerticalAlignment="Center"/>
                                <TextBlock
                                        Text="{Binding Label}"
                                        MaxWidth="190"
                                        TextTrimming="CharacterEllipsis"
                                        VerticalAlignment="Center"
                                        Visibility="{Binding IsInEditMode, Converter={StaticResource ResourceKey=InvertedBooleanToVisibilityConverter}}"/>
                                <historyNavigationPane:FileBrowserEditTextBox
                                        Margin="4 0 0 0"
                                        Text="{Binding Name, Mode=TwoWay}"
                                        MaxWidth="190"
                                        MaxHeight="56"
                                        HorizontalAlignment="Center"
                                        TextAlignment="Center"
                                        TextWrapping="Wrap"
                                        VerticalAlignment="Center"
                                        Visibility="{Binding IsInEditMode, Converter={StaticResource ResourceKey=BooleanToVisibilityConverter}}"/>
                            </StackPanel>
                            <DataTemplate.Triggers>
                                <DataTrigger Binding="{Binding DataContext.IsCut, ElementName=image}" Value="True">
                                    <Setter TargetName="image" Property="Opacity" Value="0.4"/>
                                </DataTrigger>
                                <DataTrigger Binding="{Binding DataContext.IsHidden, ElementName=image}" Value="True">
                                    <Setter TargetName="image" Property="Opacity" Value="0.4"/>
                                </DataTrigger>
                            </DataTemplate.Triggers>
                        </DataTemplate>
                    </historyNavigationPane:FileBrowserListBox.Resources>
                </historyNavigationPane:FileBrowserListBox>
            </DataTemplate>
        </historyNavigationPane:LayoutConfiguratorTemplateSelector.TilesDataTemplate>
        <historyNavigationPane:LayoutConfiguratorTemplateSelector.DetailsDataTemplate>
            <DataTemplate>
                <historyNavigationPane:FileBrowserGridView
                        SelectedItem="{Binding SelectedFileSystemObject, Mode=TwoWay}"
                        x:Name="GridView_Details"
                        CanUserDeleteRows="False"
                        AutomationProperties.Name="details layout"
                        DataContext="{Binding DataContext, RelativeSource={RelativeSource AncestorType={x:Type historyNavigationPane:FileBrowserTabControl}}}">
                    <historyNavigationPane:FileBrowserGridView.ItemsSource>
                        <MultiBinding Converter="{StaticResource DetailsGridItemsSourceConverter}">
                            <Binding Path="CurrentSearchResults" ElementName="PART_SearchPane"/>
                            <Binding Path="CurrentFileSystemObjects"/>
                            <Binding Path="IsSearchViewActive" ElementName="PART_SearchPane"/>
                        </MultiBinding>
                    </historyNavigationPane:FileBrowserGridView.ItemsSource>
                    <historyNavigationPane:FileBrowserGridView.Columns>
                        <telerik:GridViewDataColumn Header="{telerik:LocalizableResource Key=FileDialogs_NameHeader}" DataMemberBinding="{Binding Label}" IsReadOnly="False" Width="360">
                            <telerik:GridViewDataColumn.CellTemplate>
                                <DataTemplate>
                                    <Grid Margin="26 0 0 0" VerticalAlignment="Center" HorizontalAlignment="Left">
                                        <Grid.ColumnDefinitions>
                                            <ColumnDefinition Width="Auto"/>
                                            <ColumnDefinition Width="*"/>
                                        </Grid.ColumnDefinitions>
                                        <Image Source="{Binding SmallImage}" VerticalAlignment="Center" Margin="0 0 0 1">
                                            <Image.Style>
                                                <Style TargetType="Image">
                                                    <Setter Property="Opacity" Value="1"/>
                                                    <Setter Property="TextOptions.TextRenderingMode" Value="ClearType"/>
                                                    <Setter Property="SnapsToDevicePixels" Value="True"/>
                                                    <Setter Property="RenderOptions.EdgeMode" Value="Aliased"/>
                                                    <Style.Triggers>
                                                        <DataTrigger Binding="{Binding IsCut}" Value="True">
                                                            <Setter Property="Opacity" Value="0.4"/>
                                                        </DataTrigger>
                                                        <DataTrigger Binding="{Binding IsHidden}" Value="True">
                                                            <Setter Property="Opacity" Value="0.4"/>
                                                        </DataTrigger>
                                                    </Style.Triggers>
                                                </Style>
                                            </Image.Style>
                                        </Image>
                                        <TextBlock Margin="4 0 0 0" Text="{Binding Label}" HorizontalAlignment="Left" VerticalAlignment="Center" TextTrimming="CharacterEllipsis" Grid.Column="1"/>
                                    </Grid>
                                </DataTemplate>
                            </telerik:GridViewDataColumn.CellTemplate>
                            <telerik:GridViewDataColumn.CellEditTemplate>
                                <DataTemplate>
                                    <StackPanel Orientation="Horizontal" Margin="30 0 0 0" VerticalAlignment="Center">
                                        <Image Source="{Binding SmallImage}" VerticalAlignment="Center"/>
                                        <historyNavigationPane:FileBrowserEditTextBox Margin="4 0" Text="{Binding Name, Mode=TwoWay}" VerticalAlignment="Center" MaxWidth="300"/>
                                    </StackPanel>
                                </DataTemplate>
                            </telerik:GridViewDataColumn.CellEditTemplate>
                        </telerik:GridViewDataColumn>
                        <telerik:GridViewDataColumn Header="{telerik:LocalizableResource Key=FileDialogs_DateHeader}" DataMemberBinding="{Binding LastWriteTime}" IsReadOnly="True"/>
                        <telerik:GridViewDataColumn Header="{telerik:LocalizableResource Key=FileDialogs_TypeHeader}" DataMemberBinding="{Binding TypeText}" IsReadOnly="True"/>
                        <telerik:GridViewDataColumn
                                Header="{telerik:LocalizableResource Key=FileDialogs_SizeHeader}"
                                TextAlignment="Right"
                                DataMemberBinding="{Binding Size, Converter={StaticResource roundFileSizeConverter}, ConverterParameter=KB}"
                                IsReadOnly="True"/>
                    </historyNavigationPane:FileBrowserGridView.Columns>
                </historyNavigationPane:FileBrowserGridView>
            </DataTemplate>
        </historyNavigationPane:LayoutConfiguratorTemplateSelector.DetailsDataTemplate>
    </historyNavigationPane:LayoutConfiguratorTemplateSelector>
    <Style x:Key="FileDialogsBreadcrumbStyle" TargetType="{x:Type historyNavigationPane:FileDialogsBreadcrumb}" BasedOn="{StaticResource RadBreadcrumbStyle}">
        <Setter Property="VerticalAlignment" Value="Center"/>
        <Setter Property="Margin" Value="0 2 0 3"/>
        <Setter Property="Height" Value="22"/>
    </Style>
    <Style x:Key="BreadCrumbBarItemCustomStyle" TargetType="{x:Type telerikBreadcrumb:RadBreadcrumbBarItem}" BasedOn="{StaticResource RadBreadcrumbBarItemStyle}">
        <Style.Triggers>
            <Trigger Property="IsOverflow" Value="False">
                <Setter Property="Visibility">
                    <Setter.Value>
                        <MultiBinding Converter="{StaticResource hiddenFilesToVisibilityConverter}">
                            <Binding Path="DataContext.ShowHiddenFiles" RelativeSource="{RelativeSource AncestorType={x:Type telerik:RadBreadcrumb}}"/>
                            <Binding Path="IsHidden"/>
                            <Binding Path="IsSystemHiddenActual"/>
                            <Binding Path="Type" RelativeSource="{RelativeSource Self}"/>
                        </MultiBinding>
                    </Setter.Value>
                </Setter>
            </Trigger>
        </Style.Triggers>
    </Style>
    <Style TargetType="{x:Type historyNavigationPane:FileDialogsBreadcrumb}" BasedOn="{StaticResource FileDialogsBreadcrumbStyle}"/>
    <Style x:Key="NavigationTreeViewCustomPlacesItemStyle" TargetType="{x:Type telerik:RadTreeViewItem}" BasedOn="{StaticResource RadTreeViewItemStyle}">
        <Setter Property="DefaultImageSrc" Value="{Binding SmallImage}"/>
    </Style>
    <Style x:Key="ExplorerControlGridSplitter" TargetType="GridSplitter">
        <Setter Property="BorderBrush" Value="{StaticResource ControlOuterBorder_Normal}"/>
        <Setter Property="HorizontalAlignment" Value="Stretch"/>
        <Setter Property="HorizontalContentAlignment" Value="Left"/>
        <Setter Property="Cursor" Value="SizeWE"/>
        <Setter Property="IsTabStop" Value="False"/>
        <Setter Property="Template">
            <Setter.Value>
                <ControlTemplate TargetType="GridSplitter">
                    <Border x:Name="BorderVisual" Background="Transparent">
                        <Rectangle
                                Width="1"
                                Fill="{TemplateBinding BorderBrush}"
                                HorizontalAlignment="{TemplateBinding HorizontalContentAlignment}"
                                VerticalAlignment="Stretch"
                                Stroke="{x:Null}"
                                StrokeThickness="0"/>
                    </Border>
                </ControlTemplate>
            </Setter.Value>
        </Setter>
    </Style>
    <ControlTemplate x:Key="ExplorerControlTemplate" TargetType="historyNavigationPane:ExplorerControl">
        <Border BorderBrush="{StaticResource WindowInnerBorder}" BorderThickness="1">
            <Border BorderThickness="{TemplateBinding BorderThickness}" BorderBrush="{TemplateBinding BorderBrush}" Background="{TemplateBinding Background}">
                <Grid x:Name="ExplorerRootGrid">
                    <Grid.RowDefinitions>
                        <RowDefinition Height="Auto"/>
                        <RowDefinition Height="Auto"/>
                        <RowDefinition Height="*"/>
                    </Grid.RowDefinitions>
                    <Grid Grid.Row="0">
                        <Grid.ColumnDefinitions>
                            <ColumnDefinition Width="Auto"/>
                            <ColumnDefinition Width="*"/>
                            <ColumnDefinition Width="Auto"/>
                        </Grid.ColumnDefinitions>
                        <Border BorderBrush="{StaticResource ControlOuterBorder_Normal}" BorderThickness="0 0 0 1" Grid.ColumnSpan="3" Background="{StaticResource ControlBackground_Normal}">
                            <Border BorderBrush="{StaticResource ControlInnerBorder_Normal}" BorderThickness="1"/>
                        </Border>
                        <historyNavigationPane:HistoryNavigationPaneControl x:Name="PART_HistoryNavigationPane" Grid.Row="0" Grid.Column="0"/>
                        <historyNavigationPane:FileDialogsBreadcrumb x:Name="PART_PathNavigationPane"
                                Grid.Column="1"
                                CurrentItem="{Binding CurrentParentDirectory, Mode=OneWayToSource, UpdateSourceTrigger=Explicit}"
                                HeaderTemplate="{StaticResource BreadCrumbHeaderTemplate}"
                                Header="{Binding MainNavigationRoot}"
                                ItemsSource="{Binding MainNavigationRoot.ChildDirectories}"
                                HeaderMemberPath="Label"
                                HierarchicalItemsSource="ChildDirectories"
                                HierarchicalMemberPath="Label"
                                TextModePath="Name"
                                ItemContainerStyle="{StaticResource BreadCrumbBarItemCustomStyle}"/>
                        <historyNavigationPane:FileDialogSearchPane x:Name="PART_SearchPane" IsSearchViewActive="{Binding IsSearchActive, Mode=OneWayToSource}" Grid.Column="2"/>
                    </Grid>
                    <Grid Grid.Row="1" x:Name="ConfigurationPane">
                        <Border BorderBrush="{StaticResource ControlOuterBorder_Normal}" BorderThickness="0 0 0 1" Grid.ColumnSpan="3" Background="{StaticResource ControlBackground_Normal}">
                            <Border BorderBrush="{StaticResource ControlInnerBorder_Normal}" BorderThickness="1"/>
                        </Border>
                        <telerik:RadButton
                                Content="{telerik:LocalizableResource Key=FileDialogs_NewFolder}"
                                HorizontalAlignment="Left"
                                VerticalAlignment="Center"
                                x:Name="NewFolderButton"
                                IsBackgroundVisible="False"
                                Margin="4 2 4 3"
                                Command="historyNavigationPane:FileDialogsCommands.NewFolder"
                                Visibility="{Binding IsEnabled, RelativeSource={RelativeSource Mode=Self}, Converter={StaticResource ResourceKey=BooleanToVisibilityConverter}}"/>
                        <telerik:RadComboBox x:Name="PART_LayoutConfigurator"
                                AutomationProperties.Name="layout configurator"
                                HorizontalAlignment="Right"
                                SelectedItem="{Binding SelectedLayout}"
                                IsTextSearchEnabled="False"
                                ItemTemplate="{StaticResource LayoutConfiguratorItemTemplate}"
                                SelectionBoxTemplate="{StaticResource LayoutConfiguratorSelectedItemTemplate}"
                                ItemsSource="{Binding Layouts}"
                                Height="22"
                                Margin="4 2 4 3"
                                Width="64"
                                VerticalAlignment="Center"/>
                    </Grid>
                    <Grid Grid.Row="2">
                        <Grid.ColumnDefinitions>
                            <ColumnDefinition MinWidth="100" Width="*"/>
                            <ColumnDefinition Width="5"/>
                            <ColumnDefinition MinWidth="100" Width="3*"/>
                        </Grid.ColumnDefinitions>
                        <Grid x:Name="PART_TreeNavigationPane">
                            <Grid.RowDefinitions>
                                <RowDefinition Height="Auto"/>
                                <RowDefinition Height="*"/>
                            </Grid.RowDefinitions>
                            <telerik:RadTreeView x:Name="PART_CustomPlacesPane"
                                    ItemsSource="{Binding CustomPlaces}"
                                    telerik:AnimationManager.IsAnimationEnabled="False"
                                    IsVirtualizing="True"
                                    Visibility="{Binding HasItems, RelativeSource={RelativeSource Self}, Converter={StaticResource BooleanToVisibilityConverter}}"
                                    Margin="0 0 0 22"
                                    DisplayMemberPath="Label"
                                    ItemContainerStyle="{StaticResource NavigationTreeViewCustomPlacesItemStyle}"/>
                            <historyNavigationPane:FileBrowserTreeView
                                    Grid.Row="1"
                                    x:Name="PART_MainNavigationPane"
                                    AutomationProperties.Name="navigation pane"
                                    telerik:AnimationManager.IsAnimationEnabled="False"
                                    IsVirtualizing="True"
                                    IsEditable="True"
                                    ItemsIndent="10"
                                    ItemsSource="{Binding MainNavigationRootSource}"
                                    SelectionMode="Single"
                                    SelectedItem="{Binding CurrentParentDirectory, Mode=TwoWay}"
                                    DisplayMemberPath="Label"
                                    telerik:TextSearch.TextPath="Name"/>
                        </Grid>
                        <GridSplitter Grid.Column="1" Style="{StaticResource ExplorerControlGridSplitter}"/>
                        <historyNavigationPane:FileBrowserTabControl x:Name="PART_MainPane"
                                Grid.Column="2"
                                SelectedIndex="{Binding ElementName=PART_LayoutConfigurator, Path=SelectedIndex}"
                                ItemsSource="{Binding Layouts}"
                                ContentTemplateSelector="{StaticResource MainPaneTabControlTemplateSelector}">
                            <historyNavigationPane:FileBrowserTabControl.InputBindings>
                                <KeyBinding Key="Backspace" Command="historyNavigationPane:HistoryNavigationCommands.Back" CommandTarget="{Binding ElementName=PART_HistoryNavigationPane}"/>
                            </historyNavigationPane:FileBrowserTabControl.InputBindings>
                        </historyNavigationPane:FileBrowserTabControl>
                        <telerik:RadBusyIndicator Grid.Column="2" IsBusy="{Binding IsMainPaneLoadingNetwork}"/>
                    </Grid>
                </Grid>
            </Border>
        </Border>
    </ControlTemplate>
    <Style x:Key="ExplorerControlStyle" TargetType="historyNavigationPane:ExplorerControl">
        <Setter Property="Background" Value="{StaticResource WindowBackground}"/>
        <Setter Property="BorderBrush" Value="{StaticResource WindowOuterBorder}"/>
        <Setter Property="BorderThickness" Value="1"/>
        <Setter Property="SnapsToDevicePixels" Value="True"/>
        <Setter Property="IsTabStop" Value="False"/>
        <Setter Property="Template" Value="{StaticResource ExplorerControlTemplate}"/>
        <Setter Property="DragDropBehavior">
            <Setter.Value>
                <fileDialogsDragDrop:FileBrowserDragDropBehavior/>
            </Setter.Value>
        </Setter>
    </Style>
    <Style TargetType="historyNavigationPane:ExplorerControl" BasedOn="{StaticResource ExplorerControlStyle}"/>
    <converters:FilePathToIconImageSourceConverter x:Key="FilePathToIconImageSourceConverter"/>
    <converters:DialogTypeToWatermarkContentConverter x:Key="DialogTypeToWatermarkContentConverter"/>
    <DataTemplate x:Key="ImageTemplate">
        <Image Width="16" Height="16" Source="{Binding Converter={StaticResource FilePathToIconImageSourceConverter}}"/>
    </DataTemplate>
    <DataTemplate x:Key="GlyphTemplate">
        <telerik:RadGlyph Glyph="{StaticResource GlyphFile}" FontSize="16"/>
    </DataTemplate>
    <telerikControls:FilePathPickerIconTemplateSelector x:Key="FilePathPickerIconTemplateSelector" EmptyTemplate="{StaticResource GlyphTemplate}" NonEmptyTemplate="{StaticResource ImageTemplate}"/>
    <Style x:Key="ClearButtonStyle" TargetType="telerik:RadButton">
        <Setter Property="BorderThickness" Value="0"/>
        <Setter Property="Foreground" Value="#FF000000"/>
        <Setter Property="Background" Value="Transparent"/>
        <Setter Property="BorderBrush" Value="Transparent"/>
        <Setter Property="Padding" Value="2 0"/>
        <Setter Property="FocusVisualStyle" Value="{x:Null}"/>
        <Setter Property="HorizontalContentAlignment" Value="Center"/>
        <Setter Property="VerticalContentAlignment" Value="Center"/>
        <Setter Property="Template">
            <Setter.Value>
                <ControlTemplate TargetType="telerik:RadButton">
                    <Grid SnapsToDevicePixels="True">
                        <Border x:Name="OuterBorderVisual"
                                BorderThickness="{TemplateBinding BorderThickness}"
                                BorderBrush="{TemplateBinding BorderBrush}"
                                Background="{TemplateBinding Background}"/>
                        <ContentPresenter x:Name="Content"
                                TextBlock.Foreground="{TemplateBinding Foreground}"
                                Margin="{TemplateBinding Padding}"
                                Content="{TemplateBinding Content}"
                                ContentTemplate="{TemplateBinding ContentTemplate}"
                                VerticalAlignment="{TemplateBinding VerticalContentAlignment}"
                                HorizontalAlignment="{TemplateBinding HorizontalContentAlignment}"
                                ContentTemplateSelector="{TemplateBinding ContentTemplateSelector}"
                                ContentStringFormat="{TemplateBinding ContentStringFormat}"
                                RecognizesAccessKey="True"/>
                    </Grid>
                    <ControlTemplate.Triggers>
                        <Trigger Property="IsMouseOver" Value="True">
                            <Setter Property="Foreground" Value="#FFFFC92B"/>
                        </Trigger>
                        <Trigger Property="IsPressed" Value="True">
                            <Setter Property="Foreground" Value="#FFFFC92B"/>
                        </Trigger>
                        <Trigger Property="IsEnabled" Value="False">
                            <Setter TargetName="Content" Property="Opacity" Value="0.6"/>
                        </Trigger>
                    </ControlTemplate.Triggers>
                </ControlTemplate>
            </Setter.Value>
        </Setter>
    </Style>
    <ControlTemplate x:Key="RadFilePathPickerTemplate" TargetType="telerikControls:RadFilePathPicker">
        <Grid x:Name="RootElement">
            <Border x:Name="OuterBorderVisual"
                    BorderBrush="{TemplateBinding BorderBrush}"
                    BorderThickness="{TemplateBinding BorderThickness}"
                    CornerRadius="{TemplateBinding helpers:ThemeHelper.CornerRadius}">
                <Grid>
                    <Grid.ColumnDefinitions>
                        <ColumnDefinition x:Name="IconColumn" Width="Auto"/>
                        <ColumnDefinition x:Name="FilePathColumn" Width="*"/>
                        <ColumnDefinition x:Name="ShowDialogButtonColumn" Width="Auto"/>
                    </Grid.ColumnDefinitions>
                    <Border x:Name="InnerBackgroundVisual"
                            Grid.ColumnSpan="2"
                            Background="{TemplateBinding Background}"
                            CornerRadius="{Binding RelativeSource={RelativeSource TemplatedParent}, Path=(helpers:ThemeHelper.CornerRadius), Converter={StaticResource CornerRadiusConverter}, ConverterParameter=TopBottomLeft}"/>
                    <ContentPresenter x:Name="Icon"
                            Width="16"
                            Height="16"
                            Focusable="False"
                            TextBlock.Foreground="#FF000000"
                            Margin="2 0 0 0"
                            Content="{TemplateBinding FilePath}"
                            ContentTemplateSelector="{TemplateBinding IconTemplateSelector}"
                            Visibility="{TemplateBinding IconVisibility}"/>
                    <telerik:RadWatermarkTextBox x:Name="WatermarkBox"
                            Grid.Column="1"
                            MinHeight="0"
                            AutomationProperties.Name="{TemplateBinding AutomationProperties.Name}"
                            AutomationProperties.HelpText="{TemplateBinding AutomationProperties.HelpText}"
                            AutomationProperties.LabeledBy="{TemplateBinding AutomationProperties.LabeledBy}"
                            TabIndex="{TemplateBinding TabIndex}"
                            IsReadOnly="{TemplateBinding IsReadOnly}"
                            Visibility="{TemplateBinding EditorVisibility}"
                            Text="{Binding Text, RelativeSource={RelativeSource TemplatedParent}, Mode=TwoWay, UpdateSourceTrigger=PropertyChanged}"
                            WatermarkContent="{TemplateBinding WatermarkContent}"
                            WatermarkTemplate="{TemplateBinding WatermarkTemplate}"
                            VerticalContentAlignment="{TemplateBinding VerticalContentAlignment}"
                            HorizontalAlignment="{TemplateBinding HorizontalContentAlignment}"
                            Foreground="{TemplateBinding Foreground}"
                            FontSize="{TemplateBinding FontSize}"
                            Background="{x:Null}"
                            BorderBrush="{x:Null}"
                            BorderThickness="0"
                            ReadOnlyBackground="{x:Null}"
                            DisabledBackground="{x:Null}"
                            helpers:ThemeHelper.FocusBrush="Transparent"
                            helpers:ThemeHelper.MouseOverBrush="Transparent">
                        <telerik:RadWatermarkTextBox.AdditionalContent>
                            <StackPanel Orientation="Horizontal">
                                <telerik:RadButton x:Name="ClearButton"
                                        Margin="0"
                                        Visibility="{Binding ElementName=WatermarkBox, Path=Text, Converter={StaticResource NullToVisibilityConverter}}"
                                        Content="{TemplateBinding ClearButtonContent}"
                                        ContentTemplate="{TemplateBinding ClearButtonTemplate}"
                                        Command="historyNavigationPane:RadFilePathPickerCommands.Clear"
                                        CommandTarget="{Binding RelativeSource={RelativeSource TemplatedParent}}"
                                        helpers:ThemeHelper.FocusBrush="Transparent"
                                        helpers:ThemeHelper.MouseOverBrush="Transparent"
                                        helpers:ThemeHelper.PressedBrush="Transparent"
                                        helpers:ThemeHelper.CornerRadius="0"
                                        CornerRadius="0"
                                        Style="{StaticResource ClearButtonStyle}"/>
                            </StackPanel>
                        </telerik:RadWatermarkTextBox.AdditionalContent>
                    </telerik:RadWatermarkTextBox>
                    <telerik:RadButton x:Name="ShowDialogButton"
                            Grid.Column="2"
                            Padding="2 0"
                            MinHeight="0"
                            BorderBrush="{TemplateBinding BorderBrush}"
                            BorderThickness="{Binding RelativeSource={RelativeSource TemplatedParent}, Path=BorderThickness, Converter={StaticResource ThicknessToOrientedThicknessConverter}, ConverterParameter=Left}"
                            CornerRadius="{Binding RelativeSource={RelativeSource TemplatedParent}, Path=(helpers:ThemeHelper.CornerRadius), Converter={StaticResource CornerRadiusConverter}, ConverterParameter=TopBottomRight}"
                            Content="{TemplateBinding ShowDialogButtonContent}"
                            ContentTemplate="{TemplateBinding ShowDialogButtonTemplate}"
                            Command="historyNavigationPane:RadFilePathPickerCommands.ShowDialog"
                            CommandTarget="{Binding RelativeSource={RelativeSource TemplatedParent}}"
                            helpers:ThemeHelper.FocusBrush="Transparent"
                            mat:MaterialAssist.IsShadowDisabled="True"/>
                </Grid>
            </Border>
            <Border x:Name="FocusVisual"
                    Visibility="Collapsed"
                    BorderBrush="{TemplateBinding helpers:ThemeHelper.FocusBrush}"
                    BorderThickness="{TemplateBinding BorderThickness}"
                    CornerRadius="{TemplateBinding helpers:ThemeHelper.CornerRadius}"/>
        </Grid>
        <ControlTemplate.Triggers>
            <MultiTrigger>
                <MultiTrigger.Conditions>
                    <Condition Property="EditorVisibility" Value="Collapsed"/>
                    <Condition Property="IconVisibility" Value="Collapsed"/>
                </MultiTrigger.Conditions>
                <Setter TargetName="ShowDialogButton" Property="BorderThickness" Value="0"/>
                <Setter TargetName="ShowDialogButton" Property="CornerRadius" Value="{Binding RelativeSource={RelativeSource TemplatedParent}, Path=(helpers:ThemeHelper.CornerRadius)}"/>
                <Setter TargetName="ShowDialogButtonColumn" Property="Width" Value="*"/>
                <Setter TargetName="IconColumn" Property="Width" Value="0"/>
                <Setter TargetName="FilePathColumn" Property="Width" Value="0"/>
            </MultiTrigger>
            <MultiTrigger>
                <MultiTrigger.Conditions>
                    <Condition Property="IsMouseOver" Value="True"/>
                    <Condition SourceName="ClearButton" Property="IsMouseOver" Value="False"/>
                </MultiTrigger.Conditions>
                <Setter TargetName="OuterBorderVisual" Property="BorderBrush" Value="{Binding RelativeSource={RelativeSource TemplatedParent}, Path=(helpers:ThemeHelper.MouseOverBrush), Mode=OneWay}"/>
                <Setter TargetName="ShowDialogButton" Property="BorderBrush" Value="{Binding RelativeSource={RelativeSource TemplatedParent}, Path=(helpers:ThemeHelper.MouseOverBrush), Mode=OneWay}"/>
            </MultiTrigger>
            <Trigger Property="IsKeyboardFocusWithin" Value="True">
                <Setter TargetName="FocusVisual" Property="Visibility" Value="Visible"/>
            </Trigger>
            <MultiTrigger>
                <MultiTrigger.Conditions>
                    <Condition SourceName="ShowDialogButton" Property="IsKeyboardFocusWithin" Value="True"/>
                    <Condition SourceName="ShowDialogButton" Property="IsPressed" Value="False"/>
                </MultiTrigger.Conditions>
                <Setter TargetName="ShowDialogButton" Property="Foreground" Value="#FFFFC92B"/>
            </MultiTrigger>
            <MultiTrigger>
                <MultiTrigger.Conditions>
                    <Condition SourceName="ClearButton" Property="IsKeyboardFocusWithin" Value="True"/>
                    <Condition SourceName="ClearButton" Property="IsPressed" Value="False"/>
                </MultiTrigger.Conditions>
                <Setter TargetName="ClearButton" Property="Foreground" Value="#FFFFC92B"/>
            </MultiTrigger>
            <MultiTrigger>
                <MultiTrigger.Conditions>
                    <Condition Property="IconVisibility" Value="Visible"/>
                    <Condition SourceName="ClearButton" Property="Visibility" Value="Collapsed"/>
                </MultiTrigger.Conditions>
                <Setter TargetName="WatermarkBox" Property="Padding" Value="5 0"/>
            </MultiTrigger>
            <Trigger SourceName="ClearButton" Property="Visibility" Value="Visible">
                <Setter TargetName="WatermarkBox" Property="Padding" Value="5 0 0 0"/>
            </Trigger>
            <Trigger Property="IsEnabled" Value="False">
                <Setter TargetName="Icon" Property="Opacity" Value="0.6"/>
                <Setter TargetName="InnerBackgroundVisual" Property="Background" Value="#FFE0E0EA"/>
                <Setter TargetName="OuterBorderVisual" Property="BorderBrush" Value="#FF9F9FB3"/>
                <Setter TargetName="ShowDialogButton" Property="BorderBrush" Value="#FF9F9FB3"/>
