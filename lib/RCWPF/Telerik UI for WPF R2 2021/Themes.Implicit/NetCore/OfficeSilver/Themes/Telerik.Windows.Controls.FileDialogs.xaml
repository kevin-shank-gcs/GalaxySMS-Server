                <Grid Grid.Row="1" Grid.Column="2" Margin="0 10 0 0">
                    <Grid.ColumnDefinitions>
                        <ColumnDefinition Width="*"/>
                        <ColumnDefinition Width="12"/>
                        <ColumnDefinition Width="*"/>
                    </Grid.ColumnDefinitions>
                    <telerik:RadButton x:Name="OpenButton"
                            MinWidth="98"
                            Command="historyNavigationPane:FileDialogsCommands.Open"
                            telerikControls:DialogWindowBase.ResponseButton="Accept"
                            Content="{telerik:LocalizableResource Key=FileDialogs_Open}"/>
                    <telerik:RadButton x:Name="CancelButton"
                            Grid.Column="2"
                            MinWidth="98"
                            Command="historyNavigationPane:FileDialogsCommands.Cancel"
                            telerikControls:DialogWindowBase.ResponseButton="Cancel"
                            Content="{telerik:LocalizableResource Key=FileDialogs_Cancel}"/>
                </Grid>
            </Grid>
        </Grid>
        <ControlTemplate.Triggers>
            <DataTrigger Binding="{Binding IsFilterApplied}" Value="True">
                <Setter Property="Grid.ColumnSpan" Value="1" TargetName="PART_SelectedFilesAutoCompleteBox"/>
                <Setter Property="Margin" Value="0 0 12 0" TargetName="PART_SelectedFilesAutoCompleteBox"/>
            </DataTrigger>
        </ControlTemplate.Triggers>
    </ControlTemplate>
    <ControlTemplate TargetType="historyNavigationPane:SaveFileDialogControl" x:Key="SaveFileDialogControlTemplate">
        <Grid>
            <Grid.RowDefinitions>
                <RowDefinition Height="*"/>
                <RowDefinition Height="Auto"/>
            </Grid.RowDefinitions>
            <historyNavigationPane:ExplorerControl x:Name="PART_Explorer" ExpandToCurrentDirectory="{Binding RelativeSource={RelativeSource AncestorType={x:Type telerik:RadSaveFileDialog}}, Path=ExpandToCurrentDirectory, Mode=TwoWay, UpdateSourceTrigger=PropertyChanged}"/>
            <Border
                    Grid.Row="1"
                    Background="{TemplateBinding Background}"
                    BorderBrush="{TemplateBinding BorderBrush}"
                    BorderThickness="{TemplateBinding BorderThickness}"
                    CornerRadius="0 0 3 3"/>
            <Grid Grid.Row="1" x:Name="PART_OperationsPane" Margin="{TemplateBinding Padding}">
                <Grid.RowDefinitions>
                    <RowDefinition Height="Auto"/>
                    <RowDefinition Height="Auto"/>
                    <RowDefinition Height="Auto"/>
                </Grid.RowDefinitions>
                <Grid.ColumnDefinitions>
                    <ColumnDefinition Width="Auto"/>
                    <ColumnDefinition Width="*"/>
                    <ColumnDefinition Width="Auto"/>
                </Grid.ColumnDefinitions>
                <TextBlock Text="{telerik:LocalizableResource Key=FileDialogs_FileName}" Margin="0 0 10 0" VerticalAlignment="Center" HorizontalAlignment="Right"/>
                <TextBlock Text="{telerik:LocalizableResource Key=FileDialogs_SaveAsType}" Grid.Row="1" Margin="0 0 10 0" VerticalAlignment="Center" HorizontalAlignment="Right"/>
                <telerik:RadAutoCompleteBox x:Name="PART_SelectedFilesAutoCompleteBox"
                        AutomationProperties.Name="selected files"
                        AutoCompleteMode="SuggestAppend"
                        ItemsSource="{Binding RootAndCurrentFileSystemObjects}"
                        SelectedItem="{Binding SelectedFileSystemObject, Mode=TwoWay, UpdateSourceTrigger=PropertyChanged}"
                        SearchText="{Binding Name, Mode=TwoWay, UpdateSourceTrigger=PropertyChanged}"
                        DisplayMemberPath="Name"
                        Grid.Column="1"
                        Grid.ColumnSpan="2"
                        SelectionMode="Single"
                        TextSearchMode="StartsWith"
                        TextBoxStyle="{StaticResource FileDialogWatermarkStyle}"/>
                <telerik:RadComboBox x:Name="PART_FilterComboBox"
                        AutomationProperties.Name="filter"
                        Grid.Row="1"
                        Grid.Column="1"
                        Grid.ColumnSpan="2"
                        Margin="0 10"
                        ItemsSource="{Binding FilterDescriptors}"
                        DisplayMemberPath="FilterName"
                        SelectedIndex="{Binding FilterIndex}"
                        telerik:ComboBoxToolTipHelper.ShowToolTipOnTrimmedText="True">
                    <ToolTipService.ToolTip>
                        <ToolTip VerticalOffset="2" Placement="Relative" Content="{Binding SelectedFilterName}" HorizontalOffset="2"/>
                    </ToolTipService.ToolTip>
                </telerik:RadComboBox>
                <Grid Grid.Row="2" Grid.Column="2" Margin="0 10 0 0">
                    <Grid.ColumnDefinitions>
                        <ColumnDefinition Width="*"/>
                        <ColumnDefinition Width="12"/>
                        <ColumnDefinition Width="*"/>
                    </Grid.ColumnDefinitions>
                    <telerik:RadButton x:Name="SaveButton" MinWidth="98" telerikControls:DialogWindowBase.ResponseButton="Accept" Command="historyNavigationPane:FileDialogsCommands.Save">
                        <telerik:RadButton.Style>
                            <Style TargetType="telerik:RadButton" BasedOn="{StaticResource RadButtonStyle}">
                                <Setter Property="Content" Value="{Binding Manager.Culture, Source={StaticResource localizationManagerProvider}, Converter={StaticResource cultureToStringConverter}, ConverterParameter=FileDialogs_Save}"/>
                                <Style.Triggers>
                                    <DataTrigger Binding="{Binding IsDirectorySelected}" Value="True">
                                        <Setter Property="Content" Value="{Binding Manager.Culture, Source={StaticResource localizationManagerProvider}, Converter={StaticResource cultureToStringConverter}, ConverterParameter=FileDialogs_Open}"/>
                                    </DataTrigger>
                                </Style.Triggers>
                            </Style>
                        </telerik:RadButton.Style>
                    </telerik:RadButton>
                    <telerik:RadButton x:Name="CancelButton"
                            Grid.Column="2"
                            MinWidth="98"
                            Command="historyNavigationPane:FileDialogsCommands.Cancel"
                            telerikControls:DialogWindowBase.ResponseButton="Cancel"
                            Content="{telerik:LocalizableResource Key=FileDialogs_Cancel}"/>
                </Grid>
            </Grid>
        </Grid>
    </ControlTemplate>
    <ControlTemplate TargetType="historyNavigationPane:OpenFolderDialogControl" x:Key="OpenFolderDialogControlTemplate">
        <Grid>
            <Grid.RowDefinitions>
                <RowDefinition Height="*"/>
                <RowDefinition Height="Auto"/>
            </Grid.RowDefinitions>
            <historyNavigationPane:ExplorerControl x:Name="PART_Explorer" ExpandToCurrentDirectory="{Binding RelativeSource={RelativeSource AncestorType={x:Type telerik:RadOpenFolderDialog}}, Path=ExpandToCurrentDirectory, Mode=TwoWay, UpdateSourceTrigger=PropertyChanged}"/>
            <Border
                    Grid.Row="1"
                    Background="{TemplateBinding Background}"
                    BorderBrush="{TemplateBinding BorderBrush}"
                    BorderThickness="{TemplateBinding BorderThickness}"
                    CornerRadius="0 0 3 3"/>
            <Grid Grid.Row="1" x:Name="PART_OperationsPane" Margin="{TemplateBinding Padding}">
                <Grid.RowDefinitions>
                    <RowDefinition Height="*"/>
                    <RowDefinition Height="Auto"/>
                </Grid.RowDefinitions>
                <Grid.ColumnDefinitions>
                    <ColumnDefinition Width="Auto"/>
                    <ColumnDefinition Width="*"/>
                    <ColumnDefinition Width="Auto"/>
                </Grid.ColumnDefinitions>
                <TextBlock Text="{telerik:LocalizableResource Key=FileDialogs_Folder}" Margin="0 0 10 0" VerticalAlignment="Center"/>
                <telerik:RadAutoCompleteBox x:Name="PART_SelectedFilesAutoCompleteBox"
                        AutomationProperties.Name="selected files"
                        AutoCompleteMode="SuggestAppend"
                        ScrollViewer.VerticalScrollBarVisibility="Auto"
                        MaxHeight="47"
                        ItemsSource="{Binding RootAndCurrentFileSystemObjects}"
                        SearchText="{Binding Name, Mode=TwoWay, UpdateSourceTrigger=PropertyChanged}"
                        DisplayMemberPath="Name"
                        Grid.ColumnSpan="2"
                        Grid.Column="1"
                        TextSearchMode="StartsWith"
                        TextBoxStyle="{StaticResource FileDialogWatermarkStyle}"/>
                <Grid Grid.Row="1" Grid.Column="2" Margin="0 10 0 0">
                    <Grid.ColumnDefinitions>
                        <ColumnDefinition Width="*"/>
                        <ColumnDefinition Width="12"/>
                        <ColumnDefinition Width="*"/>
                    </Grid.ColumnDefinitions>
                    <telerik:RadButton x:Name="OpenButton"
                            MinWidth="98"
                            telerikControls:DialogWindowBase.ResponseButton="Accept"
                            Content="{telerik:LocalizableResource Key=FileDialogs_OpenFolder}"
                            Command="historyNavigationPane:FileDialogsCommands.SelectFolder"/>
                    <telerik:RadButton x:Name="CancelButton"
                            Grid.Column="2"
                            MinWidth="98"
                            Command="historyNavigationPane:FileDialogsCommands.Cancel"
                            telerikControls:DialogWindowBase.ResponseButton="Cancel"
                            Content="{telerik:LocalizableResource Key=FileDialogs_Cancel}"/>
                </Grid>
            </Grid>
        </Grid>
    </ControlTemplate>
    <Style TargetType="historyNavigationPane:OpenFileDialogControl" x:Key="OpenFileDialogControlStyle">
        <Setter Property="Template" Value="{StaticResource OpenFileDialogControlTemplate}"/>
        <Setter Property="Padding" Value="35 10 0 10"/>
        <Setter Property="IsTabStop" Value="False"/>
    </Style>
    <Style TargetType="historyNavigationPane:SaveFileDialogControl" x:Key="SaveFileDialogControlStyle">
        <Setter Property="Template" Value="{StaticResource SaveFileDialogControlTemplate}"/>
        <Setter Property="Padding" Value="35 10 0 10"/>
        <Setter Property="MinHeight" Value="107"/>
        <Setter Property="IsTabStop" Value="False"/>
    </Style>
    <Style TargetType="historyNavigationPane:OpenFolderDialogControl" x:Key="OpenFolderDialogControlStyle">
        <Setter Property="Template" Value="{StaticResource OpenFolderDialogControlTemplate}"/>
        <Setter Property="Padding" Value="35 10 0 10"/>
        <Setter Property="IsTabStop" Value="False"/>
    </Style>
    <Style TargetType="historyNavigationPane:OpenFileDialogControl" BasedOn="{StaticResource OpenFileDialogControlStyle}"/>
    <Style TargetType="historyNavigationPane:SaveFileDialogControl" BasedOn="{StaticResource SaveFileDialogControlStyle}"/>
    <Style TargetType="historyNavigationPane:OpenFolderDialogControl" BasedOn="{StaticResource OpenFolderDialogControlStyle}"/>
    <ControlTemplate x:Key="DialogWindowTemplate" TargetType="telerikControls:DialogWindowBase">
        <Grid x:Name="LayoutRoot">
            <Border x:Name="Shadow" CornerRadius="10" BorderBrush="#11000000" BorderThickness="1">
                <Border CornerRadius="9" BorderBrush="#22000000" BorderThickness="1">
                    <Border CornerRadius="8" BorderBrush="#33000000" BorderThickness="1">
                        <Border CornerRadius="7" BorderBrush="#44000000" BorderThickness="0 0 1 1">
                            <Border CornerRadius="6" BorderBrush="#55000000" BorderThickness="0 0 1 1"/>
                        </Border>
                    </Border>
                </Border>
            </Border>
            <Border x:Name="WindowOuterBorder" Margin="3 3 5 5" Grid.RowSpan="2" BorderBrush="{TemplateBinding BorderBrush}" BorderThickness="1" CornerRadius="5">
                <Border x:Name="WindowInnerBorder" BorderBrush="{StaticResource WindowInnerBorder}" BorderThickness="1" CornerRadius="4">
                    <AdornerDecorator>
                        <Grid x:Name="AdornerLayoutRoot">
                            <Grid.RowDefinitions>
                                <RowDefinition x:Name="WindowHeaderRow" Height="Auto"/>
                                <RowDefinition Height="*"/>
                            </Grid.RowDefinitions>
                            <Rectangle x:Name="HeaderBackgroundVisual" Grid.Row="0" Fill="{StaticResource WindowHeaderBackground}"/>
                            <Rectangle x:Name="BackgroundVisual" Grid.Row="1" Fill="{TemplateBinding BorderBackground}"/>
                            <Grid x:Name="Header">
                                <Grid.ColumnDefinitions>
                                    <ColumnDefinition Width="*"/>
                                    <ColumnDefinition Width="Auto"/>
                                </Grid.ColumnDefinitions>
                                <Grid Grid.Column="0">
                                    <Grid.ColumnDefinitions>
                                        <ColumnDefinition Width="Auto"/>
                                        <ColumnDefinition Width="*"/>
                                    </Grid.ColumnDefinitions>
                                    <ContentPresenter x:Name="IconContent"
                                            Grid.Column="0"
                                            HorizontalAlignment="Left"
                                            VerticalAlignment="Center"
                                            Margin="10 0 0 0"
                                            Content="{TemplateBinding Icon}"
                                            ContentTemplate="{TemplateBinding IconTemplate}"/>
                                    <ContentPresenter x:Name="HeaderContent"
                                            Grid.Column="1"
                                            Margin="4 0 0 0"
                                            TextBlock.Foreground="{TemplateBinding Foreground}"
                                            HorizontalAlignment="Stretch"
                                            VerticalAlignment="Center"
                                            Content="{TemplateBinding Header}"
                                            ContentTemplate="{TemplateBinding HeaderTemplate}"
                                            SnapsToDevicePixels="True"
                                            ContentTemplateSelector="{TemplateBinding HeaderTemplateSelector}"/>
                                </Grid>
                                <Border x:Name="PART_HeaderButtonsBorder" Grid.Column="1" Margin="0 0 7 0">
                                    <telerik:RadButton x:Name="PART_CloseButton"
                                            Command="{x:Static telerik:WindowCommands.Close}"
                                            Style="{StaticResource WindowButtonStyle}"
                                            Visibility="{Binding IsEnabled, RelativeSource={RelativeSource Self}, Converter={StaticResource BooleanToVisibilityConverter}}">
                                        <ToolTipService.ToolTip>
                                            <ToolTip telerik:LocalizationManager.ResourceKey="Close"/>
                                        </ToolTipService.ToolTip>
                                        <Path
                                                Fill="{StaticResource WindowButtonIconFill}"
                                                Stroke="{StaticResource WindowButtonIconStroke}"
                                                Width="14"
                                                Height="14"
                                                Data="M1.5,3.5 L2.5,2.5 4.5,2.5 6.5,4.5 8.5,2.5 10.5,2.5 11.5,3.5 8.5,6.5 8.5,7.5 11.5,10.5 10.5,11.5 8.5,11.5 6.5,9.5 4.5,11.5 2.5,11.5 1.5,10.5 4.5,7.5 4.5,6.5 z"/>
                                    </telerik:RadButton>
                                </Border>
                            </Grid>
                            <Border x:Name="ContentOuterBorder" Grid.Row="1" Background="{TemplateBinding Background}" Margin="{TemplateBinding BorderThickness}">
                                <ContentPresenter x:Name="ContentElement" Margin="{TemplateBinding Padding}" Content="{TemplateBinding Content}" ContentTemplate="{TemplateBinding ContentTemplate}"/>
                            </Border>
                            <Grid x:Name="ResizeIcon" Grid.Row="1" Margin="0 0 1 1" HorizontalAlignment="Right" VerticalAlignment="Bottom" Width="11" Height="11">
                                <Path
                                        Fill="Black"
                                        Stretch="None"
                                        Stroke="{x:Null}"
                                        Margin="1 1 0 0"
                                        Data="M8,8 L10,8 10,10 8,10 z M4,8 L6,8 6,10 4,10 z M0,8 L2,8 2,10 0,10 z M8,4 L10,4 10,6 8,6 z M4,4 L6,4 6,6 4,6 z M8,0 L10,0 10,2 8,2 z"/>
                                <Path
                                        Fill="White"
                                        Stretch="None"
                                        Stroke="{x:Null}"
                                        Data="M8,8 L10,8 10,10 8,10 z M4,8 L6,8 6,10 4,10 z M0,8 L2,8 2,10 0,10 z M8,4 L10,4 10,6 8,6 z M4,4 L6,4 6,6 4,6 z M8,0 L10,0 10,2 8,2 z"/>
                            </Grid>
                        </Grid>
                    </AdornerDecorator>
                </Border>
            </Border>
        </Grid>
        <ControlTemplate.Triggers>
            <Trigger Property="IsInActiveState" Value="False">
                <Setter TargetName="Shadow" Property="Opacity" Value="0.5"/>
                <Setter TargetName="Header" Property="Opacity" Value="0.7"/>
            </Trigger>
            <Trigger Property="WindowState" Value="Maximized">
                <Setter TargetName="WindowOuterBorder" Property="CornerRadius" Value="0"/>
                <Setter TargetName="WindowOuterBorder" Property="BorderThickness" Value="0"/>
                <Setter TargetName="WindowOuterBorder" Property="Margin" Value="0"/>
                <Setter TargetName="WindowInnerBorder" Property="CornerRadius" Value="0"/>
                <Setter TargetName="WindowInnerBorder" Property="BorderThickness" Value="0"/>
                <Setter TargetName="ResizeIcon" Property="Visibility" Value="Collapsed"/>
            </Trigger>
            <Trigger Property="WindowState" Value="Minimized">
                <Setter TargetName="WindowHeaderRow" Property="Height" Value="28"/>
                <Setter TargetName="WindowHeaderRow" Property="MinHeight" Value="28"/>
                <Setter TargetName="HeaderContent" Property="Margin" Value="0"/>
                <Setter TargetName="LayoutRoot" Property="HorizontalAlignment" Value="Left"/>
                <Setter TargetName="LayoutRoot" Property="VerticalAlignment" Value="Top"/>
                <Setter TargetName="AdornerLayoutRoot" Property="Width" Value="145"/>
            </Trigger>
            <MultiTrigger>
                <MultiTrigger.Conditions>
                    <Condition Property="Content" SourceName="IconContent" Value="{x:Null}"/>
                    <Condition Property="ContentTemplate" SourceName="IconContent" Value="{x:Null}"/>
                </MultiTrigger.Conditions>
                <Setter TargetName="IconContent" Property="Visibility" Value="Collapsed"/>
                <Setter TargetName="HeaderContent" Property="Margin" Value="10 0 0 0"/>
            </MultiTrigger>
        </ControlTemplate.Triggers>
    </ControlTemplate>
    <Style x:Key="DialogWindowStyle" TargetType="telerikControls:DialogWindowBase">
        <Setter Property="Template" Value="{StaticResource DialogWindowTemplate}"/>
        <Setter Property="IsTabStop" Value="False"/>
        <Setter Property="BorderThickness" Value="6 0 6 4"/>
        <Setter Property="CaptionHeight" Value="29"/>
        <Setter Property="ResizeBorder" Value="8 6 10 10"/>
        <Setter Property="BorderBrush" Value="{StaticResource WindowOuterBorder}"/>
        <Setter Property="BorderBackground" Value="{StaticResource WindowBorderBackground}"/>
        <Setter Property="MinWidth" Value="610"/>
        <Setter Property="MinHeight" Value="300"/>
        <Setter Property="Width" Value="946"/>
        <Setter Property="Height" Value="593"/>
        <Setter Property="HorizontalAlignment" Value="Stretch"/>
        <Setter Property="VerticalAlignment" Value="Stretch"/>
        <Setter Property="HorizontalContentAlignment" Value="Stretch"/>
        <Setter Property="VerticalContentAlignment" Value="Stretch"/>
        <Setter Property="WindowState" Value="Normal"/>
        <Setter Property="FocusVisualStyle" Value="{x:Null}"/>
        <Setter Property="animation:AnimationManager.AnimationSelector">
            <Setter.Value>
                <animation:AnimationSelector>
                    <animation:AnimationGroup AnimationName="Show">
                        <animation:FadeAnimation Direction="In" TargetElementName="LayoutRoot"/>
                        <animation:ScaleAnimation Direction="In" TargetElementName="LayoutRoot" MinScale="0.9"/>
                    </animation:AnimationGroup>
                    <animation:AnimationGroup AnimationName="Hide">
                        <animation:FadeAnimation Direction="Out" TargetElementName="LayoutRoot"/>
                        <animation:ScaleAnimation Direction="Out" TargetElementName="LayoutRoot" MinScale="0.9"/>
                    </animation:AnimationGroup>
                </animation:AnimationSelector>
            </Setter.Value>
        </Setter>
        <Setter Property="navigation:RadWindowInteropHelper.OpaqueWindowBackground" Value="{StaticResource WindowBorderBackground}"/>
        <Style.Triggers>
            <Trigger Property="navigation:RadWindowInteropHelper.AllowTransparency" Value="False">
                <Setter Property="navigation:RadWindowInteropHelper.ClipMaskCornerRadius" Value="10"/>
                <Setter Property="ResizeBorder" Value="6"/>
                <Setter Property="CaptionHeight" Value="26"/>
                <Setter Property="animation:AnimationManager.IsAnimationEnabled" Value="False"/>
            </Trigger>
        </Style.Triggers>
    </Style>
    <Style x:Key="RadOpenFileDialogStyle" TargetType="telerikControls:RadOpenFileDialog" BasedOn="{StaticResource DialogWindowStyle}">
        <Setter Property="Header" Value="{Binding Manager.Culture, Source={StaticResource localizationManagerProvider}, Converter={StaticResource cultureToStringConverter}, ConverterParameter=FileDialogs_OpenFileDialogHeader}"/>
    </Style>
    <Style x:Key="RadSaveFileDialogStyle" TargetType="telerikControls:RadSaveFileDialog" BasedOn="{StaticResource DialogWindowStyle}">
        <Setter Property="Header" Value="{Binding Manager.Culture, Source={StaticResource localizationManagerProvider}, Converter={StaticResource cultureToStringConverter}, ConverterParameter=FileDialogs_SaveFileDialogHeader}"/>
    </Style>
    <Style x:Key="RadOpenFolderDialogStyle" TargetType="telerikControls:RadOpenFolderDialog" BasedOn="{StaticResource DialogWindowStyle}">
        <Setter Property="Header" Value="{Binding Manager.Culture, Source={StaticResource localizationManagerProvider}, Converter={StaticResource cultureToStringConverter}, ConverterParameter=FileDialogs_OpenFolderDialogHeader}"/>
    </Style>
    <Style TargetType="telerikControls:RadOpenFileDialog" BasedOn="{StaticResource RadOpenFileDialogStyle}"/>
    <Style TargetType="telerikControls:RadSaveFileDialog" BasedOn="{StaticResource RadSaveFileDialogStyle}"/>
    <Style TargetType="telerikControls:RadOpenFolderDialog" BasedOn="{StaticResource RadOpenFolderDialogStyle}"/>
</ResourceDictionary>
