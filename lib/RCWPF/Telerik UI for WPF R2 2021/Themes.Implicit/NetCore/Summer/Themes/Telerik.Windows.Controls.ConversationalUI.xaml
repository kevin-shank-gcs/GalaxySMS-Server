                        <Popup x:Name="PART_InputPopup"
                                telerik1:PopupExtensions.IsOpen="{Binding IsTooltipOpen, RelativeSource={RelativeSource TemplatedParent}, Mode=TwoWay}"
                                telerik1:PopupExtensions.Placement="Top"
