                        <Trigger Property="SubTitle" Value="{x:Null}">
                            <Setter TargetName="SubTitle" Property="Visibility" Value="Collapsed"/>
                        </Trigger>
                        <Trigger Property="Text" Value="{x:Null}">
                            <Setter TargetName="Text" Property="Visibility" Value="Collapsed"/>
                        </Trigger>
