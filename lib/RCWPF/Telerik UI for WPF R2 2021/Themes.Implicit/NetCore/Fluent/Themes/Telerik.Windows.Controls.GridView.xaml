    <ControlTemplate x:Key="CloseButtonTemplate" TargetType="Button">
        <Border x:Name="CloseButton" Background="{TemplateBinding Background}" UseLayoutRounding="True">
            <telerik:RadGlyph FontSize="16" Foreground="{TemplateBinding Foreground}" HorizontalAlignment="Center" VerticalAlignment="Center" Glyph="{StaticResource GlyphClose}"/>
        </Border>
