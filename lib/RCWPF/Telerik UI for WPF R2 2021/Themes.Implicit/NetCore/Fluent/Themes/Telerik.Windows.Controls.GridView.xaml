        <Setter Property="HighlightBrush" Value="{telerik1:FluentResource ResourceKey=ValidationBrush}"/>
    </Style>
    <Style TargetType="grid:HighlightTextBlock" BasedOn="{StaticResource HighlightTextBlockStyle}"/>
    <Style x:Key="PinUnpinPathStyle" TargetType="Path">
        <Setter Property="HorizontalAlignment" Value="Center"/>
        <Setter Property="VerticalAlignment" Value="Center"/>
        <Setter Property="StrokeThickness" Value="0"/>
        <Setter Property="Width" Value="12"/>
        <Setter Property="Height" Value="12"/>
        <Setter Property="Stretch" Value="Uniform"/>
    </Style>
    <Style x:Key="GridViewPinButtonStyle" TargetType="grid:GridViewPinButton">
        <Setter Property="PathStyle" Value="{StaticResource PinUnpinPathStyle}"/>
        <Setter Property="PathGeometry" Value="{telerik:RadGlyph Glyph={StaticResource GlyphUnpin}, Font=TelerikWebUI, Type=Path}"/>
        <Setter Property="Foreground" Value="{telerik1:FluentResource ResourceKey=IconBrush}"/>
        <Setter Property="MinHeight" Value="28"/>
        <Setter Property="MinWidth" Value="22"/>
        <Setter Property="IsBackgroundVisible" Value="False"/>
        <Setter Property="Command" Value="{x:Static telerik:RadGridViewCommands.TogglePinnedRowState}"/>
        <Setter Property="CommandParameter" Value="{Binding}"/>
        <Setter Property="SnapsToDevicePixels" Value="True"/>
        <Setter Property="BorderThickness" Value="0"/>
        <Setter Property="HorizontalContentAlignment" Value="Stretch"/>
        <Setter Property="VerticalContentAlignment" Value="Stretch"/>
        <Setter Property="FocusVisualStyle" Value="{x:Null}"/>
        <Setter Property="IsTabStop" Value="False"/>
        <Setter Property="Padding" Value="0 0 2 0"/>
        <Setter Property="Template">
            <Setter.Value>
                <ControlTemplate TargetType="telerik:RadPathButton">
                    <Grid Background="Transparent">
                        <VisualStateManager.VisualStateGroups>
                            <VisualStateGroup x:Name="BackgroundVisibility">
                                <VisualState x:Name="BackgroundIsHidden">
                                    <Storyboard>
                                        <ObjectAnimationUsingKeyFrames Storyboard.TargetName="OuterBorder" Storyboard.TargetProperty="Visibility">
                                            <DiscreteObjectKeyFrame KeyTime="0">
                                                <DiscreteObjectKeyFrame.Value>
                                                    <Visibility>Collapsed</Visibility>
                                                </DiscreteObjectKeyFrame.Value>
                                            </DiscreteObjectKeyFrame>
                                        </ObjectAnimationUsingKeyFrames>
                                    </Storyboard>
                                </VisualState>
                                <VisualState x:Name="BackgroundIsVisible"/>
                            </VisualStateGroup>
                        </VisualStateManager.VisualStateGroups>
                        <Border x:Name="OuterBorder"
                                BorderThickness="{TemplateBinding BorderThickness}"
                                BorderBrush="{TemplateBinding BorderBrush}"
                                Background="{TemplateBinding Background}"
                                CornerRadius="{TemplateBinding CornerRadius}"/>
                        <Path x:Name="Path"
                                Fill="{TemplateBinding Foreground}"
                                Style="{TemplateBinding PathStyle}"
                                Data="{TemplateBinding PathGeometry}"
                                SnapsToDevicePixels="{TemplateBinding SnapsToDevicePixels}"
                                Margin="{TemplateBinding Padding}"/>
                    </Grid>
                    <ControlTemplate.Triggers>
                        <Trigger Property="IsMouseOver" Value="True">
                            <Setter Property="Foreground" Value="{telerik1:FluentResource ResourceKey=AccentMouseOverBrush}"/>
                        </Trigger>
                        <Trigger Property="IsPressed" Value="True">
                            <Setter Property="Foreground" Value="{telerik1:FluentResource ResourceKey=AccentPressedBrush}"/>
                        </Trigger>
                    </ControlTemplate.Triggers>
                </ControlTemplate>
            </Setter.Value>
        </Setter>
        <Style.Triggers>
            <Trigger Property="IsPinned" Value="True">
                <Setter Property="PathGeometry" Value="{telerik:RadGlyph Glyph={StaticResource GlyphPin}, Font=TelerikWebUI, Type=Path}"/>
            </Trigger>
        </Style.Triggers>
    </Style>
    <Style TargetType="grid:GridViewPinButton" BasedOn="{StaticResource GridViewPinButtonStyle}"/>
    <ControlTemplate x:Key="SearchAutoCompleteBoxCloseButtonControlTemplate" TargetType="telerik:RadButton">
        <Grid x:Name="LayoutRoot" Background="Transparent">
            <Ellipse x:Name="Background" Fill="{TemplateBinding Background}" Stroke="{x:Null}" StrokeThickness="0" VerticalAlignment="Stretch" HorizontalAlignment="Stretch"/>
            <telerik:RadGlyph x:Name="Content"
                    Margin="{TemplateBinding Padding}"
                    VerticalAlignment="{TemplateBinding VerticalContentAlignment}"
                    HorizontalAlignment="{TemplateBinding HorizontalContentAlignment}"
                    FontSize="16"
                    Foreground="{TemplateBinding Foreground}"
                    Glyph="{StaticResource GlyphClose}"/>
        </Grid>
        <ControlTemplate.Triggers>
            <Trigger Property="IsMouseOver" Value="True">
                <Setter Property="Background" Value="{telerik1:FluentResource ResourceKey=MouseOverBrush}"/>
            </Trigger>
            <Trigger Property="IsPressed" Value="True">
                <Setter Property="Background" Value="{telerik1:FluentResource ResourceKey=PressedBrush}"/>
            </Trigger>
            <Trigger Property="IsEnabled" Value="False">
                <Setter Property="Opacity" TargetName="Content" Value="{telerik1:FluentResource ResourceKey=DisabledOpacity}"/>
            </Trigger>
        </ControlTemplate.Triggers>
    </ControlTemplate>
    <Style x:Key="SearchAutoCompleteBoxCloseButtonStyle" TargetType="telerik:RadButton">
        <Setter Property="Template" Value="{StaticResource SearchAutoCompleteBoxCloseButtonControlTemplate}"/>
        <Setter Property="Foreground" Value="{telerik1:FluentResource ResourceKey=MarkerInvertedBrush}"/>
        <Setter Property="Width" Value="16"/>
        <Setter Property="Height" Value="16"/>
        <Setter Property="Background" Value="Transparent"/>
        <Setter Property="Padding" Value="0"/>
        <Setter Property="Margin" Value="2 1"/>
        <Setter Property="BorderThickness" Value="0"/>
    </Style>
    <Style x:Key="SearchAutoCompleteBoxItemStyle" TargetType="mccb:SearchAutoCompleteBoxItem">
        <Setter Property="mat:MaterialAssist.CornerRadius" Value="12"/>
        <Setter Property="Foreground" Value="{telerik1:FluentResource ResourceKey=MarkerInvertedBrush}"/>
        <Setter Property="Background" Value="{telerik1:FluentResource ResourceKey=AccentBrush}"/>
        <Setter Property="BorderThickness" Value="0"/>
        <Setter Property="BorderBrush" Value="Transparent"/>
        <Setter Property="Padding" Value="5 4"/>
        <Setter Property="Margin" Value="1 1 0 0"/>
        <Setter Property="Template">
            <Setter.Value>
                <ControlTemplate TargetType="mccb:SearchAutoCompleteBoxItem">
                    <Grid>
                        <Border x:Name="BorderVisual"
                                Background="{TemplateBinding Background}"
                                BorderThickness="{TemplateBinding BorderThickness}"
                                BorderBrush="{TemplateBinding BorderBrush}"
                                CornerRadius="{TemplateBinding mat:MaterialAssist.CornerRadius}">
                            <Grid>
                                <Grid.ColumnDefinitions>
                                    <ColumnDefinition Width="*"/>
                                    <ColumnDefinition Width="Auto"/>
                                </Grid.ColumnDefinitions>
                                <ContentPresenter x:Name="contentPresenter"
                                        Margin="{TemplateBinding Padding}"
                                        HorizontalAlignment="{TemplateBinding HorizontalContentAlignment}"
                                        VerticalAlignment="{TemplateBinding VerticalContentAlignment}"
                                        TextBlock.Foreground="{TemplateBinding Foreground}"/>
                                <telerik:RadButton x:Name="RemoveItemButton"
                                        Grid.Column="1"
                                        HorizontalAlignment="Right"
                                        VerticalAlignment="Stretch"
                                        CommandParameter="{Binding}"
                                        Cursor="Arrow"
                                        IsTabStop="False"
                                        Focusable="False"
                                        Command="mccb:SearchAutoCompleteBoxCommands.RemoveItem"
                                        Style="{StaticResource SearchAutoCompleteBoxCloseButtonStyle}"/>
                            </Grid>
                        </Border>
                    </Grid>
                    <ControlTemplate.Triggers>
                        <Trigger Property="IsEnabled" Value="False">
                            <Setter TargetName="contentPresenter" Property="Opacity" Value="{telerik1:FluentResource ResourceKey=DisabledOpacity}"/>
                        </Trigger>
                    </ControlTemplate.Triggers>
                </ControlTemplate>
            </Setter.Value>
        </Setter>
    </Style>
    <ControlTemplate x:Key="SearchAutoCompleteBoxTemplate" TargetType="mccb:SearchAutoCompleteBox">
        <Grid>
            <Border x:Name="BorderVisual"
                    BorderBrush="{TemplateBinding BorderBrush}"
                    BorderThickness="{TemplateBinding BorderThickness}"
                    Background="{TemplateBinding Background}"
                    CornerRadius="{TemplateBinding mat:MaterialAssist.CornerRadius}"
                    SnapsToDevicePixels="{TemplateBinding SnapsToDevicePixels}"/>
            <Border x:Name="ReadOnlyVisual"
                    BorderBrush="{telerik1:FluentResource ResourceKey=ReadOnlyBorderBrush}"
                    BorderThickness="{TemplateBinding BorderThickness}"
                    Background="{telerik1:FluentResource ResourceKey=ReadOnlyBackgroundBrush}"
                    CornerRadius="{TemplateBinding mat:MaterialAssist.CornerRadius}"
                    SnapsToDevicePixels="{TemplateBinding SnapsToDevicePixels}"
                    Visibility="{Binding IsReadOnly, RelativeSource={RelativeSource TemplatedParent}, Converter={StaticResource BooleanToVisibilityConverter}}"/>
            <ScrollViewer x:Name="PART_ScrollViewer"
                    Height="{TemplateBinding Height}"
                    VerticalAlignment="{TemplateBinding VerticalContentAlignment}"
                    VerticalContentAlignment="{TemplateBinding VerticalContentAlignment}"
                    BorderThickness="0"
                    Padding="0"
                    Background="Transparent"
                    IsTabStop="False"
                    Focusable="False"
                    Margin="{TemplateBinding BorderThickness}"
                    HorizontalScrollBarVisibility="{TemplateBinding ScrollViewer.HorizontalScrollBarVisibility}"
                    VerticalScrollBarVisibility="{TemplateBinding ScrollViewer.VerticalScrollBarVisibility}"
                    SnapsToDevicePixels="{TemplateBinding SnapsToDevicePixels}">
                <mccb:SearchAutoCompleteBoxesItemsControl x:Name="PART_Boxes"
                        SelectionBoxesVisibility="{TemplateBinding SelectionBoxesVisibility}"
                        IsTabStop="False"
                        Foreground="{TemplateBinding Foreground}"
                        Margin="{TemplateBinding Padding}"
                        ItemsSource="{TemplateBinding SelectedItems}">
                    <mccb:SearchAutoCompleteBoxesItemsControl.Editor>
                        <telerik:RadWatermarkTextBox x:Name="WatermarkTextBox"
                                AutomationProperties.Name="{TemplateBinding AutomationProperties.Name}"
                                SelectionOnFocus="{TemplateBinding SelectionOnFocus}"
                                BorderThickness="0"
                                Background="Transparent"
                                mat:MaterialAssist.FocusBrush="{x:Null}"
                                IsReadOnly="{TemplateBinding IsReadOnly}"
                                ReadOnlyBackground="{x:Null}"
                                DisabledBackground="{x:Null}"
                                WatermarkBehavior="HideOnTextEntered"
                                WatermarkContent="{TemplateBinding NullText}"
                                MinWidth="{TemplateBinding EditorMinWidth}"
                                Foreground="{TemplateBinding Foreground}"
                                FontSize="{TemplateBinding FontSize}"
                                FontFamily="{TemplateBinding FontFamily}"/>
                    </mccb:SearchAutoCompleteBoxesItemsControl.Editor>
                    <mccb:SearchAutoCompleteBoxesItemsControl.ItemsPanel>
                        <ItemsPanelTemplate>
                            <mccb:SearchAutoCompleteBoxWrapPanel/>
                        </ItemsPanelTemplate>
                    </mccb:SearchAutoCompleteBoxesItemsControl.ItemsPanel>
                </mccb:SearchAutoCompleteBoxesItemsControl>
            </ScrollViewer>
        </Grid>
        <ControlTemplate.Triggers>
            <Trigger Property="ShowNullText" Value="False">
                <Setter TargetName="WatermarkTextBox" Property="WatermarkContent" Value="{x:Null}"/>
            </Trigger>
        </ControlTemplate.Triggers>
    </ControlTemplate>
    <Style x:Key="SearchAutoCompleteBoxStyle" TargetType="mccb:SearchAutoCompleteBox">
        <Setter Property="Template" Value="{StaticResource SearchAutoCompleteBoxTemplate}"/>
        <Setter Property="mat:MaterialAssist.CornerRadius" Value="{telerik1:FluentResource ResourceKey=CornerRadius}"/>
        <Setter Property="BorderBrush" Value="Transparent"/>
        <Setter Property="BorderThickness" Value="1"/>
        <Setter Property="IsTabStop" Value="False"/>
        <Setter Property="VerticalContentAlignment" Value="Stretch"/>
        <Setter Property="ScrollViewer.VerticalScrollBarVisibility" Value="Auto"/>
    </Style>
    <Style TargetType="mccb:SearchAutoCompleteBoxItem" BasedOn="{StaticResource SearchAutoCompleteBoxItemStyle}"/>
    <Style TargetType="mccb:SearchAutoCompleteBox" BasedOn="{StaticResource SearchAutoCompleteBoxStyle}"/>
    <Style TargetType="controls:RadMultiColumnComboBox" x:Key="RadMultiColumnComboBoxStyle">
        <Style.Setters>
            <Setter Property="mat:MaterialAssist.FocusBrush" Value="{telerik1:FluentResource ResourceKey=AccentFocusedBrush}"/>
            <Setter Property="mat:MaterialAssist.MouseOverBrush" Value="{telerik1:FluentResource ResourceKey=PrimaryMouseOverBrush}"/>
            <Setter Property="Background" Value="{telerik1:FluentResource ResourceKey=PrimaryBrush}"/>
            <Setter Property="BorderBrush" Value="{telerik1:FluentResource ResourceKey=BasicSolidBrush}"/>
            <Setter Property="BorderThickness" Value="1"/>
            <Setter Property="Foreground" Value="{telerik1:FluentResource ResourceKey=MarkerBrush}"/>
            <Setter Property="FontFamily" Value="{telerik1:FluentResource ResourceKey=FontFamily}"/>
            <Setter Property="FontSize" Value="{telerik1:FluentResource ResourceKey=FontSize}"/>
            <Setter Property="MinHeight" Value="28"/>
            <Setter Property="DropDownMinWidth" Value="125"/>
            <Setter Property="DropDownMinHeight" Value="100"/>
            <Setter Property="DropDownHeight" Value="330"/>
            <Setter Property="DropDownMaxHeight" Value="500"/>
            <Setter Property="DropDownMaxWidth" Value="1000"/>
            <Setter Property="EditorMinWidth" Value="50"/>
            <Setter Property="SnapsToDevicePixels" Value="True"/>
            <Setter Property="VerticalContentAlignment" Value="Center"/>
            <Setter Property="IsTabStop" Value="False"/>
            <Setter Property="Validation.ErrorTemplate" Value="{StaticResource ValidationTooltipTemplate}"/>
            <Setter Property="Template">
                <Setter.Value>
                    <ControlTemplate TargetType="controls:RadMultiColumnComboBox">
                        <Grid>
                            <Grid.ColumnDefinitions>
                                <ColumnDefinition/>
                                <ColumnDefinition Width="Auto"/>
                            </Grid.ColumnDefinitions>
                            <Border x:Name="BackgroundVisual"
                                    Grid.ColumnSpan="2"
                                    SnapsToDevicePixels="{TemplateBinding SnapsToDevicePixels}"
                                    Background="{TemplateBinding Background}"
                                    BorderBrush="{TemplateBinding BorderBrush}"
                                    BorderThickness="{TemplateBinding BorderThickness}"/>
                            <mccb:SearchAutoCompleteBox x:Name="PART_SearchAutoCompleteBox"
                                    Grid.Column="0"
                                    AutomationProperties.Name="{TemplateBinding AutomationProperties.Name}"
                                    SelectionOnFocus="{TemplateBinding SelectionOnFocus}"
                                    SelectionBoxesVisibility="{TemplateBinding SelectionBoxesVisibility}"
                                    IsReadOnly="{TemplateBinding IsReadOnly}"
                                    SnapsToDevicePixels="{TemplateBinding SnapsToDevicePixels}"
                                    DeferTextChange="{TemplateBinding DeferTextChange}"
                                    NullText="{TemplateBinding NullText}"
                                    BorderThickness="{TemplateBinding BorderThickness}"
                                    BorderBrush="{TemplateBinding BorderBrush}"
                                    SelectedItems="{TemplateBinding SelectedItems}"
                                    EditorMinWidth="{TemplateBinding EditorMinWidth}"
                                    SelectionBoxTemplate="{TemplateBinding SelectionBoxTemplate}"
                                    SelectionBoxTemplateSelector="{TemplateBinding SelectionBoxTemplateSelector}"
                                    SelectionBoxStyle="{TemplateBinding SelectionBoxStyle}"
                                    SelectionBoxStyleSelector="{TemplateBinding SelectionBoxStyleSelector}"
                                    AutoCompleteProvider="{TemplateBinding AutoCompleteProvider}"/>
                            <telerik:RadDropDownButton x:Name="PART_DropDownButton"
                                    Grid.Column="1"
                                    Padding="0"
                                    SnapsToDevicePixels="{TemplateBinding SnapsToDevicePixels}"
                                    BorderThickness="{Binding RelativeSource={RelativeSource TemplatedParent},Path=BorderThickness, Converter={StaticResource ThicknessToOrientedThicknessConverter}, ConverterParameter=TopRightBottom}"
                                    BorderBrush="Transparent"
                                    IsTabStop="False"
                                    mat:MaterialAssist.FocusBrush="{x:Null}"
                                    KeepOpen="{TemplateBinding KeepDropDownOpen}"
                                    PopupPlacementTarget="{Binding ElementName=BackgroundVisual}"
                                    DropDownWidth="{TemplateBinding DropDownWidth}"
                                    DropDownHeight="{TemplateBinding DropDownHeight}"
                                    DropDownMinWidth="{TemplateBinding DropDownMinWidth}"
                                    DropDownMinHeight="{TemplateBinding DropDownMinHeight}"
                                    DropDownMaxHeight="{TemplateBinding DropDownMaxHeight}"
                                    DropDownMaxWidth="{TemplateBinding DropDownMaxWidth}"
                                    DropDownPlacement="{TemplateBinding DropDownPlacement}">
                                <telerik:RadDropDownButton.DropDownContent>
                                    <Grid>
                                        <Grid.ColumnDefinitions>
                                            <ColumnDefinition/>
                                            <ColumnDefinition Width="Auto"/>
                                        </Grid.ColumnDefinitions>
                                        <Grid.RowDefinitions>
                                            <RowDefinition/>
                                            <RowDefinition Height="Auto"/>
                                        </Grid.RowDefinitions>
                                        <ContentControl Content="{TemplateBinding ItemsSourceProvider}" Grid.ColumnSpan="2" Grid.RowSpan="1">
                                            <ContentControl.ContentTemplate>
                                                <DataTemplate>
                                                    <controls:RadGridView
                                                            GroupRenderMode="Flat"
                                                            CanUserSearchInHiddenColumns="{Binding CanUserSearchInHiddenColumns}"
                                                            AutoGenerateColumns="{Binding AutoGenerateColumns}"
                                                            AlternateRowStyle="{Binding AlternateRowStyle}"
                                                            AlternateRowStyleSelector="{Binding AlternateRowStyleSelector}"
                                                            RowStyle="{Binding RowStyle}"
                                                            RowStyleSelector="{Binding RowStyleSelector}"
                                                            AlternationCount="{Binding AlternationCount}"
                                                            ShowColumnFooters="{Binding ShowColumnFooters}"
                                                            ShowColumnHeaders="{Binding ShowColumnHeaders}"
                                                            ColumnWidth="{Binding ColumnWidth}"
                                                            ContextMenu="{Binding ContextMenu}"
                                                            IsReadOnly="{Binding IsReadOnly}"
                                                            RowIndicatorVisibility="{Binding RowIndicatorVisibility}"
                                                            ShowSearchPanel="False"
                                                            ShowGroupPanel="False"
                                                            CanUserGroupColumns="False"
                                                            CanUserSearch="False"
                                                            DataContext="{Binding}"
                                                            SelectionMode="Extended"
                                                            EnableLostFocusSelectedState="False"
                                                            SearchStateManager="{Binding SearchStateManager}"
                                                            ItemsSource="{Binding CollectionView}"
                                                            IsSynchronizedWithCurrentItem="False"
                                                            BorderThickness="0"/>
                                                </DataTemplate>
                                            </ContentControl.ContentTemplate>
                                        </ContentControl>
                                        <Border
                                                Grid.Row="1"
                                                Grid.ColumnSpan="2"
                                                BorderThickness="0 1 0 0"
                                                BorderBrush="{telerik1:FluentResource ResourceKey=BasicBrush}"
                                                Background="{telerik1:FluentResource ResourceKey=AlternativeBrush}"
                                                Visibility="{TemplateBinding FooterVisibility}">
                                            <ContentPresenter x:Name="Footer" Margin="5" Content="{TemplateBinding FooterContent}" ContentTemplate="{TemplateBinding FooterContentTemplate}" VerticalAlignment="Center">
                                                <ContentPresenter.Resources>
                                                    <Style TargetType="TextBlock">
                                                        <Setter Property="TextTrimming" Value="CharacterEllipsis"/>
                                                        <Setter Property="Margin" Value="{Binding ElementName=CornerResizerIcon, Path=ActualWidth, Converter={StaticResource DoubleToThicknessConverter}, ConverterParameter=Right}"/>
                                                    </Style>
                                                </ContentPresenter.Resources>
                                            </ContentPresenter>
                                        </Border>
                                        <Path x:Name="CornerResizerIcon"
                                                Grid.Row="1"
                                                Grid.ColumnSpan="2"
                                                Data="M8.0,0 L12,0 12,8 12,12 8.0,12 0,12 0,8 8.0,8 z"
                                                Fill="{telerik1:FluentResource ResourceKey=ComplementaryBrush}"
                                                Stretch="Fill"
                                                StrokeThickness="0"
                                                HorizontalAlignment="Right"
                                                VerticalAlignment="Bottom"
                                                Width="12"
                                                Height="12"
                                                Margin="2"
                                                Visibility="{TemplateBinding FooterVisibility}"/>
                                        <Thumb x:Name="PART_HorizontalResizer"
                                                Grid.Column="1"
                                                Grid.RowSpan="2"
                                                Opacity="0"
                                                Width="3"
                                                Cursor="SizeWE"
                                                HorizontalAlignment="Right"
                                                Visibility="{Binding CanUserResizeDropDown, RelativeSource={RelativeSource TemplatedParent}, Converter={StaticResource BooleanToVisibilityConverter}}"/>
                                        <Thumb x:Name="PART_CornerResizer"
                                                Grid.Column="1"
                                                Grid.RowSpan="2"
                                                Opacity="0"
                                                Height="3"
                                                Width="3"
                                                Cursor="SizeNWSE"
                                                VerticalAlignment="Bottom"
                                                HorizontalAlignment="Right"
                                                Visibility="{Binding CanUserResizeDropDown, RelativeSource={RelativeSource TemplatedParent}, Converter={StaticResource BooleanToVisibilityConverter}}"/>
                                        <Thumb x:Name="PART_VerticalResizer"
                                                Grid.Column="0"
                                                Grid.RowSpan="2"
                                                Opacity="0"
                                                Height="3"
                                                Cursor="SizeNS"
                                                VerticalAlignment="Bottom"
                                                Visibility="{Binding CanUserResizeDropDown, RelativeSource={RelativeSource TemplatedParent}, Converter={StaticResource BooleanToVisibilityConverter}}"/>
                                    </Grid>
                                </telerik:RadDropDownButton.DropDownContent>
                            </telerik:RadDropDownButton>
                            <Border x:Name="FocusVisual"
                                    Grid.ColumnSpan="2"
                                    IsHitTestVisible="False"
                                    BorderBrush="{TemplateBinding mat:MaterialAssist.FocusBrush}"
                                    BorderThickness="{telerik1:FluentResource ResourceKey=FocusThickness}"
                                    CornerRadius="{TemplateBinding mat:MaterialAssist.CornerRadius}"
                                    Visibility="Collapsed"/>
                        </Grid>
                        <ControlTemplate.Triggers>
                            <Trigger Property="IsMouseOver" Value="True">
                                <Setter TargetName="PART_SearchAutoCompleteBox" Property="Background" Value="{telerik1:FluentResource ResourceKey=MarkerInvertedBrush}"/>
                                <Setter Property="Foreground" Value="{telerik1:FluentResource ResourceKey=MarkerMouseOverBrush}"/>
                            </Trigger>
                            <Trigger SourceName="PART_DropDownButton" Property="IsOpen" Value="True">
                                <Setter TargetName="PART_SearchAutoCompleteBox" Property="Background" Value="{telerik1:FluentResource ResourceKey=MarkerInvertedBrush}"/>
                            </Trigger>
                            <Trigger Property="IsReadOnly" Value="True">
                                <Setter Property="BorderThickness" Value="{telerik1:FluentResource ResourceKey=FocusThickness}"/>
                            </Trigger>
                            <Trigger Property="IsEnabled" Value="False">
                                <Setter TargetName="PART_DropDownButton" Property="Background" Value="{x:Null}"/>
                            </Trigger>
                            <Trigger Property="IsKeyboardFocusWithin" Value="True">
                                <Setter TargetName="PART_SearchAutoCompleteBox" Property="Background" Value="{Binding RelativeSource={RelativeSource TemplatedParent}, Path=(mat:MaterialAssist.MouseOverBrush), Mode=OneWay}"/>
                                <Setter TargetName="FocusVisual" Property="Visibility" Value="Visible"/>
                                <Setter Property="Foreground" Value="{telerik1:FluentResource ResourceKey=MarkerMouseOverBrush}"/>
                            </Trigger>
                            <Trigger Property="Validation.HasError" Value="True">
                                <Setter TargetName="FocusVisual" Property="Visibility" Value="Collapsed"/>
                            </Trigger>
                            <Trigger Property="DropDownWidth" Value="NaN">
                                <Setter TargetName="PART_DropDownButton" Property="DropDownWidth" Value="{Binding RelativeSource={RelativeSource AncestorType=controls:RadMultiColumnComboBox}, Path=ActualWidth}"/>
                            </Trigger>
                            <Trigger Property="FooterVisibility" Value="Visible">
                                <Setter TargetName="PART_CornerResizer" Property="Width" Value="{Binding ElementName=CornerResizerIcon, Path=Width}"/>
                                <Setter TargetName="PART_CornerResizer" Property="Height" Value="{Binding ElementName=CornerResizerIcon, Path=Height}"/>
                                <Setter TargetName="PART_CornerResizer" Property="Grid.Row" Value="1"/>
                                <Setter TargetName="PART_VerticalResizer" Property="Grid.Row" Value="1"/>
                            </Trigger>
                            <Trigger Property="CanUserResizeDropDown" Value="False">
                                <Setter TargetName="CornerResizerIcon" Property="Visibility" Value="Collapsed"/>
                            </Trigger>
                        </ControlTemplate.Triggers>
                    </ControlTemplate>
                </Setter.Value>
            </Setter>
        </Style.Setters>
    </Style>
    <Style TargetType="controls:RadMultiColumnComboBox" BasedOn="{StaticResource RadMultiColumnComboBoxStyle}"/>
</ResourceDictionary>
