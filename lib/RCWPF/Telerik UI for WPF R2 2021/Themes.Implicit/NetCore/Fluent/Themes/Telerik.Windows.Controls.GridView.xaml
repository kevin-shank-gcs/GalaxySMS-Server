    <ControlTemplate x:Key="GridViewEditorPresenterValidationToolTipTemplate" TargetType="ToolTip">
        <Grid x:Name="Root" Opacity="0" Margin="2 0 0 0" Background="Transparent" RenderTransformOrigin="0 0">
            <Grid.RenderTransform>
                <TranslateTransform x:Name="xform" X="-25"/>
            </Grid.RenderTransform>
            <Grid.ColumnDefinitions>
                <ColumnDefinition Width="Auto"/>
                <ColumnDefinition/>
            </Grid.ColumnDefinitions>
            <Border Padding="4 3" Grid.Column="1" MinHeight="20" Background="{telerik1:FluentResource ResourceKey=ValidationBrush}">
                <TextBlock
                        MaxWidth="250"
                        FontSize="{telerik1:FluentResource ResourceKey=FontSize}"
                        FontFamily="{telerik1:FluentResource ResourceKey=FontFamily}"
                        Foreground="{telerik1:FluentResource ResourceKey=MarkerInvertedBrush}"
                        VerticalAlignment="Center"
                        Text="{TemplateBinding Content}"
                        TextWrapping="Wrap"/>
            </Border>
        </Grid>
        <ControlTemplate.Triggers>
            <Trigger Property="IsOpen" Value="True">
                <Trigger.EnterActions>
                    <BeginStoryboard x:Name="OpenAnimation">
                        <Storyboard>
                            <DoubleAnimation Storyboard.TargetName="xform" Storyboard.TargetProperty="X" To="0" Duration="0:0:0.2"/>
                        </Storyboard>
                    </BeginStoryboard>
                </Trigger.EnterActions>
                <Trigger.ExitActions>
                    <BeginStoryboard x:Name="CloseAnimation">
                        <Storyboard>
                            <DoubleAnimation Storyboard.TargetName="xform" Storyboard.TargetProperty="X" To="-25" Duration="0:0:0.2"/>
                        </Storyboard>
                    </BeginStoryboard>
                </Trigger.ExitActions>
                <Setter TargetName="Root" Property="Opacity" Value="1"/>
            </Trigger>
            <Trigger Property="IsOpen" Value="False">
                <Setter TargetName="Root" Property="Opacity" Value="0"/>
            </Trigger>
        </ControlTemplate.Triggers>
    </ControlTemplate>
    <ControlTemplate x:Key="GridViewEditorPresenterTemplate" TargetType="grid:GridViewEditorPresenter">
        <Grid Background="Transparent">
            <ContentPresenter x:Name="ContentPresenter" Margin="{TemplateBinding Padding}" VerticalAlignment="{TemplateBinding VerticalContentAlignment}"/>
            <Border x:Name="ValidationErrorElement" Visibility="Collapsed" BorderThickness="1" BorderBrush="{telerik1:FluentResource ResourceKey=ValidationBrush}">
                <ToolTipService.ToolTip>
                    <ToolTip x:Name="validationTooltip"
                            VerticalContentAlignment="Center"
                            Placement="Right"
                            Content="{TemplateBinding ErrorMessage}"
                            Template="{StaticResource GridViewEditorPresenterValidationToolTipTemplate}"/>
                </ToolTipService.ToolTip>
                <Grid Background="Transparent" Width="32" HorizontalAlignment="Right" VerticalAlignment="Top">
                    <Path
                            Data="{StaticResource ArrowTopRight8x8}"
                            Fill="{telerik1:FluentResource ResourceKey=ValidationBrush}"
                            Stretch="None"
                            HorizontalAlignment="Right"
                            VerticalAlignment="Top"
                            Margin="0 -1 -1 0"/>
                </Grid>
            </Border>
        </Grid>
        <ControlTemplate.Triggers>
            <Trigger Property="IsContentValid" Value="False">
                <Setter TargetName="ValidationErrorElement" Property="Visibility" Value="Visible"/>
            </Trigger>
            <Trigger Property="IsContentValid" Value="True">
                <Setter Property="Margin" Value="1 1 2 2"/>
            </Trigger>
        </ControlTemplate.Triggers>
    </ControlTemplate>
