        <Setter Property="Template" Value="{StaticResource RadGridViewTemplate}"/>
        <Setter Property="Background" Value="{telerik1:GreenResource ResourceKey=PrimaryBrush}"/>
        <Setter Property="BorderBrush" Value="{telerik1:GreenResource ResourceKey=BasicBrush}"/>
        <Setter Property="BorderThickness" Value="1"/>
        <Setter Property="FontFamily" Value="{telerik1:GreenResource ResourceKey=FontFamily}"/>
        <Setter Property="FontSize" Value="{telerik1:GreenResource ResourceKey=FontSize}"/>
        <Setter Property="Foreground" Value="{telerik1:GreenResource ResourceKey=MarkerBrush}"/>
        <Setter Property="GroupPanelBackground" Value="{telerik1:GreenResource ResourceKey=PrimaryBrush}"/>
        <Setter Property="GroupPanelForeground" Value="{telerik1:GreenResource ResourceKey=MarkerBrush}"/>
        <Setter Property="SearchPanelBackground" Value="{telerik1:GreenResource ResourceKey=PrimaryBrush}"/>
        <Setter Property="SearchPanelForeground" Value="{telerik1:GreenResource ResourceKey=MarkerBrush}"/>
        <Setter Property="ScrollViewer.HorizontalScrollBarVisibility" Value="Auto"/>
        <Setter Property="ScrollViewer.VerticalScrollBarVisibility" Value="Auto"/>
        <Setter Property="VerticalGridLinesBrush" Value="{telerik1:GreenResource ResourceKey=BasicBrush}"/>
        <Setter Property="HorizontalGridLinesBrush" Value="{telerik1:GreenResource ResourceKey=BasicBrush}"/>
        <Setter Property="AlternateRowBackground" Value="{telerik1:GreenResource ResourceKey=AlternativeBrush}"/>
        <Setter Property="RowHeight" Value="26"/>
        <Setter Property="Tag" Value="Green"/>
