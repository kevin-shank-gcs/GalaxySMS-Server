    </Style>
    <Style TargetType="telerikControls:RadOpenFileDialog" BasedOn="{StaticResource RadOpenFileDialogStyle}"/>
    <Style TargetType="telerikControls:RadSaveFileDialog" BasedOn="{StaticResource RadSaveFileDialogStyle}"/>
    <Style TargetType="telerikControls:RadOpenFolderDialog" BasedOn="{StaticResource RadOpenFolderDialogStyle}"/>
</ResourceDictionary>
