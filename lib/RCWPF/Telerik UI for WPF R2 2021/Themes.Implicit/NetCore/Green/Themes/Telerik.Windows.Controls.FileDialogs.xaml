                        Width="16"
                        Height="16"
                        Margin="2 0"/>
                <TextBlock Text="{telerik:LocalizableResource Key=FileDialogs_SearchResults}" Margin="0 0 5 0" Visibility="{Binding IsSearchViewActive, ElementName=PART_SearchPane, Converter={StaticResource BooleanToVisibilityConverter}}"/>
