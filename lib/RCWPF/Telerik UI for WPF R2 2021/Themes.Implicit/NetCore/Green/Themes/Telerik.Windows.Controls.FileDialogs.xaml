    <ControlTemplate TargetType="historyNavigationPane:OpenFileDialogControl" x:Key="OpenFileDialogControlTemplate">
        <Grid>
            <Grid.RowDefinitions>
                <RowDefinition Height="*"/>
                <RowDefinition Height="Auto"/>
            </Grid.RowDefinitions>
            <historyNavigationPane:ExplorerControl x:Name="PART_Explorer" ExpandToCurrentDirectory="{Binding RelativeSource={RelativeSource AncestorType={x:Type telerik:RadOpenFileDialog}}, Path=ExpandToCurrentDirectory, Mode=TwoWay, UpdateSourceTrigger=PropertyChanged}"/>
            <Grid Grid.Row="1" x:Name="PART_OperationsPane" Margin="{TemplateBinding Padding}">
                <Grid.RowDefinitions>
                    <RowDefinition Height="*"/>
                    <RowDefinition Height="Auto"/>
                </Grid.RowDefinitions>
                <Grid.ColumnDefinitions>
                    <ColumnDefinition Width="Auto"/>
                    <ColumnDefinition Width="*"/>
                    <ColumnDefinition Width="210"/>
                </Grid.ColumnDefinitions>
                <TextBlock Text="{telerik:LocalizableResource Key=FileDialogs_FileName}" Margin="0 0 10 0" VerticalAlignment="Center"/>
                <telerik:RadAutoCompleteBox x:Name="PART_SelectedFilesAutoCompleteBox"
                        AutomationProperties.Name="selected files"
                        Grid.Column="1"
                        Grid.ColumnSpan="2"
                        AutoCompleteMode="SuggestAppend"
                        ScrollViewer.VerticalScrollBarVisibility="Auto"
                        MaxHeight="42"
                        BoxesItemStyleSelector="{StaticResource FileBrowserAutocompleteBoxesItemStyleSelector}"
                        ItemsSource="{Binding RootAndCurrentFileSystemObjects}"
                        SearchText="{Binding Name, Mode=TwoWay, UpdateSourceTrigger=PropertyChanged}"
                        DisplayMemberPath="Name"
                        TextSearchMode="StartsWith"
                        TextBoxStyle="{StaticResource FileDialogWatermarkStyle}"/>
                <CheckBox
                        Grid.Row="1"
                        Grid.Column="1"
                        x:Name="ReadOnlyCheckBox"
                        Content="{telerik:LocalizableResource Key=FileDialogs_OpenReadOnly}"
                        Visibility="{Binding ShowReadOnly, Converter={StaticResource ResourceKey=BooleanToVisibilityConverter}}"
                        IsChecked="{Binding ReadOnlyChecked}"
                        Margin="0 5 0 0"/>
                <telerik:RadComboBox
                        Grid.Column="2"
                        x:Name="PART_FilterComboBox"
                        AutomationProperties.Name="filter"
                        ItemsSource="{Binding FilterDescriptors}"
                        DisplayMemberPath="FilterName"
                        SelectedIndex="{Binding FilterIndex}"
                        Visibility="{Binding IsFilterApplied, Converter={StaticResource ResourceKey=BooleanToVisibilityConverter}}"
                        telerik:ComboBoxToolTipHelper.ShowToolTipOnTrimmedText="True">
                    <ToolTipService.ToolTip>
                        <ToolTip VerticalOffset="2" Placement="Relative" Content="{Binding SelectedFilterName}" HorizontalOffset="-2"/>
                    </ToolTipService.ToolTip>
                </telerik:RadComboBox>
                <Grid Grid.Row="1" Grid.Column="2" Margin="0 10 0 0">
                    <Grid.ColumnDefinitions>
                        <ColumnDefinition Width="*"/>
                        <ColumnDefinition Width="12"/>
                        <ColumnDefinition Width="*"/>
                    </Grid.ColumnDefinitions>
                    <telerik:RadButton x:Name="OpenButton"
                            MinWidth="98"
                            Command="historyNavigationPane:FileDialogsCommands.Open"
                            telerikControls:DialogWindowBase.ResponseButton="Accept"
                            Content="{telerik:LocalizableResource Key=FileDialogs_Open}"/>
                    <telerik:RadButton x:Name="CancelButton"
                            Grid.Column="2"
                            MinWidth="98"
                            Command="historyNavigationPane:FileDialogsCommands.Cancel"
                            telerikControls:DialogWindowBase.ResponseButton="Cancel"
                            Content="{telerik:LocalizableResource Key=FileDialogs_Cancel}"/>
                </Grid>
            </Grid>
        </Grid>
        <ControlTemplate.Triggers>
            <DataTrigger Binding="{Binding IsFilterApplied}" Value="True">
                <Setter Property="Grid.ColumnSpan" Value="1" TargetName="PART_SelectedFilesAutoCompleteBox"/>
                <Setter Property="Margin" Value="0 0 12 0" TargetName="PART_SelectedFilesAutoCompleteBox"/>
            </DataTrigger>
        </ControlTemplate.Triggers>
    </ControlTemplate>
    <ControlTemplate TargetType="historyNavigationPane:SaveFileDialogControl" x:Key="SaveFileDialogControlTemplate">
