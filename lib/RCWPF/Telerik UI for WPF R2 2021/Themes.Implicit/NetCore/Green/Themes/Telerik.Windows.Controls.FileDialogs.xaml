    <ControlTemplate x:Key="GridViewValidationToolTipTemplate" TargetType="ToolTip">
        <Grid x:Name="Root" Opacity="0" Margin="2 0 0 0" Background="Transparent" RenderTransformOrigin="0 0">
            <Grid.RenderTransform>
                <TranslateTransform x:Name="xform" X="-25"/>
            </Grid.RenderTransform>
            <Grid.ColumnDefinitions>
                <ColumnDefinition Width="Auto"/>
                <ColumnDefinition/>
            </Grid.ColumnDefinitions>
            <Border Padding="4 3 4 3" Grid.Column="1" MinHeight="20" Background="{telerik1:GreenResource ResourceKey=ValidationBrush}">
                <ItemsControl ItemsSource="{TemplateBinding Content}">
                    <ItemsControl.ItemTemplate>
                        <DataTemplate>
                            <TextBlock
                                    MaxWidth="250"
                                    FontSize="{telerik1:GreenResource ResourceKey=FontSize}"
                                    FontFamily="{telerik1:GreenResource ResourceKey=FontFamily}"
                                    VerticalAlignment="Center"
                                    Foreground="{telerik1:GreenResource ResourceKey=FixedBrush}"
                                    Text="{Binding}"
                                    TextWrapping="Wrap"/>
                        </DataTemplate>
                    </ItemsControl.ItemTemplate>
                    <ItemsControl.ItemsPanel>
                        <ItemsPanelTemplate>
                            <StackPanel/>
                        </ItemsPanelTemplate>
                    </ItemsControl.ItemsPanel>
                </ItemsControl>
            </Border>
        </Grid>
        <ControlTemplate.Triggers>
            <Trigger Property="IsOpen" Value="True">
                <Trigger.EnterActions>
                    <BeginStoryboard x:Name="OpenAnimation">
                        <Storyboard>
                            <DoubleAnimation Storyboard.TargetName="xform" Storyboard.TargetProperty="X" To="0" Duration="0:0:0.2"/>
                        </Storyboard>
                    </BeginStoryboard>
                </Trigger.EnterActions>
                <Trigger.ExitActions>
                    <BeginStoryboard x:Name="CloseAnimation">
                        <Storyboard>
                            <DoubleAnimation Storyboard.TargetName="xform" Storyboard.TargetProperty="X" To="-25" Duration="0:0:0.2"/>
                        </Storyboard>
                    </BeginStoryboard>
                </Trigger.ExitActions>
                <Setter Property="Opacity" Value="1" TargetName="Root"/>
            </Trigger>
            <Trigger Property="IsOpen" Value="False">
                <Setter Property="Opacity" Value="0" TargetName="Root"/>
            </Trigger>
        </ControlTemplate.Triggers>
    </ControlTemplate>
