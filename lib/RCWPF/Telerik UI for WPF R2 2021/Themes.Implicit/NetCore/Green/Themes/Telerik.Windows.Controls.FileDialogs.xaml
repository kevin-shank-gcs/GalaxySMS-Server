            <Grid Grid.Row="1" x:Name="PART_OperationsPane" Margin="{TemplateBinding Padding}">
                <Grid.RowDefinitions>
                    <RowDefinition Height="Auto"/>
                    <RowDefinition Height="Auto"/>
                    <RowDefinition Height="Auto"/>
                </Grid.RowDefinitions>
                <Grid.ColumnDefinitions>
                    <ColumnDefinition Width="Auto"/>
                    <ColumnDefinition Width="*"/>
                    <ColumnDefinition Width="Auto"/>
                </Grid.ColumnDefinitions>
                <TextBlock Text="{telerik:LocalizableResource Key=FileDialogs_FileName}" Margin="0 0 10 0" VerticalAlignment="Center" HorizontalAlignment="Right"/>
                <TextBlock Text="{telerik:LocalizableResource Key=FileDialogs_SaveAsType}" Grid.Row="1" Margin="0 0 10 0" VerticalAlignment="Center" HorizontalAlignment="Right"/>
                <telerik:RadAutoCompleteBox x:Name="PART_SelectedFilesAutoCompleteBox"
                        AutomationProperties.Name="selected files"
                        AutoCompleteMode="SuggestAppend"
                        ItemsSource="{Binding RootAndCurrentFileSystemObjects}"
                        SelectedItem="{Binding SelectedFileSystemObject, Mode=TwoWay, UpdateSourceTrigger=PropertyChanged}"
                        SearchText="{Binding Name, Mode=TwoWay, UpdateSourceTrigger=PropertyChanged}"
                        DisplayMemberPath="Name"
                        Grid.Column="1"
                        Grid.ColumnSpan="2"
                        SelectionMode="Single"
                        TextSearchMode="StartsWith"
                        TextBoxStyle="{StaticResource FileDialogWatermarkStyle}"/>
                <telerik:RadComboBox x:Name="PART_FilterComboBox"
                        AutomationProperties.Name="filter"
                        Grid.Row="1"
                        Grid.Column="1"
                        Grid.ColumnSpan="2"
                        Margin="0 10"
                        ItemsSource="{Binding FilterDescriptors}"
                        DisplayMemberPath="FilterName"
                        SelectedIndex="{Binding FilterIndex}"
                        telerik:ComboBoxToolTipHelper.ShowToolTipOnTrimmedText="True">
                    <ToolTipService.ToolTip>
                        <ToolTip VerticalOffset="2" Placement="Relative" Content="{Binding SelectedFilterName}" HorizontalOffset="-2"/>
                    </ToolTipService.ToolTip>
                </telerik:RadComboBox>
                <Grid Grid.Row="2" Grid.Column="2" Margin="0 10 0 0">
                    <Grid.ColumnDefinitions>
                        <ColumnDefinition Width="*"/>
                        <ColumnDefinition Width="12"/>
                        <ColumnDefinition Width="*"/>
                    </Grid.ColumnDefinitions>
                    <telerik:RadButton x:Name="SaveButton" MinWidth="98" telerikControls:DialogWindowBase.ResponseButton="Accept" Command="historyNavigationPane:FileDialogsCommands.Save">
                        <telerik:RadButton.Style>
                            <Style TargetType="telerik:RadButton" BasedOn="{StaticResource RadButtonStyle}">
                                <Setter Property="Content" Value="{Binding Manager.Culture, Source={StaticResource localizationManagerProvider}, Converter={StaticResource cultureToStringConverter}, ConverterParameter=FileDialogs_Save}"/>
                                <Style.Triggers>
                                    <DataTrigger Binding="{Binding IsDirectorySelected}" Value="True">
                                        <Setter Property="Content" Value="{Binding Manager.Culture, Source={StaticResource localizationManagerProvider}, Converter={StaticResource cultureToStringConverter}, ConverterParameter=FileDialogs_Open}"/>
                                    </DataTrigger>
                                </Style.Triggers>
                            </Style>
                        </telerik:RadButton.Style>
                    </telerik:RadButton>
                    <telerik:RadButton x:Name="CancelButton"
                            Grid.Column="2"
                            MinWidth="98"
                            Command="historyNavigationPane:FileDialogsCommands.Cancel"
                            telerikControls:DialogWindowBase.ResponseButton="Cancel"
                            Content="{telerik:LocalizableResource Key=FileDialogs_Cancel}"/>
                </Grid>
            </Grid>
        </Grid>
    </ControlTemplate>
