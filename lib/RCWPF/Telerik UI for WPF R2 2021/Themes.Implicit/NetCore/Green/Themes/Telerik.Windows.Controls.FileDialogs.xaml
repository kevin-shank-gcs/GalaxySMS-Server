        </historyNavigationPane:LayoutConfiguratorTemplateSelector.SmallIconsDataTemplate>
        <historyNavigationPane:LayoutConfiguratorTemplateSelector.MediumIconsDataTemplate>
            <DataTemplate>
                <historyNavigationPane:FileBrowserListBox
                        ItemsSource="{Binding CurrentFileSystemObjects}"
                        SelectedItem="{Binding SelectedFileSystemObject, Mode=TwoWay}"
                        x:Name="Listbox_Medium"
                        AutomationProperties.Name="medium icons layout"
                        DataContext="{Binding DataContext, RelativeSource={RelativeSource AncestorType={x:Type historyNavigationPane:FileBrowserTabControl}}}">
                    <historyNavigationPane:FileBrowserListBox.ItemsPanel>
                        <ItemsPanelTemplate>
                            <telerik:VirtualizingWrapPanel Orientation="Horizontal" ItemHeight="94" ItemWidth="74" ScrollStep="94"/>
