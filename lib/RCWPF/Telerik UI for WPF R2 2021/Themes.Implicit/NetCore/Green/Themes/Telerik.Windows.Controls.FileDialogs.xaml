                    <historyNavigationPane:FileBrowserGridView.Columns>
                        <telerik:GridViewDataColumn Header="{telerik:LocalizableResource Key=FileDialogs_NameHeader}" DataMemberBinding="{Binding Label}" IsReadOnly="False" Width="360">
