    <Style x:Key="FileBrowserDragVisualStyle" TargetType="fileDialogsDragDrop:FileBrowserDragVisual">
        <Setter Property="Foreground" Value="{telerik1:GreenResource ResourceKey=MarkerBrush}"/>
        <Setter Property="Background" Value="{telerik1:GreenResource ResourceKey=AlternativeBrush}"/>
        <Setter Property="BorderBrush" Value="{telerik1:GreenResource ResourceKey=BasicBrush}"/>
        <Setter Property="BorderThickness" Value="1"/>
        <Setter Property="Margin" Value="8 0 0 0"/>
        <Setter Property="Padding" Value="0 0 4 0"/>
        <Setter Property="HorizontalContentAlignment" Value="Left"/>
        <Setter Property="VerticalContentAlignment" Value="Center"/>
        <Setter Property="SnapsToDevicePixels" Value="True"/>
        <Setter Property="IconTemplate">
            <Setter.Value>
                <DataTemplate>
                    <Image Source="{Binding Icon}" Width="64" Height="64"/>
                </DataTemplate>
            </Setter.Value>
        </Setter>
        <Setter Property="ContentTemplate">
            <Setter.Value>
                <DataTemplate>
                    <StackPanel Margin="0 0 5 0" Orientation="Horizontal">
                        <TextBlock Text="{Binding Operation, Converter={StaticResource fileOperationToStringConverter}}"/>
                        <TextBlock Margin="2 0 0 0" Text="{Binding DropFolderName}"/>
                    </StackPanel>
                </DataTemplate>
            </Setter.Value>
        </Setter>
        <Setter Property="Template">
            <Setter.Value>
                <ControlTemplate TargetType="fileDialogsDragDrop:FileBrowserDragVisual">
                    <Grid DataContext="{TemplateBinding Content}">
                        <Grid.RowDefinitions>
                            <RowDefinition Height="Auto"/>
                            <RowDefinition Height="Auto"/>
                        </Grid.RowDefinitions>
                        <Grid Margin="0 0 0 1" HorizontalAlignment="Left">
                            <Border x:Name="DragBackgroundVisual"
                                    Opacity="0.9"
                                    BorderBrush="{TemplateBinding BorderBrush}"
                                    BorderThickness="{TemplateBinding BorderThickness}"
                                    Background="{TemplateBinding Background}"
                                    CornerRadius="{telerik1:GreenResource ResourceKey=CornerRadius}">
                                <ContentPresenter ContentTemplate="{TemplateBinding IconTemplate}" Content="{TemplateBinding Content}"/>
                            </Border>
                            <Border
                                    HorizontalAlignment="Center"
                                    VerticalAlignment="Center"
                                    MinWidth="20"
                                    MinHeight="20"
                                    Padding="2"
                                    Visibility="{Binding RelativeSource={RelativeSource Mode=TemplatedParent}, Path=IsDraggingMultipleItems, Converter={StaticResource BooleanToVisibilityConverter}}"
                                    Background="{TemplateBinding Background}"
                                    BorderBrush="{TemplateBinding BorderBrush}"
                                    BorderThickness="1">
                                <TextBlock Text="{Binding Count}" HorizontalAlignment="Center" VerticalAlignment="Center"/>
                            </Border>
                        </Grid>
                        <Border
                                Grid.Row="1"
                                Padding="{TemplateBinding Padding}"
                                HorizontalAlignment="Left"
                                Margin="36 0 4 4"
                                BorderThickness="{TemplateBinding BorderThickness}"
                                BorderBrush="{TemplateBinding BorderBrush}"
                                Background="{TemplateBinding Background}">
                            <Grid>
                                <Grid.ColumnDefinitions>
                                    <ColumnDefinition Width="Auto"/>
                                    <ColumnDefinition Width="*"/>
                                </Grid.ColumnDefinitions>
                                <Path x:Name="NoneEffect"
                                        Visibility="Collapsed"
                                        Margin="5"
                                        Width="10"
                                        Height="10"
                                        HorizontalAlignment="Left"
                                        VerticalAlignment="Top"
                                        Data="{StaticResource NoneEffect}"
                                        Fill="{telerik1:GreenResource ResourceKey=ValidationBrush}"/>
                                <Path x:Name="MoveEffect"
                                        Visibility="Collapsed"
                                        Margin="5"
                                        Width="10"
                                        Height="10"
                                        HorizontalAlignment="Left"
                                        VerticalAlignment="Top"
                                        Data="{StaticResource MoveEffect}"
                                        StrokeThickness="2"
                                        Stroke="{telerik1:GreenResource ResourceKey=AccentLowBrush}"/>
                                <Path x:Name="CopyEffect"
                                        Visibility="Collapsed"
                                        Margin="5"
                                        Width="10"
                                        Height="10"
                                        HorizontalAlignment="Left"
                                        VerticalAlignment="Top"
                                        Data="{StaticResource CopyEffect}"
                                        StrokeThickness="2"
                                        Stroke="{telerik1:GreenResource ResourceKey=AccentLowBrush}"/>
                                <Path x:Name="LinkEffect"
                                        Visibility="Collapsed"
                                        Margin="5 7 4 7"
                                        Height="6"
                                        Width="11"
                                        HorizontalAlignment="Left"
                                        VerticalAlignment="Top"
                                        StrokeThickness="1"
                                        Data="{StaticResource LinkEffect}"
                                        Stroke="{telerik1:GreenResource ResourceKey=AccentLowBrush}"/>
                                <Path x:Name="ScrollEffect"
                                        Visibility="Collapsed"
                                        Margin="5 4"
                                        Width="10"
                                        Height="12"
                                        HorizontalAlignment="Left"
                                        VerticalAlignment="Top"
                                        Data="{StaticResource ScrollEffect}"
                                        Stroke="{telerik1:GreenResource ResourceKey=AccentLowBrush}"/>
                                <ContentPresenter
                                        ContentTemplate="{TemplateBinding ContentTemplate}"
                                        Content="{TemplateBinding Content}"
                                        Grid.Column="1"
                                        VerticalAlignment="{TemplateBinding VerticalContentAlignment}"
                                        Visibility="{Binding ShowDropIndicationInformation, Converter={StaticResource BooleanToVisibilityConverter}}"/>
                            </Grid>
                        </Border>
                    </Grid>
                    <ControlTemplate.Triggers>
                        <Trigger Property="Effects" Value="None">
                            <Setter TargetName="NoneEffect" Property="Visibility" Value="Visible"/>
                        </Trigger>
                        <Trigger Property="Effects" Value="Move">
                            <Setter TargetName="MoveEffect" Property="Visibility" Value="Visible"/>
                        </Trigger>
                        <Trigger Property="Effects" Value="Copy">
                            <Setter TargetName="CopyEffect" Property="Visibility" Value="Visible"/>
                        </Trigger>
                        <Trigger Property="Effects" Value="Move, Scroll">
                            <Setter TargetName="MoveEffect" Property="Visibility" Value="Visible"/>
                        </Trigger>
                        <Trigger Property="Effects" Value="Copy, Scroll">
                            <Setter TargetName="CopyEffect" Property="Visibility" Value="Visible"/>
                        </Trigger>
                        <Trigger Property="Effects" Value="Link">
                            <Setter TargetName="LinkEffect" Property="Visibility" Value="Visible"/>
                        </Trigger>
                        <Trigger Property="Effects" Value="Scroll">
                            <Setter TargetName="ScrollEffect" Property="Visibility" Value="Visible"/>
                        </Trigger>
                    </ControlTemplate.Triggers>
                </ControlTemplate>
            </Setter.Value>
        </Setter>
    </Style>
    <Style TargetType="fileDialogsDragDrop:FileBrowserDragVisual" BasedOn="{StaticResource FileBrowserDragVisualStyle}"/>
    <ControlTemplate x:Key="SearchPaneControlTemplate" TargetType="historyNavigationPane:FileDialogSearchPane">
        <Grid>
