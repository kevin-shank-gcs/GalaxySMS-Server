<telerik:RadWindow x:Class="GCS.Framework.Badging.CardExchange.UI.PrintWindow"
                   xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
                   xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
                   xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
                   xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
                   xmlns:local="clr-namespace:GCS.Framework.Badging.CardExchange.UI"
                   xmlns:telerik="http://schemas.telerik.com/2008/xaml/presentation"
                   xmlns:resx="clr-namespace:GCS.Framework.Badging.Properties"
                   mc:Ignorable="d"
                   d:DesignHeight="450" d:DesignWidth="800"
                   WindowStartupLocation="CenterOwner" Header="{x:Static resx:Resources.GenericHeader}" IsTopmost="True">
    <telerik:RadWindow.Style>
        <Style TargetType="telerik:RadWindow" BasedOn="{StaticResource RadWindowStyle}" />
    </telerik:RadWindow.Style>
    <Grid>
        <local:PrintControl x:Name="printControl"/>
    </Grid>
</telerik:RadWindow>
