    <Style x:Key="TreeListViewDragCueStyle" TargetType="controls:TreeListViewDragCue">
        <Setter Property="IsHitTestVisible" Value="False"/>
        <Setter Property="Padding" Value="4"/>
        <Setter Property="Foreground" Value="{telerik1:Office2016Resource ResourceKey=MarkerInvertedBrush}"/>
        <Setter Property="Opacity" Value="0.75"/>
        <Setter Property="Template">
            <Setter.Value>
                <ControlTemplate TargetType="controls:TreeListViewDragCue">
                    <StackPanel>
                        <Grid HorizontalAlignment="Left" Visibility="{TemplateBinding DragPreviewVisibility}" MinHeight="26">
                            <Grid.Resources>
                                <Style TargetType="TextBlock">
                                    <Setter Property="Foreground" Value="{telerik1:Office2016Resource ResourceKey=MarkerInvertedBrush}"/>
                                </Style>
                            </Grid.Resources>
                            <Border BorderBrush="{telerik1:Office2016Resource ResourceKey=AccentBrush}" Background="{telerik1:Office2016Resource ResourceKey=AccentBrush}" BorderThickness="1">
                                <ItemsPresenter Margin="{TemplateBinding Padding}"/>
                            </Border>
                        </Grid>
                        <Border
                                BorderBrush="{telerik1:Office2016Resource ResourceKey=BasicBrush}"
                                Background="{telerik1:Office2016Resource ResourceKey=MainBrush}"
                                MinHeight="22"
                                BorderThickness="1"
                                Opacity="0.9"
                                Visibility="{TemplateBinding DragTooltipVisibility}">
                            <StackPanel Orientation="Horizontal" Margin="4 0 0 0">
                                <ContentPresenter x:Name="DropImpossibleIconElement"
                                        Content="{TemplateBinding DropImpossibleIcon}"
                                        ContentTemplate="{TemplateBinding DropImpossibleIconTemplate}"
                                        Visibility="Collapsed"/>
                                <ContentPresenter x:Name="DropPossibleIconElement" Content="{TemplateBinding DropPossibleIcon}" ContentTemplate="{TemplateBinding DropPossibleIconTemplate}"/>
                                <ContentControl
                                        Margin="4 0 0 0"
                                        Content="{TemplateBinding DragActionContent}"
                                        ContentTemplate="{TemplateBinding DragActionContentTemplate}"
                                        FontSize="{TemplateBinding FontSize}"
                                        FontFamily="{TemplateBinding FontFamily}"
                                        VerticalAlignment="Center"
                                        Foreground="{telerik1:Office2016Resource ResourceKey=MarkerBrush}"/>
                                <ContentControl
                                        Foreground="{telerik1:Office2016Resource ResourceKey=MarkerInvertedBrush}"
                                        Content="{TemplateBinding DragTooltipContent}"
                                        ContentTemplate="{TemplateBinding DragTooltipContentTemplate}"
                                        FontSize="{TemplateBinding FontSize}"
                                        FontFamily="{TemplateBinding FontFamily}"
                                        VerticalAlignment="Center"/>
                            </StackPanel>
                        </Border>
                    </StackPanel>
                    <ControlTemplate.Triggers>
                        <Trigger Property="IsDropPossible" Value="False">
                            <Setter Property="Visibility" Value="Visible" TargetName="DropImpossibleIconElement"/>
                        </Trigger>
                    </ControlTemplate.Triggers>
                </ControlTemplate>
            </Setter.Value>
        </Setter>
        <Setter Property="DropImpossibleIconTemplate">
            <Setter.Value>
                <DataTemplate>
                    <Grid UseLayoutRounding="True">
                        <TextBlock
                                Text="{StaticResource GlyphClose}"
                                FontFamily="{StaticResource TelerikWebUI}"
                                Foreground="{telerik1:Office2016Resource ResourceKey=ValidationBrush}"
                                FontSize="16"
                                HorizontalAlignment="Center"
                                VerticalAlignment="Center"/>
                    </Grid>
                </DataTemplate>
            </Setter.Value>
        </Setter>
        <Setter Property="DropPossibleIconTemplate">
            <Setter.Value>
                <DataTemplate>
                    <Grid UseLayoutRounding="True">
                        <TextBlock
                                Text="{StaticResource GlyphCheck}"
                                FontFamily="{StaticResource TelerikWebUI}"
                                Foreground="{telerik1:Office2016Resource ResourceKey=AccentFocusedBrush}"
                                FontSize="16"
                                HorizontalAlignment="Center"
                                VerticalAlignment="Center"/>
                    </Grid>
                </DataTemplate>
            </Setter.Value>
        </Setter>
        <Setter Property="SnapsToDevicePixels" Value="True"/>
    </Style>
