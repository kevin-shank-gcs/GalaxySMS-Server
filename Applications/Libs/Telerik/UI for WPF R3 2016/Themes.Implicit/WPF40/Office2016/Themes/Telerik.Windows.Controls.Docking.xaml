                    <Condition Binding="{Binding IsMouseOver, ElementName=HeaderUnPinButton}" Value="True"/>
                </MultiDataTrigger.Conditions>
                <Setter TargetName="HeaderUnPinButton" Property="Background" Value="{telerik:Office2016Resource ResourceKey=AccentMouseOverBrush}"/>
