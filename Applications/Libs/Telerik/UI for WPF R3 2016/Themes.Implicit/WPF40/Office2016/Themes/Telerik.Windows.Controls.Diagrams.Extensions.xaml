                            <Setter Property="BorderBrush" Value="{telerik1:Office2016Resource ResourceKey=AccentBrush}"/>
                            <Setter TargetName="HeaderElement" Property="Foreground" Value="{telerik1:Office2016Resource ResourceKey=MarkerInvertedBrush}"/>
                        </Trigger>
                    </ControlTemplate.Triggers>
                </ControlTemplate>
            </Setter.Value>
        </Setter>
    </Style>
    <Style TargetType="extensions:RadDiagramToolbox" x:Key="RadDiagramToolboxStyle">
        <Setter Property="HeaderBackground" Value="{telerik1:Office2016Resource ResourceKey=PrimaryBrush}"/>
        <Setter Property="Foreground" Value="{telerik1:Office2016Resource ResourceKey=MarkerBrush}"/>
        <Setter Property="Background" Value="{telerik1:Office2016Resource ResourceKey=MainBrush}"/>
        <Setter Property="FontFamily" Value="{telerik1:Office2016Resource ResourceKey=FontFamily}"/>
        <Setter Property="BorderBrush" Value="{telerik1:Office2016Resource ResourceKey=BasicBrush}"/>
        <Setter Property="BorderThickness" Value="1"/>
        <Setter Property="CloseButtonStyle" Value="{StaticResource DiagramCloseButtonStyle}"/>
        <Setter Property="OpenCloseButtonStyle" Value="{StaticResource DiagramOpenCloseButtonStyle}"/>
        <Setter Property="VerticalContentAlignment" Value="Center"/>
        <Setter Property="HorizontalContentAlignment" Value="Left"/>
        <Setter Property="ItemTemplate" Value="{StaticResource ToolboxTemplate}"/>
        <Setter Property="Padding" Value="4 7"/>
        <Setter Property="ItemsPanel">
            <Setter.Value>
                <ItemsPanelTemplate>
                    <StackPanel/>
                </ItemsPanelTemplate>
            </Setter.Value>
        </Setter>
        <Setter Property="Template">
            <Setter.Value>
                <ControlTemplate TargetType="extensions:RadDiagramToolbox">
                    <Grid x:Name="RootGrid" VerticalAlignment="{TemplateBinding VerticalAlignment}" HorizontalAlignment="{TemplateBinding HorizontalAlignment}">
                        <Grid.Resources>
                            <Storyboard x:Key="hideAnimation">
                                <DoubleAnimation Duration="00:00:00.4" Storyboard.TargetName="hideablePart" Storyboard.TargetProperty="(UIElement.RenderTransform).(TranslateTransform.X)" To="100">
                                    <DoubleAnimation.EasingFunction>
                                        <CircleEase EasingMode="EaseOut"/>
                                    </DoubleAnimation.EasingFunction>
                                </DoubleAnimation>
                            </Storyboard>
                            <Storyboard x:Key="showAnimation">
                                <DoubleAnimation Duration="00:00:00.32" Storyboard.TargetName="hideablePart" Storyboard.TargetProperty="(UIElement.RenderTransform).(TranslateTransform.X)" To="0">
                                    <DoubleAnimation.EasingFunction>
                                        <CircleEase EasingMode="EaseOut"/>
                                    </DoubleAnimation.EasingFunction>
                                </DoubleAnimation>
                            </Storyboard>
                        </Grid.Resources>
                        <Grid.ColumnDefinitions>
                            <ColumnDefinition Width="Auto"/>
                            <ColumnDefinition Width="Auto"/>
                        </Grid.ColumnDefinitions>
                        <Grid x:Name="hideablePart">
                            <Grid.RowDefinitions>
                                <RowDefinition Height="Auto"/>
                                <RowDefinition Height="*"/>
                                <RowDefinition Height="Auto"/>
                            </Grid.RowDefinitions>
                            <Grid.RenderTransform>
                                <TranslateTransform/>
                            </Grid.RenderTransform>
                            <Border Grid.Row="0" Grid.RowSpan="3" Background="{TemplateBinding Background}" BorderBrush="{TemplateBinding BorderBrush}" BorderThickness="1 1 0 1"/>
                            <ContentControl
                                    Margin="{TemplateBinding Padding}"
                                    Foreground="{TemplateBinding Foreground}"
                                    ContentTemplate="{TemplateBinding TitleTemplate}"
                                    VerticalAlignment="Center"
                                    HorizontalAlignment="Left"
                                    FontFamily="{telerik1:Office2016Resource ResourceKey=FontFamily}"
                                    FontSize="{telerik1:Office2016Resource ResourceKey=FontSize}"
                                    FontWeight="Bold"
                                    Content="{TemplateBinding Title}"/>
                            <ItemsPresenter Grid.Row="1" Margin="0 9 0 0"/>
                            <telerik:RadButton x:Name="CloseButton" Grid.Row="2" Style="{TemplateBinding CloseButtonStyle}"/>
                        </Grid>
                        <Grid Grid.Column="1">
                            <Grid.RowDefinitions>
                                <RowDefinition Height="Auto"/>
                                <RowDefinition Height="*"/>
                                <RowDefinition Height="Auto"/>
                            </Grid.RowDefinitions>
                            <Border BorderBrush="{telerik1:Office2016Resource ResourceKey=BasicBrush}" Background="{TemplateBinding HeaderBackground}" BorderThickness="1 1 1 0">
                                <ContentControl
                                        Margin="{TemplateBinding Padding}"
                                        FontFamily="{telerik1:Office2016Resource ResourceKey=FontFamily}"
                                        FontSize="{telerik1:Office2016Resource ResourceKey=FontSize}"
                                        Foreground="{TemplateBinding Foreground}"
                                        Content="{TemplateBinding Header}"
                                        ContentTemplate="{TemplateBinding HeaderTemplate}"
                                        VerticalAlignment="{TemplateBinding VerticalContentAlignment}"
                                        HorizontalAlignment="{TemplateBinding HorizontalContentAlignment}"/>
                            </Border>
                            <Border Grid.Row="1" BorderThickness="1 0" BorderBrush="{TemplateBinding BorderBrush}" Background="{telerik1:Office2016Resource ResourceKey=AlternativeBrush}">
                                <ContentPresenter x:Name="SelectedTabContent"/>
                            </Border>
                            <Border Grid.Row="2" BorderThickness="1" Background="{TemplateBinding Background}" BorderBrush="{TemplateBinding BorderBrush}">
                                <StackPanel Orientation="Horizontal" VerticalAlignment="Center">
                                    <telerik:RadButton x:Name="OpenCloseButton" Style="{TemplateBinding OpenCloseButtonStyle}">
                                        <ToolTipService.ToolTip>
                                            <ToolTip Content="{telerik:LocalizableResource Key=Open_Close}"/>
                                        </ToolTipService.ToolTip>
                                    </telerik:RadButton>
                                    <TextBlock
                                            Margin="5 0 5 0"
                                            FontFamily="{telerik1:Office2016Resource ResourceKey=FontFamily}"
                                            FontSize="{telerik1:Office2016Resource ResourceKey=FontSize}"
                                            Text="{telerik:LocalizableResource Key=Toolbox_ViewGalleries}"
                                            Foreground="{telerik1:Office2016Resource ResourceKey=MarkerBrush}"
                                            VerticalAlignment="Center"/>
                                </StackPanel>
                            </Border>
                        </Grid>
                    </Grid>
                </ControlTemplate>
            </Setter.Value>
        </Setter>
    </Style>
