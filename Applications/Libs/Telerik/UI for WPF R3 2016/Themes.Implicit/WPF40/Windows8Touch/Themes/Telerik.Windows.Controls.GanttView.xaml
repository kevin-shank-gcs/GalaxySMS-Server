            <Border x:Name="PART_ExpandCollapseHitArea" MinHeight="15" Background="Transparent" Grid.Column="0" HorizontalAlignment="Stretch">
                <Path x:Name="PART_ExpandCollapseVisual"
                        StrokeThickness="1"
                        RenderTransformOrigin="0.5,0.5"
                        Stretch="Fill"
                        Data="M-12,150.2207 L-8.3125,154.84375 L-12,159.61523"
                        Width="7"
                        Height="9"
                        VerticalAlignment="Stretch"
                        HorizontalAlignment="Stretch"
                        IsHitTestVisible="False"
                        Margin="-4 0 0 0"/>
            </Border>
            <Border
                    BorderBrush="{TemplateBinding BorderBrush}"
                    BorderThickness="{TemplateBinding BorderThickness}"
                    Background="{TemplateBinding Background}"
                    HorizontalAlignment="Stretch"
                    Margin="-3 0 1 0"
                    Grid.Column="1"/>
            <ContentPresenter
                    Grid.Column="1"
                    Margin="{TemplateBinding Padding}"
                    HorizontalAlignment="{TemplateBinding HorizontalContentAlignment}"
                    VerticalAlignment="{TemplateBinding VerticalContentAlignment}"/>
        </Grid>
    </ControlTemplate>
    <Style x:Key="TreeCellHighlightContainerStyle" TargetType="gantt:TreeCellContainer" BasedOn="{StaticResource TreeCellContainerStyle}">
