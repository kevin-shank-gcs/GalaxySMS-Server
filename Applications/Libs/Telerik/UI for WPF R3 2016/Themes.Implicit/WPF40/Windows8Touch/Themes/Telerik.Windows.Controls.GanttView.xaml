<ResourceDictionary
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:telerik="http://schemas.telerik.com/2008/xaml/presentation"
        xmlns:c="http://schemas.telerik.com/2008/xaml/compile"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        mc:Ignorable="c"
        xmlns:gantt="clr-namespace:Telerik.Windows.Controls.GanttView;assembly=Telerik.Windows.Controls.GanttView"
        xmlns:gantt1="clr-namespace:Telerik.Windows.Controls;assembly=Telerik.Windows.Controls.GanttView"
        xmlns:scheduling="clr-namespace:Telerik.Windows.Controls.Scheduling;assembly=Telerik.Windows.Scheduling.Core">
    <ResourceDictionary.MergedDictionaries>
        <telerik:Windows8TouchResourceDictionary/>
    </ResourceDictionary.MergedDictionaries>
    <telerik:BooleanToVisibilityConverter x:Key="BooleanToVisibilityConverter"/>
    <telerik:InvertedBooleanToVisibilityConverter x:Key="InvertedBooleanToVisibilityConverter"/>
    <gantt1:DateTimeToCurrentUICultureStringConverter x:Key="DateTimeToCurrentUICultureStringConverter"/>
    <telerik:DragDropBooleanToVisibilityConverter x:Key="DragDropBooleanToVisibilityConverter"/>
    <SolidColorBrush x:Key="OnTimeFill" Color="#FFFFFFFF"/>
    <telerik:Windows8TouchColors x:Key="Windows8TouchColors"/>
    <SolidColorBrush x:Key="GanttView_BorderBrush" Color="Transparent"/>
    <SolidColorBrush x:Key="GroupContainer_OuterBorderBrush_Selected" Color="Transparent"/>
    <SolidColorBrush x:Key="EventBackground_Highlighted" Color="#FFE22F07"/>
    <SolidColorBrush x:Key="SummaryBackground_Normal" Color="#FF8CC221"/>
    <SolidColorBrush x:Key="SummaryBackground_MouseOver" Color="#FFD8E404"/>
    <SolidColorBrush x:Key="SummaryBackground_Selected" Color="#FF4F9205"/>
    <SolidColorBrush x:Key="SummaryBackground_Highlighted" Color="#FFE22F07"/>
    <SolidColorBrush x:Key="SummaryBackground_DragResize" Color="#E5D8E404"/>
    <SolidColorBrush x:Key="MilestoneFill_Highlighted" Color="#FFE23907"/>
    <SolidColorBrush x:Key="MilestoneStroke_Selected" Color="Black"/>
    <SolidColorBrush x:Key="CellHighlightContainerBackground" Color="Transparent"/>
    <SolidColorBrush x:Key="CellEditingBackground" Color="Transparent"/>
    <SolidColorBrush x:Key="RelationContainerBorderBrush" Color="#FF767676"/>
    <SolidColorBrush x:Key="RelationContainerBorderBrush_Highlighted" Color="#FFE22F07"/>
    <SolidColorBrush x:Key="RelationContainerBorderBrush_MouseOver" Color="#FF474747"/>
    <SolidColorBrush x:Key="BorderContainer_Background" Color="Transparent"/>
    <SolidColorBrush x:Key="GanttView_ShapeManipulation_BorderBrush" Color="{Binding Path=Palette.MediumColor, Source={StaticResource Windows8TouchColors}}" telerik:Windows8TouchPalette.IsFreezable="True"/>
    <LinearGradientBrush x:Key="GanttView_ShapeManipulation_Background" EndPoint="0.5,1" StartPoint="0.5,0">
        <GradientStop Color="{Binding Path=Palette.EffectHighColor, Source={StaticResource Windows8TouchColors}}" telerik:Windows8TouchPalette.IsFreezable="True"/>
        <GradientStop Color="{Binding Path=Palette.EffectLowColor, Source={StaticResource Windows8TouchColors}}" telerik:Windows8TouchPalette.IsFreezable="True" Offset="1"/>
    </LinearGradientBrush>
    <SolidColorBrush x:Key="GanttView_DependenciesIndicator_DragOver" Color="#FFDC797C"/>
    <SolidColorBrush x:Key="GanttView_DependenciesIndicator_MouseOver" Color="#FFBEE2F4"/>
    <SolidColorBrush x:Key="GanttView_DragCueDependencyIconBrush" Color="#FFFFFFFF"/>
    <SolidColorBrush x:Key="TransparentBrush" Color="#01FFFFFF"/>
    <Thickness x:Key="RowSelectionBorderThickness">0 0 0 0</Thickness>
    <Thickness x:Key="CellHighlightBorderThickness">1 1 1 1</Thickness>
    <Thickness x:Key="CellContainerThickness">0 0 0 0</Thickness>
    <SolidColorBrush x:Key="OnTimeBrush" Color="#FF309B46"/>
    <SolidColorBrush x:Key="DeadlineBrush" Color="#FFFF0000"/>
    <SolidColorBrush x:Key="DeadlineFill" Color="#FFE23907"/>
    <DataTemplate x:Key="GanttDragResizeVisualCueTemplate">
        <Grid Margin="{Binding Margin, RelativeSource={RelativeSource Mode=FindAncestor, AncestorType=gantt:GanttDragResizeVisualCue}}">
            <Grid.ColumnDefinitions>
                <ColumnDefinition Width="Auto"/>
                <ColumnDefinition Width="*" MinWidth="140"/>
            </Grid.ColumnDefinitions>
            <Grid.RowDefinitions>
                <RowDefinition/>
                <RowDefinition/>
                <RowDefinition/>
            </Grid.RowDefinitions>
            <TextBlock
                    Text="{Binding TargetItem.Title}"
                    Grid.ColumnSpan="2"
                    Margin="0 -1 0 0"
                    FontFamily="{telerik:Windows8TouchResource ResourceKey=FontFamily}"
                    FontSize="{telerik:Windows8TouchResource ResourceKey=FontSizeL}"/>
            <TextBlock telerik:LocalizationManager.ResourceKey="Start" Grid.Row="1" Margin="0 0 3 0"/>
            <TextBlock Text="{Binding DestinationSlot.Start, Converter={StaticResource DateTimeToCurrentUICultureStringConverter}}" Grid.Row="1" Grid.Column="1" FontWeight="Normal"/>
            <TextBlock telerik:LocalizationManager.ResourceKey="End" Grid.Row="2" Margin="0 0 3 0"/>
            <TextBlock Text="{Binding DestinationSlot.End, Converter={StaticResource DateTimeToCurrentUICultureStringConverter}}" Grid.Row="2" Grid.Column="1" FontWeight="Normal"/>
        </Grid>
    </DataTemplate>
    <DataTemplate x:Key="GanttCreateDependencyTemplate">
        <Grid Margin="{Binding Margin, RelativeSource={RelativeSource Mode=FindAncestor, AncestorType=gantt:GanttDragResizeVisualCue}}">
            <Grid.RowDefinitions>
                <RowDefinition/>
                <RowDefinition/>
            </Grid.RowDefinitions>
            <StackPanel Orientation="Horizontal">
                <TextBlock telerik:LocalizationManager.ResourceKey="DrawRelationFrom"/>
                <TextBlock Text="{Binding DestinationSlot.Source.Title}" FontWeight="Bold"/>
            </StackPanel>
            <StackPanel Grid.Row="1" Orientation="Horizontal">
                <TextBlock telerik:LocalizationManager.ResourceKey="DrawRelationTo"/>
                <TextBlock Text="{Binding DestinationSlot.Target.Title}" FontWeight="Bold"/>
            </StackPanel>
        </Grid>
    </DataTemplate>
    <DataTemplate x:Key="GanttDragReorderTemplate">
        <Grid Margin="{Binding Margin, RelativeSource={RelativeSource Mode=FindAncestor, AncestorType=gantt:GanttDragResizeVisualCue}}">
            <Grid.RowDefinitions>
                <RowDefinition/>
                <RowDefinition/>
            </Grid.RowDefinitions>
            <StackPanel Orientation="Horizontal" HorizontalAlignment="Left">
                <TextBlock telerik:LocalizationManager.ResourceKey="DraggingItem"/>
                <TextBlock Text="{Binding TargetItem.Title}" FontWeight="Bold"/>
            </StackPanel>
            <StackPanel Grid.Row="1" Orientation="Horizontal" HorizontalAlignment="Left">
                <Path
                        Data="M6,2.9 L11,2.9 L11,3.9 L6,3.9 z M4.0,4.9 L13,4.9 L13,5.9 L4.0,5.9 z M6.0,6.9 L11,6.9 L11,7.9 L6.0,7.9 z M4,3.4 L0,7 L0,0 z"
                        Height="8"
                        Stretch="Fill"
                        VerticalAlignment="Center"
                        Margin="0 0 5 0"
                        HorizontalAlignment="Left"
                        Visibility="{Binding DestinationSlot.DropPosition, Converter={StaticResource DragDropBooleanToVisibilityConverter}, ConverterParameter=Before}"
                        Fill="{telerik:Windows8TouchResource ResourceKey=InvertedForegroundBrush}"/>
                <Path
                        Data="M2,4 L7,4 L7,5 L2,5 z M0,2 L9,2 L9,3 L0,3 z M13,1.0 L13,8.0 L9,4.4 z M2,0 L7,0 L7,1 L2,1 z"
                        HorizontalAlignment="Left"
                        Height="8"
                        Stretch="Fill"
                        VerticalAlignment="Center"
                        Margin="0 0 5 0"
                        Visibility="{Binding DestinationSlot.DropPosition, Converter={StaticResource DragDropBooleanToVisibilityConverter}, ConverterParameter=After}"
                        Fill="{telerik:Windows8TouchResource ResourceKey=InvertedForegroundBrush}"/>
                <Path
                        Data="M3,0 L5,0 L5,3 L8,3 L8,5 L5,5 L5,8 L3,8 L3,5 L0,5 L0,3 L3,3 z"
                        Margin="0 0 5 0"
                        Height="8"
                        Stretch="Fill"
                        VerticalAlignment="Center"
                        Width="8"
                        HorizontalAlignment="Left"
                        Visibility="{Binding DestinationSlot.DropPosition, Converter={StaticResource DragDropBooleanToVisibilityConverter}, ConverterParameter=Inside}"
                        Fill="{telerik:Windows8TouchResource ResourceKey=InvertedForegroundBrush}"/>
                <TextBlock Text="{Binding DestinationSlot.DropPosition}"/>
                <TextBlock Text=":" Margin="0 0 4 0"/>
                <TextBlock Text="{Binding DestinationSlot.GroupKey.Title}" FontWeight="Bold"/>
            </StackPanel>
        </Grid>
    </DataTemplate>
    <DataTemplate x:Key="DragRelationTooltipTemplate">
        <StackPanel Orientation="Horizontal">
            <TextBlock telerik:LocalizationManager.ResourceKey="RelationToolTip" TextWrapping="NoWrap" TextTrimming="None"/>
        </StackPanel>
    </DataTemplate>
    <ControlTemplate x:Key="GanttPresenterSplitterTemplate" TargetType="telerik:ResizeControl">
        <Grid x:Name="Root" IsHitTestVisible="{TemplateBinding IsEnabled}">
            <VisualStateManager.VisualStateGroups>
                <VisualStateGroup x:Name="CommonStates">
                    <VisualState x:Name="Normal"/>
                    <VisualState x:Name="MouseOver"/>
                    <VisualState x:Name="Disabled"/>
                </VisualStateGroup>
                <VisualStateGroup x:Name="FocusStates">
                    <VisualState x:Name="Unfocused"/>
                    <VisualState x:Name="Focused">
                        <Storyboard>
                            <DoubleAnimation Duration="0" Storyboard.TargetName="FocusedVisual" Storyboard.TargetProperty="Opacity" To="0.5"/>
                        </Storyboard>
                    </VisualState>
                </VisualStateGroup>
            </VisualStateManager.VisualStateGroups>
            <Border
                    Width="{TemplateBinding Width}"
                    VerticalAlignment="{TemplateBinding VerticalAlignment}"
                    HorizontalAlignment="{TemplateBinding HorizontalAlignment}"
                    Background="{TemplateBinding Background}"
                    BorderBrush="{TemplateBinding BorderBrush}"
                    BorderThickness="{TemplateBinding BorderThickness}"/>
            <Rectangle x:Name="FocusedVisual" Fill="Transparent" StrokeThickness="0" Opacity="0"/>
            <Rectangle x:Name="PART_PreviewControl" VerticalAlignment="Stretch" HorizontalAlignment="Stretch" Fill="{TemplateBinding BorderBrush}" Opacity="0.5" Visibility="Collapsed"/>
        </Grid>
    </ControlTemplate>
    <Style x:Key="GanttPresenterSplitterStyle" TargetType="telerik:ResizeControl">
        <Setter Property="Background" Value="{telerik:Windows8TouchResource ResourceKey=MainBrush}"/>
        <Setter Property="IsTabStop" Value="True"/>
        <Setter Property="HorizontalAlignment" Value="Right"/>
        <Setter Property="VerticalAlignment" Value="Stretch"/>
        <Setter Property="scheduling:ZIndexManager.ZIndex" Value="100"/>
        <Setter Property="ShowsPreview" Value="True"/>
        <Setter Property="BorderThickness" Value="1 0 1 0"/>
        <Setter Property="BorderBrush" Value="{telerik:Windows8TouchResource ResourceKey=MediumBrush}"/>
        <Setter Property="Width" Value="8"/>
        <Setter Property="Template" Value="{StaticResource GanttPresenterSplitterTemplate}"/>
    </Style>
    <Style x:Key="GanttPresenterHeaderSplitterStyle" TargetType="telerik:ResizeControl" BasedOn="{StaticResource GanttPresenterSplitterStyle}">
        <Setter Property="Width" Value="15"/>
        <Setter Property="BorderThickness" Value="0 0 1 0"/>
        <Setter Property="Background" Value="{StaticResource TransparentBrush}"/>
        <Setter Property="BorderBrush" Value="{telerik:Windows8TouchResource ResourceKey=MainBrush}"/>
        <Setter Property="scheduling:ZIndexManager.ZIndex" Value="100"/>
    </Style>
    <ControlTemplate x:Key="GanttItemsPresenterTemplate" TargetType="gantt:GanttItemsPresenter">
        <telerik:GanttPresenterPanel>
            <telerik:GanttPresenterPanel.ColumnDefinitions>
                <ColumnDefinition Width="Auto"/>
                <ColumnDefinition Width="*" MinWidth="3"/>
                <ColumnDefinition Width="3*"/>
                <ColumnDefinition Width="Auto"/>
            </telerik:GanttPresenterPanel.ColumnDefinitions>
            <telerik:TreeRenderingPanel x:Name="FrozenColumnsPanel"
                    Orientation="Horizontal"
                    telerik:GanttPresenterPanel.Column="0"
                    telerik:GanttPresenterPanel.Area="Header"
                    Margin="4 0 0 0"
                    HorizontalAlignment="Stretch"
                    VerticalAlignment="Stretch"/>
            <telerik:VirtualizedGridPanel x:Name="FrozenCellsPanel"
                    HorizontalAlignment="Stretch"
                    VerticalAlignment="Top"
                    Margin="4 0 0 0"
                    telerik:GanttPresenterPanel.Column="0"
                    telerik:GanttPresenterPanel.Area="Content"/>
            <telerik:TreeRenderingPanel x:Name="ColumnsPanel"
                    Orientation="Horizontal"
                    telerik:GanttPresenterPanel.Column="1"
                    telerik:GanttPresenterPanel.Area="Header"
                    HorizontalAlignment="Left"
                    VerticalAlignment="Stretch"
                    Margin="-1 0 4 0"/>
            <telerik:VirtualizedGridPanel x:Name="CellsPanel"
                    telerik:GanttPresenterPanel.Column="1"
                    telerik:GanttPresenterPanel.Area="Content"
                    Margin="-1 0 4 0"
                    HorizontalAlignment="Left"
                    VerticalAlignment="Top"
                    VerticalOffset="{Binding VerticalOffset, ElementName=FrozenCellsPanel, Mode=TwoWay}"
                    HorizontalOffset="{Binding HorizontalOffset, ElementName=ColumnsPanel, Mode=TwoWay}"/>
            <ScrollBar
                    telerik:GanttPresenterPanel.Column="1"
                    telerik:GanttPresenterPanel.Area="ScrollBar"
                    Orientation="Horizontal"
                    Minimum="0"
                    Maximum="{Binding ExtentWidth, ElementName=ColumnsPanel}"
                    Value="{Binding HorizontalOffset, ElementName=ColumnsPanel, Mode=TwoWay}"
                    ViewportSize="{Binding ViewportWidth, ElementName=ColumnsPanel}"
                    SmallChange="40"
                    LargeChange="{Binding ViewportWidth, ElementName=ColumnsPanel}"
                    Visibility="{Binding CalculatedHorizontalScrollBarVisibility, ElementName=ColumnsPanel}"
                    Margin="0 0 8 0"/>
            <telerik:ResizeControl telerik:GanttPresenterPanel.Column="1" telerik:GanttPresenterPanel.Area="Splitter" Style="{StaticResource GanttPresenterSplitterStyle}"/>
            <telerik:LogicalCanvasPanel x:Name="TimeRulerPanel"
                    telerik:GanttPresenterPanel.Column="2"
                    telerik:GanttPresenterPanel.Area="Header"
                    VerticalAlignment="Bottom"
                    HorizontalAlignment="Left"
                    HorizontalOffset="{Binding HorizontalOffset, ElementName=EventsPanel, Mode=TwoWay}"/>
            <telerik:LogicalCanvasPanel x:Name="EventsPanel"
                    telerik:GanttPresenterPanel.Column="2"
                    telerik:GanttPresenterPanel.Area="Content"
                    VerticalOffset="{Binding VerticalOffset, ElementName=CellsPanel, Mode=TwoWay}"
                    Background="Transparent"
                    VerticalAlignment="Top"
                    HorizontalAlignment="Left"/>
            <ScrollBar
                    telerik:GanttPresenterPanel.Column="2"
                    telerik:GanttPresenterPanel.Area="ScrollBar"
                    Orientation="Horizontal"
                    Minimum="0"
                    Maximum="{Binding ExtentWidth, ElementName=EventsPanel}"
                    Value="{Binding HorizontalOffset, ElementName=EventsPanel, Mode=TwoWay}"
                    ViewportSize="{Binding ViewportWidth, ElementName=EventsPanel}"
                    SmallChange="20"
                    LargeChange="{Binding ViewportWidth, ElementName=EventsPanel}"
                    Visibility="{Binding CalculatedHorizontalScrollBarVisibility, ElementName=EventsPanel}"/>
            <ScrollBar
                    telerik:GanttPresenterPanel.Column="3"
                    telerik:GanttPresenterPanel.Area="Content"
                    Orientation="Vertical"
                    Minimum="0"
                    Maximum="{Binding ExtentHeight, ElementName=CellsPanel}"
                    Value="{Binding VerticalOffset, ElementName=CellsPanel, Mode=TwoWay}"
                    ViewportSize="{Binding ViewportHeight, ElementName=CellsPanel}"
                    Visibility="{Binding CalculatedVerticalScrollBarVisibility, ElementName=CellsPanel}"
                    SmallChange="10"
                    LargeChange="{Binding ViewportHeight, ElementName=CellsPanel}"/>
        </telerik:GanttPresenterPanel>
    </ControlTemplate>
    <Style x:Key="GanttItemsPresenterStyle" TargetType="gantt:GanttItemsPresenter">
        <Setter Property="Background" Value="{telerik:Windows8TouchResource ResourceKey=MediumBrush}"/>
        <Setter Property="Template" Value="{StaticResource GanttItemsPresenterTemplate}"/>
    </Style>
    <ControlTemplate x:Key="GanttDragResizeVisualCueControlTemplate" TargetType="gantt:GanttDragResizeVisualCue">
        <Grid>
            <VisualStateManager.VisualStateGroups>
                <VisualStateGroup x:Name="EffectStates">
                    <VisualState x:Name="None">
                        <Storyboard>
                            <ObjectAnimationUsingKeyFrames Storyboard.TargetName="NoneEffect" Storyboard.TargetProperty="Visibility">
                                <DiscreteObjectKeyFrame KeyTime="0:0:0">
                                    <DiscreteObjectKeyFrame.Value>
                                        <Visibility>Visible</Visibility>
                                    </DiscreteObjectKeyFrame.Value>
                                </DiscreteObjectKeyFrame>
                            </ObjectAnimationUsingKeyFrames>
                        </Storyboard>
                    </VisualState>
                    <VisualState x:Name="Move"/>
                    <VisualState x:Name="Copy">
                        <Storyboard>
                            <ObjectAnimationUsingKeyFrames Storyboard.TargetName="CopyEffect" Storyboard.TargetProperty="Visibility">
                                <DiscreteObjectKeyFrame KeyTime="0:0:0">
                                    <DiscreteObjectKeyFrame.Value>
                                        <Visibility>Visible</Visibility>
                                    </DiscreteObjectKeyFrame.Value>
                                </DiscreteObjectKeyFrame>
                            </ObjectAnimationUsingKeyFrames>
                        </Storyboard>
                    </VisualState>
                    <VisualState x:Name="Link">
                        <Storyboard>
                            <ObjectAnimationUsingKeyFrames Storyboard.TargetName="LinkEffect" Storyboard.TargetProperty="Visibility">
                                <DiscreteObjectKeyFrame KeyTime="0:0:0">
                                    <DiscreteObjectKeyFrame.Value>
                                        <Visibility>Visible</Visibility>
                                    </DiscreteObjectKeyFrame.Value>
                                </DiscreteObjectKeyFrame>
                            </ObjectAnimationUsingKeyFrames>
                        </Storyboard>
                    </VisualState>
                    <VisualState x:Name="Scroll">
                        <Storyboard>
                            <ObjectAnimationUsingKeyFrames Storyboard.TargetName="ScrollEffect" Storyboard.TargetProperty="Visibility">
                                <DiscreteObjectKeyFrame KeyTime="0:0:0">
                                    <DiscreteObjectKeyFrame.Value>
                                        <Visibility>Visible</Visibility>
                                    </DiscreteObjectKeyFrame.Value>
                                </DiscreteObjectKeyFrame>
                            </ObjectAnimationUsingKeyFrames>
                        </Storyboard>
                    </VisualState>
                </VisualStateGroup>
            </VisualStateManager.VisualStateGroups>
            <Border
                    Margin="0"
                    Padding="{TemplateBinding BorderThickness}"
                    Background="{TemplateBinding Background}"
                    BorderBrush="{TemplateBinding BorderBrush}"
                    BorderThickness="{TemplateBinding BorderThickness}">
                <Border.Effect>
                    <DropShadowEffect Opacity="0.3" BlurRadius="10" Direction="280" ShadowDepth="3"/>
                </Border.Effect>
                <Grid Margin="{TemplateBinding Padding}">
                    <Grid.ColumnDefinitions>
                        <ColumnDefinition Width="Auto"/>
                        <ColumnDefinition Width="*"/>
                    </Grid.ColumnDefinitions>
                    <Path x:Name="NoneEffect"
                            Visibility="Collapsed"
                            Margin="4 6 4 3"
                            Width="13"
                            Height="13"
                            HorizontalAlignment="Left"
                            VerticalAlignment="Top"
                            Data="M12.5,6.5 C12.5,9.8 9.8,12.5 6.5,12.5 C3.18,12.5 0.5,9.8 0.5,6.5 C0.5,3.1 3.1,0.5 6.5,0.5 C9.8,0.5 12.5,3.1 12.5,6.5 z M10,1.625 L3,11.125"
                            Stroke="{StaticResource GanttView_DragCueDependencyIconBrush}"
                            StrokeThickness="1"/>
                    <Path x:Name="CopyEffect"
                            Visibility="Collapsed"
                            Margin="4 6 3 3"
                            Width="10"
                            Height="10"
                            HorizontalAlignment="Left"
                            VerticalAlignment="Top"
                            Data="M5,1 L5,9 M1,5 L9,5"
                            Stroke="{telerik:Windows8TouchResource ResourceKey=HighBrush}"
                            StrokeThickness="2"/>
                    <Path x:Name="LinkEffect"
                            HorizontalAlignment="Left"
                            VerticalAlignment="Top"
                            Margin="9 6 4 3"
                            RenderTransformOrigin="0.5 0.5"
                            Stretch="Fill"
                            UseLayoutRounding="False"
                            Width="6"
                            Height="13"
                            Fill="{StaticResource GanttView_DragCueDependencyIconBrush}"
                            Visibility="Collapsed"
                            Data="M3.4,5.0 C3.7,5.0 4.0,5.0 4.5,5.2 C5.0,5.3 4.6,6.7 4.5,6.9 C3.9,6.6 3.2,6.7 2.8,6.8 L2.7,6.9 C2.1,7.3 1.7,8.0 1.7,8.9 L1.7,11.1 C1.7,12.3 2.5,13.3 3.4,13.3 C4.4,13.3 5.1,12.4 5.2,10.3 C5.2,10.3 7.0,9.4 7.0,9.4 L6.9,11.7 C6.9,13.5 5.4,15.0 3.4,15.0 C1.5,15.0 8.9E-08,13.5 0,11.7 L0,8.3 C8.9E-08,6.5 1.5,5.0 3.4,5.0 z M3.7,0 C5.6,3.7E-08 7.1,1.4 7.1,3.3 L7.1,6.6 C7.1,8.5 5.6,9.9 3.7,9.9 C3.4,9.9 3.2,10.0 2.7,9.8 C2.2,9.7 2.5,8.2 2.6,8.1 C3.2,8.3 3.9,8.2 4.3,8.1 L4.4,8.1 C5.0,7.7 5.4,6.9 5.4,6.1 L5.4,3.8 C5.4,2.6 4.6,1.5 3.7,1.6 C2.4,1.7 1.9,2.9 2.0,4.4 C2.0,4.4 0.1,5.9 0.1,5.9 L0.2,3.3 C0.2,1.4 1.8,3.7E-08 3.7,0 z">
                        <Path.RenderTransform>
                            <TransformGroup>
                                <RotateTransform Angle="45"/>
                            </TransformGroup>
                        </Path.RenderTransform>
                    </Path>
                    <Path x:Name="ScrollEffect"
                            Visibility="Collapsed"
                            Margin="0 4 3 3"
                            Width="10"
                            Height="12"
                            HorizontalAlignment="Left"
                            VerticalAlignment="Top"
                            Data="M0,5 L5,0 10,5Z M0,7 L5,12 10,7Z"
                            Fill="{telerik:Windows8TouchResource ResourceKey=HighBrush}"/>
                    <ContentControl
                            Grid.Column="1"
                            x:Name="Content"
                            Foreground="{TemplateBinding Foreground}"
                            Content="{TemplateBinding Content}"
                            ContentTemplate="{TemplateBinding ContentTemplate}"
                            HorizontalAlignment="{TemplateBinding HorizontalContentAlignment}"
                            VerticalAlignment="{TemplateBinding VerticalContentAlignment}"/>
                </Grid>
            </Border>
        </Grid>
    </ControlTemplate>
    <Style x:Key="GanttDragResizeVisualCueStyle" TargetType="gantt:GanttDragResizeVisualCue">
        <Setter Property="Margin" Value="4"/>
        <Setter Property="FontSize" Value="{telerik:Windows8TouchResource ResourceKey=FontSize}"/>
        <Setter Property="FontFamily" Value="{telerik:Windows8TouchResource ResourceKey=FontFamily}"/>
        <Setter Property="Foreground" Value="{telerik:Windows8TouchResource ResourceKey=InvertedForegroundBrush}"/>
        <Setter Property="BorderBrush" Value="{telerik:Windows8TouchResource ResourceKey=AccentBrush}"/>
        <Setter Property="Background" Value="{telerik:Windows8TouchResource ResourceKey=AccentBrush}"/>
        <Setter Property="BorderThickness" Value="1"/>
        <Setter Property="HorizontalContentAlignment" Value="Left"/>
        <Setter Property="VerticalContentAlignment" Value="Stretch"/>
        <Setter Property="CueTemplateSelector">
            <Setter.Value>
                <gantt:GanttDragVisualCueContentTemplateSelector
                        DragTemplate="{StaticResource GanttDragResizeVisualCueTemplate}"
                        ResizeTemplate="{StaticResource GanttDragResizeVisualCueTemplate}"
                        CreateDependencyTemplate="{StaticResource GanttCreateDependencyTemplate}"
                        DragReorderTemplate="{StaticResource GanttDragReorderTemplate}"/>
            </Setter.Value>
        </Setter>
        <Setter Property="Template" Value="{StaticResource GanttDragResizeVisualCueControlTemplate}"/>
    </Style>
    <Style x:Key="RadGanttViewStyle" TargetType="gantt1:RadGanttView">
        <Setter Property="telerik:ScrollingSettingsBehavior.IsEnabled" Value="True"/>
        <Setter Property="telerik:ScrollingSettingsBehavior.ScrollAreaPadding" Value="30"/>
        <Setter Property="telerik:ScrollingSettingsBehavior.ScrollStep" Value="5"/>
        <Setter Property="telerik:ScrollingSettingsBehavior.ScrollStepTime" Value="00:00:00.01"/>
        <Setter Property="UseLayoutRounding" Value="True"/>
        <Setter Property="Background" Value="{telerik:Windows8TouchResource ResourceKey=MainBrush}"/>
        <Setter Property="BorderBrush" Value="{StaticResource GanttView_BorderBrush}"/>
        <Setter Property="GapBetweenRows" Value="0"/>
        <Setter Property="GapBetweenColumns" Value="0"/>
        <Setter Property="FontFamily" Value="{telerik:Windows8TouchResource ResourceKey=FontFamily}"/>
        <Setter Property="TimeLineVisualizationBehavior">
            <Setter.Value>
                <gantt:DefaultGanttTimeLineVisualizationBehavior HasSelectionHighlight="True"/>
            </Setter.Value>
        </Setter>
        <Setter Property="Template">
            <Setter.Value>
                <ControlTemplate TargetType="gantt1:RadGanttView">
                    <Border BorderBrush="{TemplateBinding BorderBrush}" BorderThickness="{TemplateBinding BorderThickness}" Background="{TemplateBinding Background}">
                        <gantt:GanttItemsPresenter x:Name="ItemsPresenter" Margin="{TemplateBinding Padding}"/>
                    </Border>
                </ControlTemplate>
            </Setter.Value>
        </Setter>
    </Style>
    <Style x:Key="DropReorderHighlightContainerStyle" TargetType="gantt:DropReorderHighlightContainer">
        <Setter Property="Height" Value="8"/>
        <Setter Property="Margin" Value="0 -4"/>
        <Setter Property="IsHitTestVisible" Value="False"/>
        <Setter Property="scheduling:ZIndexManager.ZIndex" Value="105"/>
        <Setter Property="SnapsToDevicePixels" Value="True"/>
        <Setter Property="Template">
            <Setter.Value>
                <ControlTemplate TargetType="gantt:DropReorderHighlightContainer">
                    <Grid>
                        <VisualStateManager.VisualStateGroups>
                            <VisualStateGroup x:Name="CommonStates">
                                <VisualState x:Name="Normal"/>
                                <VisualState x:Name="Doted"/>
                            </VisualStateGroup>
                        </VisualStateManager.VisualStateGroups>
                        <Grid.ColumnDefinitions>
                            <ColumnDefinition Width="Auto"/>
                            <ColumnDefinition Width="*"/>
                        </Grid.ColumnDefinitions>
                        <Path
                                Data="M0,1 L1500,1"
                                Height="2"
                                Stretch="None"
                                Stroke="{telerik:Windows8TouchResource ResourceKey=AccentBrush}"
