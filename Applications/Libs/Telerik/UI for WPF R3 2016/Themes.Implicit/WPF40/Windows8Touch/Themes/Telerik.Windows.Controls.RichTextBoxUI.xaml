    <ControlTemplate x:Key="RulerThumbControlTemplate" TargetType="telerikControlsRTBUI:RulerThumb">
        <Grid>
            <ToolTipService.ToolTip>
                <ToolTip Content="{TemplateBinding ToolTipText}"/>
            </ToolTipService.ToolTip>
            <VisualStateManager.VisualStateGroups>
                <VisualStateGroup x:Name="ThumbState">
                    <VisualState x:Name="IndentVS">
                        <Storyboard>
                            <ObjectAnimationUsingKeyFrames Storyboard.TargetName="Indent" Storyboard.TargetProperty="Visibility">
                                <DiscreteObjectKeyFrame KeyTime="00:00:00">
                                    <DiscreteObjectKeyFrame.Value>
                                        <Visibility>Visible</Visibility>
                                    </DiscreteObjectKeyFrame.Value>
                                </DiscreteObjectKeyFrame>
                            </ObjectAnimationUsingKeyFrames>
                        </Storyboard>
                    </VisualState>
                    <VisualState x:Name="LeftIndentVS">
                        <Storyboard>
                            <ObjectAnimationUsingKeyFrames Storyboard.TargetName="LeftIndent" Storyboard.TargetProperty="Visibility">
                                <DiscreteObjectKeyFrame KeyTime="00:00:00">
                                    <DiscreteObjectKeyFrame.Value>
                                        <Visibility>Visible</Visibility>
                                    </DiscreteObjectKeyFrame.Value>
                                </DiscreteObjectKeyFrame>
                            </ObjectAnimationUsingKeyFrames>
                        </Storyboard>
                    </VisualState>
                    <VisualState x:Name="FirstLineIndentVS">
                        <Storyboard>
                            <ObjectAnimationUsingKeyFrames Storyboard.TargetName="FirstLineIndent" Storyboard.TargetProperty="Visibility">
                                <DiscreteObjectKeyFrame KeyTime="00:00:00">
                                    <DiscreteObjectKeyFrame.Value>
                                        <Visibility>Visible</Visibility>
                                    </DiscreteObjectKeyFrame.Value>
                                </DiscreteObjectKeyFrame>
                            </ObjectAnimationUsingKeyFrames>
                        </Storyboard>
                    </VisualState>
                    <VisualState x:Name="RightIndentVS">
                        <Storyboard>
                            <ObjectAnimationUsingKeyFrames Storyboard.TargetName="RightIndent" Storyboard.TargetProperty="Visibility">
                                <DiscreteObjectKeyFrame KeyTime="00:00:00">
                                    <DiscreteObjectKeyFrame.Value>
                                        <Visibility>Visible</Visibility>
                                    </DiscreteObjectKeyFrame.Value>
                                </DiscreteObjectKeyFrame>
                            </ObjectAnimationUsingKeyFrames>
                        </Storyboard>
                    </VisualState>
                    <VisualState x:Name="LeftTabStop">
                        <Storyboard>
                            <ObjectAnimationUsingKeyFrames Storyboard.TargetName="LeftTab" Storyboard.TargetProperty="Visibility">
                                <DiscreteObjectKeyFrame KeyTime="00:00:00">
                                    <DiscreteObjectKeyFrame.Value>
                                        <Visibility>Visible</Visibility>
                                    </DiscreteObjectKeyFrame.Value>
                                </DiscreteObjectKeyFrame>
                            </ObjectAnimationUsingKeyFrames>
                        </Storyboard>
                    </VisualState>
                    <VisualState x:Name="RightTabStop">
                        <Storyboard>
                            <ObjectAnimationUsingKeyFrames Storyboard.TargetName="RightTab" Storyboard.TargetProperty="Visibility">
                                <DiscreteObjectKeyFrame KeyTime="00:00:00">
                                    <DiscreteObjectKeyFrame.Value>
                                        <Visibility>Visible</Visibility>
                                    </DiscreteObjectKeyFrame.Value>
                                </DiscreteObjectKeyFrame>
                            </ObjectAnimationUsingKeyFrames>
                        </Storyboard>
                    </VisualState>
                    <VisualState x:Name="CenterTabStop">
                        <Storyboard>
                            <ObjectAnimationUsingKeyFrames Storyboard.TargetName="CenterTab" Storyboard.TargetProperty="Visibility">
                                <DiscreteObjectKeyFrame KeyTime="00:00:00">
                                    <DiscreteObjectKeyFrame.Value>
                                        <Visibility>Visible</Visibility>
                                    </DiscreteObjectKeyFrame.Value>
                                </DiscreteObjectKeyFrame>
                            </ObjectAnimationUsingKeyFrames>
                        </Storyboard>
                    </VisualState>
                    <VisualState x:Name="DecimalTabStop">
                        <Storyboard>
                            <ObjectAnimationUsingKeyFrames Storyboard.TargetName="DecimalTab" Storyboard.TargetProperty="Visibility">
                                <DiscreteObjectKeyFrame KeyTime="00:00:00">
                                    <DiscreteObjectKeyFrame.Value>
                                        <Visibility>Visible</Visibility>
                                    </DiscreteObjectKeyFrame.Value>
                                </DiscreteObjectKeyFrame>
                            </ObjectAnimationUsingKeyFrames>
                        </Storyboard>
                    </VisualState>
                    <VisualState x:Name="BarTabStop">
                        <Storyboard>
                            <ObjectAnimationUsingKeyFrames Storyboard.TargetName="BarTab" Storyboard.TargetProperty="Visibility">
                                <DiscreteObjectKeyFrame KeyTime="00:00:00">
                                    <DiscreteObjectKeyFrame.Value>
                                        <Visibility>Visible</Visibility>
                                    </DiscreteObjectKeyFrame.Value>
                                </DiscreteObjectKeyFrame>
                            </ObjectAnimationUsingKeyFrames>
                        </Storyboard>
                    </VisualState>
                </VisualStateGroup>
            </VisualStateManager.VisualStateGroups>
            <Path x:Name="Indent"
                    Stretch="Fill"
                    Visibility="Collapsed"
                    UseLayoutRounding="True"
                    RenderTransformOrigin="0.5 0.5"
                    Fill="{TemplateBinding Background}"
                    Data="M 0,16 0,10 9,0 18,10 18,16z"/>
            <Path x:Name="FirstLineIndent"
                    Stretch="Fill"
                    RenderTransformOrigin="0.5 0.5"
                    Visibility="Collapsed"
                    UseLayoutRounding="True"
                    Fill="{TemplateBinding Background}"
                    VerticalAlignment="{TemplateBinding VerticalContentAlignment}"
                    HorizontalAlignment="{TemplateBinding HorizontalContentAlignment}"
                    Data="M0,0 18,0 18,6 9,16 0,6z"/>
            <Path x:Name="RightIndent"
                    Stretch="Fill"
                    Width="18"
                    Height="16"
                    Data="M 0,16 0,10 9,0 18,10 18,16z"
                    RenderTransformOrigin="0.5 0.5"
                    Visibility="Collapsed"
                    Fill="{TemplateBinding Background}"/>
            <Rectangle x:Name="LeftIndent"
                    Fill="{TemplateBinding Background}"
                    Visibility="Collapsed"
                    VerticalAlignment="{TemplateBinding VerticalContentAlignment}"
                    HorizontalAlignment="{TemplateBinding HorizontalContentAlignment}"
                    Margin="0 13"
                    Height="10"
                    Width="18"/>
            <Path x:Name="LeftTab"
                    VerticalAlignment="{TemplateBinding VerticalContentAlignment}"
                    HorizontalAlignment="{TemplateBinding HorizontalContentAlignment}"
                    Visibility="Collapsed"
                    Fill="{TemplateBinding Foreground}"
                    Stretch="None"
                    UseLayoutRounding="True"
                    Data="M0,0 4,0 4,14 18,14 18,18 0,18z"
                    SnapsToDevicePixels="True"/>
            <Path x:Name="RightTab"
                    Visibility="Collapsed"
                    Fill="{TemplateBinding Foreground}"
                    VerticalAlignment="{TemplateBinding VerticalContentAlignment}"
                    HorizontalAlignment="{TemplateBinding HorizontalContentAlignment}"
                    UseLayoutRounding="True"
                    Stretch="None"
                    Data="M14,0 L18,0 L18,18 L0,18 L0,14 L14,14 z"
                    SnapsToDevicePixels="True"/>
            <Path x:Name="CenterTab"
                    Visibility="Collapsed"
                    Fill="{TemplateBinding Foreground}"
                    Stretch="None"
                    UseLayoutRounding="True"
                    VerticalAlignment="{TemplateBinding VerticalContentAlignment}"
                    HorizontalAlignment="{TemplateBinding HorizontalContentAlignment}"
                    Data="M0,14 10,14 10,0 14,0 14,14 24,14 24,18 0,18z"
                    SnapsToDevicePixels="True"/>
            <Path x:Name="DecimalTab"
                    Visibility="Collapsed"
                    Fill="{TemplateBinding Foreground}"
                    UseLayoutRounding="True"
                    Stretch="None"
                    VerticalAlignment="{TemplateBinding VerticalContentAlignment}"
                    HorizontalAlignment="{TemplateBinding HorizontalContentAlignment}"
                    Data="M0,14 10,14 10,0 14,0 14,14 24,14 24,18 0,18z M18,0 24,0 24,6 18,6z"
                    SnapsToDevicePixels="True"/>
            <Path x:Name="BarTab"
                    Visibility="Collapsed"
                    Data="M0 0 L5 0 L5 2 L0 2 z"
                    Fill="{TemplateBinding Foreground}"
                    UseLayoutRounding="True"
                    VerticalAlignment="{TemplateBinding VerticalContentAlignment}"
                    HorizontalAlignment="{TemplateBinding HorizontalContentAlignment}"
                    Stretch="Fill"
                    Width="8"
                    Height="20"
                    SnapsToDevicePixels="True"/>
        </Grid>
    </ControlTemplate>
