                                <telerik:RadToggleButton x:Name="buttonBold" Command="{Binding Path=ToggleBoldCommand}" IsTabStop="False" IsBackgroundVisible="False" Focusable="False">
                                    <Image Source="{telerik:IconResource IconRelativePath=16/bold.png, IconSources={StaticResource IconSources}}"/>
                                </telerik:RadToggleButton>
                                <telerik:RadToggleButton x:Name="buttonItalic" Command="{Binding Path=ToggleItalicCommand}" IsTabStop="False" IsBackgroundVisible="False" Focusable="False">
