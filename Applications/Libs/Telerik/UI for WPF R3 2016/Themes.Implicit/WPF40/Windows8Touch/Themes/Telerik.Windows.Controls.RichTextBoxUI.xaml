                                Visibility="{TemplateBinding DropDownIndicatorVisibility}"
                                SnapsToDevicePixels="True"
                                Fill="{telerik:Windows8TouchResource ResourceKey=HighBrush}"
                                Data="M0,0 L8,0 4,8 z"
                                Width="10"
                                Height="8"
                                Margin="3 6"/>
