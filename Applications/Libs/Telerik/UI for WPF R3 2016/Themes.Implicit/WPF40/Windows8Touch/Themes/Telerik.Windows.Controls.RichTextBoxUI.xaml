                </StackPanel>
            </Grid>
            <Border x:Name="checkedDecorator" Opacity="0" BorderBrush="{telerik:Windows8TouchResource ResourceKey=MediumBrush}" BorderThickness="{TemplateBinding BorderThickness}"/>
            <Popup x:Name="DropDownPopup"
                    AllowsTransparency="True"
                    StaysOpen="False"
                    Focusable="False"
                    Placement="Bottom"
                    IsOpen="{Binding Path=IsOpen, RelativeSource={RelativeSource TemplatedParent}}">
