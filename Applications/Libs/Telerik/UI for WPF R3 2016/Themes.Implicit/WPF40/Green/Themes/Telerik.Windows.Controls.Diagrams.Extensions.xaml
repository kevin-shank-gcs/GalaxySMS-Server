                        <Trigger Property="IsEnabled" Value="False">
                            <Setter TargetName="Content" Property="Opacity" Value="{telerik1:GreenResource ResourceKey=DisabledOpacity}"/>
                            <Setter TargetName="Path" Property="Opacity" Value="{telerik1:GreenResource ResourceKey=DisabledOpacity}"/>
                            <Setter TargetName="Path" Property="Fill" Value="{telerik1:GreenResource ResourceKey=MarkerBrush}"/>
                            <Setter TargetName="Ellipse" Property="Stroke" Value="{telerik1:GreenResource ResourceKey=BasicBrush}"/>
                            <Setter TargetName="Ellipse" Property="Fill" Value="{telerik1:GreenResource ResourceKey=AlternativeBrush}"/>
                        </Trigger>
                        <Trigger Property="IsFocused" Value="True">
                            <Setter TargetName="Ellipse" Property="Stroke" Value="{telerik1:GreenResource ResourceKey=AccentLowBrush}"/>
                        </Trigger>
                        <Trigger Property="IsMouseOver" Value="True">
                            <Setter TargetName="Ellipse" Property="Fill" Value="{telerik1:GreenResource ResourceKey=MouseOverBrush}"/>
                            <Setter TargetName="Ellipse" Property="Stroke" Value="{telerik1:GreenResource ResourceKey=MouseOverBrush}"/>
                            <Setter TargetName="Path" Property="Fill" Value="{telerik1:GreenResource ResourceKey=SelectedBrush}"/>
                        </Trigger>
                        <Trigger Property="IsPressed" Value="True">
                            <Setter TargetName="Ellipse" Property="Fill" Value="{telerik1:GreenResource ResourceKey=AccentHighBrush}"/>
                            <Setter TargetName="Ellipse" Property="Stroke" Value="{telerik1:GreenResource ResourceKey=AccentHighBrush}"/>
                            <Setter TargetName="Path" Property="Fill" Value="{telerik1:GreenResource ResourceKey=SelectedBrush}"/>
                        </Trigger>
                        <Trigger Property="IsChecked" Value="True">
                            <Setter TargetName="Ellipse" Property="Fill" Value="{telerik1:GreenResource ResourceKey=AccentHighBrush}"/>
                            <Setter TargetName="Ellipse" Property="Stroke" Value="{telerik1:GreenResource ResourceKey=AccentHighBrush}"/>
                            <Setter TargetName="Path" Property="Fill" Value="{telerik1:GreenResource ResourceKey=SelectedBrush}"/>
                        </Trigger>
                    </ControlTemplate.Triggers>
                </ControlTemplate>
            </Setter.Value>
        </Setter>
    </Style>
    <Style TargetType="extensions:RadDiagramThumbnail" BasedOn="{StaticResource RadDiagramThumbnailStyle}"/>
    <Style TargetType="extensions:RadDiagramNavigationPane" BasedOn="{StaticResource RadDiagramNavigationPaneStyle}"/>
    <Style TargetType="extensions:RadGeometryToggleButton" BasedOn="{StaticResource RadGeometryToggleButtonStyle}"/>
    <Style TargetType="extensions:RadGeometryDropDownButton" BasedOn="{StaticResource RadGeometryDropDownButtonStyle}"/>
    <Style TargetType="extensions:RadGeometryButton" BasedOn="{StaticResource RadGeometryButtonStyle}"/>
    <Style TargetType="extensions:RadGeometryRadioButton" BasedOn="{StaticResource RadGeometryRadioButtonStyle}"/>
    <Style TargetType="extensions:XSmallTickContainer" x:Key="XSmallTickContainerStyle">
        <Setter Property="Template">
            <Setter.Value>
                <ControlTemplate TargetType="extensions:XSmallTickContainer">
                    <Rectangle Fill="{telerik1:GreenResource ResourceKey=StrongBrush}" Width="1" Height="3"/>
                </ControlTemplate>
            </Setter.Value>
        </Setter>
    </Style>
    <Style TargetType="extensions:SmallTickContainer" x:Key="SmallTickContainerStyle">
        <Setter Property="Template">
            <Setter.Value>
                <ControlTemplate TargetType="extensions:SmallTickContainer">
                    <Rectangle Fill="{telerik1:GreenResource ResourceKey=StrongBrush}" Width="1" Height="5"/>
                </ControlTemplate>
            </Setter.Value>
        </Setter>
    </Style>
    <Style TargetType="extensions:MediumTickContainer" x:Key="MediumTickContainerStyle">
        <Setter Property="Template">
            <Setter.Value>
                <ControlTemplate TargetType="extensions:MediumTickContainer">
                    <Rectangle Fill="{telerik1:GreenResource ResourceKey=StrongBrush}" Width="1" Height="8"/>
                </ControlTemplate>
            </Setter.Value>
        </Setter>
    </Style>
    <Style TargetType="extensions:LargeTickContainer" x:Key="LargeTickContainerStyle">
        <Setter Property="Template">
            <Setter.Value>
                <ControlTemplate TargetType="extensions:LargeTickContainer">
                    <Rectangle Fill="{telerik1:GreenResource ResourceKey=StrongBrush}" Width="1" Height="20"/>
                </ControlTemplate>
            </Setter.Value>
        </Setter>
    </Style>
    <Style TargetType="extensions:LabelContainer" x:Key="LabelContainerStyle">
        <Setter Property="Template">
            <Setter.Value>
                <ControlTemplate TargetType="extensions:LabelContainer">
                    <Grid Height="20">
                        <TextBlock x:Name="RootElement" VerticalAlignment="Top" Margin="2 -2 2 0" Text="{Binding Value}" Foreground="{telerik1:GreenResource ResourceKey=MarkerBrush}"/>
                    </Grid>
                </ControlTemplate>
            </Setter.Value>
        </Setter>
    </Style>
    <Style x:Key="MouseIndicatorStyle" TargetType="Path">
        <Setter Property="HorizontalAlignment" Value="Left"/>
        <Setter Property="Stroke" Value="{telerik1:GreenResource ResourceKey=ValidationBrush}"/>
        <Setter Property="Data" Value="M 0.5,0 L 0.5,20"/>
    </Style>
    <Style TargetType="extensions:RadDiagramRuler" x:Key="RadDiagramRulerStyle">
        <Setter Property="FontFamily" Value="{telerik1:GreenResource ResourceKey=FontFamily}"/>
        <Setter Property="FontSize" Value="{telerik1:GreenResource ResourceKey=FontSizeS}"/>
        <Setter Property="Background" Value="{telerik1:GreenResource ResourceKey=MainBrush}"/>
        <Setter Property="BorderBrush" Value="{telerik1:GreenResource ResourceKey=BasicBrush}"/>
        <Setter Property="BorderThickness" Value="1"/>
        <Setter Property="MouseIndicatorStyle" Value="{StaticResource MouseIndicatorStyle}"/>
        <Setter Property="Template">
            <Setter.Value>
                <ControlTemplate TargetType="extensions:RadDiagramRuler">
                    <Grid x:Name="ContentHolder">
                        <Border Background="{TemplateBinding Background}" BorderThickness="{TemplateBinding BorderThickness}" BorderBrush="{TemplateBinding BorderBrush}"/>
                        <extensions:RulerVirtualizingPanel x:Name="RootPanel" Margin="{TemplateBinding BorderThickness}"/>
                        <Path x:Name="MouseIndicator" Style="{TemplateBinding MouseIndicatorStyle}" Visibility="{TemplateBinding MouseIndicatorVisibility}"/>
                    </Grid>
                </ControlTemplate>
            </Setter.Value>
        </Setter>
        <Setter Property="ScaleDefinitions">
            <Setter.Value>
                <extensions:DiagramScaleDefinitionCollection>
                    <extensions:DiagramScaleDefinition MaxZoom="0.99">
                        <extensions:DiagramScaleItemDefinition Interval="10" Type="SmallTick"/>
                        <extensions:DiagramScaleItemDefinition Interval="50" Type="MediumTick"/>
                        <extensions:DiagramScaleItemDefinition Interval="100" Type="LargeTick"/>
                        <extensions:DiagramScaleItemDefinition Interval="100" Type="Label"/>
                    </extensions:DiagramScaleDefinition>
                    <extensions:DiagramScaleDefinition MaxZoom="1.99">
                        <extensions:DiagramScaleItemDefinition Interval="5" Type="XSmallTick"/>
                        <extensions:DiagramScaleItemDefinition Interval="10" Type="SmallTick"/>
                        <extensions:DiagramScaleItemDefinition Interval="50" Type="MediumTick"/>
                        <extensions:DiagramScaleItemDefinition Interval="100" Type="LargeTick"/>
                        <extensions:DiagramScaleItemDefinition Interval="100" Type="Label"/>
                    </extensions:DiagramScaleDefinition>
                    <extensions:DiagramScaleDefinition MaxZoom="3.99">
                        <extensions:DiagramScaleItemDefinition Interval="2" Type="XSmallTick"/>
                        <extensions:DiagramScaleItemDefinition Interval="10" Type="SmallTick"/>
                        <extensions:DiagramScaleItemDefinition Interval="50" Type="MediumTick"/>
                        <extensions:DiagramScaleItemDefinition Interval="100" Type="LargeTick"/>
                        <extensions:DiagramScaleItemDefinition Interval="50" Type="Label"/>
                        <extensions:DiagramScaleItemDefinition Interval="100" Type="Label"/>
                    </extensions:DiagramScaleDefinition>
                    <extensions:DiagramScaleDefinition>
                        <extensions:DiagramScaleItemDefinition Interval="1" Type="XSmallTick"/>
                        <extensions:DiagramScaleItemDefinition Interval="10" Type="SmallTick"/>
                        <extensions:DiagramScaleItemDefinition Interval="50" Type="MediumTick"/>
                        <extensions:DiagramScaleItemDefinition Interval="100" Type="LargeTick"/>
                        <extensions:DiagramScaleItemDefinition Interval="10" Type="Label"/>
                        <extensions:DiagramScaleItemDefinition Interval="50" Type="Label"/>
                        <extensions:DiagramScaleItemDefinition Interval="100" Type="Label"/>
                    </extensions:DiagramScaleDefinition>
                </extensions:DiagramScaleDefinitionCollection>
            </Setter.Value>
        </Setter>
    </Style>
    <Style TargetType="extensions:XSmallTickContainer" BasedOn="{StaticResource XSmallTickContainerStyle}"/>
    <Style TargetType="extensions:SmallTickContainer" BasedOn="{StaticResource SmallTickContainerStyle}"/>
    <Style TargetType="extensions:MediumTickContainer" BasedOn="{StaticResource MediumTickContainerStyle}"/>
    <Style TargetType="extensions:LargeTickContainer" BasedOn="{StaticResource LargeTickContainerStyle}"/>
    <Style TargetType="extensions:LabelContainer" BasedOn="{StaticResource LabelContainerStyle}"/>
    <Style TargetType="extensions:RadDiagramRuler" BasedOn="{StaticResource RadDiagramRulerStyle}"/>
    <extensions:PercentageConverter x:Key="DoublePercentageConverter"/>
    <extensions:ReciprocalDoubleConverter x:Key="ReciprocalDoubleConverter"/>
    <DataTemplate x:Key="PageSizesSelectedItemTemplate">
        <StackPanel Orientation="Horizontal">
            <Path Stretch="Fill" Stroke="{Binding Foreground, ElementName=PageMediaSizeName}" Width="8" Height="11" Data="{StaticResource PageSizeIcon}"/>
            <TextBlock x:Name="PageMediaSizeName" Text="{Binding PageMediaSizeName}" Margin="5 0 0 0"/>
        </StackPanel>
    </DataTemplate>
    <Style x:Key="PrintPreviewComboStyle" TargetType="telerik:RadComboBox" BasedOn="{StaticResource RadComboBoxStyle}">
        <Setter Property="BorderBrush" Value="{telerik1:GreenResource ResourceKey=BasicBrush}"/>
        <Setter Property="Background" Value="{telerik1:GreenResource ResourceKey=MainBrush}"/>
        <Setter Property="BorderThickness" Value="1"/>
        <Setter Property="Margin" Value="0 0 0 10"/>
        <Setter Property="HorizontalContentAlignment" Value="Left"/>
    </Style>
    <Style TargetType="extensions:RadDiagramPrintPreviewItem" x:Key="RadDiagramPrintPreviewItemStyle">
        <Setter Property="BorderBrush" Value="{telerik1:GreenResource ResourceKey=BasicBrush}"/>
        <Setter Property="BorderThickness" Value="1"/>
        <Setter Property="Margin" Value="2"/>
        <Setter Property="Padding" Value="4"/>
        <Setter Property="Template">
            <Setter.Value>
                <ControlTemplate TargetType="extensions:RadDiagramPrintPreviewItem">
                    <Border BorderBrush="{TemplateBinding BorderBrush}" BorderThickness="{TemplateBinding BorderThickness}" Background="{TemplateBinding Background}">
                        <Image
                                Stretch="Uniform"
                                Source="{TemplateBinding PagePreviewImageSource}"
                                Width="{TemplateBinding PagePreviewImageWidth}"
                                Height="{TemplateBinding PagePreviewImageHeight}"
                                Margin="{TemplateBinding Padding}"/>
                    </Border>
                </ControlTemplate>
            </Setter.Value>
        </Setter>
    </Style>
    <Style TargetType="extensions:RadDiagramPrintPreview" x:Key="RadDiagramPrintPreviewStyle">
        <Setter Property="MinWidth" Value="200"/>
        <Setter Property="MinHeight" Value="200"/>
        <Setter Property="Background" Value="{telerik1:GreenResource ResourceKey=AlternativeBrush}"/>
        <Setter Property="BorderBrush" Value="{telerik1:GreenResource ResourceKey=BasicBrush}"/>
        <Setter Property="BorderThickness" Value="1"/>
        <Setter Property="ItemStyle" Value="{StaticResource RadDiagramPrintPreviewItemStyle}"/>
        <Setter Property="Template">
            <Setter.Value>
                <ControlTemplate>
                    <Grid Background="{TemplateBinding Background}">
                        <Grid.ColumnDefinitions>
                            <ColumnDefinition Width="*"/>
                            <ColumnDefinition MaxWidth="280"/>
                        </Grid.ColumnDefinitions>
                        <Grid>
                            <Grid.RowDefinitions>
                                <RowDefinition Height="*"/>
                                <RowDefinition Height="Auto"/>
                            </Grid.RowDefinitions>
                            <ScrollViewer VerticalScrollBarVisibility="Auto" HorizontalScrollBarVisibility="Auto" BorderThickness="0">
                                <telerik:RadUniformGrid x:Name="PART_PagesPanel" VerticalAlignment="Top" UseLayoutRounding="True" HorizontalAlignment="Left" Margin="15"/>
                            </ScrollViewer>
                            <StackPanel Grid.Row="1" HorizontalAlignment="Right" VerticalAlignment="Center" Orientation="Horizontal" Margin="0 4 4 4">
                                <TextBlock
                                        Width="50"
                                        VerticalAlignment="Center"
                                        Margin="0 0 4 0"
                                        Background="{telerik1:GreenResource ResourceKey=AccentHighBrush}"
                                        Padding="4 2"
                                        Text="{Binding Value, ElementName=ZoomSlider, Converter={StaticResource DoublePercentageConverter}}"
                                        FontSize="{telerik1:GreenResource ResourceKey=FontSize}"
                                        FontFamily="{telerik1:GreenResource ResourceKey=FontFamily}"
                                        Foreground="{telerik1:GreenResource ResourceKey=MarkerBrush}"/>
                                <telerik:RadSlider x:Name="ZoomSlider"
                                        TickFrequency="1"
                                        MinWidth="124"
                                        VerticalAlignment="Center"
                                        Minimum="0.1"
                                        Maximum="5"
                                        SmallChange="0.01"
                                        LargeChange="0.1"
                                        HandlesVisibility="Visible"
                                        Style="{StaticResource ThumbnailSliderStyle}"
                                        Value="{Binding RelativeSource={RelativeSource TemplatedParent}, Path=ScaleDownFactor, Mode=TwoWay, Converter={StaticResource ReciprocalDoubleConverter}}"/>
                            </StackPanel>
                            <Rectangle Grid.RowSpan="2" Width="1" Fill="{TemplateBinding BorderBrush}" HorizontalAlignment="Right" VerticalAlignment="Stretch"/>
                            <Rectangle Height="1" Fill="{TemplateBinding BorderBrush}" HorizontalAlignment="Stretch" VerticalAlignment="Bottom"/>
                        </Grid>
                        <Grid Grid.Column="1">
                            <Grid.RowDefinitions>
                                <RowDefinition Height="Auto"/>
                                <RowDefinition Height="Auto"/>
                                <RowDefinition Height="Auto"/>
                                <RowDefinition Height="Auto"/>
                                <RowDefinition Height="Auto"/>
                                <RowDefinition Height="*"/>
                                <RowDefinition Height="Auto"/>
                            </Grid.RowDefinitions>
                            <TextBlock
                                    Margin="15 15 15 0"
                                    HorizontalAlignment="Left"
                                    FontFamily="{telerik1:GreenResource ResourceKey=FontFamily}"
                                    FontSize="{telerik1:GreenResource ResourceKey=FontSize}"
                                    Foreground="{telerik1:GreenResource ResourceKey=MarkerBrush}"
                                    Text="{telerik:LocalizableResource Key=PrintPreview_Print}"/>
                            <telerik:RadComboBox
                                    Grid.Row="1"
                                    Margin="15 10 15 0"
                                    ItemsSource="{Binding PrintQueues, Mode=OneWay}"
                                    SelectedItem="{Binding SelectedPrintQueue, Mode=TwoWay}"
                                    DisplayMemberPath="Name"
                                    Style="{StaticResource PrintPreviewComboStyle}"/>
                            <TextBlock
                                    Grid.Row="2"
                                    Margin="15 15 15 0"
                                    HorizontalAlignment="Left"
                                    Foreground="{telerik1:GreenResource ResourceKey=MarkerBrush}"
                                    Text="{telerik:LocalizableResource Key=PrintPreview_Settings}"
                                    FontFamily="{telerik1:GreenResource ResourceKey=FontFamily}"
                                    FontSize="{telerik1:GreenResource ResourceKey=FontSize}"/>
                            <telerik:RadComboBox
                                    Grid.Row="3"
                                    Margin="15 10 15 0"
                                    ItemsSource="{Binding PageOrientations, Mode=OneWay}"
                                    SelectedItem="{Binding SelectedPageOrientation, Mode=TwoWay}"
                                    DisplayMemberPath=""
                                    Style="{StaticResource PrintPreviewComboStyle}"/>
                            <telerik:RadComboBox
                                    Grid.Row="4"
                                    Margin="15 15 15 0"
                                    ItemsSource="{Binding PageSizes, Mode=OneWay}"
                                    SelectedItem="{Binding SelectedPageSize, Mode=TwoWay}"
                                    DisplayMemberPath="PageMediaSizeName"
                                    SelectionBoxTemplate="{StaticResource PageSizesSelectedItemTemplate}"
                                    Style="{StaticResource PrintPreviewComboStyle}"/>
                            <telerik:RadButton x:Name="PART_PrintButton"
                                    Grid.Row="6"
                                    HorizontalContentAlignment="Center"
                                    VerticalAlignment="Bottom"
                                    Margin="15 14"
                                    Content="{telerik:LocalizableResource Key=PrintPreview_Print}"/>
                        </Grid>
                    </Grid>
                </ControlTemplate>
            </Setter.Value>
        </Setter>
    </Style>
    <Style TargetType="extensions:RadDiagramPrintPreview" BasedOn="{StaticResource RadDiagramPrintPreviewStyle}"/>
    <Style TargetType="extensions:RadDiagramPrintPreviewItem" BasedOn="{StaticResource RadDiagramPrintPreviewItemStyle}"/>
</ResourceDictionary>
