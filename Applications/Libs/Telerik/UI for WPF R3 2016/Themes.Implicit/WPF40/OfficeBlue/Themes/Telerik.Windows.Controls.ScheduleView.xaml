                        <StackPanel x:Name="DialogButtons" Grid.Row="1" Grid.ColumnSpan="2" Orientation="Horizontal" HorizontalAlignment="Right" FocusManager.FocusedElement="{Binding ElementName=OK}">
                            <telerik:RadButton x:Name="OK"
