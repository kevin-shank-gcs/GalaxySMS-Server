    </DataTemplate>
    <telerikScheduleView:AppointmentItemContentTemplateSelector x:Key="AppointmentItemTemplateSelector" DefaultTemplate="{StaticResource AppointmentItemTemplate}"/>
    <Style x:Key="AppointmentItemBaseStyle" TargetType="telerikScheduleView:AppointmentItem">
