    <ControlTemplate x:Key="ColumnHeaderContainerTemplate" TargetType="gantt:ColumnHeaderContainer">
        <Border BorderBrush="{TemplateBinding BorderBrush}" Background="{TemplateBinding Background}" BorderThickness="{TemplateBinding BorderThickness}">
            <Grid>
                <ContentPresenter
                        HorizontalAlignment="{TemplateBinding HorizontalContentAlignment}"
                        VerticalAlignment="{TemplateBinding VerticalContentAlignment}"
                        Content="{Binding SourceItem}"
                        Margin="{TemplateBinding Padding}"
                        ContentTemplate="{TemplateBinding ContentTemplate}"/>
                <telerik:ResizeControl
                        VerticalAlignment="Stretch"
                        HorizontalAlignment="Right"
                        Style="{StaticResource GanttPresenterHeaderSplitterStyle}"
                        ShowsPreview="False"
                        Grid.Column="1"
                        IsEnabled="{Binding SourceItem.IsResizable}"/>
            </Grid>
        </Border>
    </ControlTemplate>
    <Style x:Key="ColumnHeaderContainerStyle" TargetType="gantt:ColumnHeaderContainer">
        <Setter Property="Padding" Value="4 0 4 4"/>
        <Setter Property="MinHeight" Value="46"/>
        <Setter Property="VerticalContentAlignment" Value="Bottom"/>
        <Setter Property="SubitemsGap" Value="0"/>
        <Setter Property="VerticalAlignment" Value="Stretch"/>
        <Setter Property="FontFamily" Value="{telerik:VisualStudio2013Resource ResourceKey=FontFamily}"/>
        <Setter Property="FontSize" Value="{telerik:VisualStudio2013Resource ResourceKey=FontSize}"/>
