        <Setter Property="telerik:TouchManager.DragStartTrigger" Value="TapHoldAndMove"/>
        <Setter Property="HorizontalContentAlignment" Value="Left"/>
        <Setter Property="VerticalContentAlignment" Value="Top"/>
        <Setter Property="Background" Value="{StaticResource MilestoneFill_Normal}"/>
        <Setter Property="BorderBrush" Value="{StaticResource MilestoneStroke_Normal}"/>
        <Setter Property="telerik:ZIndexManager.ZIndex" Value="40"/>
        <Setter Property="Margin" Value="-9 0 -9 0"/>
        <Setter Property="ToolTipTemplate" Value="{StaticResource MilestoneToolTipTemplate}"/>
        <Setter Property="Template" Value="{StaticResource MilestoneContainerTemplate}"/>
    </Style>
    <DataTemplate x:Key="SummaryToolTipTemplate">
        <StackPanel Orientation="Horizontal">
            <TextBlock Text="{Binding Start, Converter={StaticResource DateTimeToCurrentUICultureStringConverter}}" TextWrapping="NoWrap" TextTrimming="None"/>
            <TextBlock Text="-"/>
            <TextBlock Text="{Binding End, Converter={StaticResource DateTimeToCurrentUICultureStringConverter}}" TextWrapping="NoWrap" TextTrimming="None"/>
        </StackPanel>
    </DataTemplate>
    <ControlTemplate x:Key="SummaryContainerTemplate" TargetType="gantt:SummaryContainer">
        <Grid>
            <VisualStateManager.VisualStateGroups>
                <VisualStateGroup x:Name="CommonStates">
                    <VisualState x:Name="Normal"/>
                    <VisualState x:Name="MouseOver">
                        <Storyboard>
                            <ObjectAnimationUsingKeyFrames Duration="0:0:0" Storyboard.TargetName="LeftMarker" Storyboard.TargetProperty="Fill">
                                <DiscreteObjectKeyFrame KeyTime="0:0:0" Value="{StaticResource SummaryBackground_MouseOver}"/>
                            </ObjectAnimationUsingKeyFrames>
                            <ObjectAnimationUsingKeyFrames Duration="0:0:0" Storyboard.TargetName="BackgroundBorder" Storyboard.TargetProperty="Background">
                                <DiscreteObjectKeyFrame KeyTime="0:0:0" Value="{StaticResource SummaryBackground_MouseOver}"/>
                            </ObjectAnimationUsingKeyFrames>
                            <ObjectAnimationUsingKeyFrames Duration="0:0:0" Storyboard.TargetName="RightMarker" Storyboard.TargetProperty="Fill">
                                <DiscreteObjectKeyFrame KeyTime="0:0:0" Value="{StaticResource SummaryBackground_MouseOver}"/>
                            </ObjectAnimationUsingKeyFrames>
                        </Storyboard>
                    </VisualState>
                    <VisualState x:Name="MouseOverHighlighted">
                        <Storyboard>
                            <ObjectAnimationUsingKeyFrames Duration="0:0:0" Storyboard.TargetName="LeftMarker" Storyboard.TargetProperty="Fill">
                                <DiscreteObjectKeyFrame KeyTime="0:0:0" Value="{StaticResource SummaryBackground_MouseOver}"/>
                            </ObjectAnimationUsingKeyFrames>
                            <ObjectAnimationUsingKeyFrames Duration="0:0:0" Storyboard.TargetName="BackgroundBorder" Storyboard.TargetProperty="Background">
                                <DiscreteObjectKeyFrame KeyTime="0:0:0" Value="{StaticResource SummaryBackground_MouseOver}"/>
                            </ObjectAnimationUsingKeyFrames>
                            <ObjectAnimationUsingKeyFrames Duration="0:0:0" Storyboard.TargetName="RightMarker" Storyboard.TargetProperty="Fill">
                                <DiscreteObjectKeyFrame KeyTime="0:0:0" Value="{StaticResource SummaryBackground_MouseOver}"/>
                            </ObjectAnimationUsingKeyFrames>
                        </Storyboard>
                    </VisualState>
                    <VisualState x:Name="Disabled"/>
                </VisualStateGroup>
                <VisualStateGroup x:Name="SelectionStates">
                    <VisualState x:Name="Unselected"/>
                    <VisualState x:Name="Selected">
                        <Storyboard>
                            <DoubleAnimation Duration="0" To="1" Storyboard.TargetProperty="Opacity" Storyboard.TargetName="LeftMarkerSelected"/>
                            <DoubleAnimation Duration="0" To="1" Storyboard.TargetProperty="Opacity" Storyboard.TargetName="BackgroundBorderSelected"/>
                            <DoubleAnimation Duration="0" To="1" Storyboard.TargetProperty="Opacity" Storyboard.TargetName="RightMarkerSelected"/>
                        </Storyboard>
                    </VisualState>
                </VisualStateGroup>
                <VisualStateGroup x:Name="FocusStates">
                    <VisualState x:Name="Focused"/>
                    <VisualState x:Name="Unfocused"/>
                </VisualStateGroup>
                <VisualStateGroup x:Name="HighlightStates">
                    <VisualState x:Name="NotHighlighted"/>
                    <VisualState x:Name="Highlighted">
                        <Storyboard>
                            <DoubleAnimation Duration="0" To="1" Storyboard.TargetProperty="Opacity" Storyboard.TargetName="LeftMarkerHighlighted"/>
                            <DoubleAnimation Duration="0" To="1" Storyboard.TargetProperty="Opacity" Storyboard.TargetName="BackgroundBorderHighlighted"/>
                            <DoubleAnimation Duration="0" To="1" Storyboard.TargetProperty="Opacity" Storyboard.TargetName="RightMarkerHighlighted"/>
                        </Storyboard>
                    </VisualState>
                </VisualStateGroup>
            </VisualStateManager.VisualStateGroups>
            <telerik:ToolTipManager.ToolTip>
                <ToolTip ContentTemplate="{TemplateBinding ToolTipTemplate}" Content="{Binding OriginalEvent}"/>
            </telerik:ToolTipManager.ToolTip>
            <Grid.ColumnDefinitions>
                <ColumnDefinition Width="Auto"/>
                <ColumnDefinition/>
                <ColumnDefinition Width="Auto"/>
            </Grid.ColumnDefinitions>
            <Border Background="Transparent" IsHitTestVisible="True" Grid.ColumnSpan="3" Height="12" Margin="0" VerticalAlignment="Top"/>
            <Path x:Name="LeftMarker" Data="M0,0 L10,0 L0,9 z" Stretch="Fill" Fill="{TemplateBinding Background}" Width="8" Height="8" Margin="0 0 0 -18" HorizontalAlignment="Left"/>
            <Path x:Name="LeftMarkerSelected"
                    Data="M0,0 L10,0 L0,9 z"
                    Stretch="Fill"
                    Fill="{StaticResource SummaryBackground_Selected}"
                    Width="8"
                    Height="8"
                    Margin="0 0 0 -18"
                    HorizontalAlignment="Left"
                    Opacity="0"/>
            <Path x:Name="LeftMarkerHighlighted"
                    Data="M0,0 L10,0 L0,9 z"
                    Stretch="Fill"
                    Fill="{telerik:VisualStudio2013Resource ResourceKey=ValidationBrush}"
                    Width="8"
                    Height="8"
                    Margin="0 0 0 -18"
                    HorizontalAlignment="Left"
                    Opacity="0"/>
            <Path x:Name="LeftMarkerMouseOver"
                    Data="M0,0 L10,0 L0,9 z"
                    Stretch="Fill"
                    Fill="{StaticResource SummaryBackground_MouseOver}"
                    Width="8"
                    Height="8"
                    Margin="0 0 0 -18"
                    HorizontalAlignment="Left"
                    Opacity="0"/>
            <Border x:Name="BackgroundBorder" Grid.ColumnSpan="3" Height="10" Background="{TemplateBinding Background}"/>
            <Border x:Name="BackgroundBorderSelected" Grid.ColumnSpan="3" Height="10" Background="{StaticResource SummaryBackground_Selected}" Opacity="0"/>
            <Border x:Name="BackgroundBorderHighlighted" Grid.ColumnSpan="3" Height="10" Background="{telerik:VisualStudio2013Resource ResourceKey=ValidationBrush}" Opacity="0"/>
            <Border x:Name="BackgroundBorderMouseOver" Grid.ColumnSpan="3" Height="10" Background="{StaticResource SummaryBackground_MouseOver}" Opacity="0"/>
            <Path x:Name="RightMarker"
                    Data="M0,0 L8,0 L8,8 z"
                    Grid.Column="2"
                    Stretch="Fill"
                    Fill="{TemplateBinding Background}"
                    Width="8"
                    Height="8"
                    Margin="0 0 0 -18"
                    HorizontalAlignment="Right"/>
            <Path x:Name="RightMarkerSelected"
                    Data="M0,0 L8,0 L8,8 z"
                    Grid.Column="2"
                    Stretch="Fill"
                    Fill="{StaticResource SummaryBackground_Selected}"
                    Width="8"
                    Height="8"
                    Margin="0 0 0 -18"
                    HorizontalAlignment="Right"
                    Opacity="0"/>
            <Path x:Name="RightMarkerHighlighted"
                    Data="M0,0 L8,0 L8,8 z"
                    Grid.Column="2"
                    Stretch="Fill"
                    Fill="{telerik:VisualStudio2013Resource ResourceKey=ValidationBrush}"
                    Width="8"
                    Height="8"
                    Margin="0 0 0 -18"
                    HorizontalAlignment="Right"
                    Opacity="0"/>
            <Path x:Name="RightMarkerMouseOver"
                    Data="M0,0 L8,0 L8,8 z"
                    Grid.Column="2"
                    Stretch="Fill"
                    Fill="{StaticResource SummaryBackground_MouseOver}"
                    Width="8"
                    Height="8"
                    Margin="0 0 0 -18"
                    HorizontalAlignment="Right"
                    Opacity="0"/>
        </Grid>
    </ControlTemplate>
    <Style x:Key="SummaryContainerStyle" TargetType="gantt:SummaryContainer">
