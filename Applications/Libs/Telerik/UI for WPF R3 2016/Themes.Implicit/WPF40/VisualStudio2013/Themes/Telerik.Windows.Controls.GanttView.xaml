    <Style x:Key="SimpleTreeCellContainerStyle" TargetType="gantt:SimpleCellContainer" BasedOn="{StaticResource SimpleCellContainerStyle}">
        <Setter Property="Padding" Value="0 0 5 0"/>
        <Setter Property="Margin" Value="0 -1 0 0"/>
        <Setter Property="Foreground" Value="{telerik:VisualStudio2013Resource ResourceKey=MarkerBrush}"/>
        <Setter Property="Template">
            <Setter.Value>
                <ControlTemplate TargetType="gantt:SimpleCellContainer">
                    <Border Background="{TemplateBinding Background}" BorderThickness="0" Margin="{TemplateBinding Padding}">
                        <Grid x:Name="TemplateRootVisual"
                                HorizontalAlignment="{TemplateBinding HorizontalContentAlignment}"
                                Background="{TemplateBinding Background}"
                                VerticalAlignment="{TemplateBinding VerticalAlignment}">
                            <Grid.ColumnDefinitions>
                                <ColumnDefinition Width="20"/>
                                <ColumnDefinition Width="*"/>
                            </Grid.ColumnDefinitions>
                            <Border x:Name="PART_ExpandCollapseHitArea" MinHeight="16" Background="Transparent" Grid.Column="0" HorizontalAlignment="Stretch">
                                <Path x:Name="PART_ExpandCollapseVisual"
                                        RenderTransformOrigin="0.5,0.5"
                                        Stretch="Fill"
                                        Width="6"
                                        VerticalAlignment="Stretch"
                                        HorizontalAlignment="Stretch"
                                        Data="M0,0L0,9 5,4.5z"
                                        IsHitTestVisible="False"/>
                            </Border>
                            <TextBlock Grid.Column="1" x:Name="ValueTextBlock" HorizontalAlignment="Stretch" VerticalAlignment="{TemplateBinding VerticalContentAlignment}"/>
                        </Grid>
                    </Border>
                </ControlTemplate>
            </Setter.Value>
        </Setter>
        <Setter Property="gantt:TreeCellContainer.ExpandCollapseVisualCollapsedStyle">
            <Setter.Value>
                <Style TargetType="Path">
                    <Setter Property="Stroke" Value="{telerik:VisualStudio2013Resource ResourceKey=StrongBrush}"/>
                    <Setter Property="StrokeThickness" Value="1"/>
                    <Setter Property="Height" Value="10"/>
                </Style>
            </Setter.Value>
        </Setter>
        <Setter Property="gantt:TreeCellContainer.ExpandCollapseVisualExpandedStyle">
            <Setter.Value>
                <Style TargetType="Path">
                    <Setter Property="Fill" Value="{telerik:VisualStudio2013Resource ResourceKey=MarkerBrush}"/>
                    <Setter Property="Margin" Value="0 3 0 0"/>
                    <Setter Property="Height" Value="10"/>
                    <Setter Property="RenderTransform">
                        <Setter.Value>
                            <RotateTransform Angle="44" CenterX="0.5" CenterY="0.5"/>
                        </Setter.Value>
                    </Setter>
                </Style>
            </Setter.Value>
        </Setter>
    </Style>
