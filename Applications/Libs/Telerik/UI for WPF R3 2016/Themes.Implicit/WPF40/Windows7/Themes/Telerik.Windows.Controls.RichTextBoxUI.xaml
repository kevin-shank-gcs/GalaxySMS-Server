                        ContentTemplateSelector="{TemplateBinding HeaderTemplateSelector}"/>
            </Grid>
            <Grid x:Name="ContentPanel" Width="0" Height="0">
                <Border
                        Name="OuterBorder"
                        BorderThickness="1"
                        Background="{StaticResource RibbonTabBackgroundBrush}"
                        BorderBrush="{TemplateBinding Background}"
