                    Fill="{StaticResource ControlElement_Normal}"
                    x:Name="Path"
                    Stretch="Fill"
                    HorizontalAlignment="Center"
                    VerticalAlignment="Center"
                    RenderTransformOrigin="0.5,0.5"
                    Data="M4,0 L4,6 0,3 z"
