    <SolidColorBrush x:Key="ColorPicker_InnerBorder" Color="#FFF5F5F5"/>
    <SolidColorBrush x:Key="ColorPicker_Foreground" Color="#FF1E395B"/>
    <SolidColorBrush x:Key="ColorPaletteViewItem_Border" Color="#FFBFBFBF"/>
    <SolidColorBrush x:Key="ColorPaletteViewItem_OuterBorder_MouseOver" Color="#FFB8D6FB"/>
    <SolidColorBrush x:Key="ColorPaletteViewItem_InnerBorder_MouseOver" Color="#CCFFFFFF"/>
    <SolidColorBrush x:Key="ColorPaletteViewItem_OuterBorder_Selected" Color="#FF7DA2CE"/>
    <SolidColorBrush x:Key="ColorPaletteViewItem_InnerBorder_Selected" Color="#FF91C3FF"/>
