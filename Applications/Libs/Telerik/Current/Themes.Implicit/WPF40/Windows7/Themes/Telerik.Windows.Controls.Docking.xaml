            </Border>
        </Border>
    </ControlTemplate>
    <ControlTemplate x:Key="Compass_IndicatorCenterTemplate" TargetType="ContentControl">
        <Grid>
            <Path
                    Fill="{TemplateBinding Background}"
                    Stroke="{TemplateBinding BorderBrush}"
                    Stretch="Fill"
                    StrokeMiterLimit="0"
                    Data="M0.5,15.5 C0.5,7.2 7.2,0.5 15.5,0.5 L83.5,0.5 C91.8,0.5 98.5,7.2 98.5,15.5 L98.5,78.5 C98.5,86.7 91.8,93.5 83.5,93.5 L15.5,93.5 C7.2,93.5 0.5,86.7 0.5,78.5 z"/>
            <Path
                    Stroke="{StaticResource Compass_Indicator_InnerBorderBrush}"
                    Margin="1"
                    Stretch="Fill"
                    StrokeMiterLimit="0"
                    Data="M0.5,15.5 C0.5,7.2 7.2,0.5 15.5,0.5 L83.5,0.5 C91.8,0.5 98.5,7.2 98.5,15.5 L98.5,78.5 C98.5,86.7 91.8,93.5 83.5,93.5 L15.5,93.5 C7.2,93.5 0.5,86.7 0.5,78.5 z"/>
