    <Style x:Key="OnTimeIndicatorContainerStyle" TargetType="gantt:OnTimeIndicatorContainer">
        <Setter Property="Foreground" Value="{telerik:GreenResource ResourceKey=FixedBrush}"/>
        <Setter Property="Margin" Value="13 -5 -30 0"/>
        <Setter Property="Padding" Value="5 0"/>
        <Setter Property="MinHeight" Value="17"/>
        <Setter Property="VerticalAlignment" Value="Center"/>
        <Setter Property="HorizontalAlignment" Value="Center"/>
        <Setter Property="telerik:ZIndexManager.ZIndex" Value="35"/>
        <Setter Property="IsHitTestVisible" Value="False"/>
        <Setter Property="Template">
            <Setter.Value>
                <ControlTemplate TargetType="gantt:OnTimeIndicatorContainer">
                    <Grid>
                        <Ellipse
                                Visibility="{Binding OriginalEvent.IsExpired, Converter={StaticResource BooleanToVisibilityConverter}}"
                                Fill="{telerik:GreenResource ResourceKey=ValidationBrush}"
                                Height="12"
                                VerticalAlignment="Center"
                                Width="12"/>
                        <Ellipse
                                Visibility="{Binding OriginalEvent.IsExpired, Converter={StaticResource InvertedBooleanToVisibilityConverter}}"
                                Fill="{telerik:GreenResource ResourceKey=AccentHighBrush}"
                                Height="12"
                                VerticalAlignment="Center"
                                Width="12"/>
                        <Path Stretch="Fill" Fill="{TemplateBinding Foreground}" Data="{StaticResource PointerDown14x21}" Width="6" Height="8" VerticalAlignment="Center"/>
                    </Grid>
                </ControlTemplate>
            </Setter.Value>
        </Setter>
    </Style>
