    <Geometry x:Key="ArrowRight4x5">M0,0 L4,2.5 L0,5 z</Geometry>
    <Geometry x:Key="PointerDown14x21">M5,0 L9,0 9,13 14,7 14,13 7,21 0,13 0,7 5,13z</Geometry>
    <Geometry x:Key="BeforeDragIcon">M6,3 L11,3 L11,4 L6,4 z M4,5 L13,5 L13,6 L4,6 z M6,7 L11,7 L11,8 L6,8 z M4,3.5 L0,7 L0,0 z</Geometry>
    <Geometry x:Key="AfterDragIcon">M2,4 L7,4 L7,5 L2,5 z M0,2 L9,2 L9,3 L0,3 z M13,1 L13,8 L9,4.5 z M2,0 L7,0 L7,1 L2,1 z</Geometry>
