    <Geometry x:Key="NoneEffect">M5,0 C3.7,0 2.4,0.4 1.4,1.4 C-0.4,3.4 -0.4,6.5 1.4,8.5 C3.4,10.4 6.5,10.4 8.5,8.5 C10.4,6.5 10.4,3.4 8.5,1.4 C7.5,0.4 6.2,0 5,0 C5,0 5,0 5,0 z M5,1.9 C5.7,1.9 6.5,2.2 7.1,2.8 C8,3.7 8.2,5.1 7.7,6.2 C7.7,6.2 3.7,2.2 3.7,2.2 C4.1,2 4.5,1.9 5,1.9 C5,1.9 5,1.9 5,1.9 z M2.3,3.5 C2.3,3.5 6.4,7.6 6.4,7.6 C5.3,8.3 3.8,8.1 2.8,7.1 C1.8,6.1 1.6,4.6 2.3,3.5 C2.3,3.5 2.3,3.5 2.3,3.5 z M2.3,3.5 L2.3,3.5 L2.3,3.5 z</Geometry>
    <Geometry x:Key="RelationIcon">M3.4,5 C3.7,5 4,5 4.5,5.2 C5,5.3 4.6,6.7 4.5,6.9 C3.9,6.6 3.2,6.7 2.8,6.8 L2.7,6.9 C2.1,7.3 1.7,8 1.7,8.9 L1.7,11.1 C1.7,12.3 2.5,13.3 3.4,13.3 C4.4,13.3 5.1,12.4 5.2,10.3 C5.2,10.3 7,9.4 7,9.4 L6.9,11.7 C6.9,13.5 5.4,15 3.4,15 C1.5,15 0,13.5 0,11.7 L0,8.3 C0,6.5 1.5,5 3.4,5 z M3.7,0 C5.6,0 7.1,1.4 7.1,3.3 L7.1,6.6 C7.1,8.5 5.6,9.9 3.7,9.9 C3.4,9.9 3.2,10 2.7,9.8 C2.2,9.7 2.5,8.2 2.6,8.1 C3.2,8.3 3.9,8.2 4.3,8.1 L4.4,8.1 C5,7.7 5.4,6.9 5.4,6.1 L5.4,3.8 C5.4,2.6 4.6,1.5 3.7,1.6 C2.4,1.7 1.9,2.9 2,4.4 C2,4.4 0.1,5.9 0.1,5.9 L0.2,3.3 C0.2,1.4 1.8,0 3.7,0 z</Geometry>
    <telerik:BooleanToVisibilityConverter x:Key="BooleanToVisibilityConverter"/>
    <telerik:InvertedBooleanToVisibilityConverter x:Key="InvertedBooleanToVisibilityConverter"/>
    <telerik:UppercaseConverter x:Key="UpperCaseStringConverter"/>
    <gantt1:DateTimeToCurrentUICultureStringConverter x:Key="DateTimeToCurrentUICultureStringConverter"/>
    <telerik:DragDropBooleanToVisibilityConverter x:Key="DragDropBooleanToVisibilityConverter"/>
    <SolidColorBrush x:Key="SemiTransparentBrush" Color="#01FFFFFF"/>
    <DataTemplate x:Key="GanttDragResizeVisualCueTemplate">
        <Grid Margin="{Binding Margin, RelativeSource={RelativeSource Mode=FindAncestor, AncestorType=gantt:GanttDragResizeVisualCue}}">
            <Grid.ColumnDefinitions>
                <ColumnDefinition Width="Auto"/>
                <ColumnDefinition Width="*" MinWidth="140"/>
            </Grid.ColumnDefinitions>
            <Grid.RowDefinitions>
                <RowDefinition/>
                <RowDefinition/>
                <RowDefinition/>
            </Grid.RowDefinitions>
            <TextBlock
                    Text="{Binding TargetItem.Title}"
                    Grid.ColumnSpan="2"
                    Foreground="{telerik:GreenResource ResourceKey=MarkerBrush}"
                    FontSize="{telerik:GreenResource ResourceKey=FontSizeL}"/>
            <TextBlock telerik:LocalizationManager.ResourceKey="Start" Grid.Row="1" Margin="0 0 4 0" Foreground="{telerik:GreenResource ResourceKey=MarkerBrush}"/>
            <TextBlock
                    Text="{Binding DestinationSlot.Start, Converter={StaticResource DateTimeToCurrentUICultureStringConverter}}"
                    Grid.Row="1"
                    Grid.Column="1"
                    Foreground="{telerik:GreenResource ResourceKey=AccentLowBrush}"/>
            <TextBlock telerik:LocalizationManager.ResourceKey="End" Grid.Row="2" Margin="0 0 4 0" Foreground="{telerik:GreenResource ResourceKey=MarkerBrush}"/>
            <TextBlock
                    Text="{Binding DestinationSlot.End, Converter={StaticResource DateTimeToCurrentUICultureStringConverter}}"
                    Grid.Row="2"
                    Grid.Column="1"
                    Foreground="{telerik:GreenResource ResourceKey=AccentLowBrush}"/>
        </Grid>
    </DataTemplate>
    <DataTemplate x:Key="GanttCreateDependencyTemplate">
        <Grid Margin="{Binding Margin, RelativeSource={RelativeSource Mode=FindAncestor, AncestorType=gantt:GanttDragResizeVisualCue}}">
            <Grid.RowDefinitions>
                <RowDefinition/>
                <RowDefinition/>
            </Grid.RowDefinitions>
            <StackPanel Orientation="Horizontal">
                <TextBlock telerik:LocalizationManager.ResourceKey="DrawRelationFrom" Foreground="{telerik:GreenResource ResourceKey=MarkerBrush}"/>
                <TextBlock Text="{Binding DestinationSlot.Source.Title}" Foreground="{telerik:GreenResource ResourceKey=AccentLowBrush}"/>
            </StackPanel>
            <StackPanel Grid.Row="1" Orientation="Horizontal">
                <TextBlock telerik:LocalizationManager.ResourceKey="DrawRelationTo" Foreground="{telerik:GreenResource ResourceKey=MarkerBrush}"/>
                <TextBlock Text="{Binding DestinationSlot.Target.Title}" Foreground="{telerik:GreenResource ResourceKey=AccentLowBrush}"/>
            </StackPanel>
        </Grid>
    </DataTemplate>
    <DataTemplate x:Key="GanttDragReorderTemplate">
        <Grid Margin="{Binding Margin, RelativeSource={RelativeSource Mode=FindAncestor, AncestorType=gantt:GanttDragResizeVisualCue}}">
            <Grid.RowDefinitions>
                <RowDefinition/>
                <RowDefinition/>
            </Grid.RowDefinitions>
            <StackPanel Orientation="Horizontal" HorizontalAlignment="Left">
                <TextBlock telerik:LocalizationManager.ResourceKey="DraggingItem" Foreground="{telerik:GreenResource ResourceKey=MarkerBrush}"/>
                <TextBlock Text="{Binding TargetItem.Title}" Foreground="{telerik:GreenResource ResourceKey=AccentLowBrush}"/>
            </StackPanel>
            <StackPanel Grid.Row="1" Orientation="Horizontal" HorizontalAlignment="Left">
                <Path
                        Data="{StaticResource BeforeDragIcon}"
                        Height="8"
                        Stretch="Fill"
                        VerticalAlignment="Center"
                        Margin="0 0 4 0"
                        HorizontalAlignment="Left"
                        Visibility="{Binding DestinationSlot.DropPosition, Converter={StaticResource DragDropBooleanToVisibilityConverter}, ConverterParameter=Before}"
                        Fill="{telerik:GreenResource ResourceKey=AccentLowBrush}"/>
                <Path
                        Data="{StaticResource AfterDragIcon}"
                        HorizontalAlignment="Left"
                        Height="8"
                        Stretch="Fill"
                        VerticalAlignment="Center"
                        Margin="0 0 4 0"
                        Visibility="{Binding DestinationSlot.DropPosition, Converter={StaticResource DragDropBooleanToVisibilityConverter}, ConverterParameter=After}"
                        Fill="{telerik:GreenResource ResourceKey=AccentLowBrush}"/>
                <Path
                        Data="{StaticResource InsertInsideDragIcon}"
                        Margin="0 0 4 0"
                        Height="8"
                        Stretch="Fill"
                        VerticalAlignment="Center"
                        Width="8"
                        HorizontalAlignment="Left"
                        Visibility="{Binding DestinationSlot.DropPosition, Converter={StaticResource DragDropBooleanToVisibilityConverter}, ConverterParameter=Inside}"
                        Fill="{telerik:GreenResource ResourceKey=AccentLowBrush}"/>
                <TextBlock Text="{Binding DestinationSlot.DropPosition}" Foreground="{telerik:GreenResource ResourceKey=MarkerBrush}"/>
                <TextBlock Text=":" Margin="0 0 4 0" Foreground="{telerik:GreenResource ResourceKey=MarkerBrush}"/>
                <TextBlock Text="{Binding DestinationSlot.GroupKey.Title}" Foreground="{telerik:GreenResource ResourceKey=AccentLowBrush}"/>
            </StackPanel>
        </Grid>
    </DataTemplate>
    <DataTemplate x:Key="DragRelationTooltipTemplate">
        <StackPanel Orientation="Horizontal">
            <TextBlock telerik:LocalizationManager.ResourceKey="RelationToolTip" TextWrapping="NoWrap" TextTrimming="None"/>
        </StackPanel>
    </DataTemplate>
    <ControlTemplate x:Key="GanttPresenterSplitterTemplate" TargetType="telerik:ResizeControl">
        <Grid x:Name="Root" IsHitTestVisible="{TemplateBinding IsEnabled}">
            <VisualStateManager.VisualStateGroups>
                <VisualStateGroup x:Name="CommonStates">
                    <VisualState x:Name="Normal"/>
                    <VisualState x:Name="MouseOver"/>
                    <VisualState x:Name="Disabled"/>
                </VisualStateGroup>
                <VisualStateGroup x:Name="FocusStates">
                    <VisualState x:Name="Unfocused"/>
                    <VisualState x:Name="Focused">
                        <Storyboard>
                            <DoubleAnimation Duration="0" Storyboard.TargetName="FocusedVisual" Storyboard.TargetProperty="Opacity" To="1"/>
                        </Storyboard>
                    </VisualState>
                </VisualStateGroup>
            </VisualStateManager.VisualStateGroups>
            <Border
                    Width="{TemplateBinding Width}"
                    VerticalAlignment="{TemplateBinding VerticalAlignment}"
                    HorizontalAlignment="{TemplateBinding HorizontalAlignment}"
                    Background="{TemplateBinding Background}"
                    BorderBrush="{TemplateBinding BorderBrush}"
                    BorderThickness="{TemplateBinding BorderThickness}"/>
            <Rectangle x:Name="FocusedVisual" Fill="Transparent" Margin="0" Opacity="0"/>
            <Rectangle x:Name="PART_PreviewControl"
                    VerticalAlignment="Stretch"
                    HorizontalAlignment="Stretch"
                    Fill="{telerik:GreenResource ResourceKey=AccentLowBrush}"
                    Opacity="0.5"
                    Visibility="Collapsed"
                    Width="6"/>
        </Grid>
    </ControlTemplate>
    <Style x:Key="GanttPresenterSplitterStyle" TargetType="telerik:ResizeControl">
        <Setter Property="Background" Value="{telerik:GreenResource ResourceKey=HighBrush}"/>
        <Setter Property="BorderBrush" Value="{telerik:GreenResource ResourceKey=BasicBrush}"/>
        <Setter Property="BorderThickness" Value="1 0"/>
        <Setter Property="Margin" Value="0 0 -1 0"/>
        <Setter Property="Width" Value="8"/>
        <Setter Property="HorizontalAlignment" Value="Right"/>
        <Setter Property="VerticalAlignment" Value="Stretch"/>
        <Setter Property="scheduling:ZIndexManager.ZIndex" Value="100"/>
        <Setter Property="ShowsPreview" Value="True"/>
        <Setter Property="IsTabStop" Value="True"/>
        <Setter Property="FocusVisualStyle" Value="{x:Null}"/>
        <Setter Property="Template" Value="{StaticResource GanttPresenterSplitterTemplate}"/>
    </Style>
    <Style x:Key="GanttPresenterHeaderSplitterStyle" TargetType="telerik:ResizeControl" BasedOn="{StaticResource GanttPresenterSplitterStyle}">
        <Setter Property="Width" Value="6"/>
        <Setter Property="Background" Value="{StaticResource SemiTransparentBrush}"/>
        <Setter Property="BorderBrush" Value="{telerik:GreenResource ResourceKey=BasicBrush}"/>
        <Setter Property="BorderThickness" Value="0 0 1 0"/>
        <Setter Property="scheduling:ZIndexManager.ZIndex" Value="100"/>
    </Style>
    <ControlTemplate x:Key="GanttItemsPresenterTemplate" TargetType="gantt:GanttItemsPresenter">
        <telerik:GanttPresenterPanel>
            <telerik:GanttPresenterPanel.ColumnDefinitions>
                <ColumnDefinition Width="Auto"/>
                <ColumnDefinition Width="*" MinWidth="3"/>
                <ColumnDefinition Width="3*"/>
                <ColumnDefinition Width="Auto"/>
            </telerik:GanttPresenterPanel.ColumnDefinitions>
            <telerik:TreeRenderingPanel x:Name="FrozenColumnsPanel"
                    Orientation="Horizontal"
                    telerik:GanttPresenterPanel.Column="0"
                    telerik:GanttPresenterPanel.Area="Header"
                    HorizontalAlignment="Stretch"
                    VerticalAlignment="Stretch"/>
            <telerik:VirtualizedGridPanel x:Name="FrozenCellsPanel" HorizontalAlignment="Stretch" VerticalAlignment="Top" telerik:GanttPresenterPanel.Column="0" telerik:GanttPresenterPanel.Area="Content"/>
            <telerik:TreeRenderingPanel x:Name="ColumnsPanel"
                    Orientation="Horizontal"
                    telerik:GanttPresenterPanel.Column="1"
                    telerik:GanttPresenterPanel.Area="Header"
                    HorizontalAlignment="Left"
                    VerticalAlignment="Stretch"
                    Margin="-1 0 4 0"/>
            <telerik:VirtualizedGridPanel x:Name="CellsPanel"
                    telerik:GanttPresenterPanel.Column="1"
                    telerik:GanttPresenterPanel.Area="Content"
                    Margin="-1 0 4 0"
                    HorizontalAlignment="Left"
                    VerticalAlignment="Top"
                    VerticalOffset="{Binding VerticalOffset, ElementName=FrozenCellsPanel, Mode=TwoWay}"
                    HorizontalOffset="{Binding HorizontalOffset, ElementName=ColumnsPanel, Mode=TwoWay}"/>
            <ScrollBar
                    telerik:GanttPresenterPanel.Column="1"
                    telerik:GanttPresenterPanel.Area="ScrollBar"
                    Orientation="Horizontal"
                    Minimum="0"
                    Maximum="{Binding ExtentWidth, ElementName=ColumnsPanel}"
                    Value="{Binding HorizontalOffset, ElementName=ColumnsPanel, Mode=TwoWay}"
                    ViewportSize="{Binding ViewportWidth, ElementName=ColumnsPanel}"
                    SmallChange="40"
                    LargeChange="{Binding ViewportWidth, ElementName=ColumnsPanel}"
                    Visibility="{Binding CalculatedHorizontalScrollBarVisibility, ElementName=ColumnsPanel}"
                    Margin="0 0 4 0"/>
            <telerik:ResizeControl telerik:GanttPresenterPanel.Column="1" telerik:GanttPresenterPanel.Area="Splitter" Style="{StaticResource GanttPresenterSplitterStyle}"/>
            <telerik:LogicalCanvasPanel x:Name="TimeRulerPanel"
                    telerik:GanttPresenterPanel.Column="2"
                    telerik:GanttPresenterPanel.Area="Header"
                    VerticalAlignment="Bottom"
                    HorizontalAlignment="Left"
                    FocusVisualStyle="{x:Null}"
                    HorizontalOffset="{Binding HorizontalOffset, ElementName=EventsPanel, Mode=TwoWay}"
                    Margin="0 0 -18 0"/>
            <telerik:LogicalCanvasPanel x:Name="EventsPanel"
                    telerik:GanttPresenterPanel.Column="2"
                    telerik:GanttPresenterPanel.Area="Content"
                    VerticalOffset="{Binding VerticalOffset, ElementName=CellsPanel, Mode=TwoWay}"
                    Background="Transparent"
                    VerticalAlignment="Top"
                    HorizontalAlignment="Left"/>
            <ScrollBar
                    telerik:GanttPresenterPanel.Column="2"
                    telerik:GanttPresenterPanel.Area="ScrollBar"
                    Orientation="Horizontal"
                    Minimum="0"
                    Maximum="{Binding ExtentWidth, ElementName=EventsPanel}"
                    Value="{Binding HorizontalOffset, ElementName=EventsPanel, Mode=TwoWay}"
                    ViewportSize="{Binding ViewportWidth, ElementName=EventsPanel}"
                    SmallChange="20"
                    LargeChange="{Binding ViewportWidth, ElementName=EventsPanel}"
                    Visibility="{Binding CalculatedHorizontalScrollBarVisibility, ElementName=EventsPanel}"/>
            <ScrollBar
                    telerik:GanttPresenterPanel.Column="3"
                    telerik:GanttPresenterPanel.Area="Content"
                    Orientation="Vertical"
                    Minimum="0"
                    Maximum="{Binding ExtentHeight, ElementName=CellsPanel}"
                    Value="{Binding VerticalOffset, ElementName=CellsPanel, Mode=TwoWay}"
                    ViewportSize="{Binding ViewportHeight, ElementName=CellsPanel}"
                    Visibility="{Binding CalculatedVerticalScrollBarVisibility, ElementName=CellsPanel}"
                    SmallChange="10"
                    LargeChange="{Binding ViewportHeight, ElementName=CellsPanel}"/>
        </telerik:GanttPresenterPanel>
    </ControlTemplate>
    <Style x:Key="GanttItemsPresenterStyle" TargetType="gantt:GanttItemsPresenter">
        <Setter Property="Background" Value="Transparent"/>
        <Setter Property="Template" Value="{StaticResource GanttItemsPresenterTemplate}"/>
    </Style>
    <ControlTemplate x:Key="GanttDragResizeVisualCueControlTemplate" TargetType="gantt:GanttDragResizeVisualCue">
        <Grid>
            <VisualStateManager.VisualStateGroups>
                <VisualStateGroup x:Name="EffectStates">
                    <VisualState x:Name="None">
                        <Storyboard>
                            <ObjectAnimationUsingKeyFrames Storyboard.TargetName="NoneEffect" Storyboard.TargetProperty="Visibility">
                                <DiscreteObjectKeyFrame KeyTime="0:0:0">
                                    <DiscreteObjectKeyFrame.Value>
                                        <Visibility>Visible</Visibility>
                                    </DiscreteObjectKeyFrame.Value>
                                </DiscreteObjectKeyFrame>
                            </ObjectAnimationUsingKeyFrames>
                        </Storyboard>
                    </VisualState>
                    <VisualState x:Name="Move"/>
                    <VisualState x:Name="Link">
                        <Storyboard>
                            <ObjectAnimationUsingKeyFrames Storyboard.TargetName="LinkEffect" Storyboard.TargetProperty="Visibility">
                                <DiscreteObjectKeyFrame KeyTime="0:0:0">
                                    <DiscreteObjectKeyFrame.Value>
                                        <Visibility>Visible</Visibility>
                                    </DiscreteObjectKeyFrame.Value>
                                </DiscreteObjectKeyFrame>
                            </ObjectAnimationUsingKeyFrames>
                        </Storyboard>
                    </VisualState>
                </VisualStateGroup>
            </VisualStateManager.VisualStateGroups>
            <Grid>
                <Border
                        Padding="{TemplateBinding BorderThickness}"
                        Background="{TemplateBinding Background}"
                        BorderBrush="{TemplateBinding BorderBrush}"
                        BorderThickness="{TemplateBinding BorderThickness}"
                        CornerRadius="{telerik:GreenResource ResourceKey=CornerRadius}"
                        Opacity="0.9"/>
                <Grid Margin="{TemplateBinding Padding}">
                    <Grid.ColumnDefinitions>
                        <ColumnDefinition Width="Auto"/>
                        <ColumnDefinition Width="*"/>
                    </Grid.ColumnDefinitions>
                    <Path x:Name="NoneEffect"
                            Visibility="Collapsed"
                            Margin="1 2 3 0"
                            Width="10"
                            Height="10"
                            HorizontalAlignment="Left"
                            VerticalAlignment="Top"
                            Data="{StaticResource NoneEffect}"
                            Fill="{telerik:GreenResource ResourceKey=ValidationBrush}"/>
                    <Path x:Name="LinkEffect"
                            Visibility="Collapsed"
                            HorizontalAlignment="Left"
                            VerticalAlignment="Top"
                            Margin="4 2 4 3"
                            RenderTransformOrigin="0.5 0.5"
                            Stretch="Fill"
                            UseLayoutRounding="False"
                            Width="6"
                            Height="13"
                            Data="{StaticResource RelationIcon}"
                            Fill="{telerik:GreenResource ResourceKey=AccentLowBrush}">
                        <Path.RenderTransform>
                            <TransformGroup>
                                <RotateTransform Angle="45"/>
                            </TransformGroup>
                        </Path.RenderTransform>
                    </Path>
                    <ContentControl x:Name="Content"
                            Grid.Column="1"
                            Foreground="{TemplateBinding Foreground}"
                            Margin="2 -1 0 0"
                            Content="{TemplateBinding Content}"
                            ContentTemplate="{TemplateBinding ContentTemplate}"
                            HorizontalAlignment="{TemplateBinding HorizontalContentAlignment}"
                            VerticalAlignment="{TemplateBinding VerticalContentAlignment}"/>
                </Grid>
            </Grid>
        </Grid>
    </ControlTemplate>
    <Style x:Key="GanttDragResizeVisualCueStyle" TargetType="gantt:GanttDragResizeVisualCue">
        <Setter Property="FontFamily" Value="{telerik:GreenResource ResourceKey=FontFamily}"/>
        <Setter Property="FontSize" Value="{telerik:GreenResource ResourceKey=FontSize}"/>
        <Setter Property="Foreground" Value="{telerik:GreenResource ResourceKey=MarkerBrush}"/>
        <Setter Property="Background" Value="{telerik:GreenResource ResourceKey=AlternativeBrush}"/>
        <Setter Property="BorderBrush" Value="{telerik:GreenResource ResourceKey=BasicBrush}"/>
        <Setter Property="BorderThickness" Value="1"/>
        <Setter Property="Padding" Value="4"/>
        <Setter Property="HorizontalContentAlignment" Value="Left"/>
        <Setter Property="VerticalContentAlignment" Value="Stretch"/>
        <Setter Property="CueTemplateSelector">
            <Setter.Value>
                <gantt:GanttDragVisualCueContentTemplateSelector
                        DragTemplate="{StaticResource GanttDragResizeVisualCueTemplate}"
                        ResizeTemplate="{StaticResource GanttDragResizeVisualCueTemplate}"
                        CreateDependencyTemplate="{StaticResource GanttCreateDependencyTemplate}"
                        DragReorderTemplate="{StaticResource GanttDragReorderTemplate}"/>
            </Setter.Value>
        </Setter>
        <Setter Property="Template" Value="{StaticResource GanttDragResizeVisualCueControlTemplate}"/>
    </Style>
    <Style x:Key="RadGanttViewStyle" TargetType="gantt1:RadGanttView">
        <Setter Property="FontFamily" Value="{telerik:GreenResource ResourceKey=FontFamily}"/>
        <Setter Property="FontSize" Value="{telerik:GreenResource ResourceKey=FontSize}"/>
        <Setter Property="Background" Value="{telerik:GreenResource ResourceKey=PrimaryBrush}"/>
        <Setter Property="BorderBrush" Value="{telerik:GreenResource ResourceKey=BasicBrush}"/>
        <Setter Property="BorderThickness" Value="1 0 1 1"/>
        <Setter Property="GapBetweenRows" Value="0"/>
        <Setter Property="GapBetweenColumns" Value="0"/>
        <Setter Property="UseLayoutRounding" Value="True"/>
        <Setter Property="FocusVisualStyle" Value="{x:Null}"/>
        <Setter Property="telerik:ScrollingSettingsBehavior.IsEnabled" Value="True"/>
        <Setter Property="telerik:ScrollingSettingsBehavior.ScrollAreaPadding" Value="30"/>
        <Setter Property="telerik:ScrollingSettingsBehavior.ScrollStep" Value="5"/>
        <Setter Property="telerik:ScrollingSettingsBehavior.ScrollStepTime" Value="00:00:00.01"/>
        <Setter Property="Template">
            <Setter.Value>
                <ControlTemplate TargetType="gantt1:RadGanttView">
                    <Border BorderBrush="{TemplateBinding BorderBrush}" BorderThickness="{TemplateBinding BorderThickness}" Background="{TemplateBinding Background}">
                        <gantt:GanttItemsPresenter x:Name="ItemsPresenter" Margin="{TemplateBinding Padding}"/>
                    </Border>
                </ControlTemplate>
            </Setter.Value>
        </Setter>
    </Style>
    <Style x:Key="DropReorderHighlightContainerStyle" TargetType="gantt:DropReorderHighlightContainer">
        <Setter Property="Height" Value="8"/>
        <Setter Property="Margin" Value="0 -4"/>
        <Setter Property="IsHitTestVisible" Value="False"/>
        <Setter Property="UseLayoutRounding" Value="True"/>
        <Setter Property="scheduling:ZIndexManager.ZIndex" Value="105"/>
        <Setter Property="Template">
            <Setter.Value>
                <ControlTemplate TargetType="gantt:DropReorderHighlightContainer">
                    <Grid>
                        <VisualStateManager.VisualStateGroups>
                            <VisualStateGroup x:Name="CommonStates">
                                <VisualState x:Name="Normal"/>
                                <VisualState x:Name="Doted">
                                    <Storyboard>
                                        <ObjectAnimationUsingKeyFrames Duration="0:0:0" Storyboard.TargetName="DotedElement" Storyboard.TargetProperty="Visibility">
                                            <DiscreteObjectKeyFrame KeyTime="0:0:0">
                                                <DiscreteObjectKeyFrame.Value>
                                                    <Visibility>Visible</Visibility>
                                                </DiscreteObjectKeyFrame.Value>
                                            </DiscreteObjectKeyFrame>
                                        </ObjectAnimationUsingKeyFrames>
                                    </Storyboard>
                                </VisualState>
                            </VisualStateGroup>
                        </VisualStateManager.VisualStateGroups>
                        <Grid.ColumnDefinitions>
                            <ColumnDefinition Width="Auto"/>
                            <ColumnDefinition Width="*"/>
                        </Grid.ColumnDefinitions>
                        <Ellipse x:Name="DotedElement"
                                Visibility="Collapsed"
                                Stroke="{telerik:GreenResource ResourceKey=AccentLowBrush}"
                                Fill="{telerik:GreenResource ResourceKey=AlternativeBrush}"
                                StrokeThickness="2"
                                HorizontalAlignment="Left"
                                VerticalAlignment="Center"
                                Width="8"
                                Height="8"/>
                        <Rectangle
                                Grid.Column="1"
                                Fill="{telerik:GreenResource ResourceKey=AccentLowBrush}"
                                Margin="-2 0 0 0"
                                RadiusX="2"
                                RadiusY="2"
                                Height="2"
                                VerticalAlignment="Center"
                                HorizontalAlignment="Stretch"/>
                    </Grid>
                </ControlTemplate>
            </Setter.Value>
        </Setter>
    </Style>
    <Style x:Key="DragResizeSlotHighlightContainerStyle" TargetType="gantt:DragResizeSlotHighlightContainer">
        <Setter Property="Foreground" Value="{telerik:GreenResource ResourceKey=MarkerBrush}"/>
        <Setter Property="Background" Value="{telerik:GreenResource ResourceKey=SemiAccentLowBrush}"/>
        <Setter Property="BorderBrush" Value="{x:Null}"/>
        <Setter Property="BorderThickness" Value="0"/>
        <Setter Property="VerticalAlignment" Value="Center"/>
        <Setter Property="Margin" Value="0 -1 0 0"/>
        <Setter Property="Padding" Value="5 0"/>
        <Setter Property="MinHeight" Value="19"/>
        <Setter Property="IsHitTestVisible" Value="False"/>
        <Setter Property="scheduling:ZIndexManager.ZIndex" Value="70"/>
        <Setter Property="Template">
            <Setter.Value>
                <ControlTemplate TargetType="gantt:DragResizeSlotHighlightContainer">
                    <Grid>
                        <Border Background="{TemplateBinding Background}" HorizontalAlignment="Stretch" VerticalAlignment="Stretch"/>
                    </Grid>
                </ControlTemplate>
            </Setter.Value>
        </Setter>
    </Style>
    <Style x:Key="DragResizeMilestoneSlotHighlightContainerStyle" TargetType="gantt:DragResizeMilestoneSlotHighlightContainer">
        <Setter Property="Background" Value="{telerik:GreenResource ResourceKey=SemiAccentLowBrush}"/>
        <Setter Property="BorderBrush" Value="{telerik:GreenResource ResourceKey=SemiAccentLowBrush}"/>
        <Setter Property="Margin" Value="-9 0"/>
        <Setter Property="Width" Value="12"/>
