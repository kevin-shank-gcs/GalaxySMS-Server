    <SolidColorBrush x:Key="GroupTickContainerForeground" Color="#FFDDDDDD"/>
    <SolidColorBrush x:Key="MajorTickBrush" Color="#FF707070"/>
    <SolidColorBrush x:Key="MinorTickBrush" Color="#FF707070"/>
