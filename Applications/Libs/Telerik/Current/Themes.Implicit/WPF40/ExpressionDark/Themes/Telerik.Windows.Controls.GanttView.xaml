        <Setter Property="Template">
            <Setter.Value>
                <ControlTemplate TargetType="gantt:CellContainer">
                    <Grid>
                        <VisualStateManager.VisualStateGroups>
                            <VisualStateGroup>
                                <VisualState x:Name="HighlightedState"/>
                                <VisualState x:Name="SelectedState"/>
