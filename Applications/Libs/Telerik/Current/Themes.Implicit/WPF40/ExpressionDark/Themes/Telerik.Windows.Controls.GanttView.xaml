                                    Fill="Transparent"
                                    Margin="0"
                                    RadiusX="{StaticResource Event_RadiusX}"
                                    RadiusY="{StaticResource Event_RadiusY}"/>
                        </Border>
                        <ContentPresenter Margin="{TemplateBinding Padding}" VerticalAlignment="{TemplateBinding VerticalContentAlignment}"/>
