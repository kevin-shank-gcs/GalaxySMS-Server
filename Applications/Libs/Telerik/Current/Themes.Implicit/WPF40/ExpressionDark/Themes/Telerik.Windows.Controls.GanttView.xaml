    <SolidColorBrush x:Key="DragDropHighlightContainer_OuterBorderBrush" Color="#B2DCDCDC"/>
    <SolidColorBrush x:Key="DragDropHighlightContainer_Foreground" Color="#FFDDDDDD"/>
    <SolidColorBrush x:Key="DragVisualEffectNormal" Color="#FFC5C2C2"/>
    <SolidColorBrush x:Key="DragVisualEffectImportant" Color="#FFDC000C"/>
    <SolidColorBrush x:Key="ResizerFill" Color="#FF789FC3"/>
    <SolidColorBrush x:Key="ResizerStroke" Color="#FF789FC3"/>
    <LinearGradientBrush x:Key="GanttView_ShapeManipulation_BorderBrush" EndPoint="0,0.5" StartPoint="1,0.5">
        <GradientStop Color="#FFB0B0B0" Offset="0"/>
        <GradientStop Color="#FFB0B0B0" Offset="1"/>
        <GradientStop Color="#FFE0E1E2" Offset="0.5"/>
    </LinearGradientBrush>
    <SolidColorBrush x:Key="GanttView_ShapeManipulation_Background" Color="#FF878787"/>
    <SolidColorBrush x:Key="DropReorderHighlightContainer_BackgroundBrush" Color="#FF246A9B"/>
    <SolidColorBrush x:Key="DropReorderEllipseHighlightContainer_BackgroundBrush" Color="#FF246A9B"/>
