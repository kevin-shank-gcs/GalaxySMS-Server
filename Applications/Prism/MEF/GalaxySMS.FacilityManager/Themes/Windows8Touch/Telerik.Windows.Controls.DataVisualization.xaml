    <Style x:Key="NeedleStyle" TargetType="gauges:Needle">
        <Setter Property="gauges:ScaleObject.Location" Value="Inside"/>
        <Setter Property="gauges:ScaleObject.RelativeWidth" Value="0.02*"/>
        <Setter Property="gauges:ScaleObject.Offset" Value="{StaticResource NeedleOffset}"/>
        <Setter Property="Background" Value="{telerik:Windows8TouchResource ResourceKey=AccentBrush}"/>
        <Setter Property="TailHeight" Value="{StaticResource NeedleTailHeight}"/>
        <Setter Property="Template">
            <Setter.Value>
                <ControlTemplate TargetType="gauges:Needle">
                    <Border Background="#01FFFFFF" Padding="{StaticResource NeedleBorderPadding}" Margin="{StaticResource NeedleBorderMargin}">
                        <Grid x:Name="PART_Grid">
                            <Polygon Points="0,0 1,0 1,1 0,1" Fill="{TemplateBinding Background}" Stroke="{TemplateBinding BorderBrush}" Stretch="Fill"/>
                        </Grid>
                    </Border>
                </ControlTemplate>
            </Setter.Value>
        </Setter>
    </Style>
    <Style TargetType="gauges:Needle" BasedOn="{StaticResource NeedleStyle}"/>
    <Style x:Key="MarkerStyle" TargetType="gauges:Marker">
        <Setter Property="gauges:ScaleObject.Location" Value="Inside"/>
        <Setter Property="gauges:ScaleObject.Offset" Value="0"/>
        <Setter Property="gauges:ScaleObject.RelativeHeight" Value="20"/>
        <Setter Property="gauges:ScaleObject.RelativeWidth" Value="20"/>
        <Setter Property="Background" Value="{telerik:Windows8TouchResource ResourceKey=InvertedBrush}"/>
        <Setter Property="BorderBrush" Value="{telerik:Windows8TouchResource ResourceKey=MainBrush}"/>
        <Setter Property="gauges:LinearScale.RotateForVertical" Value="True"/>
        <Setter Property="Template">
            <Setter.Value>
                <ControlTemplate TargetType="gauges:Marker">
                    <Border Margin="{StaticResource MarkerBorderMargin}" CornerRadius="18" Background="#01FFFFFF" Padding="{StaticResource MarkerBorderPadding}">
                        <Grid HorizontalAlignment="Stretch" VerticalAlignment="Stretch">
                            <Polygon Points="0.5,0 1,0.5 0.5,1 0,0.5" Stretch="Fill" Fill="{TemplateBinding Background}" Stroke="{TemplateBinding BorderBrush}" StrokeThickness="2"/>
                        </Grid>
                    </Border>
                </ControlTemplate>
            </Setter.Value>
        </Setter>
    </Style>
    <Style TargetType="gauges:Marker" BasedOn="{StaticResource MarkerStyle}"/>
