                                        HorizontalAlignment="Center"
                                        VerticalAlignment="Center"
                                        RenderTransformOrigin="0.5,0.5">
                                    <Path.RenderTransform>
                                        <TransformGroup>
                                            <RotateTransform/>
                                            <TranslateTransform Y="1"/>
                                        </TransformGroup>
                                    </Path.RenderTransform>
                                </Path>
                            </Grid>
                            <TextBlock Text="{TemplateBinding Content}" Margin="4 0 0 0" VerticalAlignment="Center"/>
                        </StackPanel>
                    </Grid>
                </ControlTemplate>
            </Setter.Value>
        </Setter>
    </Style>
    <Style x:Key="TimelineItemGroupControlStyle" TargetType="timelines:TimelineItemGroupControl">
        <Setter Property="FontFamily" Value="{telerikControls:Windows8TouchResource ResourceKey=FontFamilyLight}"/>
        <Setter Property="FontSize" Value="{telerikControls:Windows8TouchResource ResourceKey=FontSizeL}"/>
        <Setter Property="Foreground" Value="{telerikControls:Windows8TouchResource ResourceKey=MainForegroundBrush}"/>
        <Setter Property="ItemsPanel">
            <Setter.Value>
                <ItemsPanelTemplate>
                    <timelines:VirtualizingTimelinePanel Slider="{Binding ElementName=PART_Slider}" Background="#01FFFFFF"/>
                </ItemsPanelTemplate>
            </Setter.Value>
        </Setter>
        <Setter Property="Header" Value="{Binding GroupKey}"/>
        <Setter Property="ItemsSource" Value="{Binding DataItems}"/>
        <Setter Property="ItemTemplateSelector" Value="{Binding Path=ItemTemplateSelector, RelativeSource={RelativeSource Mode=FindAncestor, AncestorType={x:Type datavizControls:RadTimeline}}}"/>
        <Setter Property="DataGroups" Value="{Binding DataGroups}"/>
        <Setter Property="ExpandMode" Value="{Binding GroupExpandMode, RelativeSource={RelativeSource Mode=FindAncestor, AncestorType=datavizControls:RadTimeline}}"/>
        <Setter Property="Background" Value="Transparent"/>
        <Setter Property="HorizontalContentAlignment" Value="Left"/>
        <Setter Property="IsTabStop" Value="false"/>
        <Setter Property="animation:AnimationManager.AnimationSelector">
            <Setter.Value>
                <animation:AnimationSelector>
                    <animation:ExpanderExpandCollapseAnimation AnimationName="Expand" Direction="In" TargetElementName="Content"/>
                    <animation:ExpanderExpandCollapseAnimation AnimationName="Collapse" Direction="Out" TargetElementName="Content"/>
                    <timelines:TimelineResizeAnimation AnimationName="Resize" TargetElementName="Content"/>
                </animation:AnimationSelector>
            </Setter.Value>
        </Setter>
        <Setter Property="Template">
            <Setter.Value>
                <ControlTemplate TargetType="timelines:TimelineItemGroupControl">
                    <Border Background="{TemplateBinding Background}" BorderBrush="{TemplateBinding BorderBrush}" BorderThickness="{TemplateBinding BorderThickness}">
                        <Grid>
                            <Grid.RowDefinitions>
                                <RowDefinition Height="Auto"/>
                                <RowDefinition Height="*"/>
                            </Grid.RowDefinitions>
                            <Grid x:Name="Header"
                                    Grid.Row="0"
                                    Margin="0 5 0 0"
                                    Visibility="{Binding Path=HasHeader, RelativeSource={RelativeSource TemplatedParent}, Converter={StaticResource booleanToVisibilityConverter}}">
                                <Grid.ColumnDefinitions>
                                    <ColumnDefinition Width="Auto"/>
                                    <ColumnDefinition Width="*"/>
                                </Grid.ColumnDefinitions>
                                <telerikControls:RadToggleButton x:Name="HeaderButton"
                                        Foreground="{TemplateBinding Foreground}"
                                        Grid.Column="0"
                                        Content="{TemplateBinding Header}"
                                        ContentTemplate="{TemplateBinding HeaderTemplate}"
                                        Style="{StaticResource TimelineItemGroupControlToggleButtonStyle}"
                                        IsChecked="{Binding IsExpanded, RelativeSource={RelativeSource TemplatedParent}, Mode=TwoWay}"
                                        IsEnabled="{Binding ExpandMode, RelativeSource={RelativeSource TemplatedParent}, Converter={StaticResource groupExpandModeToBooleanConverter}}"/>
                                <Path
                                        Grid.Column="1"
                                        VerticalAlignment="Center"
                                        HorizontalAlignment="Stretch"
                                        Data="M0,0 L800,0"
                                        Height="{StaticResource Timeline_TimelineItemGroup_Separator_Height}"
                                        Margin="5 1 0 0"
                                        Stretch="Fill"
                                        Stroke="{telerikControls:Windows8TouchResource ResourceKey=HighBrush}"/>
                            </Grid>
                            <ItemsPresenter x:Name="Content" Grid.Row="1"/>
                        </Grid>
                    </Border>
                </ControlTemplate>
            </Setter.Value>
        </Setter>
    </Style>
    <Style TargetType="timelines:TimelineItemGroupControl" BasedOn="{StaticResource TimelineItemGroupControlStyle}"/>
    <Style x:Key="TimelinePeriodHeaderStyle" TargetType="Border">
        <Setter Property="Height" Value="23"/>
        <Setter Property="VerticalAlignment" Value="Top"/>
        <Setter Property="Margin" Value="0 4 0 0"/>
    </Style>
    <Style x:Key="TimelineGroupPeriodHeaderStyle" TargetType="Border">
        <Setter Property="Height" Value="30"/>
        <Setter Property="VerticalAlignment" Value="Top"/>
    </Style>
    <Style x:Key="TimelineLineStyle" TargetType="Border">
        <Setter Property="BorderBrush" Value="{StaticResource Timeline_Button_Bottom_BorderBrush}"/>
        <Setter Property="BorderThickness" Value="{StaticResource Timeline_Button_Bottom_Thickness}"/>
        <Setter Property="Background" Value="{telerikControls:Windows8TouchResource ResourceKey=MediumBrush}"/>
        <Setter Property="Height" Value="1"/>
    </Style>
    <Style x:Key="TimelineItemControlStyle" TargetType="timelines:TimelineItemControl">
        <Setter Property="Height" Value="40"/>
        <Setter Property="Margin" Value="0 5 0 5"/>
        <Setter Property="Background" Value="{StaticResource Timeline_TimelineItem_Fill}"/>
        <Setter Property="BorderBrush" Value="{StaticResource Timeline_TimelineItem_Stroke}"/>
        <Setter Property="Template">
            <Setter.Value>
                <ControlTemplate TargetType="timelines:TimelineItemControl">
                    <Border Height="40" Background="#01FFFFFF">
                        <VisualStateManager.VisualStateGroups>
                            <VisualStateGroup x:Name="CommonStates">
                                <VisualState x:Name="Normal"/>
                                <VisualState x:Name="MouseOver">
                                    <Storyboard>
                                        <ObjectAnimationUsingKeyFrames Storyboard.TargetName="timelineItem" Storyboard.TargetProperty="Fill" Duration="0.00:00:00.05">
                                            <DiscreteObjectKeyFrame KeyTime="0.00:00:00.0" Value="{StaticResource Timeline_TimelineItem_Fill_MouseOver}"/>
                                        </ObjectAnimationUsingKeyFrames>
                                        <ObjectAnimationUsingKeyFrames Storyboard.TargetName="timelineItem" Storyboard.TargetProperty="Stroke" Duration="0.00:00:00.05">
                                            <DiscreteObjectKeyFrame KeyTime="0.00:00:00.0" Value="{StaticResource Timeline_TimelineItem_Stroke_MouseOver}"/>
                                        </ObjectAnimationUsingKeyFrames>
                                    </Storyboard>
                                </VisualState>
                                <VisualState x:Name="Pressed">
                                    <Storyboard>
                                        <ObjectAnimationUsingKeyFrames Storyboard.TargetName="timelineItem" Storyboard.TargetProperty="Fill" Duration="0.00:00:00.05">
                                            <DiscreteObjectKeyFrame KeyTime="0.00:00:00.0" Value="{telerikControls:Windows8TouchResource ResourceKey=InvertedBrush}"/>
                                        </ObjectAnimationUsingKeyFrames>
                                        <ObjectAnimationUsingKeyFrames Storyboard.TargetName="timelineItem" Storyboard.TargetProperty="Stroke" Duration="0.00:00:00.05">
                                            <DiscreteObjectKeyFrame KeyTime="0.00:00:00.0" Value="{telerikControls:Windows8TouchResource ResourceKey=InvertedBrush}"/>
                                        </ObjectAnimationUsingKeyFrames>
                                    </Storyboard>
                                </VisualState>
                            </VisualStateGroup>
                        </VisualStateManager.VisualStateGroups>
                        <Rectangle x:Name="timelineItem" Margin="0 5 0 5" Fill="{TemplateBinding Background}" Stroke="{TemplateBinding BorderBrush}"/>
                    </Border>
                </ControlTemplate>
            </Setter.Value>
        </Setter>
    </Style>
    <Style x:Key="TimelineInstantItemControlStyle" TargetType="timelines:TimelineInstantItemControl">
        <Setter Property="Margin" Value="0 5 0 5"/>
        <Setter Property="Template">
            <Setter.Value>
                <ControlTemplate TargetType="timelines:TimelineInstantItemControl">
                    <Border Width="40" Height="40" CornerRadius="18" Background="#01FFFFFF">
                        <VisualStateManager.VisualStateGroups>
                            <VisualStateGroup x:Name="CommonStates">
                                <VisualState x:Name="Normal"/>
                                <VisualState x:Name="MouseOver">
                                    <Storyboard>
                                        <DoubleAnimation Storyboard.TargetName="timelineItem" Storyboard.TargetProperty="Opacity" To="0.44" Duration="0:0:0"/>
                                    </Storyboard>
                                </VisualState>
                                <VisualState x:Name="Pressed">
                                    <Storyboard>
                                        <DoubleAnimation Storyboard.TargetName="timelineItem" Storyboard.TargetProperty="Opacity" To="1" Duration="0:0:0"/>
                                        <ObjectAnimationUsingKeyFrames Storyboard.TargetName="timelineItem" Storyboard.TargetProperty="Fill" Duration="0.00:00:00.05">
                                            <DiscreteObjectKeyFrame KeyTime="0.00:00:00.0" Value="{telerikControls:Windows8TouchResource ResourceKey=InvertedBrush}"/>
                                        </ObjectAnimationUsingKeyFrames>
                                        <ObjectAnimationUsingKeyFrames Storyboard.TargetName="timelineItem" Storyboard.TargetProperty="Stroke" Duration="0.00:00:00.05">
                                            <DiscreteObjectKeyFrame KeyTime="0.00:00:00.0" Value="{telerikControls:Windows8TouchResource ResourceKey=InvertedBrush}"/>
                                        </ObjectAnimationUsingKeyFrames>
                                    </Storyboard>
                                </VisualState>
                            </VisualStateGroup>
                        </VisualStateManager.VisualStateGroups>
                        <Rectangle x:Name="timelineItem"
                                Height="14"
                                Width="14"
                                HorizontalAlignment="Center"
                                VerticalAlignment="Center"
                                StrokeThickness="{StaticResource Timeline_TimelineInstantItem_Stroke_Thickness}"
                                Fill="{telerikControls:Windows8TouchResource ResourceKey=AccentBrush}">
                            <Rectangle.RenderTransform>
                                <RotateTransform Angle="45" CenterX="7" CenterY="7"/>
                            </Rectangle.RenderTransform>
                        </Rectangle>
                    </Border>
                </ControlTemplate>
            </Setter.Value>
        </Setter>
    </Style>
    <DataTemplate x:Key="TimelineItemTemplate">
        <timelines:TimelineItemControl/>
    </DataTemplate>
    <DataTemplate x:Key="TimelineInstantItemTemplate">
        <timelines:TimelineInstantItemControl/>
    </DataTemplate>
    <Style x:Key="TimelineAnnotationStyle" TargetType="timelines:TimelineAnnotation">
        <Setter Property="IsHitTestVisible" Value="False"/>
        <Setter Property="Margin" Value="{StaticResource Timeline_TimelineAnnotation_Margin}"/>
        <Setter Property="Canvas.ZIndex" Value="300"/>
        <Setter Property="Template">
            <Setter.Value>
                <ControlTemplate TargetType="timelines:TimelineAnnotation">
                    <ContentPresenter Content="{TemplateBinding Content}" ContentTemplate="{TemplateBinding ContentTemplate}"/>
                </ControlTemplate>
            </Setter.Value>
        </Setter>
    </Style>
    <Style x:Key="SelectionRegionStyle" TargetType="timelines:SelectionRegion">
        <Setter Property="Visibility" Value="Collapsed"/>
        <Setter Property="Background" Value="{telerikControls:Windows8TouchResource ResourceKey=MainBrush}"/>
        <Setter Property="BorderBrush" Value="{telerikControls:Windows8TouchResource ResourceKey=HighBrush}"/>
        <Setter Property="Template">
            <Setter.Value>
                <ControlTemplate TargetType="timelines:SelectionRegion">
                    <Grid>
                        <Border Opacity="0.5" Background="{TemplateBinding Background}"/>
                        <Rectangle Stroke="{TemplateBinding BorderBrush}" StrokeDashArray="3 3"/>
                    </Grid>
                </ControlTemplate>
            </Setter.Value>
        </Setter>
    </Style>
    <Style x:Key="TimelineItemContainerStyle" TargetType="timelines:TimelineItemContainer">
        <Setter Property="Canvas.ZIndex" Value="200"/>
        <Setter Property="ItemsPanel">
            <Setter.Value>
                <ItemsPanelTemplate>
                    <timelines:TimelineItemGroupsPanel Padding="0 58 0 5" VerticalScrollBar="{Binding ElementName=Part_VerticalSlider}"/>
                </ItemsPanelTemplate>
            </Setter.Value>
        </Setter>
    </Style>
    <Style x:Key="RadTimelineStyle" TargetType="datavizControls:RadTimeline">
        <Setter Property="Foreground" Value="{telerikControls:Windows8TouchResource ResourceKey=MainForegroundBrush}"/>
        <Setter Property="FontFamily" Value="{telerikControls:Windows8TouchResource ResourceKey=FontFamily}"/>
        <Setter Property="Background" Value="{telerikControls:Windows8TouchResource ResourceKey=MainBrush}"/>
        <Setter Property="BorderThickness" Value="0 0 0 1"/>
        <Setter Property="BorderBrush" Value="{telerikControls:Windows8TouchResource ResourceKey=MediumBrush}"/>
        <Setter Property="SliderStyle" Value="{StaticResource Timeline_SliderStyle}"/>
        <Setter Property="TimelineItemTemplate" Value="{StaticResource TimelineItemTemplate}"/>
        <Setter Property="TimelineInstantItemTemplate" Value="{StaticResource TimelineInstantItemTemplate}"/>
        <Setter Property="PeriodHeaderStyle" Value="{StaticResource TimelinePeriodHeaderStyle}"/>
        <Setter Property="GroupPeriodHeaderStyle" Value="{StaticResource TimelineGroupPeriodHeaderStyle}"/>
        <Setter Property="Template">
            <Setter.Value>
                <ControlTemplate TargetType="datavizControls:RadTimeline">
                    <Border BorderBrush="{TemplateBinding BorderBrush}" BorderThickness="{TemplateBinding BorderThickness}" SnapsToDevicePixels="True">
                        <Grid>
                            <Grid.RowDefinitions>
                                <RowDefinition Height="*"/>
                                <RowDefinition Height="Auto"/>
                            </Grid.RowDefinitions>
                            <Grid.ColumnDefinitions>
                                <ColumnDefinition Width="*"/>
                                <ColumnDefinition Width="Auto"/>
                            </Grid.ColumnDefinitions>
                            <Border
                                    Grid.Row="0"
                                    Grid.Column="0"
                                    Height="{Binding ElementName=PART_TimelineStripLineContainer, Path=(timeBars:ActualSizeProxy.ActualHeight)}"
                                    VerticalAlignment="Bottom"
                                    Background="{TemplateBinding Background}"/>
                            <timelines:TimelineAnnotationsPanel x:Name="PART_TimelineAnnotationsPanel" Slider="{Binding ElementName=PART_Slider}" Grid.Row="0" Grid.Column="0" Grid.ColumnSpan="2">
                                <Grid Canvas.ZIndex="100">
                                    <Grid.RowDefinitions>
                                        <RowDefinition Height="Auto"/>
                                        <RowDefinition Height="27"/>
                                        <RowDefinition Height="*"/>
                                    </Grid.RowDefinitions>
                                    <timelines:TimelineStripLineContainer x:Name="PART_TimelineStripLineContainer" Grid.Row="2" timeBars:ActualSizeProxy.IsEnabled="True" ItemsSource="{TemplateBinding IntervalItems}">
                                        <timelines:TimelineStripLineContainer.ItemsPanel>
                                            <ItemsPanelTemplate>
                                                <timeBars:VirtualizingTimeBarPanel Slider="{Binding ElementName=PART_Slider}"/>
                                            </ItemsPanelTemplate>
                                        </timelines:TimelineStripLineContainer.ItemsPanel>
                                        <timelines:TimelineStripLineContainer.Template>
                                            <ControlTemplate>
                                                <ItemsPresenter/>
                                            </ControlTemplate>
                                        </timelines:TimelineStripLineContainer.Template>
                                    </timelines:TimelineStripLineContainer>
                                    <timeBars:TimeSpecialSlotContainer x:Name="PART_TimeSpecialSlotContainer" Grid.Row="2" ItemsSource="{Binding Path=SpecialSlots, RelativeSource={RelativeSource TemplatedParent}}">
                                        <timeBars:TimeSpecialSlotContainer.ItemsPanel>
                                            <ItemsPanelTemplate>
                                                <timeBars:TimeBarPanel Slider="{Binding ElementName=PART_Slider}"/>
                                            </ItemsPanelTemplate>
                                        </timeBars:TimeSpecialSlotContainer.ItemsPanel>
                                        <timeBars:TimeSpecialSlotContainer.Template>
                                            <ControlTemplate>
                                                <ItemsPresenter/>
                                            </ControlTemplate>
                                        </timeBars:TimeSpecialSlotContainer.Template>
                                    </timeBars:TimeSpecialSlotContainer>
                                    <Border Grid.Row="1" Style="{StaticResource TimelineLineStyle}" VerticalAlignment="Bottom"/>
                                    <timelines:TimelineGroupPeriodContainer x:Name="PART_TimelineGroupPeriodContainer" Grid.Row="0" ItemsSource="{TemplateBinding IntervalGroups}" ItemHeaderStyle="{TemplateBinding GroupPeriodHeaderStyle}">
                                        <timelines:TimelineGroupPeriodContainer.ItemsPanel>
                                            <ItemsPanelTemplate>
                                                <timeBars:LabelMeasurementPanel Slider="{Binding ElementName=PART_Slider}"/>
                                            </ItemsPanelTemplate>
                                        </timelines:TimelineGroupPeriodContainer.ItemsPanel>
                                        <timelines:TimelineGroupPeriodContainer.Template>
                                            <ControlTemplate>
                                                <ItemsPresenter/>
                                            </ControlTemplate>
                                        </timelines:TimelineGroupPeriodContainer.Template>
                                    </timelines:TimelineGroupPeriodContainer>
                                    <timelines:TimelinePeriodContainer x:Name="PART_TimelinePeriodContainer"
                                            Grid.Row="1"
                                            Grid.RowSpan="2"
                                            ItemsSource="{TemplateBinding IntervalItems}"
                                            ItemHeaderStyle="{TemplateBinding PeriodHeaderStyle}">
                                        <timelines:TimelinePeriodContainer.ItemsPanel>
                                            <ItemsPanelTemplate>
                                                <timeBars:LabelMeasurementPanel Slider="{Binding ElementName=PART_Slider}"/>
                                            </ItemsPanelTemplate>
                                        </timelines:TimelinePeriodContainer.ItemsPanel>
                                        <timelines:TimelinePeriodContainer.Template>
                                            <ControlTemplate>
                                                <ItemsPresenter/>
                                            </ControlTemplate>
                                        </timelines:TimelinePeriodContainer.Template>
                                    </timelines:TimelinePeriodContainer>
                                </Grid>
                                <timelines:TimelineItemContainer x:Name="PART_TimelineItemContainer"
                                        GroupExpandMode="{TemplateBinding GroupExpandMode}"
                                        ItemsSource="{TemplateBinding GroupedDataItems}"
                                        SelectionRegionControl="{Binding ElementName=PART_SelectionRegion}">
                                    <timelines:TimelineItemContainer.Template>
                                        <ControlTemplate>
                                            <Grid Background="#01FFFFFF">
                                                <ItemsPresenter/>
                                            </Grid>
                                        </ControlTemplate>
                                    </timelines:TimelineItemContainer.Template>
                                </timelines:TimelineItemContainer>
                            </timelines:TimelineAnnotationsPanel>
                            <timelines:SelectionRegionPanel x:Name="PART_SelectionRegionPanel" Grid.Row="0" Grid.Column="0" Slider="{Binding ElementName=PART_Slider}">
                                <timelines:SelectionRegion x:Name="PART_SelectionRegion"/>
                            </timelines:SelectionRegionPanel>
                            <Border
                                    Grid.Row="1"
                                    Grid.Column="1"
                                    Background="{telerikControls:Windows8TouchResource ResourceKey=MainBrush}"
                                    Visibility="{Binding Visibility, ElementName=Part_VerticalSlider}"/>
                            <timelines:TimelineScrollBar x:Name="Part_VerticalSlider"
                                    Grid.Row="0"
                                    Grid.Column="1"
                                    VerticalAlignment="Bottom"
                                    Visibility="Collapsed"
                                    ScrollBarVisibility="{TemplateBinding VerticalScrollBarVisibility}"
                                    Style="{TemplateBinding SliderStyle}"
                                    Height="{Binding ElementName=PART_TimelineStripLineContainer, Path=(timeBars:ActualSizeProxy.ActualHeight)}"
                                    Minimum="0"
                                    Maximum="1"
                                    SelectionStart="0"
                                    SelectionEnd="1"
                                    Orientation="Vertical"
                                    IsDirectionReversed="True"
                                    ThumbVisibility="Collapsed"/>
                            <telerikControls:RadSlider x:Name="PART_Slider"
                                    Grid.Row="1"
                                    Grid.Column="0"
                                    Style="{TemplateBinding SliderStyle}"
                                    Width="Auto"
                                    Minimum="{Binding Path=PeriodStart, RelativeSource={RelativeSource TemplatedParent}, Converter={StaticResource dateConverter}}"
                                    Maximum="{Binding Path=PeriodEnd, RelativeSource={RelativeSource TemplatedParent}, Converter={StaticResource dateConverter}}"
                                    MinimumRangeSpan="{Binding Path=ActualMinZoomRange, RelativeSource={RelativeSource TemplatedParent}, Converter={StaticResource timespanConverter}}"
                                    MaximumRangeSpan="{Binding Path=MaxZoomRange, RelativeSource={RelativeSource TemplatedParent}, Converter={StaticResource timeSpanToMaximumRangeSpanConverter}}"
                                    Selection="{Binding Path=ActualVisiblePeriodRange, RelativeSource={RelativeSource TemplatedParent}, Mode=TwoWay, Converter={StaticResource selectionRangeConverter}}"
                                    SmallChange="{Binding Path=SmallChange, RelativeSource={RelativeSource TemplatedParent}, Converter={StaticResource timespanConverter}}"
                                    LargeChange="{Binding Path=LargeChange, RelativeSource={RelativeSource TemplatedParent}, Converter={StaticResource timespanConverter}}"
                                    ThumbVisibility="{Binding Path=ScrollMode, RelativeSource={RelativeSource TemplatedParent}, Converter={StaticResource scrollModeToThumbVisibilityConverter}}"
                                    Visibility="{Binding Path=ScrollMode, RelativeSource={RelativeSource TemplatedParent}, Converter={StaticResource scrollModeToVisibilityConverter}}"
                                    IsCoercionSuppressed="True"/>
                        </Grid>
                    </Border>
                </ControlTemplate>
            </Setter.Value>
        </Setter>
    </Style>
    <Style TargetType="timelines:TimelineItemControl" BasedOn="{StaticResource TimelineItemControlStyle}"/>
    <Style TargetType="timelines:TimelineInstantItemControl" BasedOn="{StaticResource TimelineInstantItemControlStyle}"/>
    <Style TargetType="timelines:TimelineAnnotation" BasedOn="{StaticResource TimelineAnnotationStyle}"/>
    <Style TargetType="timelines:SelectionRegion" BasedOn="{StaticResource SelectionRegionStyle}"/>
    <Style TargetType="timelines:TimelineItemContainer" BasedOn="{StaticResource TimelineItemContainerStyle}"/>
    <Style TargetType="datavizControls:RadTimeline" BasedOn="{StaticResource RadTimelineStyle}"/>
</ResourceDictionary>
