    <Style x:Key="LinearScaleMarkerStyle" TargetType="gauges:LinearScaleMarker">
        <Setter Property="gauges:ScaleObject.Location" Value="Inside"/>
        <Setter Property="gauges:ScaleObject.Offset" Value="0"/>
        <Setter Property="gauges:LinearScale.RotateForVertical" Value="True"/>
        <Setter Property="gauges:ScaleObject.RelativeHeight" Value="20"/>
        <Setter Property="gauges:ScaleObject.RelativeWidth" Value="20"/>
        <Setter Property="Background" Value="{telerik:Windows8TouchResource ResourceKey=InvertedBrush}"/>
        <Setter Property="BorderBrush" Value="{telerik:Windows8TouchResource ResourceKey=MainBrush}"/>
        <Setter Property="Template">
            <Setter.Value>
                <ControlTemplate TargetType="gauges:LinearScaleMarker">
                    <Border Margin="{StaticResource MarkerBorderMargin}" CornerRadius="18" Background="#01FFFFFF" Padding="{StaticResource MarkerBorderPadding}">
                        <VisualStateManager.VisualStateGroups>
                            <VisualStateGroup x:Name="CommonStates">
                                <VisualState x:Name="Normal"/>
                                <VisualState x:Name="MouseOver"/>
                                <VisualState x:Name="Pressed">
                                    <Storyboard>
                                        <ObjectAnimationUsingKeyFrames Storyboard.TargetName="SelectionTriangleWrapper" Storyboard.TargetProperty="Visibility" Duration="0.00:00:00.05">
                                            <DiscreteObjectKeyFrame KeyTime="0.00:00:00.0">
                                                <DiscreteObjectKeyFrame.Value>
                                                    <Visibility>Visible</Visibility>
                                                </DiscreteObjectKeyFrame.Value>
                                            </DiscreteObjectKeyFrame>
                                        </ObjectAnimationUsingKeyFrames>
                                    </Storyboard>
                                </VisualState>
                            </VisualStateGroup>
                        </VisualStateManager.VisualStateGroups>
                        <Grid HorizontalAlignment="Stretch" VerticalAlignment="Stretch">
                            <Polygon x:Name="Polygon" Points="0.5,0 1,0.5 0.5,1 0,0.5" Stretch="Fill" Fill="{TemplateBinding Background}" Stroke="{TemplateBinding BorderBrush}" StrokeThickness="2"/>
                            <Canvas x:Name="SelectionTriangleWrapper" Visibility="Collapsed">
                                <Path
                                        Data="M 5 0 L 0 8 L 10 8 Z"
                                        Stretch="None"
                                        Stroke="{TemplateBinding BorderBrush}"
                                        StrokeThickness="1"
                                        Canvas.Top="-15"
                                        Canvas.Left="5"
                                        Fill="{TemplateBinding Background}"/>
                            </Canvas>
                        </Grid>
                    </Border>
                </ControlTemplate>
            </Setter.Value>
        </Setter>
    </Style>
    <Style TargetType="gauges:LinearScaleMarker" BasedOn="{StaticResource LinearScaleMarkerStyle}"/>
