<telerik:RadRibbonTab x:Class="GalaxySMS.Site.RibbonTabs.SiteViewTab"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
             xmlns:local="clr-namespace:GalaxySMS.Site.RibbonTabs"
             xmlns:telerik="http://schemas.telerik.com/2008/xaml/presentation"
             xmlns:resx="clr-namespace:GalaxySMS.Site.Properties"
             mc:Ignorable="d"
             Header="{x:Static resx:Resources.SiteViewRibbonTab_Header}">
    <telerik:RadRibbonGroup Header="{x:Static resx:Resources.SiteViewRibbonTab_Header}">
        <telerik:RadRibbonButton Text="{x:Static resx:Resources.SiteViewRibbonTab_RibbonGroupButton1Text}" ToolTip="{x:Static resx:Resources.SiteViewRibbonTab_RibbonGroupButton1ToolTip}"  Command="{Binding RefreshCommand}" />
    </telerik:RadRibbonGroup>
</telerik:RadRibbonTab>
