<core:UserControlViewBase x:Class="GalaxySMSModule_MEF_TelerikRibbonTabs_Template.Views.ViewA"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
             xmlns:prism="http://prismlibrary.com/"
             xmlns:core="http://www.galaxysys.com/GCS/Core"
             xmlns:local="clr-namespace:GalaxySMSModule_MEF_TelerikRibbonTabs_Template.Views"
             xmlns:resx="clr-namespace:GalaxySMSModule_MEF_TelerikRibbonTabs_Template.Properties"
             prism:ViewModelLocator.AutoWireViewModel="True"
             mc:Ignorable="d">
    <core:UserControlViewBase.Resources>

    </core:UserControlViewBase.Resources>
    <Grid>
        <StackPanel HorizontalAlignment="Center" VerticalAlignment="Center" >
            <TextBlock Text="{Binding ViewTitle}" FontSize="20"></TextBlock>
            <Button Content="{x:Static resx:Resources.ViewA_Button1_Text}" ToolTip="{x:Static resx:Resources.ViewA_Button1_ToolTip}" Command="{Binding UpdateCommand}" Width="100"></Button>
        </StackPanel>
    </Grid>
</core:UserControlViewBase>
